{
  "pli-tv-bi-vb-as1:0.1": "Theravāda Vinaya ",
  "pli-tv-bi-vb-as1:0.2": "Bhikkhunivibhaṅga ",
  "pli-tv-bi-vb-as1:0.3": "Adhikaraṇasamatha ",
  "pli-tv-bi-vb-as1:1.1": "Ime kho panāyyāyo satta adhikaraṇasamathā dhammā uddesaṁ āgacchanti. ",
  "pli-tv-bi-vb-as1:2.1": "Uppannuppannānaṁ adhikaraṇānaṁ samathāya vūpasamāya ",
  "pli-tv-bi-vb-as1:2.2": "sammukhāvinayo dātabbo, "
}