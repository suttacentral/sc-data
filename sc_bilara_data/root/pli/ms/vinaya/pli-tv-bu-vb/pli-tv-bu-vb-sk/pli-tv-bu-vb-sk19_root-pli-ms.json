{
  "pli-tv-bu-vb-sk19:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk19:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk19:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk19:0.4": "Ujjagghikavagga ",
  "pli-tv-bu-vb-sk19:0.5": "19. Sīsappacālakasikkhāpada ",
  "pli-tv-bu-vb-sk19:1.0": "",
  "pli-tv-bu-vb-sk19:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk19:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṁ antaraghare gacchanti sīsaṁ olambentā …pe…. ",
  "pli-tv-bu-vb-sk19:1.3.0": "",
  "pli-tv-bu-vb-sk19:1.3.1": "“Na sīsappacālakaṁ antaraghare gamissāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk19:1.4": "Na sīsappacālakaṁ antaraghare gantabbaṁ. ",
  "pli-tv-bu-vb-sk19:1.5": "Sīsaṁ paggahetvā gantabbaṁ. ",
  "pli-tv-bu-vb-sk19:1.6": "Yo anādariyaṁ paṭicca sīsappacālakaṁ antaraghare gacchati sīsaṁ olambento, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk19:1.7.0": "",
  "pli-tv-bu-vb-sk19:1.7.1": "Anāpatti—",
  "pli-tv-bu-vb-sk19:1.8.1": "asañcicca …pe… ",
  "pli-tv-bu-vb-sk19:1.8.2": "",
  "pli-tv-bu-vb-sk19:1.8.3": "",
  "pli-tv-bu-vb-sk19:1.8.4": "",
  "pli-tv-bu-vb-sk19:1.8.5": "",
  "pli-tv-bu-vb-sk19:1.8.6": "",
  "pli-tv-bu-vb-sk19:1.9": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk19:1.10": "Navamasikkhāpadaṁ niṭṭhitaṁ. "
}