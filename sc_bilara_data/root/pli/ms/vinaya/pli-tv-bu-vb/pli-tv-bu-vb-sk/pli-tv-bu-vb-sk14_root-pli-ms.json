{
  "pli-tv-bu-vb-sk14:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk14:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk14:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk14:0.4": "Ujjagghikavagga ",
  "pli-tv-bu-vb-sk14:0.5": "14. Dutiyauccasaddasikkhāpada ",
  "pli-tv-bu-vb-sk14:1.0": "",
  "pli-tv-bu-vb-sk14:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk14:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū uccāsaddaṁ mahāsaddaṁ karontā antaraghare nisīdanti …pe…. ",
  "pli-tv-bu-vb-sk14:1.3.0": "",
  "pli-tv-bu-vb-sk14:1.3.1": "“Appasaddo antaraghare nisīdissāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk14:1.4": "Appasaddena antaraghare nisīditabbaṁ. ",
  "pli-tv-bu-vb-sk14:1.5": "Yo anādariyaṁ paṭicca uccāsaddaṁ mahāsaddaṁ karonto antaraghare nisīdati, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk14:1.6.0": "",
  "pli-tv-bu-vb-sk14:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk14:1.7.1": "asañcicca …pe… ",
  "pli-tv-bu-vb-sk14:1.7.2": "",
  "pli-tv-bu-vb-sk14:1.7.3": "",
  "pli-tv-bu-vb-sk14:1.7.4": "",
  "pli-tv-bu-vb-sk14:1.7.5": "",
  "pli-tv-bu-vb-sk14:1.7.6": "",
  "pli-tv-bu-vb-sk14:1.8": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk14:1.9": "Catutthasikkhāpadaṁ niṭṭhitaṁ. "
}