{
  "pli-tv-bu-vb-sk30:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk30:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk30:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk30:0.4": "Khambhakatavagga",
  "pli-tv-bu-vb-sk30:0.5": "30. Samatittikasikkhāpada",
  "pli-tv-bu-vb-sk30:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk30:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū thūpīkataṁ piṇḍapātaṁ paṭiggaṇhanti …pe….",
  "pli-tv-bu-vb-sk30:1.3.1": "“Samatittikaṁ piṇḍapātaṁ paṭiggahessāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk30:1.4": "Samatittiko piṇḍapāto paṭiggahetabbo.",
  "pli-tv-bu-vb-sk30:1.5": "Yo anādariyaṁ paṭicca thūpīkataṁ piṇḍapātaṁ paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk30:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk30:1.7.1": "asañcicca, ",
  "pli-tv-bu-vb-sk30:1.7.2": "assatiyā, ",
  "pli-tv-bu-vb-sk30:1.7.3": "ajānantassa, ",
  "pli-tv-bu-vb-sk30:1.7.4": "āpadāsu,",
  "pli-tv-bu-vb-sk30:1.8": "ummattakassa,",
  "pli-tv-bu-vb-sk30:1.9": "ādikammikassāti.",
  "pli-tv-bu-vb-sk30:1.10": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bu-vb-sk30:1.11": "Khambhakatavaggo tatiyo."
}