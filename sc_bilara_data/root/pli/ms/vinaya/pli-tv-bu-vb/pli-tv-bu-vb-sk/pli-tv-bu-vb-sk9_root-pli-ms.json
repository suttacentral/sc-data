{
  "pli-tv-bu-vb-sk9:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk9:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk9:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk9:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk9:0.5": "9. Ukkhittakasikkhāpada",
  "pli-tv-bu-vb-sk9:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk9:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū ukkhittakāya antaraghare gacchanti …pe….",
  "pli-tv-bu-vb-sk9:1.3.1": "“Na ukkhittakāya antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk9:1.4": "Na ukkhittakāya antaraghare gantabbaṁ.",
  "pli-tv-bu-vb-sk9:1.5": "Yo anādariyaṁ paṭicca ekato vā ubhato vā ukkhipitvā antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk9:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk9:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk9:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk9:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk9:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk9:1.11": "āpadāsu",
  "pli-tv-bu-vb-sk9:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk9:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk9:1.14": "Navamasikkhāpadaṁ niṭṭhitaṁ."
}