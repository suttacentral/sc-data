{
  "pli-tv-bu-vb-sk59:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk59:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk59:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk59:0.4": "Surusuruvagga ",
  "pli-tv-bu-vb-sk59:0.5": "59. Satthapāṇisikkhāpada ",
  "pli-tv-bu-vb-sk59:1.0": "",
  "pli-tv-bu-vb-sk59:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk59:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū satthapāṇissa dhammaṁ desenti …pe…. ",
  "pli-tv-bu-vb-sk59:1.3.0": "",
  "pli-tv-bu-vb-sk59:1.3.1": "“Na satthapāṇissa agilānassa dhammaṁ desessāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk59:1.4.0": "",
  "pli-tv-bu-vb-sk59:1.4.1": "Satthaṁ nāma ",
  "pli-tv-bu-vb-sk59:1.5": "ekatodhāraṁ ubhatodhāraṁ paharaṇaṁ. ",
  "pli-tv-bu-vb-sk59:1.6": "Na satthapāṇissa agilānassa dhammo desetabbo. ",
  "pli-tv-bu-vb-sk59:1.7": "Yo anādariyaṁ paṭicca satthapāṇissa agilānassa dhammaṁ deseti, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk59:1.8.0": "",
  "pli-tv-bu-vb-sk59:1.8.1": "Anāpatti—",
  "pli-tv-bu-vb-sk59:1.9.1": "asañcicca …pe… ",
  "pli-tv-bu-vb-sk59:1.9.2": "",
  "pli-tv-bu-vb-sk59:1.9.3": "",
  "pli-tv-bu-vb-sk59:1.9.4": "",
  "pli-tv-bu-vb-sk59:1.9.5": "",
  "pli-tv-bu-vb-sk59:1.9.6": "",
  "pli-tv-bu-vb-sk59:1.10": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk59:1.11": "Navamasikkhāpadaṁ niṭṭhitaṁ. "
}