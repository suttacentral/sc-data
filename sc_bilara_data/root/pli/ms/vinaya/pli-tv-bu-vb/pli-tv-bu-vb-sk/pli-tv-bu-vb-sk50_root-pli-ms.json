{
  "pli-tv-bu-vb-sk50:0.1": "Theravāda Vinaya ",
  "pli-tv-bu-vb-sk50:0.2": "Mahāvibhaṅga ",
  "pli-tv-bu-vb-sk50:0.3": "Sekhiyakaṇḍa ",
  "pli-tv-bu-vb-sk50:0.4": "Kabaḷavagga ",
  "pli-tv-bu-vb-sk50:0.5": "50. Capucapukārakasikkhāpada ",
  "pli-tv-bu-vb-sk50:1.0": "",
  "pli-tv-bu-vb-sk50:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme. ",
  "pli-tv-bu-vb-sk50:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū capucapukārakaṁ bhuñjanti …pe… ",
  "pli-tv-bu-vb-sk50:1.3.0": "",
  "pli-tv-bu-vb-sk50:1.3.1": "“Na capucapukārakaṁ bhuñjissāmīti sikkhā karaṇīyā”ti. ",
  "pli-tv-bu-vb-sk50:1.4": "Na capucapukārakaṁ bhuñjitabbaṁ. ",
  "pli-tv-bu-vb-sk50:1.5": "Yo anādariyaṁ paṭicca capucapukārakaṁ bhuñjati, āpatti dukkaṭassa. ",
  "pli-tv-bu-vb-sk50:1.6.0": "",
  "pli-tv-bu-vb-sk50:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk50:1.7.1": "asañcicca …pe… ",
  "pli-tv-bu-vb-sk50:1.7.2": "",
  "pli-tv-bu-vb-sk50:1.7.3": "",
  "pli-tv-bu-vb-sk50:1.7.4": "",
  "pli-tv-bu-vb-sk50:1.7.5": "",
  "pli-tv-bu-vb-sk50:1.7.6": "",
  "pli-tv-bu-vb-sk50:1.8": "ādikammikassāti. ",
  "pli-tv-bu-vb-sk50:1.9": "Dasamasikkhāpadaṁ niṭṭhitaṁ. ",
  "pli-tv-bu-vb-sk50:1.10": "Kabaḷavaggo pañcamo. "
}