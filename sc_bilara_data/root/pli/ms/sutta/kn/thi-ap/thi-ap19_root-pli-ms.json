{
  "thi-ap19:0.1": "Therīapadāna",
  "thi-ap19:0.2": "Ekūposathikavagga",
  "thi-ap19:0.3": "9. Uppalavaṇṇātherīapadāna",
  "thi-ap19:1.1": "“Bhikkhunī uppalavaṇṇā,",
  "thi-ap19:1.2": "iddhiyā pāramiṁ gatā;",
  "thi-ap19:1.3": "Vanditvā satthuno pāde,",
  "thi-ap19:1.4": "idaṁ vacanamabravi.",
  "thi-ap19:2.1": "‘Nitthiṇṇā jātisaṁsāraṁ,",
  "thi-ap19:2.2": "pattāhaṁ acalaṁ padaṁ;",
  "thi-ap19:2.3": "Sabbadukkhaṁ mayā khīṇaṁ,",
  "thi-ap19:2.4": "ārocemi mahāmuni.",
  "thi-ap19:3.1": "Yāvatā parisā atthi,",
  "thi-ap19:3.2": "pasannā jinasāsane;",
  "thi-ap19:3.3": "Yassā ca meparādhotthi,",
  "thi-ap19:3.4": "khamantu jinasammukhā.",
  "thi-ap19:4.1": "Saṁsāre saṁsarantiyā,",
  "thi-ap19:4.2": "khalitaṁ me sace bhave;",
  "thi-ap19:4.3": "Ārocemi mahāvīra,",
  "thi-ap19:4.4": "aparādhaṁ khamassu taṁ’.",
  "thi-ap19:5.1": "‘Iddhiñcāpi nidassehi,",
  "thi-ap19:5.2": "mama sāsanakārike;",
  "thi-ap19:5.3": "Catasso parisā ajja,",
  "thi-ap19:5.4": "kaṅkhaṁ chindāhi yāvatā’.",
  "thi-ap19:6.1": "‘Dhītā tuyhaṁ mahāvīra,",
  "thi-ap19:6.2": "paññavanta jutindhara;",
  "thi-ap19:6.3": "Bahuñca dukkaraṁ kammaṁ,",
  "thi-ap19:6.4": "kataṁ me atidukkaraṁ.",
  "thi-ap19:7.1": "Uppalasseva me vaṇṇo,",
  "thi-ap19:7.2": "nāmenuppalanāmikā;",
  "thi-ap19:7.3": "Sāvikā te mahāvīra,",
  "thi-ap19:7.4": "pāde vandāmi cakkhuma.",
  "thi-ap19:8.1": "Rāhulo ca ahañceva,",
  "thi-ap19:8.2": "nekajātisate bahū;",
  "thi-ap19:8.3": "Ekasmiṁ sambhave jātā,",
  "thi-ap19:8.4": "samānachandamānasā.",
  "thi-ap19:9.1": "Nibbatti ekato hoti,",
  "thi-ap19:9.2": "jātiyāpi ca ekato;",
  "thi-ap19:9.3": "Pacchime bhave sampatte,",
  "thi-ap19:9.4": "ubhopi nānāsambhavā.",
  "thi-ap19:10.1": "Putto ca rāhulo nāma,",
  "thi-ap19:10.2": "dhītā uppalasavhayā;",
  "thi-ap19:10.3": "Passa vīra mamaṁ iddhiṁ,",
  "thi-ap19:10.4": "balaṁ dassemi satthuno’.",
  "thi-ap19:11.1": "Mahāsamudde caturo,",
  "thi-ap19:11.2": "pakkhipi hatthapātiyaṁ;",
  "thi-ap19:11.3": "Telaṁ hatthagatañceva,",
  "thi-ap19:11.4": "khiḍḍo komārako yathā.",
  "thi-ap19:12.1": "Ubbattayitvā pathaviṁ,",
  "thi-ap19:12.2": "pakkhipi hatthapātiyaṁ;",
  "thi-ap19:12.3": "Cittaṁ muñjaṁ yathā nāma,",
  "thi-ap19:12.4": "luñci komārako yuvā.",
  "thi-ap19:13.1": "Cakkavāḷasamaṁ pāṇiṁ,",
  "thi-ap19:13.2": "chādayitvāna matthake;",
  "thi-ap19:13.3": "Vassāpetvāna phusitaṁ,",
  "thi-ap19:13.4": "nānāvaṇṇaṁ punappunaṁ.",
  "thi-ap19:14.1": "Bhūmiṁ udukkhalaṁ katvā,",
  "thi-ap19:14.2": "dhaññaṁ katvāna sakkharaṁ;",
  "thi-ap19:14.3": "Sineruṁ musalaṁ katvā,",
  "thi-ap19:14.4": "maddi komārikā yathā.",
  "thi-ap19:15.1": "‘Dhītāhaṁ buddhaseṭṭhassa,",
  "thi-ap19:15.2": "nāmenuppalasavhayā;",
  "thi-ap19:15.3": "Abhiññāsu vasībhūtā,",
  "thi-ap19:15.4": "tava sāsanakārikā.",
  "thi-ap19:16.1": "Nānāvikubbanaṁ katvā,",
  "thi-ap19:16.2": "dassetvā lokanāyakaṁ;",
  "thi-ap19:16.3": "Nāmagottañca sāvetvā,",
  "thi-ap19:16.4": "pāde vandāmi cakkhuma.",
  "thi-ap19:17.1": "Iddhīsu ca vasī homi,",
  "thi-ap19:17.2": "dibbāya sotadhātuyā;",
  "thi-ap19:17.3": "Cetopariyañāṇassa,",
  "thi-ap19:17.4": "vasī homi mahāmune.",
  "thi-ap19:18.1": "Pubbenivāsaṁ jānāmi,",
  "thi-ap19:18.2": "dibbacakkhu visodhitaṁ;",
  "thi-ap19:18.3": "Sabbāsavā parikkhīṇā,",
  "thi-ap19:18.4": "natthi dāni punabbhavo.",
  "thi-ap19:19.1": "Atthadhammaniruttīsu,",
  "thi-ap19:19.2": "Paṭibhāne tatheva ca;",
  "thi-ap19:19.3": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap19:19.4": "Sabhāvena mahesino.",
  "thi-ap19:20.1": "Purimānaṁ jinaggānaṁ,",
  "thi-ap19:20.2": "saṅgamaṁ te nidassitaṁ;",
  "thi-ap19:20.3": "Adhikāraṁ bahuṁ mayhaṁ,",
  "thi-ap19:20.4": "tuyhatthāya mahāmuni.",
  "thi-ap19:21.1": "Yaṁ mayā pūritaṁ kammaṁ,",
  "thi-ap19:21.2": "Kusalaṁ sara me muni;",
  "thi-ap19:21.3": "Tavatthāya mahāvīra,",
  "thi-ap19:21.4": "Puññaṁ upacitaṁ mayā.",
  "thi-ap19:22.1": "Abhabbaṭṭhāne vajjetvā,",
  "thi-ap19:22.2": "vārayantī anācaraṁ;",
  "thi-ap19:22.3": "Tavatthāya mahāvīra,",
  "thi-ap19:22.4": "cattaṁ me jīvituttamaṁ.",
  "thi-ap19:23.1": "Dasakoṭisahassāni,",
  "thi-ap19:23.2": "adāsiṁ mama jīvitaṁ;",
  "thi-ap19:23.3": "Pariccattā ca me homi,",
  "thi-ap19:23.4": "tavatthāya mahāmuni’.",
  "thi-ap19:24.1": "Tadātivimhitā sabbā,",
  "thi-ap19:24.2": "sirasāva katañjalī;",
  "thi-ap19:24.3": "‘Avocayye kathaṁ āsi,",
  "thi-ap19:24.4": "atuliddhiparakkamā’.",
  "thi-ap19:25.1": "Satasahassito kappe,",
  "thi-ap19:25.2": "nāgakaññā ahaṁ tadā;",
  "thi-ap19:25.3": "Vimalā nāma nāmena,",
  "thi-ap19:25.4": "kaññānaṁ sādhusammatā.",
  "thi-ap19:26.1": "Mahorago mahānāgo,",
  "thi-ap19:26.2": "pasanno jinasāsane;",
  "thi-ap19:26.3": "Padumuttaraṁ mahātejaṁ,",
  "thi-ap19:26.4": "nimantesi sasāvakaṁ.",
  "thi-ap19:27.1": "Ratanamayaṁ maṇḍapaṁ,",
  "thi-ap19:27.2": "pallaṅkaṁ ratanāmayaṁ;",
  "thi-ap19:27.3": "Ratanaṁ vālukākiṇṇaṁ,",
  "thi-ap19:27.4": "upabhogaṁ ratanāmayaṁ.",
  "thi-ap19:28.1": "Maggañca paṭiyādesi,",
  "thi-ap19:28.2": "ratanaddhajabhūsitaṁ;",
  "thi-ap19:28.3": "Paccuggantvāna sambuddhaṁ,",
  "thi-ap19:28.4": "vajjanto tūriyehi so.",
  "thi-ap19:29.1": "Parisāhi ca catūhi,",
  "thi-ap19:29.2": "parivuto lokanāyako;",
  "thi-ap19:29.3": "Mahoragassa bhavane,",
  "thi-ap19:29.4": "nisīdi paramāsane.",
  "thi-ap19:30.1": "Annaṁ pānaṁ khādanīyaṁ,",
  "thi-ap19:30.2": "bhojanañca mahārahaṁ;",
  "thi-ap19:30.3": "Varaṁ varañca pādāsi,",
  "thi-ap19:30.4": "nāgarājā mahāyasaṁ.",
  "thi-ap19:31.1": "Bhuñjitvāna sambuddho,",
  "thi-ap19:31.2": "pattaṁ dhovitvā yoniso;",
  "thi-ap19:31.3": "Anumodanīyaṅkāsi,",
  "thi-ap19:31.4": "nāgakaññā mahiddhikā.",
  "thi-ap19:32.1": "Sabbaññuṁ phullitaṁ disvā,",
  "thi-ap19:32.2": "nāgakaññā mahāyasaṁ;",
  "thi-ap19:32.3": "Pasannaṁ satthuno cittaṁ,",
  "thi-ap19:32.4": "sunibandhañca mānasaṁ.",
  "thi-ap19:33.1": "Mamañca cittamaññāya,",
  "thi-ap19:33.2": "jalajuttamanāmako;",
  "thi-ap19:33.3": "Tasmiṁ khaṇe mahāvīro,",
  "thi-ap19:33.4": "bhikkhuniṁ dassayiddhiyā.",
  "thi-ap19:34.1": "Iddhī anekā dassesi,",
  "thi-ap19:34.2": "bhikkhunī sā visāradā;",
  "thi-ap19:34.3": "Pamoditā vedajātā,",
  "thi-ap19:34.4": "satthāraṁ idamabravi.",
  "thi-ap19:35.1": "‘Addasāhaṁ imaṁ iddhiṁ,",
  "thi-ap19:35.2": "sumanaṁ itarāyapi;",
  "thi-ap19:35.3": "Kathaṁ ahosi sā vīra,",
  "thi-ap19:35.4": "iddhiyā suvisāradā’.",
  "thi-ap19:36.1": "‘Orasā mukhato jātā,",
  "thi-ap19:36.2": "dhītā mama mahiddhikā;",
  "thi-ap19:36.3": "Mamānusāsanikarā,",
  "thi-ap19:36.4": "iddhiyā suvisāradā’.",
  "thi-ap19:37.1": "Buddhassa vacanaṁ sutvā,",
  "thi-ap19:37.2": "evaṁ patthesahaṁ tadā;",
  "thi-ap19:37.3": "‘Ahampi tādisā homi,",
  "thi-ap19:37.4": "iddhiyā suvisāradā.",
  "thi-ap19:38.1": "Pamoditāhaṁ sumanā,",
  "thi-ap19:38.2": "patthe uttamamānasā;",
  "thi-ap19:38.3": "Anāgatamhi addhāne,",
  "thi-ap19:38.4": "īdisā homi nāyaka’.",
  "thi-ap19:39.1": "Maṇimayamhi pallaṅke,",
  "thi-ap19:39.2": "maṇḍapamhi pabhassare;",
  "thi-ap19:39.3": "Annapānena tappetvā,",
  "thi-ap19:39.4": "sasaṅghaṁ lokanāyakaṁ.",
  "thi-ap19:40.1": "Nāgānaṁ pavaraṁ pupphaṁ,",
  "thi-ap19:40.2": "aruṇaṁ nāma uppalaṁ;",
  "thi-ap19:40.3": "‘Vaṇṇaṁ me īdisaṁ hotu’,",
  "thi-ap19:40.4": "pūjesiṁ lokanāyakaṁ.",
  "thi-ap19:41.1": "Tena kammena sukatena,",
  "thi-ap19:41.2": "cetanāpaṇidhīhi ca;",
  "thi-ap19:41.3": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap19:41.4": "tāvatiṁsamagacchahaṁ.",
  "thi-ap19:42.1": "Tato cutāhaṁ manuje,",
  "thi-ap19:42.2": "upapannā sayambhuno;",
  "thi-ap19:42.3": "Uppalehi paṭicchannaṁ,",
  "thi-ap19:42.4": "piṇḍapātamadāsahaṁ.",
  "thi-ap19:43.1": "Ekanavutito kappe,",
  "thi-ap19:43.2": "vipassī nāma nāyako;",
  "thi-ap19:43.3": "Uppajji cārudassano,",
  "thi-ap19:43.4": "sabbadhammesu cakkhumā.",
  "thi-ap19:44.1": "Seṭṭhidhītā tadā hutvā,",
  "thi-ap19:44.2": "bārāṇasipuruttame;",
  "thi-ap19:44.3": "Nimantetvāna sambuddhaṁ,",
  "thi-ap19:44.4": "sasaṅghaṁ lokanāyakaṁ.",
  "thi-ap19:45.1": "Mahādānaṁ daditvāna,",
  "thi-ap19:45.2": "uppalehi vināyakaṁ;",
  "thi-ap19:45.3": "Pūjayitvā cetasāva,",
  "thi-ap19:45.4": "vaṇṇasobhaṁ apatthayiṁ.",
  "thi-ap19:46.1": "Imamhi bhaddake kappe,",
  "thi-ap19:46.2": "brahmabandhu mahāyaso;",
  "thi-ap19:46.3": "Kassapo nāma gottena,",
  "thi-ap19:46.4": "uppajji vadataṁ varo.",
  "thi-ap19:47.1": "Upaṭṭhāko mahesissa,",
  "thi-ap19:47.2": "tadā āsi narissaro;",
  "thi-ap19:47.3": "Kāsirājā kikī nāma,",
  "thi-ap19:47.4": "bārāṇasipuruttame.",
  "thi-ap19:48.1": "Tassāsiṁ dutiyā dhītā,",
  "thi-ap19:48.2": "samaṇaguttasavhayā;",
  "thi-ap19:48.3": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap19:48.4": "pabbajjaṁ samarocayiṁ.",
  "thi-ap19:49.1": "Anujāni na no tāto,",
  "thi-ap19:49.2": "agāreva tadā mayaṁ;",
  "thi-ap19:49.3": "Vīsavassasahassāni,",
  "thi-ap19:49.4": "vicarimha atanditā.",
  "thi-ap19:50.1": "Komāribrahmacariyaṁ,",
  "thi-ap19:50.2": "rājakaññā sukhedhitā;",
  "thi-ap19:50.3": "Buddhopaṭṭhānaniratā,",
  "thi-ap19:50.4": "muditā sattadhītaro.",
  "thi-ap19:51.1": "Samaṇī samaṇaguttā ca,",
  "thi-ap19:51.2": "bhikkhunī bhikkhudāyikā;",
  "thi-ap19:51.3": "Dhammā ceva sudhammā ca,",
  "thi-ap19:51.4": "sattamī saṅghadāyikā.",
  "thi-ap19:52.1": "Ahaṁ khemā ca sappaññā,",
  "thi-ap19:52.2": "paṭācārā ca kuṇḍalā;",
  "thi-ap19:52.3": "Kisāgotamī dhammadinnā,",
  "thi-ap19:52.4": "visākhā hoti sattamī.",
  "thi-ap19:53.1": "Tehi kammehi sukatehi,",
  "thi-ap19:53.2": "cetanāpaṇidhīhi ca;",
  "thi-ap19:53.3": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap19:53.4": "tāvatiṁsamagacchahaṁ.",
  "thi-ap19:54.1": "Tato cutā manussesu,",
  "thi-ap19:54.2": "upapannā mahākule;",
  "thi-ap19:54.3": "Pītaṁ maṭṭhaṁ varaṁ dussaṁ,",
  "thi-ap19:54.4": "adaṁ arahato ahaṁ.",
  "thi-ap19:55.1": "Tato cutāriṭṭhapure,",
  "thi-ap19:55.2": "jātā vippakule ahaṁ;",
  "thi-ap19:55.3": "Dhītā tiriṭivacchassa,",
  "thi-ap19:55.4": "ummādantī manoharā.",
  "thi-ap19:56.1": "Tato cutā janapade,",
  "thi-ap19:56.2": "kule aññatare ahaṁ;",
  "thi-ap19:56.3": "Pasūtā nātiphītamhi,",
  "thi-ap19:56.4": "sāliṁ gopemahaṁ tadā.",
  "thi-ap19:57.1": "Disvā paccekasambuddhaṁ,",
  "thi-ap19:57.2": "pañcalājāsatānihaṁ;",
  "thi-ap19:57.3": "Datvā padumacchannāni,",
  "thi-ap19:57.4": "pañca puttasatānihaṁ.",
  "thi-ap19:58.1": "Patthayiṁ tepi patthesuṁ,",
  "thi-ap19:58.2": "madhuṁ datvā sayambhuno;",
  "thi-ap19:58.3": "Tato cutā araññehaṁ,",
  "thi-ap19:58.4": "ajāyiṁ padumodare.",
  "thi-ap19:59.1": "Kāsirañño mahesīhaṁ,",
  "thi-ap19:59.2": "hutvā sakkatapūjitā;",
  "thi-ap19:59.3": "Ajaniṁ rājaputtānaṁ,",
  "thi-ap19:59.4": "anūnaṁ satapañcakaṁ.",
  "thi-ap19:60.1": "Yadā te yobbanappattā,",
  "thi-ap19:60.2": "kīḷantā jalakīḷitaṁ;",
  "thi-ap19:60.3": "Disvā opattapadumaṁ,",
  "thi-ap19:60.4": "āsuṁ paccekanāyakā.",
  "thi-ap19:61.1": "Sāhaṁ tehi vinābhūtā,",
  "thi-ap19:61.2": "sutavīrehi sokinī;",
  "thi-ap19:61.3": "Cutā isigilipasse,",
  "thi-ap19:61.4": "gāmakamhi ajāyihaṁ.",
  "thi-ap19:62.1": "Yadā buddhā sutamatī,",
  "thi-ap19:62.2": "sutānaṁ bhattunopi ca;",
  "thi-ap19:62.3": "Yāguṁ ādāya gacchantī,",
  "thi-ap19:62.4": "aṭṭha paccekanāyake.",
  "thi-ap19:63.1": "Bhikkhāya gāmaṁ gacchante,",
  "thi-ap19:63.2": "disvā putte anussariṁ;",
  "thi-ap19:63.3": "Khīradhārā viniggacchi,",
  "thi-ap19:63.4": "tadā me puttapemasā.",
  "thi-ap19:64.1": "Tato tesaṁ adaṁ yāguṁ,",
  "thi-ap19:64.2": "pasannā sehi pāṇibhi;",
  "thi-ap19:64.3": "Tato cutāhaṁ tidasaṁ,",
  "thi-ap19:64.4": "nandanaṁ upapajjahaṁ.",
  "thi-ap19:65.1": "Anubhotvā sukhaṁ dukkhaṁ,",
  "thi-ap19:65.2": "saṁsaritvā bhavābhave;",
  "thi-ap19:65.3": "Tavatthāya mahāvīra,",
  "thi-ap19:65.4": "pariccattañca jīvitaṁ.",
  "thi-ap19:66.1": "Evaṁ bahuvidhaṁ dukkhaṁ,",
  "thi-ap19:66.2": "sampattī ca bahubbidhā;",
  "thi-ap19:66.3": "Pacchime bhave sampatte,",
  "thi-ap19:66.4": "jātā sāvatthiyaṁ pure.",
  "thi-ap19:67.1": "Mahādhanaseṭṭhikule,",
  "thi-ap19:67.2": "sukhite sajjite tathā;",
  "thi-ap19:67.3": "Nānāratanapajjote,",
  "thi-ap19:67.4": "sabbakāmasamiddhine.",
  "thi-ap19:68.1": "Sakkatā pūjitā ceva,",
  "thi-ap19:68.2": "mānitāpacitā tathā;",
  "thi-ap19:68.3": "Rūpasobhaggasampannā,",
  "thi-ap19:68.4": "kulesu atisakkatā.",
  "thi-ap19:69.1": "Atīva patthitā cāsiṁ,",
  "thi-ap19:69.2": "rūpabhogasirīhi ca;",
  "thi-ap19:69.3": "Patthitā seṭṭhiputtehi,",
  "thi-ap19:69.4": "anekehi satehipi.",
  "thi-ap19:70.1": "Agāraṁ pajahitvāna,",
  "thi-ap19:70.2": "pabbajiṁ anagāriyaṁ;",
  "thi-ap19:70.3": "Aḍḍhamāse asampatte,",
  "thi-ap19:70.4": "catusaccamapāpuṇiṁ.",
  "thi-ap19:71.1": "Iddhiyā abhinimmitvā,",
  "thi-ap19:71.2": "caturassaṁ rathaṁ ahaṁ;",
  "thi-ap19:71.3": "Buddhassa pāde vandissaṁ,",
  "thi-ap19:71.4": "lokanāthassa tādino.",
  "thi-ap19:72.1": "‘Supupphitaggaṁ upagamma pādapaṁ,",
  "thi-ap19:72.2": "Ekā tuvaṁ tiṭṭhasi sālamūle;",
  "thi-ap19:72.3": "Na cāpi te dutiyo atthi koci,",
  "thi-ap19:72.4": "Bāle na tvaṁ bhāyasi dhuttakānaṁ’.",
  "thi-ap19:73.1": "‘Sataṁ sahassānipi dhuttakānaṁ,",
  "thi-ap19:73.2": "Samāgatā edisakā bhaveyyuṁ;",
  "thi-ap19:73.3": "Lomaṁ na iñje na sampavedhe,",
  "thi-ap19:73.4": "Kiṁ me tuvaṁ māra karissaseko.",
  "thi-ap19:74.1": "Esā antaradhāyāmi,",
  "thi-ap19:74.2": "kucchiṁ vā pavisāmi te;",
  "thi-ap19:74.3": "Bhamukantarikāyampi,",
  "thi-ap19:74.4": "tiṭṭhantiṁ maṁ na dakkhasi.",
  "thi-ap19:75.1": "Cittasmiṁ vasībhūtāmhi,",
  "thi-ap19:75.2": "iddhipādā subhāvitā;",
  "thi-ap19:75.3": "Sabbabandhanamuttāmhi,",
  "thi-ap19:75.4": "na taṁ bhāyāmi āvuso.",
  "thi-ap19:76.1": "Sattisūlūpamā kāmā,",
  "thi-ap19:76.2": "khandhāsaṁ adhikuṭṭanā;",
  "thi-ap19:76.3": "Yaṁ tvaṁ kāmaratiṁ brūsi,",
  "thi-ap19:76.4": "aratī dāni sā mama.",
  "thi-ap19:77.1": "Sabbattha vihatā nandī,",
  "thi-ap19:77.2": "tamokhandho padālito;",
  "thi-ap19:77.3": "Evaṁ jānāhi pāpima,",
  "thi-ap19:77.4": "nihato tvamasi antaka’.",
  "thi-ap19:78.1": "Jino tamhi guṇe tuṭṭho,",
  "thi-ap19:78.2": "etadagge ṭhapesi maṁ;",
  "thi-ap19:78.3": "Aggā iddhimatīnanti,",
  "thi-ap19:78.4": "parisāsu vināyako.",
  "thi-ap19:79.1": "Pariciṇṇo mayā satthā,",
  "thi-ap19:79.2": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap19:79.3": "Ohito garuko bhāro,",
  "thi-ap19:79.4": "bhavanettisamūhatā.",
  "thi-ap19:80.1": "Yassatthāya pabbajitā,",
  "thi-ap19:80.2": "agārasmānagāriyaṁ;",
  "thi-ap19:80.3": "So me attho anuppatto,",
  "thi-ap19:80.4": "sabbasaṁyojanakkhayo.",
  "thi-ap19:81.1": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap19:81.2": "paccayaṁ sayanāsanaṁ;",
  "thi-ap19:81.3": "Khaṇena upanāmenti,",
  "thi-ap19:81.4": "sahassāni samantato.",
  "thi-ap19:82.1": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap19:82.2": "…pe…",
  "thi-ap19:82.3": "viharāmi anāsavā.",
  "thi-ap19:83.1": "Svāgataṁ vata me āsi,",
  "thi-ap19:83.2": "…pe…",
  "thi-ap19:83.3": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap19:84.1": "Paṭisambhidā catasso,",
  "thi-ap19:84.2": "…pe…",
  "thi-ap19:84.3": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap19:85.1": "Itthaṁ sudaṁ uppalavaṇṇā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap19:86.1": "Uppalavaṇṇātheriyāpadānaṁ navamaṁ."
}