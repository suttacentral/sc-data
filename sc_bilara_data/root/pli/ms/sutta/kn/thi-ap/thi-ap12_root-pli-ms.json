{
  "thi-ap12:0.1": "Therīapadāna",
  "thi-ap12:0.2": "Ekūposathikavagga",
  "thi-ap12:0.3": "2 Saḷalapupphikātherīapadāna",
  "thi-ap12:1.1": "“Candabhāgānadītīre,",
  "thi-ap12:1.2": "ahosiṁ kinnarī tadā;",
  "thi-ap12:1.3": "Addasāhaṁ devadevaṁ,",
  "thi-ap12:1.4": "caṅkamantaṁ narāsabhaṁ.",
  "thi-ap12:2.1": "Ocinitvāna saḷalaṁ,",
  "thi-ap12:2.2": "buddhaseṭṭhassadāsahaṁ;",
  "thi-ap12:2.3": "Upasiṅghi mahāvīro,",
  "thi-ap12:2.4": "saḷalaṁ devagandhikaṁ.",
  "thi-ap12:3.1": "Paṭiggahetvā sambuddho,",
  "thi-ap12:3.2": "vipassī lokanāyako;",
  "thi-ap12:3.3": "Upasiṅghi mahāvīro,",
  "thi-ap12:3.4": "pekkhamānāya me tadā.",
  "thi-ap12:4.1": "Añjaliṁ paggahetvāna,",
  "thi-ap12:4.2": "vanditvā dvipaduttamaṁ;",
  "thi-ap12:4.3": "Sakaṁ cittaṁ pasādetvā,",
  "thi-ap12:4.4": "tato pabbatamāruhiṁ.",
  "thi-ap12:5.1": "Ekanavutito kappe,",
  "thi-ap12:5.2": "yaṁ pupphamadadiṁ tadā;",
  "thi-ap12:5.3": "Duggatiṁ nābhijānāmi,",
  "thi-ap12:5.4": "buddhapūjāyidaṁ phalaṁ.",
  "thi-ap12:6.1": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap12:6.2": "…pe…",
  "thi-ap12:6.3": "viharāmi anāsavā.",
  "thi-ap12:7.1": "Svāgataṁ vata me āsi,",
  "thi-ap12:7.2": "…pe…",
  "thi-ap12:7.3": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap12:8.1": "Paṭisambhidā catasso,",
  "thi-ap12:8.2": "…pe…",
  "thi-ap12:8.3": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap12:9.1": "Itthaṁ sudaṁ saḷalapupphikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap12:10.1": "Saḷalapupphikātheriyāpadānaṁ dutiyaṁ."
}