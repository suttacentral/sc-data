{
  "thi-ap32:0.1": "Therīapadāna",
  "thi-ap32:0.2": "Khattiyāvagga",
  "thi-ap32:0.3": "2 Caturāsītibhikkhunīsahassaapadāna",
  "thi-ap32:1.1": "“Cullāsītisahassāni,",
  "thi-ap32:1.2": "brāhmaññakulasambhavā;",
  "thi-ap32:1.3": "Sukhumālahatthapādā,",
  "thi-ap32:1.4": "pure tuyhaṁ mahāmune.",
  "thi-ap32:2.1": "Vessasuddakule jātā,",
  "thi-ap32:2.2": "devā nāgā ca kinnarā;",
  "thi-ap32:2.3": "Cātuddīpā bahū kaññā,",
  "thi-ap32:2.4": "pure tuyhaṁ mahāmune.",
  "thi-ap32:3.1": "Kāci pabbajitā atthi,",
  "thi-ap32:3.2": "sabbadassāvino bahū;",
  "thi-ap32:3.3": "Devā ca kinnarā nāgā,",
  "thi-ap32:3.4": "phusissanti anāgate.",
  "thi-ap32:4.1": "Anubhotvā yasaṁ sabbaṁ,",
  "thi-ap32:4.2": "Patvāna sabbasampadā;",
  "thi-ap32:4.3": "Tumhaṁ pasādaṁ paṭiladdhā,",
  "thi-ap32:4.4": "Bujjhissanti anāgate.",
  "thi-ap32:5.1": "Amhe brāhmaṇadhītā tu,",
  "thi-ap32:5.2": "brāhmaññakulasambhavā;",
  "thi-ap32:5.3": "Pekkhato no mahāvīra,",
  "thi-ap32:5.4": "pāde vandāma cakkhuma.",
  "thi-ap32:6.1": "Upāhatā bhavā sabbe,",
  "thi-ap32:6.2": "mūlataṇhā samūhatā;",
  "thi-ap32:6.3": "Samucchinnā anusayā,",
  "thi-ap32:6.4": "puññasaṅkhāradālitā.",
  "thi-ap32:7.1": "Samādhigocarā sabbā,",
  "thi-ap32:7.2": "samāpattivasī katā;",
  "thi-ap32:7.3": "Jhānena dhammaratiyā,",
  "thi-ap32:7.4": "viharissāma no sadā.",
  "thi-ap32:8.1": "Bhavanetti avijjā ca,",
  "thi-ap32:8.2": "saṅkhārāpi ca khepitā;",
  "thi-ap32:8.3": "Sududdasaṁ padaṁ gantvā,",
  "thi-ap32:8.4": "anujānātha nāyaka”.",
  "thi-ap32:9.1": "“Upakārā mamaṁ tumhe,",
  "thi-ap32:9.2": "dīgharattaṁ katāvino;",
  "thi-ap32:9.3": "Catunnaṁ saṁsayaṁ chetvā,",
  "thi-ap32:9.4": "sabbā gacchantu nibbutiṁ”.",
  "thi-ap32:10.1": "Vanditvā munino pāde,",
  "thi-ap32:10.2": "katvā iddhivikubbanaṁ;",
  "thi-ap32:10.3": "Kāci dassenti ālokaṁ,",
  "thi-ap32:10.4": "andhakāramathāparā.",
  "thi-ap32:11.1": "Dassenti candasūriye,",
  "thi-ap32:11.2": "sāgarañca samacchakaṁ;",
  "thi-ap32:11.3": "Sineruṁ paribhaṇḍañca,",
  "thi-ap32:11.4": "dassenti pārichattakaṁ.",
  "thi-ap32:12.1": "Tāvatiṁsañca bhavanaṁ,",
  "thi-ap32:12.2": "yāmaṁ dassenti iddhiyā;",
  "thi-ap32:12.3": "Tusitaṁ nimmite deve,",
  "thi-ap32:12.4": "vasavattī mahissare.",
  "thi-ap32:13.1": "Brahmāno kāci dassenti,",
  "thi-ap32:13.2": "caṅkamañca mahārahaṁ;",
  "thi-ap32:13.3": "Brahmavaṇṇañca māpetvā,",
  "thi-ap32:13.4": "dhammaṁ desenti suññataṁ.",
  "thi-ap32:14.1": "Nānāvikubbanaṁ katvā,",
  "thi-ap32:14.2": "iddhiṁ dassiya satthuno;",
  "thi-ap32:14.3": "Dassayiṁsu balaṁ sabbā,",
  "thi-ap32:14.4": "pāde vandiṁsu satthuno.",
  "thi-ap32:15.1": "“Iddhīsu ca vasī homa,",
  "thi-ap32:15.2": "dibbāya sotadhātuyā;",
  "thi-ap32:15.3": "Cetopariyañāṇassa,",
  "thi-ap32:15.4": "vasī homa mahāmune.",
  "thi-ap32:16.1": "Pubbenivāsaṁ jānāma,",
  "thi-ap32:16.2": "dibbacakkhu visodhitaṁ;",
  "thi-ap32:16.3": "Sabbāsavā parikkhīṇā,",
  "thi-ap32:16.4": "natthi dāni punabbhavo.",
  "thi-ap32:17.1": "Atthadhammaniruttīsu,",
  "thi-ap32:17.2": "paṭibhāne tatheva ca;",
  "thi-ap32:17.3": "Ñāṇaṁ amhaṁ mahāvīra,",
  "thi-ap32:17.4": "uppannaṁ tava santike.",
  "thi-ap32:18.1": "Pubbānaṁ lokanāthānaṁ,",
  "thi-ap32:18.2": "saṅgamaṁ no nidassitaṁ;",
  "thi-ap32:18.3": "Adhikāraṁ bahuṁ amhaṁ,",
  "thi-ap32:18.4": "tuyhatthāya mahāmune.",
  "thi-ap32:19.1": "Yaṁ amhehi kataṁ kammaṁ,",
  "thi-ap32:19.2": "kusalaṁ sara taṁ mune;",
  "thi-ap32:19.3": "Tuyhatthāya mahāvīra,",
  "thi-ap32:19.4": "puññānupacitāni no.",
  "thi-ap32:20.1": "Satasahassito kappe,",
  "thi-ap32:20.2": "padumuttaro mahāmuni;",
  "thi-ap32:20.3": "Puraṁ haṁsavatī nāma,",
  "thi-ap32:20.4": "sambuddhassa kulāsayaṁ.",
  "thi-ap32:21.1": "Dvārena haṁsavatiyā,",
  "thi-ap32:21.2": "gaṅgā sandati sabbadā;",
  "thi-ap32:21.3": "Ubbāḷhā nadiyā bhikkhū,",
  "thi-ap32:21.4": "gamanaṁ na labhanti te.",
  "thi-ap32:22.1": "Divasaṁ dve tayo ceva,",
  "thi-ap32:22.2": "sattāhaṁ māsikaṁ tato;",
  "thi-ap32:22.3": "Catumāsampi sampuṇṇaṁ,",
  "thi-ap32:22.4": "gamanaṁ na labhanti te.",
  "thi-ap32:23.1": "Tadā ahu sattasāro,",
  "thi-ap32:23.2": "jaṭilo nāma raṭṭhiko;",
  "thi-ap32:23.3": "Oruddhe bhikkhavo disvā,",
  "thi-ap32:23.4": "setuṁ gaṅgāya kārayi.",
  "thi-ap32:24.1": "Tadā satasahassehi,",
  "thi-ap32:24.2": "setuṁ gaṅgāya kārayi;",
  "thi-ap32:24.3": "Saṅghassa orime tīre,",
  "thi-ap32:24.4": "vihārañca akārayi.",
  "thi-ap32:25.1": "Itthiyo purisā ceva,",
  "thi-ap32:25.2": "uccanīcakulāni ca;",
  "thi-ap32:25.3": "Tassa setuṁ vihārañca,",
  "thi-ap32:25.4": "samabhāgaṁ akaṁsu te.",
  "thi-ap32:26.1": "Amhe aññe ca mānujā,",
  "thi-ap32:26.2": "vippasannena cetasā;",
  "thi-ap32:26.3": "Tassa dhammesu dāyādā,",
  "thi-ap32:26.4": "nagare janapadesu ca.",
  "thi-ap32:27.1": "Itthī pumā kumārā ca,",
  "thi-ap32:27.2": "bahū ceva kumārikā;",
  "thi-ap32:27.3": "Setuno ca vihārassa,",
  "thi-ap32:27.4": "vālukā ākiriṁsu te.",
  "thi-ap32:28.1": "Vīthiṁ sammajjanaṁ katvā,",
  "thi-ap32:28.2": "kadalīpuṇṇaghaṭe dhaje;",
  "thi-ap32:28.3": "Dhūpaṁ cuṇṇañca mālañca,",
  "thi-ap32:28.4": "kāraṁ katvāna satthuno.",
  "thi-ap32:29.1": "Setuvihāre kāretvā,",
  "thi-ap32:29.2": "nimantetvā vināyakaṁ;",
  "thi-ap32:29.3": "Mahādānaṁ daditvāna,",
  "thi-ap32:29.4": "sambodhiṁ abhipatthayiṁ.",
  "thi-ap32:30.1": "Padumuttaro mahāvīro,",
  "thi-ap32:30.2": "tārako sabbapāṇinaṁ;",
  "thi-ap32:30.3": "Anumodanīyaṅkāsi,",
  "thi-ap32:30.4": "jaṭilassa mahāmuni.",
  "thi-ap32:31.1": "‘Satasahassātikkante,",
  "thi-ap32:31.2": "kappo hessati bhaddako;",
  "thi-ap32:31.3": "Bhavābhavenubhotvāna,",
  "thi-ap32:31.4": "pāpuṇissati bodhiyaṁ.",
  "thi-ap32:32.1": "Kāci hatthaparikammaṁ,",
  "thi-ap32:32.2": "katāvī naranāriyo;",
  "thi-ap32:32.3": "Anāgatamhi addhāne,",
  "thi-ap32:32.4": "sabbā hessanti sammukhā’.",
  "thi-ap32:33.1": "Tena kammavipākena,",
  "thi-ap32:33.2": "cetanāpaṇidhīhi ca;",
  "thi-ap32:33.3": "Uppannā devabhavanaṁ,",
  "thi-ap32:33.4": "tuyhaṁ tā paricārikā.",
  "thi-ap32:34.1": "Dibbasukhaṁ asaṅkhiyaṁ,",
  "thi-ap32:34.2": "mānusañca asaṅkhiyaṁ;",
  "thi-ap32:34.3": "Tuyhaṁ te paricārema,",
  "thi-ap32:34.4": "saṁsarimha bhavābhave.",
  "thi-ap32:35.1": "Satasahassito kappe,",
  "thi-ap32:35.2": "sukataṁ kammasampadaṁ;",
  "thi-ap32:35.3": "Sukhumālī manussānaṁ,",
  "thi-ap32:35.4": "atho devapure vare.",
  "thi-ap32:36.1": "Rūpabhogayase ceva,",
  "thi-ap32:36.2": "atho kittiñca sakkataṁ;",
  "thi-ap32:36.3": "Labhāma satataṁ sabbaṁ,",
  "thi-ap32:36.4": "sukataṁ kammasampadaṁ.",
  "thi-ap32:37.1": "Pacchime bhave sampatte,",
  "thi-ap32:37.2": "jātāmha brāhmaṇe kule;",
  "thi-ap32:37.3": "Sukhumālahatthapādā,",
  "thi-ap32:37.4": "sakyaputtanivesane.",
  "thi-ap32:38.1": "Sabbakālampi pathaviṁ,",
  "thi-ap32:38.2": "na passāma na laṅkataṁ;",
  "thi-ap32:38.3": "Cikkhallabhūmimasuciṁ,",
  "thi-ap32:38.4": "na passāma mahāmune.",
  "thi-ap32:39.1": "Agāraṁ vasante amhe,",
  "thi-ap32:39.2": "sakkāraṁ sabbakālikaṁ;",
  "thi-ap32:39.3": "Upanenti sadā sabbaṁ,",
  "thi-ap32:39.4": "pubbakammaphalena no.",
  "thi-ap32:40.1": "Agāraṁ pajahitvāna,",
  "thi-ap32:40.2": "pabbajitvānagāriyaṁ;",
  "thi-ap32:40.3": "Saṁsārapathanitthiṇṇā,",
  "thi-ap32:40.4": "vītarāgā bhavāmase.",
  "thi-ap32:41.1": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap32:41.2": "paccayaṁ sayanāsanaṁ;",
  "thi-ap32:41.3": "Upanenti sadā amhe,",
  "thi-ap32:41.4": "sahassāni tato tato.",
  "thi-ap32:42.1": "Kilesā jhāpitā amhaṁ,",
  "thi-ap32:42.2": "…pe…",
  "thi-ap32:42.3": "viharāma anāsavā.",
  "thi-ap32:43.1": "Svāgataṁ vata no āsi,",
  "thi-ap32:43.2": "…pe…",
  "thi-ap32:43.3": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap32:44.1": "Paṭisambhidā catasso,",
  "thi-ap32:44.2": "…pe…",
  "thi-ap32:44.3": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap32:45.1": "Itthaṁ sudaṁ caturāsītibrāhmaṇakaññābhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap32:46.1": "Caturāsītibhikkhunīsahassāpadānaṁ dutiyaṁ."
}