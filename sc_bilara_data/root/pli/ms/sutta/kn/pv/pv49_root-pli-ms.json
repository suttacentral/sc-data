{
  "pv49:0.1": "Petavatthu",
  "pv49:0.2": "Mahāvagga",
  "pv49:0.3": "14. Bhogasaṁharaṇapetavatthu",
  "pv49:1.1": "“Mayaṁ bhoge saṁharimhā,",
  "pv49:1.2": "samena visamena ca;",
  "pv49:1.3": "Te aññe paribhuñjanti,",
  "pv49:1.4": "mayaṁ dukkhassa bhāginī”ti.",
  "pv49:2.1": "Bhogasaṁharaṇapetavatthu cuddasamaṁ."
}