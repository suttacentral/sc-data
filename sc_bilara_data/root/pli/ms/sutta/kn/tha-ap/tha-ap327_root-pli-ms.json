{
  "tha-ap327:0.1": "Therāpadāna",
  "tha-ap327:0.2": "Umāpupphiyavagga",
  "tha-ap327:0.3": "5. Nimittasaññakattheraapadāna",
  "tha-ap327:1.1": "“Candabhāgānadītīre,",
  "tha-ap327:1.2": "vasāmi assame ahaṁ;",
  "tha-ap327:1.3": "Suvaṇṇamigamaddakkhiṁ,",
  "tha-ap327:1.4": "carantaṁ vipine ahaṁ.",
  "tha-ap327:2.1": "Mige cittaṁ pasādetvā,",
  "tha-ap327:2.2": "lokajeṭṭhaṁ anussariṁ;",
  "tha-ap327:2.3": "Tena cittappasādena,",
  "tha-ap327:2.4": "aññe buddhe anussariṁ.",
  "tha-ap327:3.1": "Abbhatītā ca ye buddhā,",
  "tha-ap327:3.2": "vattamānā anāgatā;",
  "tha-ap327:3.3": "Evamevaṁ virocanti,",
  "tha-ap327:3.4": "migarājāva te tayo.",
  "tha-ap327:4.1": "Catunnavutito kappe,",
  "tha-ap327:4.2": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap327:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap327:4.4": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap327:5.1": "Sattavīse ito kappe,",
  "tha-ap327:5.2": "eko āsiṁ mahīpati;",
  "tha-ap327:5.3": "Araññasattho nāmena,",
  "tha-ap327:5.4": "cakkavattī mahabbalo.",
  "tha-ap327:6.1": "Paṭisambhidā catasso,",
  "tha-ap327:6.2": "…pe…",
  "tha-ap327:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap327:7.1": "Itthaṁ sudaṁ āyasmā nimittasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap327:8.1": "Nimittasaññakattherassāpadānaṁ pañcamaṁ."
}