{
  "tha-ap244:0.1": "Therāpadāna",
  "tha-ap244:0.2": "Tuvaradāyakavagga",
  "tha-ap244:0.3": "2. Nāgakesariyattheraapadāna",
  "tha-ap244:1.1": "“Dhanuṁ advejjhaṁ katvāna,",
  "tha-ap244:1.2": "vanamajjhogahiṁ ahaṁ;",
  "tha-ap244:1.3": "Kesaraṁ ogataṁ disvā,",
  "tha-ap244:1.4": "patapattaṁ samuṭṭhitaṁ.",
  "tha-ap244:2.1": "Ubho hatthehi paggayha,",
  "tha-ap244:2.2": "sire katvāna añjaliṁ;",
  "tha-ap244:2.3": "Buddhassa abhiropesiṁ,",
  "tha-ap244:2.4": "tissassa lokabandhuno.",
  "tha-ap244:3.1": "Dvenavute ito kappe,",
  "tha-ap244:3.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap244:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap244:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap244:4.1": "Tesattatimhi kappamhi,",
  "tha-ap244:4.2": "satta kesaranāmakā;",
  "tha-ap244:4.3": "Sattaratanasampannā,",
  "tha-ap244:4.4": "cakkavattī mahabbalā.",
  "tha-ap244:5.1": "Paṭisambhidā catasso,",
  "tha-ap244:5.2": "…pe…",
  "tha-ap244:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap244:6.1": "Itthaṁ sudaṁ āyasmā nāgakesariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap244:7.1": "Nāgakesariyattherassāpadānaṁ dutiyaṁ."
}