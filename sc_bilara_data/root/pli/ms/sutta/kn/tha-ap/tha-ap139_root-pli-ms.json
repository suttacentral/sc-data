{
  "tha-ap139:0.1": "Therāpadāna",
  "tha-ap139:0.2": "Sobhitavagga",
  "tha-ap139:0.3": "7. Atthasandassakattheraapadāna",
  "tha-ap139:1.1": "“Visālamāḷe āsīno,",
  "tha-ap139:1.2": "addasaṁ lokanāyakaṁ;",
  "tha-ap139:1.3": "Khīṇāsavaṁ balappattaṁ,",
  "tha-ap139:1.4": "bhikkhusaṅghapurakkhataṁ.",
  "tha-ap139:2.1": "Satasahassā tevijjā,",
  "tha-ap139:2.2": "chaḷabhiññā mahiddhikā;",
  "tha-ap139:2.3": "Parivārenti sambuddhaṁ,",
  "tha-ap139:2.4": "ko disvā nappasīdati.",
  "tha-ap139:3.1": "Ñāṇe upanidhā yassa,",
  "tha-ap139:3.2": "na vijjati sadevake;",
  "tha-ap139:3.3": "Anantañāṇaṁ sambuddhaṁ,",
  "tha-ap139:3.4": "ko disvā nappasīdati.",
  "tha-ap139:4.1": "Dhammakāyañca dīpentaṁ,",
  "tha-ap139:4.2": "kevalaṁ ratanākaraṁ;",
  "tha-ap139:4.3": "Vikappetuṁ na sakkonti,",
  "tha-ap139:4.4": "ko disvā nappasīdati.",
  "tha-ap139:5.1": "Imāhi tīhi gāthāhi,",
  "tha-ap139:5.2": "nāradovhayavacchalo;",
  "tha-ap139:5.3": "Padumuttaraṁ thavitvāna,",
  "tha-ap139:5.4": "sambuddhaṁ aparājitaṁ.",
  "tha-ap139:6.1": "Tena cittappasādena,",
  "tha-ap139:6.2": "buddhasanthavanena ca;",
  "tha-ap139:6.3": "Kappānaṁ satasahassaṁ,",
  "tha-ap139:6.4": "duggatiṁ nupapajjahaṁ.",
  "tha-ap139:7.1": "Ito tiṁsakappasate,",
  "tha-ap139:7.2": "sumitto nāma khattiyo;",
  "tha-ap139:7.3": "Sattaratanasampanno,",
  "tha-ap139:7.4": "cakkavattī mahabbalo.",
  "tha-ap139:8.1": "Paṭisambhidā catasso,",
  "tha-ap139:8.2": "…pe…",
  "tha-ap139:8.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap139:9.1": "Itthaṁ sudaṁ āyasmā atthasandassako thero imā gāthāyo abhāsitthāti.",
  "tha-ap139:10.1": "Atthasandassakattherassāpadānaṁ sattamaṁ."
}