{
  "tha-ap311:0.1": "Therāpadāna",
  "tha-ap311:0.2": "Padumakesaravagga",
  "tha-ap311:0.3": "9 Buddhasaññakattheraapadāna",
  "tha-ap311:1.1": "“Udentaṁ sataraṁsiṁva,",
  "tha-ap311:1.2": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap311:1.3": "Vanantaragataṁ santaṁ,",
  "tha-ap311:1.4": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap311:2.1": "Addasaṁ supinantena,",
  "tha-ap311:2.2": "siddhatthaṁ lokanāyakaṁ;",
  "tha-ap311:2.3": "Tattha cittaṁ pasādetvā,",
  "tha-ap311:2.4": "sugatiṁ upapajjahaṁ.",
  "tha-ap311:3.1": "Catunnavutito kappe,",
  "tha-ap311:3.2": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap311:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap311:3.4": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap311:4.1": "Paṭisambhidā catasso,",
  "tha-ap311:4.2": "…pe…",
  "tha-ap311:4.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap311:5.1": "Itthaṁ sudaṁ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap311:6.1": "Buddhasaññakattherassāpadānaṁ navamaṁ."
}