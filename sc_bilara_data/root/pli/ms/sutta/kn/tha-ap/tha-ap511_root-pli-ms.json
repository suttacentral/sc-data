{
  "tha-ap511:0.1": "Therāpadāna",
  "tha-ap511:0.2": "Kaṇikāravagga",
  "tha-ap511:0.3": "9 Tālaphaladāyakattheraapadāna",
  "tha-ap511:1.1": "“Sataraṁsī nāma bhagavā,",
  "tha-ap511:1.2": "sayambhū aparājito;",
  "tha-ap511:1.3": "Vivekā vuṭṭhahitvāna,",
  "tha-ap511:1.4": "gocarāyābhinikkhami.",
  "tha-ap511:2.1": "Phalahattho ahaṁ disvā,",
  "tha-ap511:2.2": "upagacchiṁ narāsabhaṁ;",
  "tha-ap511:2.3": "Pasannacitto sumano,",
  "tha-ap511:2.4": "tālaphalaṁ adāsahaṁ.",
  "tha-ap511:3.1": "Catunnavutito kappe,",
  "tha-ap511:3.2": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap511:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap511:3.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap511:4.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap511:4.2": "…pe…",
  "tha-ap511:4.3": "viharāmi anāsavo.",
  "tha-ap511:5.1": "Svāgataṁ vata me āsi,",
  "tha-ap511:5.2": "…pe…",
  "tha-ap511:5.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap511:6.1": "Paṭisambhidā catasso,",
  "tha-ap511:6.2": "…pe…",
  "tha-ap511:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap511:7.1": "Itthaṁ sudaṁ āyasmā tālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap511:8.1": "Tālaphaladāyakattherassāpadānaṁ navamaṁ."
}