{
  "tha-ap527:0.1": "Therāpadāna",
  "tha-ap527:0.2": "Tiṇadāyakavagga",
  "tha-ap527:0.3": "5. Supaṭadāyakattheraapadāna",
  "tha-ap527:1.1": "“Divāvihārā nikkhanto,",
  "tha-ap527:1.2": "vipassī lokanāyako;",
  "tha-ap527:1.3": "Lahuṁ supaṭakaṁ datvā,",
  "tha-ap527:1.4": "kappaṁ saggamhi modahaṁ.",
  "tha-ap527:2.1": "Ekanavutito kappe,",
  "tha-ap527:2.2": "supaṭakamadāsahaṁ;",
  "tha-ap527:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap527:2.4": "supaṭassa idaṁ phalaṁ.",
  "tha-ap527:3.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap527:3.2": "…pe…",
  "tha-ap527:3.3": "viharāmi anāsavo.",
  "tha-ap527:4.1": "Svāgataṁ vata me āsi,",
  "tha-ap527:4.2": "…pe…",
  "tha-ap527:4.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap527:5.1": "Paṭisambhidā catasso,",
  "tha-ap527:5.2": "…pe…",
  "tha-ap527:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap527:6.1": "Itthaṁ sudaṁ āyasmā supaṭadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap527:7.1": "Supaṭadāyakattherassāpadānaṁ pañcamaṁ."
}