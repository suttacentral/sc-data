{
  "tha-ap353:0.1": "Therāpadāna",
  "tha-ap353:0.2": "Saddasaññakavagga",
  "tha-ap353:0.3": "1. Saddasaññakattheraapadāna",
  "tha-ap353:1.1": "“Migaluddo pure āsiṁ,",
  "tha-ap353:1.2": "araññe kānane ahaṁ;",
  "tha-ap353:1.3": "Tatthaddasāsiṁ sambuddhaṁ,",
  "tha-ap353:1.4": "devasaṅghapurakkhataṁ.",
  "tha-ap353:2.1": "Catusaccaṁ pakāsentaṁ,",
  "tha-ap353:2.2": "uddharantaṁ mahājanaṁ;",
  "tha-ap353:2.3": "Assosiṁ madhuraṁ vācaṁ,",
  "tha-ap353:2.4": "karavīkarudopamaṁ.",
  "tha-ap353:3.1": "Brahmasarassa munino,",
  "tha-ap353:3.2": "sikhino lokabandhuno;",
  "tha-ap353:3.3": "Ghose cittaṁ pasādetvā,",
  "tha-ap353:3.4": "pattomhi āsavakkhayaṁ.",
  "tha-ap353:4.1": "Ekattiṁse ito kappe,",
  "tha-ap353:4.2": "yaṁ kammamakariṁ tadā;",
  "tha-ap353:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap353:4.4": "pasādassa idaṁ phalaṁ.",
  "tha-ap353:5.1": "Paṭisambhidā catasso,",
  "tha-ap353:5.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap353:5.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap353:5.4": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap353:6.1": "Itthaṁ sudaṁ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap353:7.1": "Saddasaññakattherassāpadānaṁ paṭhamaṁ."
}