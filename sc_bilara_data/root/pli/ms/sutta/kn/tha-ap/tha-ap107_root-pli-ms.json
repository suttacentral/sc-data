{
  "tha-ap107:0.1": "Therāpadāna",
  "tha-ap107:0.2": "Bhikkhadāyivagga",
  "tha-ap107:0.3": "5. Muṭṭhipupphiyattheraapadāna",
  "tha-ap107:1.1": "“Sudassanoti nāmena,",
  "tha-ap107:1.2": "mālākāro ahaṁ tadā;",
  "tha-ap107:1.3": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap107:1.4": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap107:2.1": "Jātipupphaṁ gahetvāna,",
  "tha-ap107:2.2": "pūjayiṁ padumuttaraṁ;",
  "tha-ap107:2.3": "Visuddhacakkhu sumano,",
  "tha-ap107:2.4": "dibbacakkhuṁ samajjhagaṁ.",
  "tha-ap107:3.1": "Etissā pupphapūjāya,",
  "tha-ap107:3.2": "cittassa paṇidhīhi ca;",
  "tha-ap107:3.3": "Kappānaṁ satasahassaṁ,",
  "tha-ap107:3.4": "duggatiṁ nupapajjahaṁ.",
  "tha-ap107:4.1": "Soḷasāsiṁsu rājāno,",
  "tha-ap107:4.2": "devuttarasanāmakā;",
  "tha-ap107:4.3": "Chattiṁsamhi ito kappe,",
  "tha-ap107:4.4": "cakkavattī mahabbalā.",
  "tha-ap107:5.1": "Paṭisambhidā catasso,",
  "tha-ap107:5.2": "…pe…",
  "tha-ap107:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap107:6.1": "Itthaṁ sudaṁ āyasmā muṭṭhipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap107:7.1": "Muṭṭhipupphiyattherassāpadānaṁ pañcamaṁ."
}