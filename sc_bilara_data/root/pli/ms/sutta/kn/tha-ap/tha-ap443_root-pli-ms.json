{
  "tha-ap443:0.1": "Therāpadāna",
  "tha-ap443:0.2": "Vibhītakavagga",
  "tha-ap443:0.3": "1 Vibhītakamiñjiyattheraapadāna",
  "tha-ap443:1.1": "“Kakusandho mahāvīro,",
  "tha-ap443:1.2": "sabbadhammāna pāragū;",
  "tha-ap443:1.3": "Gaṇamhā vūpakaṭṭho so,",
  "tha-ap443:1.4": "agamāsi vanantaraṁ.",
  "tha-ap443:2.1": "Bījamiñjaṁ gahetvāna,",
  "tha-ap443:2.2": "latāya āvuṇiṁ ahaṁ;",
  "tha-ap443:2.3": "Bhagavā tamhi samaye,",
  "tha-ap443:2.4": "jhāyate pabbatantare.",
  "tha-ap443:3.1": "Disvānahaṁ devadevaṁ,",
  "tha-ap443:3.2": "vippasannena cetasā;",
  "tha-ap443:3.3": "Dakkhiṇeyyassa vīrassa,",
  "tha-ap443:3.4": "bījamiñjamadāsahaṁ.",
  "tha-ap443:4.1": "Imasmiṁyeva kappamhi,",
  "tha-ap443:4.2": "yaṁ miñjamadadiṁ tadā;",
  "tha-ap443:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap443:4.4": "bījamiñjassidaṁ phalaṁ.",
  "tha-ap443:5.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap443:5.2": "bhavā sabbe samūhatā;",
  "tha-ap443:5.3": "Nāgova bandhanaṁ chetvā,",
  "tha-ap443:5.4": "viharāmi anāsavo.",
  "tha-ap443:6.1": "Svāgataṁ vata me āsi,",
  "tha-ap443:6.2": "mama buddhassa santike;",
  "tha-ap443:6.3": "Tisso vijjā anuppattā,",
  "tha-ap443:6.4": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap443:7.1": "Paṭisambhidā catasso,",
  "tha-ap443:7.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap443:7.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap443:7.4": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap443:8.1": "Itthaṁ sudaṁ āyasmā vibhītakamiñjiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap443:9.1": "Vibhītakamiñjiyattherassāpadānaṁ paṭhamaṁ."
}