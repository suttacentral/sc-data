{
  "tha-ap529:0.1": "Therāpadāna",
  "tha-ap529:0.2": "Tiṇadāyakavagga",
  "tha-ap529:0.3": "7. Girinelapūjakattheraapadāna",
  "tha-ap529:1.1": "“Migaluddo pure āsiṁ,",
  "tha-ap529:1.2": "vipine vicaraṁ ahaṁ;",
  "tha-ap529:1.3": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap529:1.4": "sabbadhammāna pāraguṁ.",
  "tha-ap529:2.1": "Tasmiṁ mahākāruṇike,",
  "tha-ap529:2.2": "sabbasattahite rate;",
  "tha-ap529:2.3": "Pasannacitto sumano,",
  "tha-ap529:2.4": "nelapupphamapūjayiṁ.",
  "tha-ap529:3.1": "Ekatiṁse ito kappe,",
  "tha-ap529:3.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap529:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap529:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap529:4.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap529:4.2": "…pe…",
  "tha-ap529:4.3": "viharāmi anāsavo.",
  "tha-ap529:5.1": "Svāgataṁ vata me āsi,",
  "tha-ap529:5.2": "…pe…",
  "tha-ap529:5.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap529:6.1": "Paṭisambhidā catasso,",
  "tha-ap529:6.2": "…pe…",
  "tha-ap529:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap529:7.1": "Itthaṁ sudaṁ āyasmā girinelapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap529:8.1": "Girinelapūjakattherassāpadānaṁ sattamaṁ."
}