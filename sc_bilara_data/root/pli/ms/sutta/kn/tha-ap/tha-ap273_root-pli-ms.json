{
  "tha-ap273:0.1": "Therāpadāna",
  "tha-ap273:0.2": "Suvaṇṇabibbohanavagga",
  "tha-ap273:0.3": "1 Suvaṇṇabibbohaniyattheraapadāna",
  "tha-ap273:1.1": "“Ekāsanaṁ ahamadaṁ,",
  "tha-ap273:1.2": "pasanno sehi pāṇibhi;",
  "tha-ap273:1.3": "Bibbohanañca pādāsiṁ,",
  "tha-ap273:1.4": "uttamatthassa pattiyā.",
  "tha-ap273:2.1": "Ekanavutito kappe,",
  "tha-ap273:2.2": "bibbohanamadāsahaṁ;",
  "tha-ap273:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap273:2.4": "bibbohanassidaṁ phalaṁ.",
  "tha-ap273:3.1": "Ito tesaṭṭhime kappe,",
  "tha-ap273:3.2": "asamo nāma khattiyo;",
  "tha-ap273:3.3": "Sattaratanasampanno,",
  "tha-ap273:3.4": "cakkavattī mahabbalo.",
  "tha-ap273:4.1": "Paṭisambhidā catasso,",
  "tha-ap273:4.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap273:4.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap273:4.4": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap273:5.1": "Itthaṁ sudaṁ āyasmā suvaṇṇabibbohaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap273:6.1": "Suvaṇṇabibbohaniyattherassāpadānaṁ paṭhamaṁ."
}