{
  "tha-ap56:0.1": "Therāpadāna",
  "tha-ap56:0.2": "Bījanivagga",
  "tha-ap56:0.3": "4 Gandhodakiyattheraapadāna",
  "tha-ap56:1.1": "“Padumuttarabuddhassa,",
  "tha-ap56:1.2": "mahābodhimaho ahu;",
  "tha-ap56:1.3": "Vicittaṁ ghaṭamādāya,",
  "tha-ap56:1.4": "gandhodakamadāsahaṁ.",
  "tha-ap56:2.1": "Nhānakāle ca bodhiyā,",
  "tha-ap56:2.2": "mahāmegho pavassatha;",
  "tha-ap56:2.3": "Ninnādo ca mahā āsi,",
  "tha-ap56:2.4": "asaniyā phalantiyā.",
  "tha-ap56:3.1": "Tenevāsanivegena,",
  "tha-ap56:3.2": "tattha kālaṅkato ahaṁ;",
  "tha-ap56:3.3": "Devaloke ṭhito santo,",
  "tha-ap56:3.4": "imā gāthā abhāsahaṁ.",
  "tha-ap56:4.1": "‘Aho buddhā aho dhammā,",
  "tha-ap56:4.2": "aho no satthusampadā;",
  "tha-ap56:4.3": "Kaḷevaraṁ me patitaṁ,",
  "tha-ap56:4.4": "devaloke ramāmahaṁ.",
  "tha-ap56:5.1": "Ubbiddhaṁ bhavanaṁ mayhaṁ,",
  "tha-ap56:5.2": "Satabhūmaṁ samuggataṁ;",
  "tha-ap56:5.3": "Kaññāsatasahassāni,",
  "tha-ap56:5.4": "Parivārenti maṁ sadā.",
  "tha-ap56:6.1": "Ābādhā me na vijjanti,",
  "tha-ap56:6.2": "soko mayhaṁ na vijjati;",
  "tha-ap56:6.3": "Pariḷāhaṁ na passāmi,",
  "tha-ap56:6.4": "puññakammassidaṁ phalaṁ.",
  "tha-ap56:7.1": "Aṭṭhavīse kappasate,",
  "tha-ap56:7.2": "rājā saṁvasito ahuṁ;",
  "tha-ap56:7.3": "Sattaratanasampanno,",
  "tha-ap56:7.4": "cakkavattī mahabbalo’.",
  "tha-ap56:8.1": "Paṭisambhidā catasso,",
  "tha-ap56:8.2": "…pe…",
  "tha-ap56:8.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap56:9.1": "Itthaṁ sudaṁ āyasmā gandhodakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap56:10.1": "Gandhodakiyattherassāpadānaṁ catutthaṁ."
}