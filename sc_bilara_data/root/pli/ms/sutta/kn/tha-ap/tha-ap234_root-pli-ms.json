{
  "tha-ap234:0.1": "Therāpadāna",
  "tha-ap234:0.2": "Udakāsanavagga",
  "tha-ap234:0.3": "2 Bhājanapālakattheraapadāna",
  "tha-ap234:1.1": "“Nagare bandhumatiyā,",
  "tha-ap234:1.2": "kumbhakāro ahaṁ tadā;",
  "tha-ap234:1.3": "Bhājanaṁ anupālesiṁ,",
  "tha-ap234:1.4": "bhikkhusaṅghassa tāvade.",
  "tha-ap234:2.1": "Ekanavutito kappe,",
  "tha-ap234:2.2": "bhājanaṁ anupālayiṁ;",
  "tha-ap234:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap234:2.4": "bhājanassa idaṁ phalaṁ.",
  "tha-ap234:3.1": "Tepaññāse ito kappe,",
  "tha-ap234:3.2": "anantajālināmako;",
  "tha-ap234:3.3": "Sattaratanasampanno,",
  "tha-ap234:3.4": "cakkavattī mahabbalo.",
  "tha-ap234:4.1": "Paṭisambhidā catasso,",
  "tha-ap234:4.2": "…pe…",
  "tha-ap234:4.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap234:5.1": "Itthaṁ sudaṁ āyasmā bhājanapālako thero imā gāthāyo abhāsitthāti.",
  "tha-ap234:6.1": "Bhājanapālakattherassāpadānaṁ dutiyaṁ."
}