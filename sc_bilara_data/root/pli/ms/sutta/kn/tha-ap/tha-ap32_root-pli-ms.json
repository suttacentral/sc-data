{
  "tha-ap32:0.1": "Therāpadāna ",
  "tha-ap32:0.2": "Subhūtivagga ",
  "tha-ap32:0.3": "10. Khomadāyakattheraapadāna ",
  "tha-ap32:1.1": "“Nagare bandhumatiyā, ",
  "tha-ap32:1.2": "ahosiṁ vāṇijo tadā; ",
  "tha-ap32:1.3": "Teneva dāraṁ posemi, ",
  "tha-ap32:1.4": "ropemi bījasampadaṁ. ",
  "tha-ap32:2.1": "Rathiyaṁ paṭipannassa, ",
  "tha-ap32:2.2": "vipassissa mahesino; ",
  "tha-ap32:2.3": "Ekaṁ khomaṁ mayā dinnaṁ, ",
  "tha-ap32:2.4": "kusalatthāya satthuno. ",
  "tha-ap32:3.1": "Ekanavutito kappe, ",
  "tha-ap32:3.2": "yaṁ khomamadadiṁ tadā; ",
  "tha-ap32:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap32:3.4": "khomadānassidaṁ phalaṁ. ",
  "tha-ap32:4.1": "Sattarase ito kappe, ",
  "tha-ap32:4.2": "eko sindhavasandhano; ",
  "tha-ap32:4.3": "Sattaratanasampanno, ",
  "tha-ap32:4.4": "catudīpamhi issaro. ",
  "tha-ap32:5.1": "Paṭisambhidā catasso, ",
  "tha-ap32:5.2": "Vimokkhāpi ca aṭṭhime; ",
  "tha-ap32:5.3": "Chaḷabhiññā sacchikatā, ",
  "tha-ap32:5.4": "Kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap32:6.1": "Itthaṁ sudaṁ āyasmā khomadāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap32:7.1": "Khomadāyakattherassāpadānaṁ dasamaṁ. ",
  "tha-ap32:8.1": "Tassuddānaṁ ",
  "tha-ap32:9.1": "Subhūti upavāno ca, ",
  "tha-ap32:9.2": "saraṇo sīlagāhako; ",
  "tha-ap32:9.3": "Annasaṁsāvako dhūpo, ",
  "tha-ap32:9.4": "pulino uttiyena ca. ",
  "tha-ap32:10.1": "Añjalī khomadāyī ca, ",
  "tha-ap32:10.2": "daseva tatiye gaṇe; ",
  "tha-ap32:10.3": "Pañcāsītisataṁ vuttā, ",
  "tha-ap32:10.4": "gāthāyo sabbapiṇḍitā. ",
  "tha-ap32:11.1": "Subhūtivaggo tatiyo. ",
  "tha-ap32:12.1": "Catutthabhāṇavāraṁ. "
}