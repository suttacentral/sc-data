{
  "tha-ap514:0.1": "Therāpadāna",
  "tha-ap514:0.2": "Phaladāyakavagga",
  "tha-ap514:0.3": "2 Kapitthaphaladāyakattheraapadāna",
  "tha-ap514:1.1": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap514:1.2": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap514:1.3": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap514:1.4": "Kapitthaṁ adadiṁ phalaṁ.",
  "tha-ap514:2.1": "Ekanavutito kappe,",
  "tha-ap514:2.2": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap514:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap514:2.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap514:3.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap514:3.2": "…pe…",
  "tha-ap514:3.3": "viharāmi anāsavo.",
  "tha-ap514:4.1": "Svāgataṁ vata me āsi,",
  "tha-ap514:4.2": "…pe…",
  "tha-ap514:4.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap514:5.1": "Paṭisambhidā catasso,",
  "tha-ap514:5.2": "…pe…",
  "tha-ap514:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap514:6.1": "Itthaṁ sudaṁ āyasmā kapitthaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap514:7.1": "Kapitthaphaladāyakattherassāpadānaṁ dutiyaṁ."
}