{
  "tha-ap261:0.1": "Therāpadāna",
  "tha-ap261:0.2": "Thomakavagga",
  "tha-ap261:0.3": "9. Kuṭidāyakattheraapadāna",
  "tha-ap261:1.1": "“Vipinacārī sambuddho,",
  "tha-ap261:1.2": "rukkhamūle vasī tadā;",
  "tha-ap261:1.3": "Paṇṇasālaṁ karitvāna,",
  "tha-ap261:1.4": "adāsiṁ aparājite.",
  "tha-ap261:2.1": "Ekanavutito kappe,",
  "tha-ap261:2.2": "yaṁ paṇṇakuṭikaṁ adaṁ;",
  "tha-ap261:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap261:2.4": "kuṭidānassidaṁ phalaṁ.",
  "tha-ap261:3.1": "Aṭṭhavīse ito kappe,",
  "tha-ap261:3.2": "soḷasāsiṁsu rājāno;",
  "tha-ap261:3.3": "Sabbattha abhivassīti,",
  "tha-ap261:3.4": "vuccare cakkavattino.",
  "tha-ap261:4.1": "Paṭisambhidā catasso,",
  "tha-ap261:4.2": "…pe…",
  "tha-ap261:4.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap261:5.1": "Itthaṁ sudaṁ āyasmā kuṭidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap261:6.1": "Kuṭidāyakattherassāpadānaṁ navamaṁ."
}