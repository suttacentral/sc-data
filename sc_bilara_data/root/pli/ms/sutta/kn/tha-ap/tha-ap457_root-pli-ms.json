{
  "tha-ap457:0.1": "Therāpadāna",
  "tha-ap457:0.2": "Jagatidāyakavagga",
  "tha-ap457:0.3": "5. Akkamanadāyakattheraapadāna",
  "tha-ap457:1.1": "“Kakusandhassa munino,",
  "tha-ap457:1.2": "brāhmaṇassa vusīmato;",
  "tha-ap457:1.3": "Divāvihāraṁ vajato,",
  "tha-ap457:1.4": "akkamanamadāsahaṁ.",
  "tha-ap457:2.1": "Imasmiṁyeva kappamhi,",
  "tha-ap457:2.2": "yaṁ dānamadadiṁ tadā;",
  "tha-ap457:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap457:2.4": "akkamanassidaṁ phalaṁ.",
  "tha-ap457:3.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap457:3.2": "…pe…",
  "tha-ap457:3.3": "viharāmi anāsavo.",
  "tha-ap457:4.1": "Svāgataṁ vata me āsi,",
  "tha-ap457:4.2": "…pe…",
  "tha-ap457:4.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap457:5.1": "Paṭisambhidā catasso,",
  "tha-ap457:5.2": "…pe…",
  "tha-ap457:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap457:6.1": "Itthaṁ sudaṁ āyasmā akkamanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap457:7.1": "Akkamanadāyakattherassāpadānaṁ pañcamaṁ."
}