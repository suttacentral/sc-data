{
  "tha-ap352:0.1": "Therāpadāna ",
  "tha-ap352:0.2": "Ekapadumiyavagga ",
  "tha-ap352:0.3": "10. Ekañjaliyattheraapadāna ",
  "tha-ap352:1.1": "“Romaso nāma sambuddho, ",
  "tha-ap352:1.2": "nadīkūle vasī tadā; ",
  "tha-ap352:1.3": "Addasaṁ virajaṁ buddhaṁ, ",
  "tha-ap352:1.4": "pītaraṁsiṁva bhāṇumaṁ. ",
  "tha-ap352:2.1": "Ukkāmukhapahaṭṭhaṁva, ",
  "tha-ap352:2.2": "khadiraṅgārasannibhaṁ; ",
  "tha-ap352:2.3": "Osadhiṁva virocantaṁ, ",
  "tha-ap352:2.4": "ekañjalimakāsahaṁ. ",
  "tha-ap352:3.1": "Catunnavutito kappe, ",
  "tha-ap352:3.2": "yaṁ añjalimakāsahaṁ; ",
  "tha-ap352:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap352:3.4": "añjaliyā idaṁ phalaṁ. ",
  "tha-ap352:4.1": "Paṭisambhidā catasso, ",
  "tha-ap352:4.2": "…pe… ",
  "tha-ap352:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap352:5.1": "Itthaṁ sudaṁ āyasmā ekañjaliyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap352:6.1": "Ekañjaliyattherassāpadānaṁ dasamaṁ. ",
  "tha-ap352:7.1": "Ekapadumiyavaggo pañcatiṁsatimo. ",
  "tha-ap352:8.1": "Tassuddānaṁ ",
  "tha-ap352:9.1": "Padumī uppalamālī, ",
  "tha-ap352:9.2": "dhajo kiṅkaṇikaṁ naḷaṁ; ",
  "tha-ap352:9.3": "Campako padumo muṭṭhi, ",
  "tha-ap352:9.4": "tindukekañjalī tathā; ",
  "tha-ap352:9.5": "Cha ca saṭṭhi ca gāthāyo, ",
  "tha-ap352:9.6": "gaṇitāyo vibhāvibhi. "
}