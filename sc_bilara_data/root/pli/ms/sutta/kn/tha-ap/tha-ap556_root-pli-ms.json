{
  "tha-ap556:0.1": "Therāpadāna",
  "tha-ap556:0.2": "Yasavagga",
  "tha-ap556:0.3": "4 Kimilattheraapadāna",
  "tha-ap556:1.1": "“Nibbute kakusandhamhi,",
  "tha-ap556:1.2": "brāhmaṇamhi vusīmati;",
  "tha-ap556:1.3": "Gahetvā salalaṁ mālaṁ,",
  "tha-ap556:1.4": "maṇḍapaṁ kārayiṁ ahaṁ.",
  "tha-ap556:2.1": "Tāvatiṁsaṁ gato santo,",
  "tha-ap556:2.2": "labhimha byamhamuttamaṁ;",
  "tha-ap556:2.3": "Aññe devetirocāmi,",
  "tha-ap556:2.4": "puññakammassidaṁ phalaṁ.",
  "tha-ap556:3.1": "Divā vā yadi vā rattiṁ,",
  "tha-ap556:3.2": "caṅkamanto ṭhito cahaṁ;",
  "tha-ap556:3.3": "Channo salalapupphehi,",
  "tha-ap556:3.4": "puññakammassidaṁ phalaṁ.",
  "tha-ap556:4.1": "Imasmiṁyeva kappamhi,",
  "tha-ap556:4.2": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap556:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap556:4.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap556:5.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap556:5.2": "…pe…",
  "tha-ap556:5.3": "viharāmi anāsavo.",
  "tha-ap556:6.1": "Svāgataṁ vata me āsi,",
  "tha-ap556:6.2": "…pe…",
  "tha-ap556:6.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap556:7.1": "Paṭisambhidā catasso,",
  "tha-ap556:7.2": "…pe…",
  "tha-ap556:7.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap556:8.1": "Itthaṁ sudaṁ āyasmā kimilo thero imā gāthāyo abhāsitthāti.",
  "tha-ap556:9.1": "Kimilattherassāpadānaṁ catutthaṁ."
}