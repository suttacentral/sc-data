{
  "tha-ap175:0.1": "Therāpadāna",
  "tha-ap175:0.2": "Kumudavagga",
  "tha-ap175:0.3": "3 Rattipupphiyattheraapadāna",
  "tha-ap175:1.1": "“Migaluddo pure āsiṁ,",
  "tha-ap175:1.2": "araññe kānane ahaṁ;",
  "tha-ap175:1.3": "Vipassiṁ addasaṁ buddhaṁ,",
  "tha-ap175:1.4": "devadevaṁ narāsabhaṁ.",
  "tha-ap175:2.1": "Rattikaṁ pupphitaṁ disvā,",
  "tha-ap175:2.2": "kuṭajaṁ dharaṇīruhaṁ;",
  "tha-ap175:2.3": "Samūlaṁ paggahetvāna,",
  "tha-ap175:2.4": "upanesiṁ mahesino.",
  "tha-ap175:3.1": "Ekanavutito kappe,",
  "tha-ap175:3.2": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap175:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap175:3.4": "pupphadānassidaṁ phalaṁ.",
  "tha-ap175:4.1": "Ito ca aṭṭhame kappe,",
  "tha-ap175:4.2": "suppasannasanāmako;",
  "tha-ap175:4.3": "Sattaratanasampanno,",
  "tha-ap175:4.4": "rājāhosiṁ mahabbalo.",
  "tha-ap175:5.1": "Paṭisambhidā catasso,",
  "tha-ap175:5.2": "…pe…",
  "tha-ap175:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap175:6.1": "Itthaṁ sudaṁ āyasmā rattipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap175:7.1": "Rattipupphiyattherassāpadānaṁ tatiyaṁ."
}