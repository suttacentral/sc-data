{
  "tha-ap168:0.1": "Therāpadāna",
  "tha-ap168:0.2": "Supāricariyavagga",
  "tha-ap168:0.3": "6. Sappidāyakattheraapadāna",
  "tha-ap168:1.1": "“Phusso nāmāsi bhagavā,",
  "tha-ap168:1.2": "āhutīnaṁ paṭiggaho;",
  "tha-ap168:1.3": "Gacchate vīthiyaṁ vīro,",
  "tha-ap168:1.4": "nibbāpento mahājanaṁ.",
  "tha-ap168:2.1": "Anupubbena bhagavā,",
  "tha-ap168:2.2": "āgacchi mama santikaṁ;",
  "tha-ap168:2.3": "Tato taṁ pattaṁ paggayha,",
  "tha-ap168:2.4": "sappitelamadāsahaṁ.",
  "tha-ap168:3.1": "Dvenavute ito kappe,",
  "tha-ap168:3.2": "yaṁ sappimadadiṁ tadā;",
  "tha-ap168:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap168:3.4": "sappidānassidaṁ phalaṁ.",
  "tha-ap168:4.1": "Chappaññāse ito kappe,",
  "tha-ap168:4.2": "eko āsi samodako;",
  "tha-ap168:4.3": "Sattaratanasampanno,",
  "tha-ap168:4.4": "cakkavattī mahabbalo.",
  "tha-ap168:5.1": "Paṭisambhidā catasso,",
  "tha-ap168:5.2": "…pe…",
  "tha-ap168:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap168:6.1": "Itthaṁ sudaṁ āyasmā sappidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap168:7.1": "Sappidāyakattherassāpadānaṁ chaṭṭhaṁ."
}