{
  "thig5.1:0.1": "Therīgāthā 5.1 ",
  "thig5.1:0.2": "Pañcakanipāta ",
  "thig5.1:0.3": "Aññataratherīgāthā ",
  "thig5.1:1.1": "“Paṇṇavīsativassāni, ",
  "thig5.1:1.2": "yato pabbajitā ahaṁ; ",
  "thig5.1:1.3": "Nāccharāsaṅghātamattampi, ",
  "thig5.1:1.4": "cittassūpasamajjhagaṁ. ",
  "thig5.1:2.1": "Aladdhā cetaso santiṁ, ",
  "thig5.1:2.2": "kāmarāgenavassutā; ",
  "thig5.1:2.3": "Bāhā paggayha kandantī, ",
  "thig5.1:2.4": "vihāraṁ pāvisiṁ ahaṁ. ",
  "thig5.1:3.1": "Sā bhikkhuniṁ upāgacchiṁ, ",
  "thig5.1:3.2": "yā me saddhāyikā ahu; ",
  "thig5.1:3.3": "Sā me dhammamadesesi, ",
  "thig5.1:3.4": "khandhāyatanadhātuyo. ",
  "thig5.1:4.1": "Tassā dhammaṁ suṇitvāna, ",
  "thig5.1:4.2": "ekamante upāvisiṁ; ",
  "thig5.1:4.3": "Pubbenivāsaṁ jānāmi, ",
  "thig5.1:4.4": "dibbacakkhu visodhitaṁ. ",
  "thig5.1:5.1": "Cetopariccañāṇañca, ",
  "thig5.1:5.2": "sotadhātu visodhitā; ",
  "thig5.1:5.3": "Iddhīpi me sacchikatā, ",
  "thig5.1:5.4": "patto me āsavakkhayo; ",
  "thig5.1:5.5": "Chaḷabhiññā sacchikatā, ",
  "thig5.1:5.6": "kataṁ buddhassa sāsanan”ti. ",
  "thig5.1:6.1": "… Aññatarā therī …. "
}