{
  "thig5.10:0.1": "Therīgāthā",
  "thig5.10:0.2": "Pañcakanipāta",
  "thig5.10:0.3": "10. Paṭācārātherīgāthā",
  "thig5.10:1.1": "“Naṅgalehi kasaṁ khettaṁ,",
  "thig5.10:1.2": "bījāni pavapaṁ chamā;",
  "thig5.10:1.3": "Puttadārāni posentā,",
  "thig5.10:1.4": "dhanaṁ vindanti māṇavā.",
  "thig5.10:2.1": "Kimahaṁ sīlasampannā,",
  "thig5.10:2.2": "satthusāsanakārikā;",
  "thig5.10:2.3": "Nibbānaṁ nādhigacchāmi,",
  "thig5.10:2.4": "akusītā anuddhatā.",
  "thig5.10:3.1": "Pāde pakkhālayitvāna,",
  "thig5.10:3.2": "udakesu karomahaṁ;",
  "thig5.10:3.3": "Pādodakañca disvāna,",
  "thig5.10:3.4": "thalato ninnamāgataṁ.",
  "thig5.10:4.1": "Tato cittaṁ samādhesiṁ,",
  "thig5.10:4.2": "assaṁ bhadraṁvajāniyaṁ;",
  "thig5.10:4.3": "Tato dīpaṁ gahetvāna,",
  "thig5.10:4.4": "vihāraṁ pāvisiṁ ahaṁ;",
  "thig5.10:4.5": "Seyyaṁ olokayitvāna,",
  "thig5.10:4.6": "mañcakamhi upāvisiṁ.",
  "thig5.10:5.1": "Tato sūciṁ gahetvāna,",
  "thig5.10:5.2": "vaṭṭiṁ okassayāmahaṁ;",
  "thig5.10:5.3": "Padīpasseva nibbānaṁ,",
  "thig5.10:5.4": "vimokkho ahu cetaso”ti.",
  "thig5.10:6.1": "… Paṭācārā therī …."
}