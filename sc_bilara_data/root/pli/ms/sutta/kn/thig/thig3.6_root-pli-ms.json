{
  "thig3.6:0.1": "Therīgāthā 3.6 ",
  "thig3.6:0.2": "Tikanipāta ",
  "thig3.6:0.3": "Sukkātherīgāthā ",
  "thig3.6:1.1": "“Kiṁme katā rājagahe manussā, ",
  "thig3.6:1.2": "Madhuṁ pītāva acchare; ",
  "thig3.6:1.3": "Ye sukkaṁ na upāsanti, ",
  "thig3.6:1.4": "Desentiṁ buddhasāsanaṁ. ",
  "thig3.6:2.1": "Tañca appaṭivānīyaṁ, ",
  "thig3.6:2.2": "asecanakamojavaṁ; ",
  "thig3.6:2.3": "Pivanti maññe sappaññā, ",
  "thig3.6:2.4": "valāhakamivaddhagū. ",
  "thig3.6:3.1": "Sukkā sukkehi dhammehi, ",
  "thig3.6:3.2": "vītarāgā samāhitā; ",
  "thig3.6:3.3": "Dhāreti antimaṁ dehaṁ, ",
  "thig3.6:3.4": "jetvā māraṁ savāhinin”ti. ",
  "thig3.6:4.1": "… Sukkā therī …. "
}