{
  "thig9.1:0.1": "Therīgāthā",
  "thig9.1:0.2": "Navakanipāta",
  "thig9.1:0.3": "1. Vaḍḍhamātutherīgāthā",
  "thig9.1:1.1": "“Mā su te vaḍḍha lokamhi,",
  "thig9.1:1.2": "vanatho āhu kudācanaṁ;",
  "thig9.1:1.3": "Mā puttaka punappunaṁ,",
  "thig9.1:1.4": "ahu dukkhassa bhāgimā.",
  "thig9.1:2.1": "Sukhañhi vaḍḍha munayo,",
  "thig9.1:2.2": "anejā chinnasaṁsayā;",
  "thig9.1:2.3": "Sītibhūtā damappattā,",
  "thig9.1:2.4": "viharanti anāsavā.",
  "thig9.1:3.1": "Tehānuciṇṇaṁ isībhi,",
  "thig9.1:3.2": "maggaṁ dassanapattiyā;",
  "thig9.1:3.3": "Dukkhassantakiriyāya,",
  "thig9.1:3.4": "tvaṁ vaḍḍha anubrūhaya”.",
  "thig9.1:4.1": "“Visāradāva bhaṇasi,",
  "thig9.1:4.2": "etamatthaṁ janetti me;",
  "thig9.1:4.3": "Maññāmi nūna māmike,",
  "thig9.1:4.4": "vanatho te na vijjati”.",
  "thig9.1:5.1": "“Ye keci vaḍḍha saṅkhārā,",
  "thig9.1:5.2": "hīnā ukkaṭṭhamajjhimā;",
  "thig9.1:5.3": "Aṇūpi aṇumattopi,",
  "thig9.1:5.4": "vanatho me na vijjati.",
  "thig9.1:6.1": "Sabbe me āsavā khīṇā,",
  "thig9.1:6.2": "appamattassa jhāyato;",
  "thig9.1:6.3": "Tisso vijjā anuppattā,",
  "thig9.1:6.4": "kataṁ buddhassa sāsanaṁ”.",
  "thig9.1:7.1": "“Uḷāraṁ vata me mātā,",
  "thig9.1:7.2": "patodaṁ samavassari;",
  "thig9.1:7.3": "Paramatthasañhitā gāthā,",
  "thig9.1:7.4": "yathāpi anukampikā.",
  "thig9.1:8.1": "Tassāhaṁ vacanaṁ sutvā,",
  "thig9.1:8.2": "anusiṭṭhiṁ janettiyā;",
  "thig9.1:8.3": "Dhammasaṁvegamāpādiṁ,",
  "thig9.1:8.4": "yogakkhemassa pattiyā.",
  "thig9.1:9.1": "Sohaṁ padhānapahitatto,",
  "thig9.1:9.2": "rattindivamatandito;",
  "thig9.1:9.3": "Mātarā codito santo,",
  "thig9.1:9.4": "aphusiṁ santimuttamaṁ”.",
  "thig9.1:10.1": "…",
  "thig9.1:10.2": "Vaḍḍhamātā therī ….",
  "thig9.1:11.1": "Navakanipāto niṭṭhito."
}