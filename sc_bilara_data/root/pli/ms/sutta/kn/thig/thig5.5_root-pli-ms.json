{
  "thig5.5:0.1": "Therīgāthā",
  "thig5.5:0.2": "Pañcakanipāta",
  "thig5.5:0.3": "5. Nanduttarātherīgāthā",
  "thig5.5:1.1": "“Aggiṁ candañca sūriyañca,",
  "thig5.5:1.2": "devatā ca namassihaṁ;",
  "thig5.5:1.3": "Nadītitthāni gantvāna,",
  "thig5.5:1.4": "udakaṁ oruhāmihaṁ.",
  "thig5.5:2.1": "Bahūvatasamādānā,",
  "thig5.5:2.2": "aḍḍhaṁ sīsassa olikhiṁ;",
  "thig5.5:2.3": "Chamāya seyyaṁ kappemi,",
  "thig5.5:2.4": "rattiṁ bhattaṁ na bhuñjahaṁ.",
  "thig5.5:3.1": "Vibhūsāmaṇḍanaratā,",
  "thig5.5:3.2": "nhāpanucchādanehi ca;",
  "thig5.5:3.3": "Upakāsiṁ imaṁ kāyaṁ,",
  "thig5.5:3.4": "kāmarāgena aṭṭitā.",
  "thig5.5:4.1": "Tato saddhaṁ labhitvāna,",
  "thig5.5:4.2": "pabbajiṁ anagāriyaṁ;",
  "thig5.5:4.3": "Disvā kāyaṁ yathābhūtaṁ,",
  "thig5.5:4.4": "kāmarāgo samūhato.",
  "thig5.5:5.1": "Sabbe bhavā samucchinnā,",
  "thig5.5:5.2": "icchā ca patthanāpi ca;",
  "thig5.5:5.3": "Sabbayogavisaṁyuttā,",
  "thig5.5:5.4": "santiṁ pāpuṇi cetaso”ti.",
  "thig5.5:6.1": "… Nanduttarā therī …."
}