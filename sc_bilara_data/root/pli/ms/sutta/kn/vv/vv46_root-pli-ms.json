{
  "vv46:0.1": "Vimānavatthu",
  "vv46:0.2": "Itthivimāna",
  "vv46:0.3": "Mañjiṭṭhakavagga",
  "vv46:0.4": "8. Ambavimānavatthu",
  "vv46:1.1": "“Dibbaṁ te ambavanaṁ rammaṁ,",
  "vv46:1.2": "Pāsādettha mahallako;",
  "vv46:1.3": "Nānātūriyasaṅghuṭṭho,",
  "vv46:1.4": "Accharāgaṇaghosito.",
  "vv46:2.1": "Padīpo cettha jalati,",
  "vv46:2.2": "niccaṁ sovaṇṇayo mahā;",
  "vv46:2.3": "Dussaphalehi rukkhehi,",
  "vv46:2.4": "samantā parivārito.",
  "vv46:3.1": "Kena tetādiso vaṇṇo,",
  "vv46:3.2": "…pe…",
  "vv46:3.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv46:4.1": "Sā devatā attamanā,",
  "vv46:4.2": "…pe…",
  "vv46:4.3": "yassa kammassidaṁ phalaṁ.",
  "vv46:5.1": "“Ahaṁ manussesu manussabhūtā,",
  "vv46:5.2": "Purimāya jātiyā manussaloke;",
  "vv46:5.3": "Vihāraṁ saṅghassa kāresiṁ,",
  "vv46:5.4": "Ambehi parivāritaṁ.",
  "vv46:6.1": "Pariyosite vihāre,",
  "vv46:6.2": "kārente niṭṭhite mahe;",
  "vv46:6.3": "Ambehi chādayitvāna,",
  "vv46:6.4": "katvā dussamaye phale.",
  "vv46:7.1": "Padīpaṁ tattha jāletvā,",
  "vv46:7.2": "bhojayitvā gaṇuttamaṁ;",
  "vv46:7.3": "Niyyādesiṁ taṁ saṅghassa,",
  "vv46:7.4": "pasannā sehi pāṇibhi.",
  "vv46:8.1": "Tena me ambavanaṁ rammaṁ,",
  "vv46:8.2": "pāsādettha mahallako;",
  "vv46:8.3": "Nānātūriyasaṅghuṭṭho,",
  "vv46:8.4": "accharāgaṇaghosito.",
  "vv46:9.1": "Padīpo cettha jalati,",
  "vv46:9.2": "niccaṁ sovaṇṇayo mahā;",
  "vv46:9.3": "Dussaphalehi rukkhehi,",
  "vv46:9.4": "samantā parivārito.",
  "vv46:10.1": "Tena metādiso vaṇṇo,",
  "vv46:10.2": "…pe…",
  "vv46:10.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv46:11.1": "Ambavimānaṁ aṭṭhamaṁ."
}