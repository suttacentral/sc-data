{
  "vv4:0.1": "Vimānavatthu",
  "vv4:0.2": "Itthivimāna",
  "vv4:0.3": "Pīṭhavagga",
  "vv4:0.4": "4. Catutthapīṭhavimānavatthu",
  "vv4:1.1": "“Pīṭhaṁ te veḷuriyamayaṁ uḷāraṁ,",
  "vv4:1.2": "Manojavaṁ gacchati yenakāmaṁ;",
  "vv4:1.3": "Alaṅkate malyadhare suvatthe,",
  "vv4:1.4": "Obhāsasi vijjurivabbhakūṭaṁ.",
  "vv4:2.1": "Kena tetādiso vaṇṇo,",
  "vv4:2.2": "kena te idha mijjhati;",
  "vv4:2.3": "Uppajjanti ca te bhogā,",
  "vv4:2.4": "ye keci manaso piyā.",
  "vv4:3.1": "Pucchāmi taṁ devi mahānubhāve,",
  "vv4:3.2": "Manussabhūtā kimakāsi puññaṁ;",
  "vv4:3.3": "Kenāsi evaṁ jalitānubhāvā,",
  "vv4:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv4:4.1": "Sā devatā attamanā,",
  "vv4:4.2": "moggallānena pucchitā;",
  "vv4:4.3": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv4:4.4": "yassa kammassidaṁ phalaṁ.",
  "vv4:5.1": "“Appassa kammassa phalaṁ mamedaṁ,",
  "vv4:5.2": "Yenamhi evaṁ jalitānubhāvā;",
  "vv4:5.3": "Ahaṁ manussesu manussabhūtā,",
  "vv4:5.4": "Purimāya jātiyā manussaloke.",
  "vv4:6.1": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv4:6.2": "vippasannamanāvilaṁ;",
  "vv4:6.3": "Tassa adāsahaṁ pīṭhaṁ,",
  "vv4:6.4": "pasannā sehi pāṇibhi.",
  "vv4:7.1": "Tena metādiso vaṇṇo,",
  "vv4:7.2": "tena me idha mijjhati;",
  "vv4:7.3": "Uppajjanti ca me bhogā,",
  "vv4:7.4": "ye keci manaso piyā.",
  "vv4:8.1": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv4:8.2": "Manussabhūtā yamakāsi puññaṁ;",
  "vv4:8.3": "Tenamhi evaṁ jalitānubhāvā,",
  "vv4:8.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv4:9.1": "Catutthapīṭhavimānaṁ catutthaṁ."
}