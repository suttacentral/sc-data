{
  "vv25:0.1": "Vimānavatthu",
  "vv25:0.2": "Itthivimāna",
  "vv25:0.3": "Cittalatāvagga",
  "vv25:0.4": "8. Niddāvimānavatthu",
  "vv25:1.1": "“Abhikkantena vaṇṇena,",
  "vv25:1.2": "yā tvaṁ tiṭṭhasi devate;",
  "vv25:1.3": "Obhāsentī disā sabbā,",
  "vv25:1.4": "osadhī viya tārakā.",
  "vv25:2.1": "Kena tetādiso vaṇṇo,",
  "vv25:2.2": "…pe…",
  "vv25:2.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv25:3.1": "Sā devatā attamanā,",
  "vv25:3.2": "…pe…",
  "vv25:3.3": "yassa kammassidaṁ phalaṁ.",
  "vv25:4.1": "“Niddāti mamaṁ aññaṁsu,",
  "vv25:4.2": "Rājagahasmiṁ upāsikā;",
  "vv25:4.3": "Saddhā sīlena sampannā,",
  "vv25:4.4": "Saṁvibhāgaratā sadā.",
  "vv25:5.1": "Acchādanañca bhattañca,",
  "vv25:5.2": "senāsanaṁ padīpiyaṁ;",
  "vv25:5.3": "Adāsiṁ ujubhūtesu,",
  "vv25:5.4": "vippasannena cetasā.",
  "vv25:6.1": "Cātuddasiṁ pañcadasiṁ,",
  "vv25:6.2": "yā ca pakkhassa aṭṭhamī;",
  "vv25:6.3": "Pāṭihāriyapakkhañca,",
  "vv25:6.4": "aṭṭhaṅgasusamāgataṁ.",
  "vv25:7.1": "Uposathaṁ upavasissaṁ,",
  "vv25:7.2": "sadā sīlesu saṁvutā;",
  "vv25:7.3": "Saññamā saṁvibhāgā ca,",
  "vv25:7.4": "vimānaṁ āvasāmahaṁ.",
  "vv25:8.1": "Pāṇātipātā viratā,",
  "vv25:8.2": "musāvādā ca saññatā;",
  "vv25:8.3": "Theyyā ca aticārā ca,",
  "vv25:8.4": "majjapānā ca ārakā.",
  "vv25:9.1": "Pañcasikkhāpade ratā,",
  "vv25:9.2": "ariyasaccāna kovidā;",
  "vv25:9.3": "Upāsikā cakkhumato,",
  "vv25:9.4": "gotamassa yasassino.",
  "vv25:10.1": "Tena metādiso vaṇṇo,",
  "vv25:10.2": "…pe…",
  "vv25:10.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv25:11.1": "Niddāvimānaṁ aṭṭhamaṁ."
}