{
  "iti86:0.1": "Itivuttaka 86",
  "iti86:0.2": "Tikanipāta",
  "iti86:0.3": "Catutthavagga",
  "iti86:0.4": "Dhammānudhammapaṭipannasutta",
  "iti86:1.1": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti86:2.1": "“Dhammānudhammapaṭipannassa bhikkhuno ayamanudhammo hoti veyyākaraṇāya—",
  "iti86:2.2": "dhammānudhammapaṭipannoyanti bhāsamāno dhammaññeva bhāsati no adhammaṁ, vitakkayamāno vā dhammavitakkaññeva vitakketi no adhammavitakkaṁ, tadubhayaṁ vā pana abhinivejjetvā upekkhako viharati sato sampajāno”ti.",
  "iti86:2.3": "Etamatthaṁ bhagavā avoca.",
  "iti86:2.4": "Tatthetaṁ iti vuccati:",
  "iti86:3.1": "“Dhammārāmo dhammarato,",
  "iti86:3.2": "dhammaṁ anuvicintayaṁ;",
  "iti86:3.3": "Dhammaṁ anussaraṁ bhikkhu,",
  "iti86:3.4": "saddhammā na parihāyati.",
  "iti86:4.1": "Caraṁ vā yadi vā tiṭṭhaṁ,",
  "iti86:4.2": "nisinno uda vā sayaṁ;",
  "iti86:4.3": "Ajjhattaṁ samayaṁ cittaṁ,",
  "iti86:4.4": "santimevādhigacchatī”ti.",
  "iti86:5.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti86:5.2": "Sattamaṁ."
}