{
  "thag1.116:0.1": "Theragāthā",
  "thag1.116:0.2": "Ekakanipāta",
  "thag1.116:0.3": "Dvādasamavagga",
  "thag1.116:0.4": "1.116 Pārāpariyattheragāthā",
  "thag1.116:1.1": "“Chaphassāyatane hitvā,",
  "thag1.116:1.2": "guttadvāro susaṁvuto;",
  "thag1.116:1.3": "Aghamūlaṁ vamitvāna,",
  "thag1.116:1.4": "patto me āsavakkhayo”ti.",
  "thag1.116:1.5": "… Pārāpariyo thero …."
}