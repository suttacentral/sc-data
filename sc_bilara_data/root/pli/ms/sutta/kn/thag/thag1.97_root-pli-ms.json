{
  "thag1.97:0.1": "Theragāthā 1.97 ",
  "thag1.97:0.2": "Ekakanipāta ",
  "thag1.97:0.3": "Dasamavagga ",
  "thag1.97:0.4": "Tissattheragāthā ",
  "thag1.97:1.1": "“Hitvā satapalaṁ kaṁsaṁ, ",
  "thag1.97:1.2": "Sovaṇṇaṁ satarājikaṁ; ",
  "thag1.97:1.3": "Aggahiṁ mattikāpattaṁ, ",
  "thag1.97:1.4": "Idaṁ dutiyābhisecanan”ti. ",
  "thag1.97:1.5": "… Tisso thero …. "
}