{
  "thag1.68:0.1": "Theragāthā 1.68 ",
  "thag1.68:0.2": "Ekakanipāta ",
  "thag1.68:0.3": "Sattamavagga ",
  "thag1.68:0.4": "Ekudāniyattheragāthā ",
  "thag1.68:1.1": "“Adhicetaso appamajjato, ",
  "thag1.68:1.2": "Munino monapathesu sikkhato; ",
  "thag1.68:1.3": "Sokā na bhavanti tādino, ",
  "thag1.68:1.4": "Upasantassa sadā satīmato”ti. ",
  "thag1.68:1.5": "… Ekudāniyo thero …. "
}