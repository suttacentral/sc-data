{
  "thag1.59:0.1": "Theragāthā 1.59 ",
  "thag1.59:0.2": "Ekakanipāta ",
  "thag1.59:0.3": "Chaṭṭhavagga ",
  "thag1.59:0.4": "Kosalavihārittheragāthā ",
  "thag1.59:1.1": "“Saddhāyāhaṁ pabbajito, ",
  "thag1.59:1.2": "araññe me kuṭikā katā; ",
  "thag1.59:1.3": "Appamatto ca ātāpī, ",
  "thag1.59:1.4": "sampajāno patissato”ti. ",
  "thag1.59:1.5": "… Kosalavihāritthero …. "
}