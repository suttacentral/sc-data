{
  "thag1.85:0.1": "Theragāthā 1.85 ",
  "thag1.85:0.2": "Ekakanipāta ",
  "thag1.85:0.3": "Navamavagga ",
  "thag1.85:0.4": "Sunāgattheragāthā ",
  "thag1.85:1.1": "“Cittanimittassa kovido, ",
  "thag1.85:1.2": "Pavivekarasaṁ vijāniya; ",
  "thag1.85:1.3": "Jhāyaṁ nipako patissato, ",
  "thag1.85:1.4": "Adhigaccheyya sukhaṁ nirāmisan”ti. ",
  "thag1.85:1.5": "… Sunāgo thero …. "
}