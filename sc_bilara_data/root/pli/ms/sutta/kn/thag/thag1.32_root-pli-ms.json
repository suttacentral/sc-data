{
  "thag1.32:0.1": "Theragāthā 1.32 ",
  "thag1.32:0.2": "Ekakanipāta ",
  "thag1.32:0.3": "Catutthavagga ",
  "thag1.32:0.4": "Suppiyattheragāthā ",
  "thag1.32:1.1": "“Ajaraṁ jīramānena, ",
  "thag1.32:1.2": "tappamānena nibbutiṁ; ",
  "thag1.32:1.3": "Nimiyaṁ paramaṁ santiṁ, ",
  "thag1.32:1.4": "yogakkhemaṁ anuttaran”ti. ",
  "thag1.32:1.5": "… Suppiyo thero …. "
}