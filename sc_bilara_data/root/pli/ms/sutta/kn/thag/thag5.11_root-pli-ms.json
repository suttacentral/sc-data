{
  "thag5.11:0.1": "Theragāthā 5.11 ",
  "thag5.11:0.2": "Pañcakanipāta ",
  "thag5.11:0.3": "Paṭhamavagga ",
  "thag5.11:0.4": "Soṇakuṭikaṇṇattheragāthā ",
  "thag5.11:1.1": "“Upasampadā ca me laddhā, ",
  "thag5.11:1.2": "vimutto camhi anāsavo; ",
  "thag5.11:1.3": "So ca me bhagavā diṭṭho, ",
  "thag5.11:1.4": "vihāre ca sahāvasiṁ. ",
  "thag5.11:2.1": "Bahudeva rattiṁ bhagavā, ",
  "thag5.11:2.2": "abbhokāsetināmayi; ",
  "thag5.11:2.3": "Vihārakusalo satthā, ",
  "thag5.11:2.4": "vihāraṁ pāvisī tadā. ",
  "thag5.11:3.1": "Santharitvāna saṅghāṭiṁ, ",
  "thag5.11:3.2": "seyyaṁ kappesi gotamo; ",
  "thag5.11:3.3": "Sīho selaguhāyaṁva, ",
  "thag5.11:3.4": "pahīnabhayabheravo. ",
  "thag5.11:4.1": "Tato kalyāṇavākkaraṇo, ",
  "thag5.11:4.2": "sammāsambuddhasāvako; ",
  "thag5.11:4.3": "Soṇo abhāsi saddhammaṁ, ",
  "thag5.11:4.4": "buddhaseṭṭhassa sammukhā. ",
  "thag5.11:5.1": "Pañcakkhandhe pariññāya, ",
  "thag5.11:5.2": "Bhāvayitvāna añjasaṁ; ",
  "thag5.11:5.3": "Pappuyya paramaṁ santiṁ, ",
  "thag5.11:5.4": "Parinibbissatyanāsavo”ti. ",
  "thag5.11:5.5": "… Soṇo kuṭikaṇṇatthero …. "
}