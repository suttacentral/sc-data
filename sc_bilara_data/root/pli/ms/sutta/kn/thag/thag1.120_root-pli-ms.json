{
  "thag1.120:0.1": "Theragāthā",
  "thag1.120:0.2": "Ekakanipāta",
  "thag1.120:0.3": "Dvādasamavagga",
  "thag1.120:0.4": "1.120. Isidattattheragāthā",
  "thag1.120:1.1": "“Pañcakkhandhā pariññātā,",
  "thag1.120:1.2": "tiṭṭhanti chinnamūlakā;",
  "thag1.120:1.3": "Dukkhakkhayo anuppatto,",
  "thag1.120:1.4": "patto me āsavakkhayo”ti.",
  "thag1.120:1.5": "…",
  "thag1.120:1.6": "Isidatto thero ….",
  "thag1.120:1.7": "vaggo dvādasamo.",
  "thag1.120:2.0": "Tassuddānaṁ",
  "thag1.120:2.1": "Jento ca vacchagotto ca,",
  "thag1.120:2.2": "vaccho ca vanasavhayo;",
  "thag1.120:2.3": "Adhimutto mahānāmo,",
  "thag1.120:2.4": "pārāpariyo yasopi ca;",
  "thag1.120:2.5": "Kimilo vajjiputto ca,",
  "thag1.120:2.6": "isidatto mahāyasoti.",
  "thag1.120:2.7": "Ekakanipāto niṭṭhito.",
  "thag1.120:3.0": "Tatruddānaṁ",
  "thag1.120:3.1": "Vīsuttarasataṁ therā,",
  "thag1.120:3.2": "katakiccā anāsavā;",
  "thag1.120:3.3": "Ekakeva nipātamhi,",
  "thag1.120:3.4": "susaṅgītā mahesibhīti."
}