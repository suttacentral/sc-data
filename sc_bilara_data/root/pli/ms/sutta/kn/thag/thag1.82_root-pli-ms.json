{
  "thag1.82:0.1": "Theragāthā 1.82 ",
  "thag1.82:0.2": "Ekakanipāta ",
  "thag1.82:0.3": "Navamavagga ",
  "thag1.82:0.4": "Kassapattheragāthā ",
  "thag1.82:1.1": "“Yena yena subhikkhāni, ",
  "thag1.82:1.2": "sivāni abhayāni ca; ",
  "thag1.82:1.3": "Tena puttaka gacchassu, ",
  "thag1.82:1.4": "mā sokāpahato bhavā”ti. ",
  "thag1.82:1.5": "… Kassapo thero …. "
}