{
  "thag1.86:0.1": "Theragāthā",
  "thag1.86:0.2": "Ekakanipāta",
  "thag1.86:0.3": "Navamavagga",
  "thag1.86:0.4": "1.86. Nāgitattheragāthā",
  "thag1.86:1.1": "“Ito bahiddhā puthu aññavādinaṁ,",
  "thag1.86:1.2": "Maggo na nibbānagamo yathā ayaṁ;",
  "thag1.86:1.3": "Itissu saṅghaṁ bhagavānusāsati,",
  "thag1.86:1.4": "Satthā sayaṁ pāṇitaleva dassayan”ti.",
  "thag1.86:1.5": "… Nāgito thero …."
}