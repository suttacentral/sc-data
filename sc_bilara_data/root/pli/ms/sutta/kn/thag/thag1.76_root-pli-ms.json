{
  "thag1.76:0.1": "Theragāthā 1.76 ",
  "thag1.76:0.2": "Ekakanipāta ",
  "thag1.76:0.3": "Aṭṭhamavagga ",
  "thag1.76:0.4": "Piyañjahattheragāthā ",
  "thag1.76:1.1": "“Uppatantesu nipate, ",
  "thag1.76:1.2": "nipatantesu uppate; ",
  "thag1.76:1.3": "Vase avasamānesu, ",
  "thag1.76:1.4": "ramamānesu no rame”ti. ",
  "thag1.76:1.5": "… Piyañjaho thero …. "
}