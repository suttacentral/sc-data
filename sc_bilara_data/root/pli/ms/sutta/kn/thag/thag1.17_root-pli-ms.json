{
  "thag1.17:0.1": "Theragāthā 1.17 ",
  "thag1.17:0.2": "Ekakanipāta ",
  "thag1.17:0.3": "Dutiyavagga ",
  "thag1.17:0.4": "Dāsakattheragāthā ",
  "thag1.17:1.1": "“Middhī yadā hoti mahagghaso ca, ",
  "thag1.17:1.2": "Niddāyitā samparivattasāyī; ",
  "thag1.17:1.3": "Mahāvarāhova nivāpapuṭṭho, ",
  "thag1.17:1.4": "Punappunaṁ gabbhamupeti mando”ti. ",
  "thag1.17:1.5": "… Dāsako thero …. "
}