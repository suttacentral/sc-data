{
  "thag2.8:0.1": "Theragāthā",
  "thag2.8:0.2": "Dukanipāta",
  "thag2.8:0.3": "Paṭhamavagga",
  "thag2.8:0.4": "2.8. Surādhattheragāthā",
  "thag2.8:1.1": "“Khīṇā hi mayhaṁ jāti,",
  "thag2.8:1.2": "vusitaṁ jinasāsanaṁ;",
  "thag2.8:1.3": "Pahīno jālasaṅkhāto,",
  "thag2.8:1.4": "bhavanetti samūhatā.",
  "thag2.8:2.1": "Yassatthāya pabbajito,",
  "thag2.8:2.2": "agārasmānagāriyaṁ;",
  "thag2.8:2.3": "So me attho anuppatto,",
  "thag2.8:2.4": "sabbasaṁyojanakkhayo”ti.",
  "thag2.8:2.5": "… Surādho thero …."
}