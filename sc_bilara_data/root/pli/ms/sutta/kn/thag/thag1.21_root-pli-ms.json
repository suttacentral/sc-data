{
  "thag1.21:0.1": "Theragāthā",
  "thag1.21:0.2": "Ekakanipāta",
  "thag1.21:0.3": "Tatiyavagga",
  "thag1.21:0.4": "1.21. Nigrodhattheragāthā",
  "thag1.21:1.1": "“Nāhaṁ bhayassa bhāyāmi,",
  "thag1.21:1.2": "Satthā no amatassa kovido;",
  "thag1.21:1.3": "Yattha bhayaṁ nāvatiṭṭhati,",
  "thag1.21:1.4": "Tena maggena vajanti bhikkhavo”ti.",
  "thag1.21:1.5": "… Nigrodho thero …."
}