{
  "thag1.46:0.1": "Theragāthā 1.46 ",
  "thag1.46:0.2": "Ekakanipāta ",
  "thag1.46:0.3": "Pañcamavagga ",
  "thag1.46:0.4": "Samiddhittheragāthā ",
  "thag1.46:1.1": "“Saddhāyāhaṁ pabbajito, ",
  "thag1.46:1.2": "Agārasmānagāriyaṁ; ",
  "thag1.46:1.3": "Sati paññā ca me vuḍḍhā, ",
  "thag1.46:1.4": "Cittañca susamāhitaṁ; ",
  "thag1.46:1.5": "Kāmaṁ karassu rūpāni, ",
  "thag1.46:1.6": "Neva maṁ byādhayissasī”ti. ",
  "thag1.46:1.7": "… Samiddhitthero …. "
}