{
  "thag1.45:0.1": "Theragāthā 1.45 ",
  "thag1.45:0.2": "Ekakanipāta ",
  "thag1.45:0.3": "Pañcamavagga ",
  "thag1.45:0.4": "Ramaṇīyavihārittheragāthā ",
  "thag1.45:1.1": "“Yathāpi bhaddo ājañño, ",
  "thag1.45:1.2": "khalitvā patitiṭṭhati; ",
  "thag1.45:1.3": "Evaṁ dassanasampannaṁ, ",
  "thag1.45:1.4": "sammāsambuddhasāvakan”ti. ",
  "thag1.45:1.5": "… Ramaṇīyavihāritthero …. "
}