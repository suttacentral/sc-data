{
  "thag1.9:0.1": "Theragāthā",
  "thag1.9:0.2": "Ekakanipāta",
  "thag1.9:0.3": "Paṭhamavagga",
  "thag1.9:0.4": "1.9 Pilindavacchattheragāthā",
  "thag1.9:1.1": "“Svāgataṁ na durāgataṁ,",
  "thag1.9:1.2": "Nayidaṁ dumantitaṁ mama;",
  "thag1.9:1.3": "Saṁvibhattesu dhammesu,",
  "thag1.9:1.4": "Yaṁ seṭṭhaṁ tadupāgamin”ti.",
  "thag1.9:1.5": "Itthaṁ sudaṁ āyasmā pilindavaccho thero gāthaṁ abhāsitthāti."
}