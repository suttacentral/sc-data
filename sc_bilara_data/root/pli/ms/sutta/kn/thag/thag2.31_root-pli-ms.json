{
  "thag2.31:0.1": "Theragāthā 2.31 ",
  "thag2.31:0.2": "Dukanipāta ",
  "thag2.31:0.3": "Catutthavagga ",
  "thag2.31:0.4": "Migasirattheragāthā ",
  "thag2.31:1.1": "“Yato ahaṁ pabbajito, ",
  "thag2.31:1.2": "sammāsambuddhasāsane; ",
  "thag2.31:1.3": "Vimuccamāno uggacchiṁ, ",
  "thag2.31:1.4": "kāmadhātuṁ upaccagaṁ. ",
  "thag2.31:2.1": "Brahmuno pekkhamānassa, ",
  "thag2.31:2.2": "tato cittaṁ vimucci me; ",
  "thag2.31:2.3": "Akuppā me vimuttīti, ",
  "thag2.31:2.4": "sabbasaṁyojanakkhayā”ti. ",
  "thag2.31:2.5": "… Migasiro thero …. "
}