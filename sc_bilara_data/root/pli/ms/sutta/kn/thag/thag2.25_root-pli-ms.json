{
  "thag2.25:0.1": "Theragāthā 2.25 ",
  "thag2.25:0.2": "Dukanipāta ",
  "thag2.25:0.3": "Tatiyavagga ",
  "thag2.25:0.4": "Vītasokattheragāthā ",
  "thag2.25:1.1": "“Kese me olikhissanti, ",
  "thag2.25:1.2": "kappako upasaṅkami; ",
  "thag2.25:1.3": "Tato ādāsamādāya, ",
  "thag2.25:1.4": "sarīraṁ paccavekkhisaṁ. ",
  "thag2.25:2.1": "Tuccho kāyo adissittha, ",
  "thag2.25:2.2": "andhakāro tamo byagā; ",
  "thag2.25:2.3": "Sabbe coḷā samucchinnā, ",
  "thag2.25:2.4": "natthi dāni punabbhavo”ti. ",
  "thag2.25:2.5": "… Vītasoko thero …. "
}