{
  "thag2.18:0.1": "Theragāthā 2.18 ",
  "thag2.18:0.2": "Dukanipāta ",
  "thag2.18:0.3": "Dutiyavagga ",
  "thag2.18:0.4": "Kimilattheragāthā ",
  "thag2.18:1.1": "“Pācīnavaṁsadāyamhi, ",
  "thag2.18:1.2": "sakyaputtā sahāyakā; ",
  "thag2.18:1.3": "Pahāyānappake bhoge, ",
  "thag2.18:1.4": "uñchāpattāgate ratā. ",
  "thag2.18:2.1": "Āraddhavīriyā pahitattā, ",
  "thag2.18:2.2": "Niccaṁ daḷhaparakkamā; ",
  "thag2.18:2.3": "Ramanti dhammaratiyā, ",
  "thag2.18:2.4": "Hitvāna lokiyaṁ ratin”ti. ",
  "thag2.18:2.5": "… Kimilo thero …. "
}