{
  "thag6.8:0.1": "Theragāthā 6.8 ",
  "thag6.8:0.2": "Chakkanipāta ",
  "thag6.8:0.3": "Paṭhamavagga ",
  "thag6.8:0.4": "Migajālattheragāthā ",
  "thag6.8:1.1": "“Sudesito cakkhumatā, ",
  "thag6.8:1.2": "buddhenādiccabandhunā; ",
  "thag6.8:1.3": "Sabbasaṁyojanātīto, ",
  "thag6.8:1.4": "sabbavaṭṭavināsano. ",
  "thag6.8:2.1": "Niyyāniko uttaraṇo, ",
  "thag6.8:2.2": "taṇhāmūlavisosano; ",
  "thag6.8:2.3": "Visamūlaṁ āghātanaṁ, ",
  "thag6.8:2.4": "chetvā pāpeti nibbutiṁ. ",
  "thag6.8:3.1": "Aññāṇamūlabhedāya, ",
  "thag6.8:3.2": "kammayantavighāṭano; ",
  "thag6.8:3.3": "Viññāṇānaṁ pariggahe, ",
  "thag6.8:3.4": "ñāṇavajiranipātano. ",
  "thag6.8:4.1": "Vedanānaṁ viññāpano, ",
  "thag6.8:4.2": "upādānappamocano; ",
  "thag6.8:4.3": "Bhavaṁ aṅgārakāsuṁva, ",
  "thag6.8:4.4": "ñāṇena anupassano. ",
  "thag6.8:5.1": "Mahāraso sugambhīro, ",
  "thag6.8:5.2": "jarāmaccunivāraṇo; ",
  "thag6.8:5.3": "Ariyo aṭṭhaṅgiko maggo, ",
  "thag6.8:5.4": "dukkhūpasamano sivo. ",
  "thag6.8:6.1": "Kammaṁ kammanti ñatvāna, ",
  "thag6.8:6.2": "vipākañca vipākato; ",
  "thag6.8:6.3": "Paṭiccuppannadhammānaṁ, ",
  "thag6.8:6.4": "yathāvālokadassano; ",
  "thag6.8:6.5": "Mahākhemaṅgamo santo, ",
  "thag6.8:6.6": "pariyosānabhaddako”ti. ",
  "thag6.8:6.7": "… Migajālo thero …. "
}