{
  "thag1.41:0.1": "Theragāthā 1.41 ",
  "thag1.41:0.2": "Ekakanipāta ",
  "thag1.41:0.3": "Pañcamavagga ",
  "thag1.41:0.4": "Sirivaḍḍhattheragāthā ",
  "thag1.41:1.1": "“Vivaramanupatanti vijjutā, ",
  "thag1.41:1.2": "Vebhārassa ca paṇḍavassa ca; ",
  "thag1.41:1.3": "Nagavivaragato ca jhāyati, ",
  "thag1.41:1.4": "Putto appaṭimassa tādino”ti. ",
  "thag1.41:1.5": "… Sirivaḍḍho thero …. "
}