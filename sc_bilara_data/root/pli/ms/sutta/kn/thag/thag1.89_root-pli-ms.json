{
  "thag1.89:0.1": "Theragāthā 1.89 ",
  "thag1.89:0.2": "Ekakanipāta ",
  "thag1.89:0.3": "Navamavagga ",
  "thag1.89:0.4": "(Paṭhama) Devasabhattheragāthā ",
  "thag1.89:1.1": "“Uttiṇṇā paṅkapalipā, ",
  "thag1.89:1.2": "pātālā parivajjitā; ",
  "thag1.89:1.3": "Mutto oghā ca ganthā ca, ",
  "thag1.89:1.4": "sabbe mānā visaṁhatā”ti. ",
  "thag1.89:1.5": "… Devasabho thero …. "
}