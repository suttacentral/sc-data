{
  "thag1.95:0.1": "Theragāthā 1.95 ",
  "thag1.95:0.2": "Ekakanipāta ",
  "thag1.95:0.3": "Dasamavagga ",
  "thag1.95:0.4": "Cakkhupālattheragāthā ",
  "thag1.95:1.1": "“Andhohaṁ hatanettosmi, ",
  "thag1.95:1.2": "kantāraddhānapakkhando; ",
  "thag1.95:1.3": "Sayamānopi gacchissaṁ, ",
  "thag1.95:1.4": "na sahāyena pāpenā”ti. ",
  "thag1.95:1.5": "… Cakkhupālo thero …. "
}