{
  "thag1.31:0.1": "Theragāthā 1.31 ",
  "thag1.31:0.2": "Ekakanipāta ",
  "thag1.31:0.3": "Catutthavagga ",
  "thag1.31:0.4": "Gahvaratīriyattheragāthā ",
  "thag1.31:1.1": "“Phuṭṭho ḍaṁsehi makasehi, ",
  "thag1.31:1.2": "araññasmiṁ brahāvane; ",
  "thag1.31:1.3": "Nāgo saṅgāmasīseva, ",
  "thag1.31:1.4": "sato tatrādhivāsaye”ti. ",
  "thag1.31:1.5": "… Gahvaratīriyo thero …. "
}