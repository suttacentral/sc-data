{
  "thag1.63:0.1": "Theragāthā 1.63 ",
  "thag1.63:0.2": "Ekakanipāta ",
  "thag1.63:0.3": "Sattamavagga ",
  "thag1.63:0.4": "Pakkhattheragāthā ",
  "thag1.63:1.1": "“Cutā patanti patitā, ",
  "thag1.63:1.2": "giddhā ca punarāgatā; ",
  "thag1.63:1.3": "Kataṁ kiccaṁ rataṁ rammaṁ, ",
  "thag1.63:1.4": "sukhenanvāgataṁ sukhan”ti. ",
  "thag1.63:1.5": "… Pakkho thero …. "
}