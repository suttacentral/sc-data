{
  "thag1.16:0.1": "Theragāthā 1.16 ",
  "thag1.16:0.2": "Ekakanipāta ",
  "thag1.16:0.3": "Dutiyavagga ",
  "thag1.16:0.4": "Belaṭṭhasīsattheragāthā ",
  "thag1.16:1.1": "“Yathāpi bhaddo ājañño, ",
  "thag1.16:1.2": "naṅgalāvattanī sikhī; ",
  "thag1.16:1.3": "Gacchati appakasirena, ",
  "thag1.16:1.4": "evaṁ rattindivā mama; ",
  "thag1.16:1.5": "Gacchanti appakasirena, ",
  "thag1.16:1.6": "sukhe laddhe nirāmise”ti. ",
  "thag1.16:1.7": "… Belaṭṭhasīso thero …. "
}