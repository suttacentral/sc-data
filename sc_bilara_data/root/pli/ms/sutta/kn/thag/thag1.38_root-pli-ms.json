{
  "thag1.38:0.1": "Theragāthā 1.38 ",
  "thag1.38:0.2": "Ekakanipāta ",
  "thag1.38:0.3": "Catutthavagga ",
  "thag1.38:0.4": "Gavampatittheragāthā ",
  "thag1.38:1.1": "“Yo iddhiyā sarabhuṁ aṭṭhapesi, ",
  "thag1.38:1.2": "So gavampati asito anejo; ",
  "thag1.38:1.3": "Taṁ sabbasaṅgātigataṁ mahāmuniṁ, ",
  "thag1.38:1.4": "Devā namassanti bhavassa pāragun”ti. ",
  "thag1.38:1.5": "… Gavampatitthero …. "
}