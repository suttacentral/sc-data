{
  "thag1.40:0.1": "Theragāthā",
  "thag1.40:0.2": "Ekakanipāta",
  "thag1.40:0.3": "Catutthavagga",
  "thag1.40:0.4": "1.40. Vaḍḍhamānattheragāthā",
  "thag1.40:1.1": "“Sattiyā viya omaṭṭho,",
  "thag1.40:1.2": "ḍayhamānova matthake;",
  "thag1.40:1.3": "Bhavarāgappahānāya,",
  "thag1.40:1.4": "sato bhikkhu paribbaje”ti.",
  "thag1.40:1.5": "…",
  "thag1.40:1.6": "Vaḍḍhamāno thero ….",
  "thag1.40:1.7": "vaggo catuttho.",
  "thag1.40:2.0": "Tassuddānaṁ",
  "thag1.40:2.1": "Gahvaratīriyo suppiyo,",
  "thag1.40:2.2": "sopāko ceva posiyo;",
  "thag1.40:2.3": "Sāmaññakāni kumāputto,",
  "thag1.40:2.4": "kumāputtasahāyako;",
  "thag1.40:2.5": "Gavampati tissatthero,",
  "thag1.40:2.6": "vaḍḍhamāno mahāyasoti."
}