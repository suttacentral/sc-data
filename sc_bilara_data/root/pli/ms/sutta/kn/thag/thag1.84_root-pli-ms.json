{
  "thag1.84:0.1": "Theragāthā 1.84 ",
  "thag1.84:0.2": "Ekakanipāta ",
  "thag1.84:0.3": "Navamavagga ",
  "thag1.84:0.4": "Nītattheragāthā ",
  "thag1.84:1.1": "“Sabbarattiṁ supitvāna, ",
  "thag1.84:1.2": "divā saṅgaṇike rato; ",
  "thag1.84:1.3": "Kudāssu nāma dummedho, ",
  "thag1.84:1.4": "dukkhassantaṁ karissatī”ti. ",
  "thag1.84:1.5": "… Nīto thero …. "
}