{
  "thag1.103:0.1": "Theragāthā 1.103 ",
  "thag1.103:0.2": "Ekakanipāta ",
  "thag1.103:0.3": "Ekādasamavagga ",
  "thag1.103:0.4": "Bandhurattheragāthā ",
  "thag1.103:1.1": "“Nāhaṁ etena atthiko, ",
  "thag1.103:1.2": "Sukhito dhammarasena tappito; ",
  "thag1.103:1.3": "Pitvā rasaggamuttamaṁ, ",
  "thag1.103:1.4": "Na ca kāhāmi visena santhavan”ti. ",
  "thag1.103:1.5": "… Bandhuro thero …. "
}