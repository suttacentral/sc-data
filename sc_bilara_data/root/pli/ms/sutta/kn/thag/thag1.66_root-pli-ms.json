{
  "thag1.66:0.1": "Theragāthā 1.66 ",
  "thag1.66:0.2": "Ekakanipāta ",
  "thag1.66:0.3": "Sattamavagga ",
  "thag1.66:0.4": "Meghiyattheragāthā ",
  "thag1.66:1.1": "“Anusāsi mahāvīro, ",
  "thag1.66:1.2": "sabbadhammāna pāragū; ",
  "thag1.66:1.3": "Tassāhaṁ dhammaṁ sutvāna, ",
  "thag1.66:1.4": "vihāsiṁ santike sato; ",
  "thag1.66:1.5": "Tisso vijjā anuppattā, ",
  "thag1.66:1.6": "kataṁ buddhassa sāsanan”ti. ",
  "thag1.66:1.7": "… Meghiyo thero …. "
}