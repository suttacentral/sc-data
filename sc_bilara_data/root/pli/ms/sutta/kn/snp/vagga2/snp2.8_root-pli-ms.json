{
  "snp2.8:0.1": "Sutta Nipāta 2.8",
  "snp2.8:0.2": "Dhamma (nāvā) sutta",
  "snp2.8:1.1": "Yasmā hi dhammaṁ puriso vijaññā,",
  "snp2.8:1.2": "Indaṁva naṁ devatā pūjayeyya;",
  "snp2.8:1.3": "So pūjito tasmi pasannacitto,",
  "snp2.8:1.4": "Bahussuto pātukaroti dhammaṁ.",
  "snp2.8:2.1": "Tadaṭṭhikatvāna nisamma dhīro,",
  "snp2.8:2.2": "Dhammānudhammaṁ paṭipajjamāno;",
  "snp2.8:2.3": "Viññū vibhāvī nipuṇo ca hoti,",
  "snp2.8:2.4": "Yo tādisaṁ bhajati appamatto.",
  "snp2.8:3.1": "Khuddañca bālaṁ upasevamāno,",
  "snp2.8:3.2": "Anāgatatthañca usūyakañca;",
  "snp2.8:3.3": "Idheva dhammaṁ avibhāvayitvā,",
  "snp2.8:3.4": "Avitiṇṇakaṅkho maraṇaṁ upeti.",
  "snp2.8:4.1": "Yathā naro āpagamotaritvā,",
  "snp2.8:4.2": "Mahodakaṁ salilaṁ sīghasotaṁ;",
  "snp2.8:4.3": "So vuyhamāno anusotagāmī,",
  "snp2.8:4.4": "Kiṁ so pare sakkhati tārayetuṁ.",
  "snp2.8:5.1": "Tatheva dhammaṁ avibhāvayitvā,",
  "snp2.8:5.2": "Bahussutānaṁ anisāmayatthaṁ;",
  "snp2.8:5.3": "Sayaṁ ajānaṁ avitiṇṇakaṅkho,",
  "snp2.8:5.4": "Kiṁ so pare sakkhati nijjhapetuṁ.",
  "snp2.8:6.1": "Yathāpi nāvaṁ daḷhamāruhitvā,",
  "snp2.8:6.2": "Phiyena rittena samaṅgibhūto;",
  "snp2.8:6.3": "So tāraye tattha bahūpi aññe,",
  "snp2.8:6.4": "Tatrūpayaññū kusalo mutīmā.",
  "snp2.8:7.1": "Evampi yo vedagu bhāvitatto,",
  "snp2.8:7.2": "Bahussuto hoti avedhadhammo;",
  "snp2.8:7.3": "So kho pare nijjhapaye pajānaṁ,",
  "snp2.8:7.4": "Sotāvadhānūpanisūpapanne.",
  "snp2.8:8.1": "Tasmā have sappurisaṁ bhajetha,",
  "snp2.8:8.2": "Medhāvinañceva bahussutañca;",
  "snp2.8:8.3": "Aññāya atthaṁ paṭipajjamāno,",
  "snp2.8:8.4": "Viññātadhammo sa sukhaṁ labhethāti.",
  "snp2.8:8.5": "Nāvāsuttaṁ aṭṭhamaṁ."
}