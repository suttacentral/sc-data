{
  "snp5.12:0.1": "Sutta Nipāta 5.12",
  "snp5.12:0.2": "Jatukaṇṇimāṇavapucchā",
  "snp5.12:1.1": "“Sutvānahaṁ vīramakāmakāmiṁ,",
  "snp5.12:1.2": "(iccāyasmā jatukaṇṇi)",
  "snp5.12:1.3": "Oghātigaṁ puṭṭhumakāmamāgamaṁ;",
  "snp5.12:1.4": "Santipadaṁ brūhi sahajanetta,",
  "snp5.12:1.5": "Yathātacchaṁ bhagavā brūhi metaṁ.",
  "snp5.12:2.1": "Bhagavā hi kāme abhibhuyya iriyati,",
  "snp5.12:2.2": "Ādiccova pathaviṁ tejī tejasā;",
  "snp5.12:2.3": "Parittapaññassa me bhūripañña,",
  "snp5.12:2.4": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
  "snp5.12:2.5": "Jātijarāya idha vippahānaṁ”.",
  "snp5.12:3.1": "“Kāmesu vinaya gedhaṁ,",
  "snp5.12:3.2": "(jatukaṇṇīti bhagavā)",
  "snp5.12:3.3": "Nekkhammaṁ daṭṭhu khemato;",
  "snp5.12:3.4": "Uggahītaṁ nirattaṁ vā,",
  "snp5.12:3.5": "Mā te vijjittha kiñcanaṁ.",
  "snp5.12:4.1": "Yaṁ pubbe taṁ visosehi,",
  "snp5.12:4.2": "pacchā te māhu kiñcanaṁ;",
  "snp5.12:4.3": "Majjhe ce no gahessasi,",
  "snp5.12:4.4": "upasanto carissasi.",
  "snp5.12:5.1": "Sabbaso nāmarūpasmiṁ,",
  "snp5.12:5.2": "Vītagedhassa brāhmaṇa;",
  "snp5.12:5.3": "Āsavāssa na vijjanti,",
  "snp5.12:5.4": "Yehi maccuvasaṁ vaje”ti.",
  "snp5.12:5.5": "Jatukaṇṇimāṇavapucchā ekādasamā."
}