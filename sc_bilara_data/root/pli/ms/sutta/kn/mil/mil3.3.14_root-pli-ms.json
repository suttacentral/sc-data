{
  "mil3.3.14:0.1": "Milindapañha",
  "mil3.3.14:0.2": "Vicāravagga",
  "mil3.3.14:0.3": "14. Vicāralakkhaṇapañha",
  "mil3.3.14:1.1": "“Bhante nāgasena, kiṁlakkhaṇo vicāro”ti?",
  "mil3.3.14:1.2": "“Anumajjanalakkhaṇo, mahārāja, vicāro”ti.",
  "mil3.3.14:2.1": "“Opammaṁ karohī”ti.",
  "mil3.3.14:2.2": "“Yathā, mahārāja, kaṁsathālaṁ ākoṭitaṁ pacchā anuravati anusandahati, yathā, mahārāja, ākoṭanā, evaṁ vitakko daṭṭhabbo.",
  "mil3.3.14:2.3": "Yathā anuravanā, evaṁ vicāro daṭṭhabbo”ti.",
  "mil3.3.14:3.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.14:4.1": "Vicāralakkhaṇapañho cuddasamo.",
  "mil3.3.14:5.1": "Vicāravaggo tatiyo.",
  "mil3.3.14:6.1": "Imasmiṁ vagge cuddasa pañhā."
}