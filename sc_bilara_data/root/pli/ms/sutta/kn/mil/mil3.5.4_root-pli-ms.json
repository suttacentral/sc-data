{
  "mil3.5.4:0.1": "Milindapañha",
  "mil3.5.4:0.2": "Buddhavagga",
  "mil3.5.4:0.3": "4. Dhammadiṭṭhapañha",
  "mil3.5.4:1.1": "Rājā āha—",
  "mil3.5.4:1.2": "“bhante nāgasena, dhammo tayā diṭṭho”ti.",
  "mil3.5.4:1.3": "“Buddhanettiyā kho, mahārāja, buddhapaññattiyā yāvajīvaṁ sāvakehi vattitabban”ti.",
  "mil3.5.4:2.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.4:3.1": "Dhammadiṭṭhapañho catuttho."
}