{
  "ja533:0.1": "Jātaka",
  "ja533:0.2": "Asītinipāta",
  "ja533:0.3": "Cūḷahaṁsavagga",
  "ja533:0.4": "1 Cūḷahaṁsajātaka",
  "ja533:1.1": "“Sumukha anupacinantā,",
  "ja533:1.2": "pakkamanti vihaṅgamā;",
  "ja533:1.3": "Gaccha tuvampi mā kaṅkhi,",
  "ja533:1.4": "natthi baddhe sahāyatā”.",
  "ja533:2.1": "“Gacche vāhaṁ na vā gacche,",
  "ja533:2.2": "na tena amaro siyaṁ;",
  "ja533:2.3": "Sukhitaṁ taṁ upāsitvā,",
  "ja533:2.4": "dukkhitaṁ taṁ kathaṁ jahe.",
  "ja533:3.1": "Maraṇaṁ vā tayā saddhiṁ,",
  "ja533:3.2": "jīvitaṁ vā tayā vinā;",
  "ja533:3.3": "Tadeva maraṇaṁ seyyo,",
  "ja533:3.4": "yañce jīve tayā vinā.",
  "ja533:4.1": "Nesa dhammo mahārāja,",
  "ja533:4.2": "yaṁ taṁ evaṁ gataṁ jahe;",
  "ja533:4.3": "Yā gati tuyhaṁ sā mayhaṁ,",
  "ja533:4.4": "ruccate vihagādhipa”.",
  "ja533:5.1": "“Kā nu pāsena baddhassa,",
  "ja533:5.2": "gati aññā mahānasā;",
  "ja533:5.3": "Sā kathaṁ cetayānassa,",
  "ja533:5.4": "muttassa tava ruccati.",
  "ja533:6.1": "Kaṁ vā tvaṁ passase atthaṁ,",
  "ja533:6.2": "mama tuyhañca pakkhima;",
  "ja533:6.3": "Ñātīnaṁ vāvasiṭṭhānaṁ,",
  "ja533:6.4": "ubhinnaṁ jīvitakkhaye.",
  "ja533:7.1": "Yaṁ na kañcanadepiñcha,",
  "ja533:7.2": "Andhena tamasā gataṁ;",
  "ja533:7.3": "Tādise sañcajaṁ pāṇaṁ,",
  "ja533:7.4": "Kamatthamabhijotaye”.",
  "ja533:8.1": "“Kathaṁ nu patataṁ seṭṭha,",
  "ja533:8.2": "Dhamme atthaṁ na bujjhasi;",
  "ja533:8.3": "Dhammo apacito santo,",
  "ja533:8.4": "Atthaṁ dasseti pāṇinaṁ.",
  "ja533:9.1": "Sohaṁ dhammaṁ apekkhāno,",
  "ja533:9.2": "dhammā catthaṁ samuṭṭhitaṁ;",
  "ja533:9.3": "Bhattiñca tayi sampassaṁ,",
  "ja533:9.4": "nāvakaṅkhāmi jīvitaṁ.",
  "ja533:10.1": "Addhā eso sataṁ dhammo,",
  "ja533:10.2": "yo mitto mittamāpade;",
  "ja533:10.3": "Na caje jīvitassāpi,",
  "ja533:10.4": "hetudhammamanussaraṁ”.",
  "ja533:11.1": "“Svāyaṁ dhammo ca te ciṇṇo,",
  "ja533:11.2": "bhatti ca viditā mayi;",
  "ja533:11.3": "Kāmaṁ karassu mayhetaṁ,",
  "ja533:11.4": "gacchevānumato mayā.",
  "ja533:12.1": "Api tvevaṁ gate kāle,",
  "ja533:12.2": "yaṁ khaṇḍaṁ ñātinaṁ mayā;",
  "ja533:12.3": "Tayā taṁ buddhisampannaṁ,",
  "ja533:12.4": "assa paramasaṁvutaṁ”.",
  "ja533:13.1": "Iccevaṁ mantayantānaṁ,",
  "ja533:13.2": "Ariyānaṁ ariyavuttinaṁ;",
  "ja533:13.3": "Paccadissatha nesādo,",
  "ja533:13.4": "Āturānamivantako.",
  "ja533:14.1": "Te sattumabhisañcikkha,",
  "ja533:14.2": "dīgharattaṁ hitā dijā;",
  "ja533:14.3": "Tuṇhīmāsittha ubhayo,",
  "ja533:14.4": "na sañcalesumāsanā.",
  "ja533:15.1": "Dhataraṭṭhe ca disvāna,",
  "ja533:15.2": "samuḍḍente tato tato;",
  "ja533:15.3": "Abhikkamatha vegena,",
  "ja533:15.4": "dijasattu dijādhipe.",
  "ja533:16.1": "So ca vegenabhikkamma,",
  "ja533:16.2": "āsajja parame dije;",
  "ja533:16.3": "Paccakamittha nesādo,",
  "ja533:16.4": "baddhā iti vicintayaṁ.",
  "ja533:17.1": "Ekaṁva baddhamāsīnaṁ,",
  "ja533:17.2": "abaddhañca punāparaṁ;",
  "ja533:17.3": "Āsajja baddhamāsīnaṁ,",
  "ja533:17.4": "pekkhamānamadīnavaṁ.",
  "ja533:18.1": "Tato so vimatoyeva,",
  "ja533:18.2": "paṇḍare ajjhabhāsatha;",
  "ja533:18.3": "Pavaḍḍhakāye āsīne,",
  "ja533:18.4": "dijasaṅghagaṇādhipe.",
  "ja533:19.1": "“Yannu pāsena mahatā,",
  "ja533:19.2": "baddho na kurute disaṁ;",
  "ja533:19.3": "Atha kasmā abaddho tvaṁ,",
  "ja533:19.4": "balī pakkhi na gacchasi.",
  "ja533:20.1": "Kiṁ nu tyāyaṁ dijo hoti,",
  "ja533:20.2": "mutto baddhaṁ upāsasi;",
  "ja533:20.3": "Ohāya sakuṇā yanti,",
  "ja533:20.4": "kiṁ eko avahīyasi”.",
  "ja533:21.1": "“Rājā me so dijāmitta,",
  "ja533:21.2": "sakhā pāṇasamo ca me;",
  "ja533:21.3": "Neva naṁ vijahissāmi,",
  "ja533:21.4": "yāva kālassa pariyāyaṁ”.",
  "ja533:22.1": "“Kathaṁ panāyaṁ vihaṅgo,",
  "ja533:22.2": "nāddasa pāsamoḍḍitaṁ;",
  "ja533:22.3": "Padañhetaṁ mahantānaṁ,",
  "ja533:22.4": "boddhumarahanti āpadaṁ”.",
  "ja533:23.1": "“Yadā parābhavo hoti,",
  "ja533:23.2": "poso jīvitasaṅkhaye;",
  "ja533:23.3": "Atha jālañca pāsañca,",
  "ja533:23.4": "āsajjāpi na bujjhati.",
  "ja533:24.1": "Api tveva mahāpañña,",
  "ja533:24.2": "pāsā bahuvidhā tatā;",
  "ja533:24.3": "Guyhamāsajja bajjhanti,",
  "ja533:24.4": "athevaṁ jīvitakkhaye”.",
  "ja533:25.1": "“Api nāyaṁ tayā saddhiṁ,",
  "ja533:25.2": "saṁvāsassa sukhudrayo;",
  "ja533:25.3": "Api no anumaññāsi,",
  "ja533:25.4": "api no jīvitaṁ dade”.",
  "ja533:26.1": "“Na ceva me tvaṁ baddhosi,",
  "ja533:26.2": "napi icchāmi te vadhaṁ;",
  "ja533:26.3": "Kāmaṁ khippamito gantvā,",
  "ja533:26.4": "jīva tvaṁ anigho ciraṁ”.",
  "ja533:27.1": "“Nevāhametamicchāmi,",
  "ja533:27.2": "aññatretassa jīvitā;",
  "ja533:27.3": "Sace ekena tuṭṭhosi,",
  "ja533:27.4": "muñcetaṁ mañca bhakkhaya.",
  "ja533:28.1": "Ārohapariṇāhena,",
  "ja533:28.2": "tulyāsmā vayasā ubho;",
  "ja533:28.3": "Na te lābhena jīvatthi,",
  "ja533:28.4": "etena niminā tuvaṁ.",
  "ja533:29.1": "Tadiṅgha samapekkhassu,",
  "ja533:29.2": "hotu giddhi tavamhasu;",
  "ja533:29.3": "Maṁ pubbe bandha pāsena,",
  "ja533:29.4": "pacchā muñca dijādhipaṁ.",
  "ja533:30.1": "Tāvadeva ca te lābho,",
  "ja533:30.2": "katāssa yācanāya ca;",
  "ja533:30.3": "Mitti ca dhataraṭṭhehi,",
  "ja533:30.4": "yāvajīvāya te siyā”.",
  "ja533:31.1": "“Passantu no mahāsaṅghā,",
  "ja533:31.2": "tayā muttaṁ ito gataṁ;",
  "ja533:31.3": "Mittāmaccā ca bhaccā ca,",
  "ja533:31.4": "puttadārā ca bandhavā.",
  "ja533:32.1": "Na ca te tādisā mittā,",
  "ja533:32.2": "bahūnaṁ idha vijjati;",
  "ja533:32.3": "Yathā tvaṁ dhataraṭṭhassa,",
  "ja533:32.4": "pāṇasādhāraṇo sakhā.",
  "ja533:33.1": "So te sahāyaṁ muñcāmi,",
  "ja533:33.2": "hotu rājā tavānugo;",
  "ja533:33.3": "Kāmaṁ khippamito gantvā,",
  "ja533:33.4": "ñātimajjhe virocatha”.",
  "ja533:34.1": "“So patīto pamuttena,",
  "ja533:34.2": "bhattunā bhattugāravo;",
  "ja533:34.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:34.4": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:35.1": "Evaṁ luddaka nandassu,",
  "ja533:35.2": "saha sabbehi ñātibhi;",
  "ja533:35.3": "Yathāhamajja nandāmi,",
  "ja533:35.4": "muttaṁ disvā dijādhipaṁ”.",
  "ja533:36.1": "“Ehi taṁ anusikkhāmi,",
  "ja533:36.2": "Yathā tvamapi lacchase;",
  "ja533:36.3": "Lābhaṁ tavāyaṁ dhataraṭṭho,",
  "ja533:36.4": "Pāpaṁ kiñci na dakkhati.",
  "ja533:37.1": "Khippamantepuraṁ netvā,",
  "ja533:37.2": "rañño dassehi no ubho;",
  "ja533:37.3": "Abaddhe pakatibhūte,",
  "ja533:37.4": "kāje ubhayato ṭhite.",
  "ja533:38.1": "Dhataraṭṭhā mahārāja,",
  "ja533:38.2": "haṁsādhipatino ime;",
  "ja533:38.3": "Ayañhi rājā haṁsānaṁ,",
  "ja533:38.4": "ayaṁ senāpatītaro.",
  "ja533:39.1": "Asaṁsayaṁ imaṁ disvā,",
  "ja533:39.2": "haṁsarājaṁ narādhipo;",
  "ja533:39.3": "Patīto sumano vitto,",
  "ja533:39.4": "bahuṁ dassati te dhanaṁ”.",
  "ja533:40.1": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:40.2": "kammunā upapādayi;",
  "ja533:40.3": "Khippamantepuraṁ gantvā,",
  "ja533:40.4": "rañño haṁse adassayi;",
  "ja533:40.5": "Abaddhe pakatibhūte,",
  "ja533:40.6": "kāje ubhayato ṭhite.",
  "ja533:41.1": "“Dhataraṭṭhā mahārāja,",
  "ja533:41.2": "haṁsādhipatino ime;",
  "ja533:41.3": "Ayañhi rājā haṁsānaṁ,",
  "ja533:41.4": "ayaṁ senāpatītaro”.",
  "ja533:42.1": "“Kathaṁ panime vihaṅgā,",
  "ja533:42.2": "tava hatthattamāgatā;",
  "ja533:42.3": "Kathaṁ luddo mahantānaṁ,",
  "ja533:42.4": "issare idha ajjhagā”.",
  "ja533:43.1": "“Vihitā santime pāsā,",
  "ja533:43.2": "pallalesu janādhipa;",
  "ja533:43.3": "Yaṁ yadāyatanaṁ maññe,",
  "ja533:43.4": "dijānaṁ pāṇarodhanaṁ.",
  "ja533:44.1": "Tādisaṁ pāsamāsajja,",
  "ja533:44.2": "haṁsarājā abajjhatha;",
  "ja533:44.3": "Taṁ abaddho upāsīno,",
  "ja533:44.4": "mamāyaṁ ajjhabhāsatha.",
  "ja533:45.1": "Sudukkaraṁ anariyehi,",
  "ja533:45.2": "dahate bhāvamuttamaṁ;",
  "ja533:45.3": "Bhatturatthe parakkanto,",
  "ja533:45.4": "dhammayutto vihaṅgamo.",
  "ja533:46.1": "Attanāyaṁ cajitvāna,",
  "ja533:46.2": "jīvitaṁ jīvitāraho;",
  "ja533:46.3": "Anutthunanto āsīno,",
  "ja533:46.4": "bhattu yācittha jīvitaṁ.",
  "ja533:47.1": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:47.2": "pasādamahamajjhagā;",
  "ja533:47.3": "Tato naṁ pāmuciṁ pāsā,",
  "ja533:47.4": "anuññāsiṁ sukhena ca.",
  "ja533:48.1": "So patīto pamuttena,",
  "ja533:48.2": "bhattunā bhattugāravo;",
  "ja533:48.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:48.4": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:49.1": "‘Evaṁ luddaka nandassu,",
  "ja533:49.2": "saha sabbehi ñātibhi;",
  "ja533:49.3": "Yathāhamajja nandāmi,",
  "ja533:49.4": "muttaṁ disvā dijādhipaṁ.",
  "ja533:50.1": "Ehi taṁ anusikkhāmi,",
  "ja533:50.2": "yathā tvamapi lacchase;",
  "ja533:50.3": "Lābhaṁ tavāyaṁ dhataraṭṭho,",
  "ja533:50.4": "pāpaṁ kiñci na dakkhati.",
  "ja533:51.1": "Khippamantepuraṁ netvā,",
  "ja533:51.2": "rañño dassehi no ubho;",
  "ja533:51.3": "Abaddhe pakatibhūte,",
  "ja533:51.4": "kāje ubhayato ṭhite.",
  "ja533:52.1": "“Dhataraṭṭhā mahārāja,",
  "ja533:52.2": "haṁsādhipatino ime;",
  "ja533:52.3": "Ayañhi rājā haṁsānaṁ,",
  "ja533:52.4": "ayaṁ senāpatītaro”.",
  "ja533:53.1": "Asaṁsayaṁ imaṁ disvā,",
  "ja533:53.2": "haṁsarājaṁ narādhipo;",
  "ja533:53.3": "Patīto sumano vitto,",
  "ja533:53.4": "bahuṁ dassati te dhanaṁ’.",
  "ja533:54.1": "Evametassa vacanā,",
  "ja533:54.2": "ānītāme ubho mayā;",
  "ja533:54.3": "Ettheva hi ime āsuṁ,",
  "ja533:54.4": "ubho anumatā mayā.",
  "ja533:55.1": "Soyaṁ evaṁ gato pakkhī,",
  "ja533:55.2": "dijo paramadhammiko;",
  "ja533:55.3": "Mādisassa hi luddassa,",
  "ja533:55.4": "janayeyyātha maddavaṁ.",
  "ja533:56.1": "Upāyanañca te deva,",
  "ja533:56.2": "nāññaṁ passāmi edisaṁ;",
  "ja533:56.3": "Sabbasākuṇikāgāme,",
  "ja533:56.4": "taṁ passa manujādhipa”.",
  "ja533:57.1": "Disvā nisinnaṁ rājānaṁ,",
  "ja533:57.2": "pīṭhe sovaṇṇaye subhe;",
  "ja533:57.3": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:57.4": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:58.1": "“Kaccinnu bhoto kusalaṁ,",
  "ja533:58.2": "kacci bhoto anāmayaṁ;",
  "ja533:58.3": "Kacci raṭṭhamidaṁ phītaṁ,",
  "ja533:58.4": "dhammena manusāsasi”.",
  "ja533:59.1": "“Kusalañceva me haṁsa,",
  "ja533:59.2": "Atho haṁsa anāmayaṁ;",
  "ja533:59.3": "Atho raṭṭhamidaṁ phītaṁ,",
  "ja533:59.4": "Dhammena manusāsahaṁ”.",
  "ja533:60.1": "“Kacci bhoto amaccesu,",
  "ja533:60.2": "doso koci na vijjati;",
  "ja533:60.3": "Kacci ca te tavatthesu,",
  "ja533:60.4": "nāvakaṅkhanti jīvitaṁ”.",
  "ja533:61.1": "“Athopi me amaccesu,",
  "ja533:61.2": "doso koci na vijjati;",
  "ja533:61.3": "Athopi te mamatthesu,",
  "ja533:61.4": "nāvakaṅkhanti jīvitaṁ”.",
  "ja533:62.1": "“Kacci te sādisī bhariyā,",
  "ja533:62.2": "assavā piyabhāṇinī;",
  "ja533:62.3": "Puttarūpayasūpetā,",
  "ja533:62.4": "tava chandavasānugā”.",
  "ja533:63.1": "“Atho me sādisī bhariyā,",
  "ja533:63.2": "assavā piyabhāṇinī;",
  "ja533:63.3": "Puttarūpayasūpetā,",
  "ja533:63.4": "mama chandavasānugā.",
  "ja533:64.1": "Bhavantaṁ kacci nu mahā-",
  "ja533:64.2": "sattuhatthattataṁ gato;",
  "ja533:64.3": "Dukkhamāpajji vipulaṁ,",
  "ja533:64.4": "tasmiṁ paṭhamamāpade.",
  "ja533:65.1": "Kacci yantāpatitvāna,",
  "ja533:65.2": "daṇḍena samapothayi;",
  "ja533:65.3": "Evametesaṁ jammānaṁ,",
  "ja533:65.4": "pātikaṁ bhavati tāvade”.",
  "ja533:66.1": "“Khemamāsi mahārāja,",
  "ja533:66.2": "evamāpadiyā sati;",
  "ja533:66.3": "Na cāyaṁ kiñci rasmāsu,",
  "ja533:66.4": "sattūva samapajjatha.",
  "ja533:67.1": "Paccagamittha nesādo,",
  "ja533:67.2": "pubbeva ajjhabhāsatha;",
  "ja533:67.3": "Tadāyaṁ sumukhoyeva,",
  "ja533:67.4": "paṇḍito paccabhāsatha.",
  "ja533:68.1": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:68.2": "pasādamayamajjhagā;",
  "ja533:68.3": "Tato maṁ pāmucī pāsā,",
  "ja533:68.4": "anuññāsi sukhena ca.",
  "ja533:69.1": "Idañca sumukheneva,",
  "ja533:69.2": "etadatthāya cintitaṁ;",
  "ja533:69.3": "Bhoto sakāsegamanaṁ,",
  "ja533:69.4": "etassa dhanamicchatā”.",
  "ja533:70.1": "“Svāgatañcevidaṁ bhavataṁ,",
  "ja533:70.2": "patīto casmi dassanā;",
  "ja533:70.3": "Eso cāpi bahuṁ vittaṁ,",
  "ja533:70.4": "labhataṁ yāvadicchati”.",
  "ja533:71.1": "Santappayitvā nesādaṁ,",
  "ja533:71.2": "bhogehi manujādhipo;",
  "ja533:71.3": "Ajjhabhāsatha vakkaṅgaṁ,",
  "ja533:71.4": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:72.1": "“Yaṁ khalu dhammamādhīnaṁ,",
  "ja533:72.2": "vaso vattati kiñcanaṁ;",
  "ja533:72.3": "Sabbatthissariyaṁ tava,",
  "ja533:72.4": "taṁ pasāsa yadicchatha.",
  "ja533:73.1": "Dānatthaṁ upabhottuṁ vā,",
  "ja533:73.2": "yaṁ caññaṁ upakappati;",
  "ja533:73.3": "Etaṁ dadāmi vo vittaṁ,",
  "ja533:73.4": "issariyaṁ vissajāmi vo”.",
  "ja533:74.1": "“Yathā ca myāyaṁ sumukho,",
  "ja533:74.2": "ajjhabhāseyya paṇḍito;",
  "ja533:74.3": "Kāmasā buddhisampanno,",
  "ja533:74.4": "taṁ myāssa paramappiyaṁ”.",
  "ja533:75.1": "“Ahaṁ khalu mahārāja,",
  "ja533:75.2": "nāgarājārivantaraṁ;",
  "ja533:75.3": "Paṭivattuṁ na sakkomi,",
  "ja533:75.4": "na me so vinayo siyā.",
  "ja533:76.1": "Amhākañceva so seṭṭho,",
  "ja533:76.2": "tvañca uttamasattavo;",
  "ja533:76.3": "Bhūmipālo manussindo,",
  "ja533:76.4": "pūjā bahūhi hetuhi.",
  "ja533:77.1": "Tesaṁ ubhinnaṁ bhaṇataṁ,",
  "ja533:77.2": "vattamāne vinicchaye;",
  "ja533:77.3": "Nantaraṁ paṭivattabbaṁ,",
  "ja533:77.4": "pessena manujādhipa”.",
  "ja533:78.1": "“Dhammena kira nesādo,",
  "ja533:78.2": "paṇḍito aṇḍajo iti;",
  "ja533:78.3": "Na heva akatattassa,",
  "ja533:78.4": "nayo etādiso siyā.",
  "ja533:79.1": "Evaṁ aggapakatimā,",
  "ja533:79.2": "evaṁ uttamasattavo;",
  "ja533:79.3": "Yāvatatthi mayā diṭṭhā,",
  "ja533:79.4": "nāññaṁ passāmi edisaṁ.",
  "ja533:80.1": "Tuṭṭhosmi vo pakatiyā,",
  "ja533:80.2": "vākyena madhurena ca;",
  "ja533:80.3": "Eso cāpi mamacchando,",
  "ja533:80.4": "ciraṁ passeyya vo ubho”.",
  "ja533:81.1": "“Yaṁ kiccaṁ parame mitte,",
  "ja533:81.2": "katamasmāsu taṁ tayā;",
  "ja533:81.3": "Pattā nissaṁsayaṁ tyāmhā,",
  "ja533:81.4": "bhattirasmāsu yā tava.",
  "ja533:82.1": "Aduñca nūna sumahā,",
  "ja533:82.2": "ñātisaṅghassa mantaraṁ;",
  "ja533:82.3": "Adassanena asmākaṁ,",
  "ja533:82.4": "dukkhaṁ bahūsu pakkhisu.",
  "ja533:83.1": "Tesaṁ sokavighātāya,",
  "ja533:83.2": "Tayā anumatā mayaṁ;",
  "ja533:83.3": "Taṁ padakkhiṇato katvā,",
  "ja533:83.4": "Ñātiṁ passemurindama.",
  "ja533:84.1": "Addhāhaṁ vipulaṁ pītiṁ,",
  "ja533:84.2": "bhavataṁ vindāmi dassanā;",
  "ja533:84.3": "Eso cāpi mahā attho,",
  "ja533:84.4": "ñātivissāsanā siyā”.",
  "ja533:85.1": "Idaṁ vatvā dhataraṭṭho,",
  "ja533:85.2": "haṁsarājā narādhipaṁ;",
  "ja533:85.3": "Uttamaṁ javamanvāya,",
  "ja533:85.4": "ñātisaṅghaṁ upāgamuṁ.",
  "ja533:86.1": "Te aroge anuppatte,",
  "ja533:86.2": "disvāna parame dije;",
  "ja533:86.3": "Kekāti makaruṁ haṁsā,",
  "ja533:86.4": "puthusaddo ajāyatha.",
  "ja533:87.1": "Te patītā pamuttena,",
  "ja533:87.2": "bhattunā bhattugāravā;",
  "ja533:87.3": "Samantā parikiriṁsu,",
  "ja533:87.4": "aṇḍajā laddhapaccayā.",
  "ja533:88.1": "Evaṁ mittavataṁ atthā,",
  "ja533:88.2": "sabbe honti padakkhiṇā;",
  "ja533:88.3": "Haṁsā yathā dhataraṭṭhā,",
  "ja533:88.4": "ñātisaṅghaṁ upāgamunti.",
  "ja533:89.1": "Cūḷahaṁsajātakaṁ paṭhamaṁ."
}