{
  "ja453:0.1": "Jātaka",
  "ja453:0.2": "Dasakanipāta",
  "ja453:0.3": "Catudvāravagga",
  "ja453:0.4": "15. Mahāmaṅgalajātaka",
  "ja453:1.1": "“Kiṁsu naro jappamadhiccakāle,",
  "ja453:1.2": "Kaṁ vā vijjaṁ katamaṁ vā sutānaṁ;",
  "ja453:1.3": "So macco asmiñca paramhi loke,",
  "ja453:1.4": "Kathaṁ karo sotthānena gutto”.",
  "ja453:2.1": "“Yassa devā pitaro ca sabbe,",
  "ja453:2.2": "Sarīsapā sabbabhūtāni cāpi;",
  "ja453:2.3": "Mettāya niccaṁ apacitāni honti,",
  "ja453:2.4": "Bhūtesu ve sotthānaṁ tadāhu”.",
  "ja453:3.1": "“Yo sabbalokassa nivātavutti,",
  "ja453:3.2": "Itthīpumānaṁ sahadārakānaṁ;",
  "ja453:3.3": "Khantā duruttānamappaṭikūlavādī,",
  "ja453:3.4": "Adhivāsanaṁ sotthānaṁ tadāhu.",
  "ja453:4.1": "Yo nāvajānāti sahāyamitte,",
  "ja453:4.2": "Sippena kulyāhi dhanena jaccā;",
  "ja453:4.3": "Rucipañño atthakāle matīmā,",
  "ja453:4.4": "Sahāyesu ve sotthānaṁ tadāhu.",
  "ja453:5.1": "Mittāni ve yassa bhavanti santo,",
  "ja453:5.2": "Saṁvissatthā avisaṁvādakassa;",
  "ja453:5.3": "Na mittadubbhī saṁvibhāgī dhanena,",
  "ja453:5.4": "Mittesu ve sotthānaṁ tadāhu.",
  "ja453:6.1": "Yassa bhariyā tulyavayā samaggā,",
  "ja453:6.2": "Anubbatā dhammakāmā pajātā;",
  "ja453:6.3": "Koliniyā sīlavatī patibbatā,",
  "ja453:6.4": "Dāresu ve sotthānaṁ tadāhu.",
  "ja453:7.1": "Yassa rājā bhūtapati yasassī,",
  "ja453:7.2": "Jānāti soceyyaṁ parakkamañca;",
  "ja453:7.3": "Advejjhatā suhadayaṁ mamanti,",
  "ja453:7.4": "Rājūsu ve sotthānaṁ tadāhu.",
  "ja453:8.1": "Annañca pānañca dadāti saddho,",
  "ja453:8.2": "Mālañca gandhañca vilepanañca;",
  "ja453:8.3": "Pasannacitto anumodamāno,",
  "ja453:8.4": "Saggesu ve sotthānaṁ tadāhu.",
  "ja453:9.1": "Yamariyadhammena punanti vuddhā,",
  "ja453:9.2": "Ārādhitā samacariyāya santo;",
  "ja453:9.3": "Bahussutā isayo sīlavanto,",
  "ja453:9.4": "Arahantamajjhe sotthānaṁ tadāhu.",
  "ja453:10.1": "Etāni kho sotthānāni loke,",
  "ja453:10.2": "Viññuppasatthāni sukhudrayāni;",
  "ja453:10.3": "Tānīdha sevetha naro sapañño,",
  "ja453:10.4": "Na hi maṅgale kiñcanamatthi saccan”ti.",
  "ja453:11.1": "Mahāmaṅgalajātakaṁ pannarasamaṁ."
}