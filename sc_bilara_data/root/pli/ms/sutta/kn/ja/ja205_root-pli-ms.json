{
  "ja205:0.1": "Jātaka",
  "ja205:0.2": "Dukanipāta",
  "ja205:0.3": "Nataṁdaḷhavagga",
  "ja205:0.4": "5. Gaṅgeyyajātaka",
  "ja205:1.1": "“Sobhati maccho gaṅgeyyo,",
  "ja205:1.2": "atho sobhati yāmuno;",
  "ja205:1.3": "Catuppadoyaṁ puriso,",
  "ja205:1.4": "nigrodhaparimaṇḍalo;",
  "ja205:1.5": "Īsakāyatagīvo ca,",
  "ja205:1.6": "sabbeva atirocati”.",
  "ja205:2.1": "“Yaṁ pucchito na taṁ akkhāsi,",
  "ja205:2.2": "Aññaṁ akkhāsi pucchito;",
  "ja205:2.3": "Attappasaṁsako poso,",
  "ja205:2.4": "Nāyaṁ asmāka ruccatī”ti.",
  "ja205:3.1": "Gaṅgeyyajātakaṁ pañcamaṁ."
}