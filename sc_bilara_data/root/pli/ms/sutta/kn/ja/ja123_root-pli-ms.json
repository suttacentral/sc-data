{
  "ja123:0.1": "Jātaka",
  "ja123:0.2": "Ekakanipāta",
  "ja123:0.3": "Kusanāḷivagga",
  "ja123:0.4": "3. Naṅgalīsajātaka",
  "ja123:1.1": "“Asabbatthagāmiṁ vācaṁ,",
  "ja123:1.2": "Bālo sabbattha bhāsati;",
  "ja123:1.3": "Nāyaṁ dadhiṁ vedi na naṅgalīsaṁ,",
  "ja123:1.4": "Dadhippayaṁ maññati naṅgalīsan”ti.",
  "ja123:2.1": "Naṅgalīsajātakaṁ tatiyaṁ."
}