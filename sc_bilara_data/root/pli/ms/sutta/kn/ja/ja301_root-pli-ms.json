{
  "ja301:0.1": "Jātaka",
  "ja301:0.2": "Catukkanipāta",
  "ja301:0.3": "Kāliṅgavagga",
  "ja301:0.4": "1. Cūḷakāliṅgajātaka",
  "ja301:1.1": "“Vivarathimāsaṁ dvāraṁ,",
  "ja301:1.2": "Nagaraṁ pavisantu aruṇarājassa;",
  "ja301:1.3": "Sīhena susiṭṭhena,",
  "ja301:1.4": "Surakkhitaṁ nandisenena”.",
  "ja301:2.1": "“Jayo kaliṅgānamasayhasāhinaṁ,",
  "ja301:2.2": "Parājayo anayo assakānaṁ;",
  "ja301:2.3": "Icceva te bhāsitaṁ brahmacāri,",
  "ja301:2.4": "Na ujjubhūtā vitathaṁ bhaṇanti”.",
  "ja301:3.1": "“Devā musāvādamupātivattā,",
  "ja301:3.2": "Saccaṁ dhanaṁ paramaṁ tesu sakka;",
  "ja301:3.3": "Taṁ te musā bhāsitaṁ devarāja,",
  "ja301:3.4": "Kiṁ vā paṭicca maghavā mahinda”.",
  "ja301:4.1": "“Nanu te sutaṁ brāhmaṇa bhaññamāne,",
  "ja301:4.2": "Devā na issanti purisaparakkamassa;",
  "ja301:4.3": "Damo samādhi manaso abhejjo,",
  "ja301:4.4": "Abyaggatā nikkamanañca kāle;",
  "ja301:4.5": "Daḷhañca viriyaṁ purisaparakkamo ca,",
  "ja301:4.6": "Teneva āsi vijayo assakānan”ti.",
  "ja301:5.1": "Cūḷakāliṅgajātakaṁ paṭhamaṁ."
}