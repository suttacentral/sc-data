{
  "ja66:0.1": "Jātaka",
  "ja66:0.2": "Ekakanipāta",
  "ja66:0.3": "Itthivagga",
  "ja66:0.4": "6. Mudulakkhaṇajātaka",
  "ja66:1.1": "“Ekā icchā pure āsi,",
  "ja66:1.2": "aladdhā mudulakkhaṇaṁ;",
  "ja66:1.3": "Yato laddhā aḷārakkhī,",
  "ja66:1.4": "icchā icchaṁ vijāyathā”ti.",
  "ja66:2.1": "Mudulakkhaṇajātakaṁ chaṭṭhaṁ."
}