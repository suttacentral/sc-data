{
  "ja323:0.1": "Jātaka",
  "ja323:0.2": "Catukkanipāta",
  "ja323:0.3": "Kuṭidūsakavagga",
  "ja323:0.4": "3. Brahmadattajātaka",
  "ja323:1.1": "“Dvayaṁ yācanako rāja,",
  "ja323:1.2": "brahmadatta nigacchati;",
  "ja323:1.3": "Alābhaṁ dhanalābhaṁ vā,",
  "ja323:1.4": "evaṁ dhammā hi yācanā.",
  "ja323:2.1": "Yācanaṁ rodanaṁ āhu,",
  "ja323:2.2": "pañcālānaṁ rathesabha;",
  "ja323:2.3": "Yo yācanaṁ paccakkhāti,",
  "ja323:2.4": "tamāhu paṭirodanaṁ.",
  "ja323:3.1": "Mā maddasaṁsu rodantaṁ,",
  "ja323:3.2": "pañcālā susamāgatā;",
  "ja323:3.3": "Tuvaṁ vā paṭirodantaṁ,",
  "ja323:3.4": "tasmā icchāmahaṁ raho”.",
  "ja323:4.1": "“Dadāmi te brāhmaṇa rohiṇīnaṁ,",
  "ja323:4.2": "Gavaṁ sahassaṁ saha puṅgavena;",
  "ja323:4.3": "Ariyo hi ariyassa kathaṁ na dajjā,",
  "ja323:4.4": "Sutvāna gāthā tava dhammayuttā”ti.",
  "ja323:5.1": "Brahmadattajātakaṁ tatiyaṁ."
}