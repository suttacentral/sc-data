{
  "ja114:0.1": "Jātaka",
  "ja114:0.2": "Ekakanipāta",
  "ja114:0.3": "Haṁcivagga",
  "ja114:0.4": "4. Mitacintijātaka",
  "ja114:1.1": "“Bahucintī appacintī,",
  "ja114:1.2": "ubho jāle abajjhare;",
  "ja114:1.3": "Mitacintī pamocesī,",
  "ja114:1.4": "ubho tattha samāgatā”ti.",
  "ja114:2.1": "Mitacintijātakaṁ catutthaṁ."
}