{
  "ja16:0.1": "Jātaka",
  "ja16:0.2": "Ekakanipāta",
  "ja16:0.3": "Sīlavagga",
  "ja16:0.4": "6. Tipallatthamigajātaka",
  "ja16:1.1": "“Migaṁ tipallatthamanekamāyaṁ,",
  "ja16:1.2": "Aṭṭhakkhuraṁ aḍḍharattāpapāyiṁ;",
  "ja16:1.3": "Ekena sotena chamāssasanto,",
  "ja16:1.4": "Chahi kalāhitibhoti bhāgineyyo”ti.",
  "ja16:2.1": "Tipallatthamigajātakaṁ chaṭṭhaṁ."
}