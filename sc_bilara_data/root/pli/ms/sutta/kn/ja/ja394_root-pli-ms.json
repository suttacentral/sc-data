{
  "ja394:0.1": "Jātaka",
  "ja394:0.2": "Chakkanipāta",
  "ja394:0.3": "Kharaputtavagga",
  "ja394:0.4": "9. Vaṭṭakajātaka",
  "ja394:1.1": "“Paṇītaṁ bhuñjase bhattaṁ,",
  "ja394:1.2": "sappitelañca mātula;",
  "ja394:1.3": "Atha kena nu vaṇṇena,",
  "ja394:1.4": "kiso tvamasi vāyasa”.",
  "ja394:2.1": "“Amittamajjhe vasato,",
  "ja394:2.2": "tesu āmisamesato;",
  "ja394:2.3": "Niccaṁ ubbiggahadayassa,",
  "ja394:2.4": "kuto kākassa daḷhiyaṁ.",
  "ja394:3.1": "Niccaṁ ubbegino kākā,",
  "ja394:3.2": "dhaṅkā pāpena kammunā;",
  "ja394:3.3": "Laddho piṇḍo na pīṇeti,",
  "ja394:3.4": "kiso tenasmi vaṭṭaka.",
  "ja394:4.1": "Lūkhāni tiṇabījāni,",
  "ja394:4.2": "appasnehāni bhuñjasi;",
  "ja394:4.3": "Atha kena nu vaṇṇena,",
  "ja394:4.4": "thūlo tvamasi vaṭṭaka”.",
  "ja394:5.1": "“Appicchā appacintāya,",
  "ja394:5.2": "adūragamanena ca;",
  "ja394:5.3": "Laddhāladdhena yāpento,",
  "ja394:5.4": "thūlo tenasmi vāyasa.",
  "ja394:6.1": "Appicchassa hi posassa,",
  "ja394:6.2": "appacintasukhassa ca;",
  "ja394:6.3": "Susaṅgahitamānassa,",
  "ja394:6.4": "vuttī susamudānayā”ti.",
  "ja394:7.1": "Vaṭṭakajātakaṁ navamaṁ."
}