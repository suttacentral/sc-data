{
  "ja392:0.1": "Jātaka",
  "ja392:0.2": "Chakkanipāta",
  "ja392:0.3": "Kharaputtavagga",
  "ja392:0.4": "7. Siṅghapupphajātaka",
  "ja392:1.1": "“Yametaṁ vārijaṁ pupphaṁ,",
  "ja392:1.2": "Adinnaṁ upasiṅghasi;",
  "ja392:1.3": "Ekaṅgametaṁ theyyānaṁ,",
  "ja392:1.4": "Gandhathenosi mārisa”.",
  "ja392:2.1": "“Na harāmi na bhañjāmi,",
  "ja392:2.2": "ārā siṅghāmi vārijaṁ;",
  "ja392:2.3": "Atha kena nu vaṇṇena,",
  "ja392:2.4": "gandhathenoti vuccati.",
  "ja392:3.1": "Yoyaṁ bhisāni khaṇati,",
  "ja392:3.2": "puṇḍarīkāni bhañjati;",
  "ja392:3.3": "Evaṁ ākiṇṇakammanto,",
  "ja392:3.4": "kasmā eso na vuccati”.",
  "ja392:4.1": "“Ākiṇṇaluddo puriso,",
  "ja392:4.2": "dhāticelaṁva makkhito;",
  "ja392:4.3": "Tasmiṁ me vacanaṁ natthi,",
  "ja392:4.4": "tañcārahāmi vattave.",
  "ja392:5.1": "Anaṅgaṇassa posassa,",
  "ja392:5.2": "niccaṁ sucigavesino;",
  "ja392:5.3": "Vālaggamattaṁ pāpassa,",
  "ja392:5.4": "abbhāmattaṁva khāyati”.",
  "ja392:6.1": "“Addhā maṁ yakkha jānāsi,",
  "ja392:6.2": "atho maṁ anukampasi;",
  "ja392:6.3": "Punapi yakkha vajjāsi,",
  "ja392:6.4": "yadā passasi edisaṁ”.",
  "ja392:7.1": "“Neva taṁ upajīvāmi,",
  "ja392:7.2": "napi te bhatakāmhase;",
  "ja392:7.3": "Tvameva bhikkhu jāneyya,",
  "ja392:7.4": "yena gaccheyya suggatin”ti.",
  "ja392:8.1": "Siṅghapupphajātakaṁ sattamaṁ."
}