{
  "ja62:0.1": "Jātaka",
  "ja62:0.2": "Ekakanipāta",
  "ja62:0.3": "Itthivagga",
  "ja62:0.4": "2. Aṇḍabhūtajātaka",
  "ja62:1.1": "“Yaṁ brāhmaṇo avādesi,",
  "ja62:1.2": "vīṇaṁ samukhaveṭhito;",
  "ja62:1.3": "Aṇḍabhūtā bhatā bhariyā,",
  "ja62:1.4": "tāsu ko jātu vissase”ti.",
  "ja62:2.1": "Aṇḍabhūtajātakaṁ dutiyaṁ."
}