{
  "ja284:0.1": "Jātaka",
  "ja284:0.2": "Tikanipāta",
  "ja284:0.3": "Abbhantaravagga",
  "ja284:0.4": "4. Sirijātaka",
  "ja284:1.1": "“Yaṁ ussukā saṅgharanti,",
  "ja284:1.2": "alakkhikā bahuṁ dhanaṁ;",
  "ja284:1.3": "Sippavanto asippā ca,",
  "ja284:1.4": "lakkhivā tāni bhuñjati.",
  "ja284:2.1": "Sabbattha katapuññassa,",
  "ja284:2.2": "aticcaññeva pāṇino;",
  "ja284:2.3": "Uppajjanti bahū bhogā,",
  "ja284:2.4": "appanāyatanesupi.",
  "ja284:3.1": "Kukkuṭo maṇayo daṇḍo,",
  "ja284:3.2": "thiyo ca puññalakkhaṇā;",
  "ja284:3.3": "Uppajjanti apāpassa,",
  "ja284:3.4": "katapuññassa jantuno”ti.",
  "ja284:4.1": "Sirijātakaṁ catutthaṁ."
}