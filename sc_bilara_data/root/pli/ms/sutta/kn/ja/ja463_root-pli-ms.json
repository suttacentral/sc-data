{
  "ja463:0.1": "Jātaka ",
  "ja463:0.2": "Ekādasakanipāta ",
  "ja463:0.3": "Mātuposakavagga ",
  "ja463:0.4": "9. Suppārakajātaka ",
  "ja463:1.1": "“Ummujjanti nimujjanti, ",
  "ja463:1.2": "manussā khuranāsikā; ",
  "ja463:1.3": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:1.4": "samuddo katamo ayaṁ”. ",
  "ja463:2.1": "“Kurukacchā payātānaṁ, ",
  "ja463:2.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:2.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:2.4": "khuramālīti vuccati”. ",
  "ja463:3.1": "“Yathā aggīva suriyova, ",
  "ja463:3.2": "samuddo paṭidissati; ",
  "ja463:3.3": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:3.4": "samuddo katamo ayaṁ”. ",
  "ja463:4.1": "“Kurukacchā payātānaṁ, ",
  "ja463:4.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:4.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:4.4": "aggimālīti vuccati”. ",
  "ja463:5.1": "“Yathā dadhīva khīraṁva, ",
  "ja463:5.2": "samuddo paṭidissati; ",
  "ja463:5.3": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:5.4": "samuddo katamo ayaṁ”. ",
  "ja463:6.1": "“Kurukacchā payātānaṁ, ",
  "ja463:6.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:6.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:6.4": "dadhimālīti vuccati”. ",
  "ja463:7.1": "“Yathā kusova sassova, ",
  "ja463:7.2": "samuddo paṭidissati; ",
  "ja463:7.3": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:7.4": "samuddo katamo ayaṁ”. ",
  "ja463:8.1": "“Kurukacchā payātānaṁ, ",
  "ja463:8.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:8.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:8.4": "kusamālīti vuccati”. ",
  "ja463:9.1": "“Yathā naḷova veḷūva, ",
  "ja463:9.2": "samuddo paṭidissati; ",
  "ja463:9.3": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:9.4": "samuddo katamo ayaṁ”. ",
  "ja463:10.1": "“Kurukacchā payātānaṁ, ",
  "ja463:10.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:10.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:10.4": "naḷamālīti vuccati”. ",
  "ja463:11.1": "“Mahabbhayo bhiṁsanako, ",
  "ja463:11.2": "saddo suyyatimānuso; ",
  "ja463:11.3": "Yathā sobbho papātova, ",
  "ja463:11.4": "samuddo paṭidissati; ",
  "ja463:11.5": "Suppārakaṁ taṁ pucchāma, ",
  "ja463:11.6": "samuddo katamo ayaṁ”. ",
  "ja463:12.1": "“Kurukacchā payātānaṁ, ",
  "ja463:12.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja463:12.3": "Nāvāya vippanaṭṭhāya, ",
  "ja463:12.4": "baḷavāmukhīti vuccati. ",
  "ja463:13.1": "Yato sarāmi attānaṁ, ",
  "ja463:13.2": "Yato pattosmi viññutaṁ; ",
  "ja463:13.3": "Nābhijānāmi sañcicca, ",
  "ja463:13.4": "Ekapāṇampi hiṁsitaṁ; ",
  "ja463:13.5": "Etena saccavajjena, ",
  "ja463:13.6": "Sotthiṁ nāvā nivattatū”ti. ",
  "ja463:14.1": "Suppārakajātakaṁ navamaṁ. ",
  "ja463:15.1": "Ekādasakanipātaṁ niṭṭhitaṁ. ",
  "ja463:16.1": "Tassuddānaṁ ",
  "ja463:17.1": "Sirimātusuposakanāgavaro, ",
  "ja463:17.2": "Puna juṇhaka dhammamudayavaro; ",
  "ja463:17.3": "Atha pāni yudhañcayako ca dasa- ",
  "ja463:17.4": "Ratha saṁvara pāragatena navāti. "
}