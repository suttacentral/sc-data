{
  "ja288:0.1": "Jātaka",
  "ja288:0.2": "Tikanipāta",
  "ja288:0.3": "Abbhantaravagga",
  "ja288:0.4": "8. Macchuddānajātaka",
  "ja288:1.1": "“Agghanti macchā adhikaṁ sahassaṁ,",
  "ja288:1.2": "Na so atthi yo imaṁ saddaheyya;",
  "ja288:1.3": "Mayhañca assu idha satta māsā,",
  "ja288:1.4": "Ahampi taṁ macchuddānaṁ kiṇeyyaṁ”.",
  "ja288:2.1": "“Macchānaṁ bhojanaṁ datvā,",
  "ja288:2.2": "mama dakkhiṇamādisi;",
  "ja288:2.3": "Taṁ dakkhiṇaṁ sarantiyā,",
  "ja288:2.4": "kataṁ apacitiṁ tayā”.",
  "ja288:3.1": "“Paduṭṭhacittassa na phāti hoti,",
  "ja288:3.2": "Na cāpi taṁ devatā pūjayanti;",
  "ja288:3.3": "Yo bhātaraṁ pettikaṁ sāpateyyaṁ,",
  "ja288:3.4": "Avañcayī dukkaṭakammakārī”ti.",
  "ja288:4.1": "Macchuddānajātakaṁ aṭṭhamaṁ."
}