{
  "ja71:0.1": "Jātaka",
  "ja71:0.2": "Ekakanipāta",
  "ja71:0.3": "Varuṇavagga",
  "ja71:0.4": "1. Varuṇajātaka",
  "ja71:1.1": "“Yo pubbe karaṇīyāni,",
  "ja71:1.2": "pacchā so kātumicchati;",
  "ja71:1.3": "Varuṇakaṭṭhabhañjova,",
  "ja71:1.4": "sa pacchā manutappatī”ti.",
  "ja71:2.1": "Varuṇajātakaṁ paṭhamaṁ."
}