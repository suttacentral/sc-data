{
  "ja182:0.1": "Jātaka",
  "ja182:0.2": "Dukanipāta",
  "ja182:0.3": "Asadisavagga",
  "ja182:0.4": "2. Saṅgāmāvacarajātaka",
  "ja182:1.1": "“Saṅgāmāvacaro sūro,",
  "ja182:1.2": "balavā iti vissuto;",
  "ja182:1.3": "Kiṁ nu toraṇamāsajja,",
  "ja182:1.4": "paṭikkamasi kuñjara.",
  "ja182:2.1": "Omadda khippaṁ palighaṁ,",
  "ja182:2.2": "esikāni ca abbaha;",
  "ja182:2.3": "Toraṇāni ca madditvā,",
  "ja182:2.4": "khippaṁ pavisa kuñjarā”ti.",
  "ja182:3.1": "Saṅgāmāvacarajātakaṁ dutiyaṁ."
}