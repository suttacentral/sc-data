{
  "ja280:0.1": "Jātaka",
  "ja280:0.2": "Tikanipāta",
  "ja280:0.3": "Udapānavagga",
  "ja280:0.4": "10. Puṭadūsakajātaka",
  "ja280:1.1": "“Addhā hi nūna migarājā,",
  "ja280:1.2": "puṭakammassa kovido;",
  "ja280:1.3": "Tathā hi puṭaṁ dūseti,",
  "ja280:1.4": "aññaṁ nūna karissati”.",
  "ja280:2.1": "“Na me mātā vā pitā vā,",
  "ja280:2.2": "puṭakammassa kovido;",
  "ja280:2.3": "Kataṁ kataṁ kho dūsema,",
  "ja280:2.4": "evaṁ dhammamidaṁ kulaṁ”.",
  "ja280:3.1": "“Yesaṁ vo ediso dhammo,",
  "ja280:3.2": "Adhammo pana kīdiso;",
  "ja280:3.3": "Mā vo dhammaṁ adhammaṁ vā,",
  "ja280:3.4": "Addasāma kudācanan”ti.",
  "ja280:4.1": "Puṭadūsakajātakaṁ dasamaṁ.",
  "ja280:4.2": "Udapānavaggo tatiyo.",
  "ja280:5.1": "Tassuddānaṁ",
  "ja280:6.1": "Udapānavaraṁ vanabyaggha kapi,",
  "ja280:6.2": "Sikhinī ca balāka ruciravaro;",
  "ja280:6.3": "Sujanādhiparomakadūsa puna,",
  "ja280:6.4": "Satapattavaro puṭakamma dasāti."
}