{
  "ja405:0.1": "Jātaka",
  "ja405:0.2": "Sattakanipāta",
  "ja405:0.3": "Kukkuvagga",
  "ja405:0.4": "10. Bakajātaka",
  "ja405:1.1": "“Dvāsattati gotama puññakammā,",
  "ja405:1.2": "Vasavattino jātijaraṁ atītā;",
  "ja405:1.3": "Ayamantimā vedagū brahmapatti,",
  "ja405:1.4": "Asmābhijappanti janā anekā”.",
  "ja405:2.1": "“Appañhi etaṁ na hi dīghamāyu,",
  "ja405:2.2": "Yaṁ tvaṁ baka maññasi dīghamāyuṁ;",
  "ja405:2.3": "Sataṁ sahassāni nirabbudānaṁ,",
  "ja405:2.4": "Āyuṁ pajānāmi tavāha brahme”.",
  "ja405:3.1": "“Anantadassī bhagavāhamasmi,",
  "ja405:3.2": "Jātijjaraṁ sokamupātivatto;",
  "ja405:3.3": "Kiṁ me purāṇaṁ vatasīlavattaṁ,",
  "ja405:3.4": "Ācikkha me taṁ yamahaṁ vijaññaṁ”.",
  "ja405:4.1": "“Yaṁ tvaṁ apāyesi bahū manusse,",
  "ja405:4.2": "Pipāsite ghammani samparete;",
  "ja405:4.3": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:4.4": "Suttappabuddhova anussarāmi.",
  "ja405:5.1": "Yaṁ eṇikūlasmi janaṁ gahītaṁ,",
  "ja405:5.2": "Amocayī gayhaka niyyamānaṁ;",
  "ja405:5.3": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:5.4": "Suttappabuddhova anussarāmi.",
  "ja405:6.1": "Gaṅgāya sotasmiṁ gahītanāvaṁ,",
  "ja405:6.2": "Luddena nāgena manussakappā;",
  "ja405:6.3": "Amocayi tvaṁ balasā pasayha,",
  "ja405:6.4": "Taṁ te purāṇaṁ vatasīlavattaṁ;",
  "ja405:6.5": "Suttappabuddhova anussarāmi.",
  "ja405:7.1": "Kappo ca te baddhacaro ahosiṁ,",
  "ja405:7.2": "Sambuddhimantaṁ vatinaṁ amaññaṁ;",
  "ja405:7.3": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:7.4": "Suttappabuddhova anussarāmi”.",
  "ja405:8.1": "“Addhā pajānāsi mametamāyuṁ,",
  "ja405:8.2": "Aññampi jānāsi tathā hi buddho;",
  "ja405:8.3": "Tathā hi tāyaṁ jalitānubhāvo,",
  "ja405:8.4": "Obhāsayaṁ tiṭṭhati brahmalokan”ti.",
  "ja405:9.1": "Bakajātakaṁ dasamaṁ.",
  "ja405:9.2": "Kukkuvaggo paṭhamo.",
  "ja405:10.1": "Tassuddānaṁ",
  "ja405:11.1": "Varakaṇṇika cāpavaro sutano,",
  "ja405:11.2": "Atha gijjha sarohitamacchavaro;",
  "ja405:11.3": "Puna paṇṇaka senaka yācanako,",
  "ja405:11.4": "Atha veri sabrahmabakena dasāti."
}