{
  "ja468:0.1": "Jātaka",
  "ja468:0.2": "Dvādasakanipāta",
  "ja468:0.3": "Cūḷakuṇālavagga",
  "ja468:0.4": "5. Janasandhajātaka",
  "ja468:1.1": "“Dasa khalu imāni ṭhānāni,",
  "ja468:1.2": "Yāni pubbe akaritvā;",
  "ja468:1.3": "Sa pacchā manutappati,",
  "ja468:1.4": "Iccevāha janasandho.",
  "ja468:2.1": "Aladdhā vittaṁ tappati,",
  "ja468:2.2": "pubbe asamudānitaṁ;",
  "ja468:2.3": "Na pubbe dhanamesissaṁ,",
  "ja468:2.4": "iti pacchānutappati.",
  "ja468:3.1": "Sakyarūpaṁ pure santaṁ,",
  "ja468:3.2": "mayā sippaṁ na sikkhitaṁ;",
  "ja468:3.3": "Kicchā vutti asippassa,",
  "ja468:3.4": "iti pacchānutappati.",
  "ja468:4.1": "Kūṭavedī pure āsiṁ,",
  "ja468:4.2": "pisuṇo piṭṭhimaṁsiko;",
  "ja468:4.3": "Caṇḍo ca pharuso cāpi,",
  "ja468:4.4": "iti pacchānutappati.",
  "ja468:5.1": "Pāṇātipātī pure āsiṁ,",
  "ja468:5.2": "luddo cāpi anāriyo;",
  "ja468:5.3": "Bhūtānaṁ nāpacāyissaṁ,",
  "ja468:5.4": "iti pacchānutappati.",
  "ja468:6.1": "Bahūsu vata santīsu,",
  "ja468:6.2": "anāpādāsu itthisu;",
  "ja468:6.3": "Paradāraṁ asevissaṁ,",
  "ja468:6.4": "iti pacchānutappati.",
  "ja468:7.1": "Bahumhi vata santamhi,",
  "ja468:7.2": "annapāne upaṭṭhite;",
  "ja468:7.3": "Na pubbe adadaṁ dānaṁ,",
  "ja468:7.4": "iti pacchānutappati.",
  "ja468:8.1": "Mātaraṁ pitarañcāpi,",
  "ja468:8.2": "jiṇṇakaṁ gatayobbanaṁ;",
  "ja468:8.3": "Pahu santo na posissaṁ,",
  "ja468:8.4": "iti pacchānutappati.",
  "ja468:9.1": "Ācariyamanusatthāraṁ,",
  "ja468:9.2": "sabbakāmarasāharaṁ;",
  "ja468:9.3": "Pitaraṁ atimaññissaṁ,",
  "ja468:9.4": "iti pacchānutappati.",
  "ja468:10.1": "Samaṇe brāhmaṇe cāpi,",
  "ja468:10.2": "sīlavante bahussute;",
  "ja468:10.3": "Na pubbe payirupāsissaṁ,",
  "ja468:10.4": "iti pacchānutappati.",
  "ja468:11.1": "Sādhu hoti tapo ciṇṇo,",
  "ja468:11.2": "santo ca payirupāsito;",
  "ja468:11.3": "Na ca pubbe tapo ciṇṇo,",
  "ja468:11.4": "iti pacchānutappati.",
  "ja468:12.1": "Yo ca etāni ṭhānāni,",
  "ja468:12.2": "yoniso paṭipajjati;",
  "ja468:12.3": "Karaṁ purisakiccāni,",
  "ja468:12.4": "sa pacchā nānutappatī”ti.",
  "ja468:13.1": "Janasandhajātakaṁ pañcamaṁ."
}