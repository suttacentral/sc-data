{
  "ja87:0.1": "Jātaka",
  "ja87:0.2": "Ekakanipāta",
  "ja87:0.3": "Apāyimhavagga",
  "ja87:0.4": "7. Maṅgalajātaka",
  "ja87:1.1": "“Yassa maṅgalā samūhatāse,",
  "ja87:1.2": "Uppātā supinā ca lakkhaṇā ca;",
  "ja87:1.3": "So maṅgaladosavītivatto,",
  "ja87:1.4": "Yugayogādhigato na jātumetī”ti.",
  "ja87:2.1": "Maṅgalajātakaṁ sattamaṁ."
}