{
  "ja352:0.1": "Jātaka",
  "ja352:0.2": "Pañcakanipāta",
  "ja352:0.3": "Maṇikuṇḍalavagga",
  "ja352:0.4": "2. Sujātajātaka",
  "ja352:1.1": "“Kiṁ nu santaramānova,",
  "ja352:1.2": "lāyitvā haritaṁ tiṇaṁ;",
  "ja352:1.3": "Khāda khādāti lapasi,",
  "ja352:1.4": "gatasattaṁ jaraggavaṁ.",
  "ja352:2.1": "Na hi annena pānena,",
  "ja352:2.2": "mato goṇo samuṭṭhahe;",
  "ja352:2.3": "Tvañca tucchaṁ vilapasi,",
  "ja352:2.4": "yathā taṁ dummatī tathā”.",
  "ja352:3.1": "“Tatheva tiṭṭhati sīsaṁ,",
  "ja352:3.2": "hatthapādā ca vāladhi;",
  "ja352:3.3": "Sotā tatheva tiṭṭhanti,",
  "ja352:3.4": "maññe goṇo samuṭṭhahe.",
  "ja352:4.1": "Nevayyakassa sīsañca,",
  "ja352:4.2": "hatthapādā ca dissare;",
  "ja352:4.3": "Rudaṁ mattikathūpasmiṁ,",
  "ja352:4.4": "nanu tvaññeva dummati”.",
  "ja352:5.1": "“Ādittaṁ vata maṁ santaṁ,",
  "ja352:5.2": "ghatasittaṁva pāvakaṁ;",
  "ja352:5.3": "Vārinā viya osiñcaṁ,",
  "ja352:5.4": "sabbaṁ nibbāpaye daraṁ.",
  "ja352:6.1": "Abbahī vata me sallaṁ,",
  "ja352:6.2": "yamāsi hadayassitaṁ;",
  "ja352:6.3": "Yo me sokaparetassa,",
  "ja352:6.4": "pitusokaṁ apānudi.",
  "ja352:7.1": "Sohaṁ abbūḷhasallosmi,",
  "ja352:7.2": "vītasoko anāvilo;",
  "ja352:7.3": "Na socāmi na rodāmi,",
  "ja352:7.4": "tava sutvāna māṇava.",
  "ja352:8.1": "Evaṁ karonti sappaññā,",
  "ja352:8.2": "ye honti anukampakā;",
  "ja352:8.3": "Vinivattenti sokamhā,",
  "ja352:8.4": "sujāto pitaraṁ yathā”ti.",
  "ja352:9.1": "Sujātajātakaṁ dutiyaṁ."
}