{
  "ja262:0.1": "Jātaka",
  "ja262:0.2": "Tikanipāta",
  "ja262:0.3": "Padumavagga",
  "ja262:0.4": "2. Mudupāṇijātaka",
  "ja262:1.1": "“Pāṇi ce muduko cassa,",
  "ja262:1.2": "nāgo cassa sukārito;",
  "ja262:1.3": "Andhakāro ca vasseyya,",
  "ja262:1.4": "atha nūna tadā siyā”.",
  "ja262:2.1": "“Analā mudusambhāsā,",
  "ja262:2.2": "duppūrā tā nadīsamā;",
  "ja262:2.3": "Sīdanti naṁ viditvāna,",
  "ja262:2.4": "ārakā parivajjaye.",
  "ja262:3.1": "Yaṁ etā upasevanti,",
  "ja262:3.2": "chandasā vā dhanena vā;",
  "ja262:3.3": "Jātavedova saṁ ṭhānaṁ,",
  "ja262:3.4": "khippaṁ anudahanti nan”ti.",
  "ja262:4.1": "Mudupāṇijātakaṁ dutiyaṁ."
}