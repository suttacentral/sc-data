{
  "ja281:0.1": "Jātaka",
  "ja281:0.2": "Tikanipāta",
  "ja281:0.3": "Abbhantaravagga",
  "ja281:0.4": "1. Abbhantarajātaka",
  "ja281:1.1": "“Abbhantaro nāma dumo,",
  "ja281:1.2": "yassa dibyamidaṁ phalaṁ;",
  "ja281:1.3": "Bhutvā dohaḷinī nārī,",
  "ja281:1.4": "cakkavattiṁ vijāyati.",
  "ja281:2.1": "Tvampi bhadde mahesīsi,",
  "ja281:2.2": "sā cāpi patino piyā;",
  "ja281:2.3": "Āharissati te rājā,",
  "ja281:2.4": "idaṁ abbhantaraṁ phalaṁ”.",
  "ja281:3.1": "“Bhatturatthe parakkanto,",
  "ja281:3.2": "yaṁ ṭhānamadhigacchati;",
  "ja281:3.3": "Sūro attapariccāgī,",
  "ja281:3.4": "labhamāno bhavāmahan”ti.",
  "ja281:4.1": "Abbhantarajātakaṁ paṭhamaṁ."
}