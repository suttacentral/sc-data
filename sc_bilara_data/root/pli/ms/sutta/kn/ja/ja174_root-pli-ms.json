{
  "ja174:0.1": "Jātaka",
  "ja174:0.2": "Dukanipāta",
  "ja174:0.3": "Kalyāṇavagga",
  "ja174:0.4": "4. Dubbhiyamakkaṭajātaka",
  "ja174:1.1": "“Adamha te vāri pahūtarūpaṁ,",
  "ja174:1.2": "Ghammābhitattassa pipāsitassa;",
  "ja174:1.3": "So dāni pitvāna kiriṅkarosi,",
  "ja174:1.4": "Asaṅgamo pāpajanena seyyo”.",
  "ja174:2.1": "“Ko te suto vā diṭṭho vā,",
  "ja174:2.2": "sīlavā nāma makkaṭo;",
  "ja174:2.3": "Idāni kho taṁ ohacchaṁ,",
  "ja174:2.4": "esā asmāka dhammatā”ti.",
  "ja174:3.1": "Dubbhiyamakkaṭajātakaṁ catutthaṁ."
}