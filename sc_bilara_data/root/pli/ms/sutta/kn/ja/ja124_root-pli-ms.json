{
  "ja124:0.1": "Jātaka",
  "ja124:0.2": "Ekakanipāta",
  "ja124:0.3": "Kusanāḷivagga",
  "ja124:0.4": "4. Ambajātaka",
  "ja124:1.1": "“Vāyametheva puriso,",
  "ja124:1.2": "na nibbindeyya paṇḍito;",
  "ja124:1.3": "Vāyāmassa phalaṁ passa,",
  "ja124:1.4": "bhuttā ambā anītihan”ti.",
  "ja124:2.1": "Ambajātakaṁ catutthaṁ."
}