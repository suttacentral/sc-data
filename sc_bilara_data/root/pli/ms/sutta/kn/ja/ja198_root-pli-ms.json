{
  "ja198:0.1": "Jātaka",
  "ja198:0.2": "Dukanipāta",
  "ja198:0.3": "Ruhakavagga",
  "ja198:0.4": "8. Rādhajātaka",
  "ja198:1.1": "“Pavāsā āgato tāta,",
  "ja198:1.2": "idāni nacirāgato;",
  "ja198:1.3": "Kaccinnu tāta te mātā,",
  "ja198:1.4": "na aññamupasevati”.",
  "ja198:2.1": "“Na kho panetaṁ subhaṇaṁ,",
  "ja198:2.2": "Giraṁ saccupasaṁhitaṁ;",
  "ja198:2.3": "Sayetha poṭṭhapādova,",
  "ja198:2.4": "Mummure upakūthito”ti.",
  "ja198:3.1": "Rādhajātakaṁ aṭṭhamaṁ."
}