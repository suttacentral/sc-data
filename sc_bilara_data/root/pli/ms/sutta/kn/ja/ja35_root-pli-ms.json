{
  "ja35:0.1": "Jātaka",
  "ja35:0.2": "Ekakanipāta",
  "ja35:0.3": "Kulāvakavagga",
  "ja35:0.4": "5. Vaṭṭakajātaka",
  "ja35:1.1": "“Santi pakkhā apatanā,",
  "ja35:1.2": "santi pādā avañcanā;",
  "ja35:1.3": "Mātāpitā ca nikkhantā,",
  "ja35:1.4": "jātaveda paṭikkamā”ti.",
  "ja35:2.1": "Vaṭṭakajātakaṁ pañcamaṁ."
}