{
  "ja214:0.1": "Jātaka",
  "ja214:0.2": "Dukanipāta",
  "ja214:0.3": "Bīraṇathambhavagga",
  "ja214:0.4": "4. Puṇṇanadījātaka",
  "ja214:1.1": "“Puṇṇaṁ nadiṁ yena ca peyyamāhu,",
  "ja214:1.2": "Jātaṁ yavaṁ yena ca guyhamāhu;",
  "ja214:1.3": "Dūraṁ gataṁ yena ca avhayanti,",
  "ja214:1.4": "So tyāgato handa ca bhuñja brāhmaṇa”.",
  "ja214:2.1": "“Yato maṁ saratī rājā,",
  "ja214:2.2": "vāyasampi pahetave;",
  "ja214:2.3": "Haṁsā koñcā mayūrā ca,",
  "ja214:2.4": "asatīyeva pāpiyā”ti.",
  "ja214:3.1": "Puṇṇanadījātakaṁ catutthaṁ."
}