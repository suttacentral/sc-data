{
  "ja441:0.1": "Jātaka",
  "ja441:0.2": "Dasakanipāta",
  "ja441:0.3": "Catudvāravagga",
  "ja441:0.4": "3. Catuposathiyajātaka",
  "ja441:1.1": "“Yo kopaneyye na karoti kopaṁ,",
  "ja441:1.2": "Na kujjhati sappuriso kadāci;",
  "ja441:1.3": "Kuddhopi so nāvikaroti kopaṁ,",
  "ja441:1.4": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:2.1": "“Ūnūdaro yo sahate jighacchaṁ,",
  "ja441:2.2": "Danto tapassī mitapānabhojano;",
  "ja441:2.3": "Āhārahetu na karoti pāpaṁ,",
  "ja441:2.4": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:3.1": "“Khiḍḍaṁ ratiṁ vippajahitvāna sabbaṁ,",
  "ja441:3.2": "Na cālikaṁ bhāsasi kiñci loke;",
  "ja441:3.3": "Vibhūsaṭṭhānā virato methunasmā,",
  "ja441:3.4": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:4.1": "“Pariggahaṁ lobhadhammañca sabbaṁ,",
  "ja441:4.2": "Yo ve pariññāya pariccajeti;",
  "ja441:4.3": "Dantaṁ ṭhitattaṁ amamaṁ nirāsaṁ,",
  "ja441:4.4": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:5.1": "“Pucchāma kattāramanomapaññaṁ,",
  "ja441:5.2": "Kathāsu no viggaho atthi jāto;",
  "ja441:5.3": "Chindajja kaṅkhaṁ vicikicchitāni,",
  "ja441:5.4": "Tadajja kaṅkhaṁ vitaremu sabbe”.",
  "ja441:6.1": "“Ye paṇḍitā atthadasā bhavanti,",
  "ja441:6.2": "Bhāsanti te yoniso tattha kāle;",
  "ja441:6.3": "Kathaṁ nu kathānaṁ abhāsitānaṁ,",
  "ja441:6.4": "Atthaṁ nayeyyuṁ kusalā janindā.",
  "ja441:7.1": "Kathaṁ have bhāsati nāgarājā,",
  "ja441:7.2": "Garuḷo pana venateyyo kimāha;",
  "ja441:7.3": "Gandhabbarājā pana kiṁ vadesi,",
  "ja441:7.4": "Kathaṁ pana kurūnaṁ rājaseṭṭho”.",
  "ja441:8.1": "“Khantiṁ have bhāsati nāgarājā,",
  "ja441:8.2": "Appāhāraṁ garuḷo venateyyo;",
  "ja441:8.3": "Gandhabbarājā rativippahānaṁ,",
  "ja441:8.4": "Akiñcanaṁ kurūnaṁ rājaseṭṭho”.",
  "ja441:9.1": "“Sabbāni etāni subhāsitāni,",
  "ja441:9.2": "Na hettha dubbhāsitamatthi kiñci;",
  "ja441:9.3": "Yasmiñca etāni patiṭṭhitāni,",
  "ja441:9.4": "Arāva nābhyā susamohitāni;",
  "ja441:9.5": "Catubbhi dhammehi samaṅgibhūtaṁ,",
  "ja441:9.6": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:10.1": "“Tuvañhi seṭṭho tvamanuttarosi,",
  "ja441:10.2": "Tvaṁ dhammagū dhammavidū sumedho;",
  "ja441:10.3": "Paññāya pañhaṁ samadhiggahetvā,",
  "ja441:10.4": "Acchecchi dhīro vicikicchitāni;",
  "ja441:10.5": "Acchecchi kaṅkhaṁ vicikicchitāni,",
  "ja441:10.6": "Cundo yathā nāgadantaṁ kharena.",
  "ja441:11.1": "Nīluppalābhaṁ vimalaṁ anagghaṁ,",
  "ja441:11.2": "Vatthaṁ idaṁ dhūmasamānavaṇṇaṁ;",
  "ja441:11.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:11.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:12.1": "Suvaṇṇamālaṁ satapattaphullitaṁ,",
  "ja441:12.2": "Sakesaraṁ ratnasahassamaṇḍitaṁ;",
  "ja441:12.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:12.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:13.1": "Maṇiṁ anagghaṁ ruciraṁ pabhassaraṁ,",
  "ja441:13.2": "Kaṇṭhāvasattaṁ maṇibhūsitaṁ me;",
  "ja441:13.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:13.4": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:14.1": "Gavaṁ sahassaṁ usabhañca nāgaṁ,",
  "ja441:14.2": "Ājaññayutte ca rathe dasa ime;",
  "ja441:14.3": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:14.4": "Dadāmi te gāmavarāni soḷasa”.",
  "ja441:15.1": "“Sāriputto tadā nāgo,",
  "ja441:15.2": "supaṇṇo pana kolito;",
  "ja441:15.3": "Gandhabbarājā anuruddho,",
  "ja441:15.4": "rājā ānandapaṇḍito;",
  "ja441:15.5": "Vidhuro bodhisatto ca,",
  "ja441:15.6": "evaṁ dhāretha jātakan”ti.",
  "ja441:16.1": "Catuposathiyajātakaṁ tatiyaṁ."
}