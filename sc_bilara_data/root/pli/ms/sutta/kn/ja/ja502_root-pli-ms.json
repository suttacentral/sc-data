{
  "ja502:0.1": "Jātaka",
  "ja502:0.2": "Vīsatinipāta",
  "ja502:0.3": "Mātaṅgavagga",
  "ja502:0.4": "6. Cūḷahaṁsajātaka",
  "ja502:1.1": "“Ete haṁsā pakkamanti,",
  "ja502:1.2": "vakkaṅgā bhayameritā;",
  "ja502:1.3": "Harittaca hemavaṇṇa,",
  "ja502:1.4": "kāmaṁ sumukha pakkama.",
  "ja502:2.1": "Ohāya maṁ ñātigaṇā,",
  "ja502:2.2": "ekaṁ pāsavasaṁ gataṁ;",
  "ja502:2.3": "Anapekkhamānā gacchanti,",
  "ja502:2.4": "kiṁ eko avahiyyasi.",
  "ja502:3.1": "Pateva patataṁ seṭṭha,",
  "ja502:3.2": "natthi baddhe sahāyatā;",
  "ja502:3.3": "Mā anīghāya hāpesi,",
  "ja502:3.4": "kāmaṁ sumukha pakkama”.",
  "ja502:4.1": "“Nāhaṁ ‘dukkhapareto’ti,",
  "ja502:4.2": "dhataraṭṭha tuvaṁ jahe;",
  "ja502:4.3": "Jīvitaṁ maraṇaṁ vā me,",
  "ja502:4.4": "tayā saddhiṁ bhavissati”.",
  "ja502:5.1": "“Etadariyassa kalyāṇaṁ,",
  "ja502:5.2": "yaṁ tvaṁ sumukha bhāsasi;",
  "ja502:5.3": "Tañca vīmaṁsamānohaṁ,",
  "ja502:5.4": "‘patatetaṁ’ avassajiṁ”.",
  "ja502:6.1": "“Apadena padaṁ yāti,",
  "ja502:6.2": "antalikkhacaro dijo;",
  "ja502:6.3": "Ārā pāsaṁ na bujjhi tvaṁ,",
  "ja502:6.4": "haṁsānaṁ pavaruttama”.",
  "ja502:7.1": "“Yadā parābhavo hoti,",
  "ja502:7.2": "poso jīvitasaṅkhaye;",
  "ja502:7.3": "Atha jālañca pāsañca,",
  "ja502:7.4": "āsajjāpi na bujjhati”.",
  "ja502:8.1": "“Ete haṁsā pakkamanti,",
  "ja502:8.2": "vakkaṅgā bhayameritā;",
  "ja502:8.3": "Harittaca hemavaṇṇa,",
  "ja502:8.4": "tvaññeva avahiyyasi.",
  "ja502:9.1": "Ete bhutvā ca pitvā ca,",
  "ja502:9.2": "pakkamanti vihaṅgamā;",
  "ja502:9.3": "Anapekkhamānā vakkaṅgā,",
  "ja502:9.4": "tvaññeveko upāsasi.",
  "ja502:10.1": "Kiṁ nu tyāyaṁ dijo hoti,",
  "ja502:10.2": "mutto baddhaṁ upāsasi;",
  "ja502:10.3": "Ohāya sakuṇā yanti,",
  "ja502:10.4": "kiṁ eko avahiyyasi”.",
  "ja502:11.1": "“Rājā me so dijo mitto,",
  "ja502:11.2": "sakhā pāṇasamo ca me;",
  "ja502:11.3": "Neva naṁ vijahissāmi,",
  "ja502:11.4": "yāva kālassa pariyāyaṁ”.",
  "ja502:12.1": "“Yo ca tvaṁ sakhino hetu,",
  "ja502:12.2": "pāṇaṁ cajitumicchasi;",
  "ja502:12.3": "So te sahāyaṁ muñcāmi,",
  "ja502:12.4": "hotu rājā tavānugo”.",
  "ja502:13.1": "“Evaṁ luddaka nandassu,",
  "ja502:13.2": "saha sabbehi ñātibhi;",
  "ja502:13.3": "Yathāhamajja nandāmi,",
  "ja502:13.4": "muttaṁ disvā dijādhipaṁ”.",
  "ja502:14.1": "“Kaccinnu bhoto kusalaṁ,",
  "ja502:14.2": "kacci bhoto anāmayaṁ;",
  "ja502:14.3": "Kacci raṭṭhamidaṁ phītaṁ,",
  "ja502:14.4": "dhammena manusāsasi”.",
  "ja502:15.1": "“Kusalañceva me haṁsa,",
  "ja502:15.2": "atho haṁsa anāmayaṁ;",
  "ja502:15.3": "Atho raṭṭhamidaṁ phītaṁ,",
  "ja502:15.4": "dhammena manusāsahaṁ”.",
  "ja502:16.1": "“Kacci bhoto amaccesu,",
  "ja502:16.2": "doso koci na vijjati;",
  "ja502:16.3": "Kacci ārā amittā te,",
  "ja502:16.4": "chāyā dakkhiṇatoriva”.",
  "ja502:17.1": "“Athopi me amaccesu,",
  "ja502:17.2": "doso koci na vijjati;",
  "ja502:17.3": "Atho ārā amittā me,",
  "ja502:17.4": "chāyā dakkhiṇatoriva”.",
  "ja502:18.1": "“Kacci te sādisī bhariyā,",
  "ja502:18.2": "assavā piyabhāṇinī;",
  "ja502:18.3": "Puttarūpayasūpetā,",
  "ja502:18.4": "tava chandavasānugā”.",
  "ja502:19.1": "“Atho me sādisī bhariyā,",
  "ja502:19.2": "assavā piyabhāṇinī;",
  "ja502:19.3": "Puttarūpayasūpetā,",
  "ja502:19.4": "mama chandavasānugā”.",
  "ja502:20.1": "“Kacci te bahavo puttā,",
  "ja502:20.2": "sujātā raṭṭhavaḍḍhana;",
  "ja502:20.3": "Paññājavena sampannā,",
  "ja502:20.4": "sammodanti tato tato”.",
  "ja502:21.1": "“Satameko ca me puttā,",
  "ja502:21.2": "dhataraṭṭha mayā sutā;",
  "ja502:21.3": "Tesaṁ tvaṁ kiccamakkhāhi,",
  "ja502:21.4": "nāvarujjhanti te vaco”.",
  "ja502:22.1": "“Upapannopi ce hoti,",
  "ja502:22.2": "jātiyā vinayena vā;",
  "ja502:22.3": "Atha pacchā kurute yogaṁ,",
  "ja502:22.4": "kicche āpāsu sīdati.",
  "ja502:23.1": "Tassa saṁhīrapaññassa,",
  "ja502:23.2": "vivaro jāyate mahā;",
  "ja502:23.3": "Rattimandhova rūpāni,",
  "ja502:23.4": "thūlāni manupassati.",
  "ja502:24.1": "Asāre sārayogaññū,",
  "ja502:24.2": "matiṁ na tveva vindati;",
  "ja502:24.3": "Sarabhova giriduggasmiṁ,",
  "ja502:24.4": "antarāyeva sīdati.",
  "ja502:25.1": "Hīnajaccopi ce hoti,",
  "ja502:25.2": "uṭṭhātā dhitimā naro;",
  "ja502:25.3": "Ācārasīlasampanno,",
  "ja502:25.4": "nise aggīva bhāsati.",
  "ja502:26.1": "Etaṁ me upamaṁ katvā,",
  "ja502:26.2": "putte vijjāsu vācaya;",
  "ja502:26.3": "Saṁvirūḷhetha medhāvī,",
  "ja502:26.4": "khette bījaṁva vuṭṭhiyā”ti.",
  "ja502:27.1": "Cūḷahaṁsajātakaṁ chaṭṭhaṁ."
}