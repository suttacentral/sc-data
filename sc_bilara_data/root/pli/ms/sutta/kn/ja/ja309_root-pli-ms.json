{
  "ja309:0.1": "Jātaka",
  "ja309:0.2": "Catukkanipāta",
  "ja309:0.3": "Kāliṅgavagga",
  "ja309:0.4": "9. Chavakajātaka",
  "ja309:1.1": "“Sabbamidaṁ carimaṁ kataṁ,",
  "ja309:1.2": "Ubho dhammaṁ na passare;",
  "ja309:1.3": "Ubho pakatiyā cutā,",
  "ja309:1.4": "Yo cāyaṁ mantejjhāpeti;",
  "ja309:1.5": "Yo ca mantaṁ adhīyati”.",
  "ja309:2.1": "“Sālīnaṁ odanaṁ bhuñje,",
  "ja309:2.2": "suciṁ maṁsūpasecanaṁ;",
  "ja309:2.3": "Tasmā etaṁ na sevāmi,",
  "ja309:2.4": "dhammaṁ isīhi sevitaṁ”.",
  "ja309:3.1": "“Paribbaja mahā loko,",
  "ja309:3.2": "pacantaññepi pāṇino;",
  "ja309:3.3": "Mā taṁ adhammo ācarito,",
  "ja309:3.4": "asmā kumbhamivābhidā.",
  "ja309:4.1": "Dhiratthu taṁ yasalābhaṁ,",
  "ja309:4.2": "dhanalābhañca brāhmaṇa;",
  "ja309:4.3": "Yā vutti vinipātena,",
  "ja309:4.4": "adhammacaraṇena vā”ti.",
  "ja309:5.1": "Chavakajātakaṁ navamaṁ."
}