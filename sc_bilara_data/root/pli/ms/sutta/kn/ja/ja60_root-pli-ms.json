{
  "ja60:0.1": "Jātaka",
  "ja60:0.2": "Ekakanipāta",
  "ja60:0.3": "Āsīsavagga",
  "ja60:0.4": "10. Saṅkhadhamajātaka",
  "ja60:1.1": "“Dhame dhame nātidhame,",
  "ja60:1.2": "atidhantañhi pāpakaṁ;",
  "ja60:1.3": "Dhantenādhigatā bhogā,",
  "ja60:1.4": "te tāto vidhamī dhaman”ti.",
  "ja60:2.1": "Saṅkhadhamajātakaṁ dasamaṁ.",
  "ja60:2.2": "Āsīsavaggo chaṭṭho.",
  "ja60:3.1": "Tassuddānaṁ",
  "ja60:4.1": "Yathā icchiṁ tathāhudakā thalā,",
  "ja60:4.2": "Sura sāduphalo ca alīnamano;",
  "ja60:4.3": "Sampahaṭṭhamano caturo ca tayo,",
  "ja60:4.4": "Sataladdhaka bhogadhanena dasāti."
}