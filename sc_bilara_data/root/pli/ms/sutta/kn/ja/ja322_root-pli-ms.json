{
  "ja322:0.1": "Jātaka",
  "ja322:0.2": "Catukkanipāta",
  "ja322:0.3": "Kuṭidūsakavagga",
  "ja322:0.4": "2. Duddubhajātaka",
  "ja322:1.1": "“Duddubhāyati bhaddante,",
  "ja322:1.2": "yasmiṁ dese vasāmahaṁ;",
  "ja322:1.3": "Ahampetaṁ na jānāmi,",
  "ja322:1.4": "kimetaṁ duddubhāyati”.",
  "ja322:2.1": "“Beluvaṁ patitaṁ sutvā,",
  "ja322:2.2": "duddubhanti saso javi;",
  "ja322:2.3": "Sasassa vacanaṁ sutvā,",
  "ja322:2.4": "santattā migavāhinī.",
  "ja322:3.1": "Appatvā padaviññāṇaṁ,",
  "ja322:3.2": "paraghosānusārino;",
  "ja322:3.3": "Panādaparamā bālā,",
  "ja322:3.4": "te honti parapattiyā.",
  "ja322:4.1": "Ye ca sīlena sampannā,",
  "ja322:4.2": "paññāyūpasame ratā;",
  "ja322:4.3": "Ārakā viratā dhīrā,",
  "ja322:4.4": "na honti parapattiyā”ti.",
  "ja322:5.1": "Duddubhajātakaṁ dutiyaṁ."
}