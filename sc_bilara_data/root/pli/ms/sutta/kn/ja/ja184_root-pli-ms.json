{
  "ja184:0.1": "Jātaka",
  "ja184:0.2": "Dukanipāta",
  "ja184:0.3": "Asadisavagga",
  "ja184:0.4": "4. Giridattajātaka",
  "ja184:1.1": "“Dūsito giridattena,",
  "ja184:1.2": "hayo sāmassa paṇḍavo;",
  "ja184:1.3": "Porāṇaṁ pakatiṁ hitvā,",
  "ja184:1.4": "tassevānuvidhiyyati.",
  "ja184:2.1": "Sace ca tanujo poso,",
  "ja184:2.2": "sikharākārakappito;",
  "ja184:2.3": "Ānane naṁ gahetvāna,",
  "ja184:2.4": "maṇḍale parivattaye;",
  "ja184:2.5": "Khippameva pahantvāna,",
  "ja184:2.6": "tassevānuvidhiyyatī”ti.",
  "ja184:3.1": "Giridattajātakaṁ catutthaṁ."
}