{
  "ja147:0.1": "Jātaka",
  "ja147:0.2": "Ekakanipāta",
  "ja147:0.3": "Kakaṇṭakavagga",
  "ja147:0.4": "7. Puppharattajātaka",
  "ja147:1.1": "“Nayidaṁ dukkhaṁ aduṁ dukkhaṁ,",
  "ja147:1.2": "Yaṁ maṁ tudati vāyaso;",
  "ja147:1.3": "Yaṁ sāmā puppharattena,",
  "ja147:1.4": "Kattikaṁ nānubhossatī”ti.",
  "ja147:2.1": "Puppharattajātakaṁ sattamaṁ."
}