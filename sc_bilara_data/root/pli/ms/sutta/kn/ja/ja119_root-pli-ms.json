{
  "ja119:0.1": "Jātaka",
  "ja119:0.2": "Ekakanipāta",
  "ja119:0.3": "Haṁcivagga",
  "ja119:0.4": "9. Akālarāvijātaka",
  "ja119:1.1": "“Amātāpitarasaṁvaddho,",
  "ja119:1.2": "anācerakule vasaṁ;",
  "ja119:1.3": "Nāyaṁ kālaṁ akālaṁ vā,",
  "ja119:1.4": "abhijānāti kukkuṭo”ti.",
  "ja119:2.1": "Akālarāvijātakaṁ navamaṁ."
}