{
  "ja304:0.1": "Jātaka",
  "ja304:0.2": "Catukkanipāta",
  "ja304:0.3": "Kāliṅgavagga",
  "ja304:0.4": "4. Daddarajātaka",
  "ja304:1.1": "“Imāni maṁ daddara tāpayanti,",
  "ja304:1.2": "Vācāduruttāni manussaloke;",
  "ja304:1.3": "Maṇḍūkabhakkhā udakantasevī,",
  "ja304:1.4": "Āsīvisaṁ maṁ avisā sapanti”.",
  "ja304:2.1": "“Sakā raṭṭhā pabbājito,",
  "ja304:2.2": "aññaṁ janapadaṁ gato;",
  "ja304:2.3": "Mahantaṁ koṭṭhaṁ kayirātha,",
  "ja304:2.4": "duruttānaṁ nidhetave.",
  "ja304:3.1": "Yattha posaṁ na jānanti,",
  "ja304:3.2": "jātiyā vinayena vā;",
  "ja304:3.3": "Na tattha mānaṁ kayirātha,",
  "ja304:3.4": "vasamaññātake jane.",
  "ja304:4.1": "Videsavāsaṁ vasato,",
  "ja304:4.2": "jātavedasamenapi;",
  "ja304:4.3": "Khamitabbaṁ sapaññena,",
  "ja304:4.4": "api dāsassa tajjitan”ti.",
  "ja304:5.1": "Daddarajātakaṁ catutthaṁ."
}