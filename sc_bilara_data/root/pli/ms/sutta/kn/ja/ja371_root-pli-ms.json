{
  "ja371:0.1": "Jātaka",
  "ja371:0.2": "Pañcakanipāta",
  "ja371:0.3": "Aḍḍhavagga",
  "ja371:0.4": "1. Dīghītikosalajātaka",
  "ja371:1.1": "“Evaṁbhūtassa te rāja,",
  "ja371:1.2": "āgatassa vase mama;",
  "ja371:1.3": "Atthi nu koci pariyāyo,",
  "ja371:1.4": "yo taṁ dukkhā pamocaye”.",
  "ja371:2.1": "“Evaṁbhūtassa me tāta,",
  "ja371:2.2": "āgatassa vase tava;",
  "ja371:2.3": "Natthi no koci pariyāyo,",
  "ja371:2.4": "yo maṁ dukkhā pamocaye”.",
  "ja371:3.1": "“Nāññaṁ sucaritaṁ rāja,",
  "ja371:3.2": "nāññaṁ rāja subhāsitaṁ;",
  "ja371:3.3": "Tāyate maraṇakāle,",
  "ja371:3.4": "evamevitaraṁ dhanaṁ.",
  "ja371:4.1": "Akkocchi maṁ avadhi maṁ,",
  "ja371:4.2": "ajini maṁ ahāsi me;",
  "ja371:4.3": "Ye ca taṁ upanayhanti,",
  "ja371:4.4": "veraṁ tesaṁ na sammati.",
  "ja371:5.1": "Akkocchi maṁ avadhi maṁ,",
  "ja371:5.2": "ajini maṁ ahāsi me;",
  "ja371:5.3": "Ye ca taṁ nupanayhanti,",
  "ja371:5.4": "veraṁ tesūpasammati.",
  "ja371:6.1": "Na hi verena verāni,",
  "ja371:6.2": "sammantīdha kudācanaṁ;",
  "ja371:6.3": "Averena ca sammanti,",
  "ja371:6.4": "esa dhammo sanantano”ti.",
  "ja371:7.1": "Dīghītikosalajātakaṁ paṭhamaṁ."
}