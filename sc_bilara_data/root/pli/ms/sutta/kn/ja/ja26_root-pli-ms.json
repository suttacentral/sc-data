{
  "ja26:0.1": "Jātaka",
  "ja26:0.2": "Ekakanipāta",
  "ja26:0.3": "Kuruṅgavagga",
  "ja26:0.4": "6. Mahiḷāmukhajātaka",
  "ja26:1.1": "“Purāṇacorāna vaco nisamma,",
  "ja26:1.2": "Mahiḷāmukho pothayamanvacārī;",
  "ja26:1.3": "Susaññatānañhi vaco nisamma,",
  "ja26:1.4": "Gajuttamo sabbaguṇesu aṭṭhā”ti.",
  "ja26:2.1": "Mahiḷāmukhajātakaṁ chaṭṭhaṁ."
}