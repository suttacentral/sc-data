{
  "ja243:0.1": "Jātaka",
  "ja243:0.2": "Dukanipāta",
  "ja243:0.3": "Siṅgālavagga",
  "ja243:0.4": "3. Guttilajātaka",
  "ja243:1.1": "“Sattatantiṁ sumadhuraṁ,",
  "ja243:1.2": "rāmaṇeyyaṁ avācayiṁ;",
  "ja243:1.3": "So maṁ raṅgamhi avheti,",
  "ja243:1.4": "saraṇaṁ me hohi kosiya”.",
  "ja243:2.1": "“Ahaṁ taṁ saraṇaṁ samma,",
  "ja243:2.2": "Ahamācariyapūjako;",
  "ja243:2.3": "Na taṁ jayissati sisso,",
  "ja243:2.4": "Sissamācariya jessasī”ti.",
  "ja243:3.1": "Guttilajātakaṁ tatiyaṁ."
}