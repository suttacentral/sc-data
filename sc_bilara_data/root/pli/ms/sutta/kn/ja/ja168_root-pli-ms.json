{
  "ja168:0.1": "Jātaka",
  "ja168:0.2": "Dukanipāta",
  "ja168:0.3": "Santhavavagga",
  "ja168:0.4": "8. Sakuṇagghijātaka",
  "ja168:1.1": "“Seno balasā patamāno,",
  "ja168:1.2": "lāpaṁ gocaraṭhāyinaṁ;",
  "ja168:1.3": "Sahasā ajjhappattova,",
  "ja168:1.4": "maraṇaṁ tenupāgami.",
  "ja168:2.1": "Sohaṁ nayena sampanno,",
  "ja168:2.2": "pettike gocare rato;",
  "ja168:2.3": "Apetasattu modāmi,",
  "ja168:2.4": "sampassaṁ atthamattano”ti.",
  "ja168:3.1": "Sakuṇagghijātakaṁ aṭṭhamaṁ."
}