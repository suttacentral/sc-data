{
  "ja413:0.1": "Jātaka",
  "ja413:0.2": "Sattakanipāta",
  "ja413:0.3": "Gandhāravagga",
  "ja413:0.4": "8. Dhūmakārijātaka",
  "ja413:1.1": "“Rājā apucchi vidhuraṁ,",
  "ja413:1.2": "dhammakāmo yudhiṭṭhilo;",
  "ja413:1.3": "Api brāhmaṇa jānāsi,",
  "ja413:1.4": "ko eko bahu socati”.",
  "ja413:2.1": "“Brāhmaṇo ajayūthena,",
  "ja413:2.2": "pahūtedho vane vasaṁ;",
  "ja413:2.3": "Dhūmaṁ akāsi vāseṭṭho,",
  "ja413:2.4": "rattindivamatandito.",
  "ja413:3.1": "Tassa taṁ dhūmagandhena,",
  "ja413:3.2": "sarabhā makasaḍḍitā;",
  "ja413:3.3": "Vassāvāsaṁ upāgacchuṁ,",
  "ja413:3.4": "dhūmakārissa santike.",
  "ja413:4.1": "Sarabhesu manaṁ katvā,",
  "ja413:4.2": "ajā so nāvabujjhatha;",
  "ja413:4.3": "Āgacchantī vajantī vā,",
  "ja413:4.4": "tassa tā vinasuṁ ajā.",
  "ja413:5.1": "Sarabhā sarade kāle,",
  "ja413:5.2": "pahīnamakase vane;",
  "ja413:5.3": "Pāvisuṁ giriduggāni,",
  "ja413:5.4": "nadīnaṁ pabhavāni ca.",
  "ja413:6.1": "Sarabhe ca gate disvā,",
  "ja413:6.2": "ajā ca vibhavaṁ gatā;",
  "ja413:6.3": "Kiso ca vivaṇṇo cāsi,",
  "ja413:6.4": "paṇḍurogī ca brāhmaṇo.",
  "ja413:7.1": "Evaṁ yo saṁ niraṅkatvā,",
  "ja413:7.2": "āgantuṁ kurute piyaṁ;",
  "ja413:7.3": "So eko bahu socati,",
  "ja413:7.4": "dhūmakārīva brāhmaṇo”ti.",
  "ja413:8.1": "Dhūmakārijātakaṁ aṭṭhamaṁ."
}