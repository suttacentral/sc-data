{
  "ja88:0.1": "Jātaka",
  "ja88:0.2": "Ekakanipāta",
  "ja88:0.3": "Apāyimhavagga",
  "ja88:0.4": "8. Sārambhajātaka",
  "ja88:1.1": "“Kalyāṇimeva muñceyya,",
  "ja88:1.2": "na hi muñceyya pāpikaṁ;",
  "ja88:1.3": "Mokkho kalyāṇiyā sādhu,",
  "ja88:1.4": "mutvā tappati pāpikan”ti.",
  "ja88:2.1": "Sārambhajātakaṁ aṭṭhamaṁ."
}