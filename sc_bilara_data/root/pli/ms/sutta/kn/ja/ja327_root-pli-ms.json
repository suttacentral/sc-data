{
  "ja327:0.1": "Jātaka",
  "ja327:0.2": "Catukkanipāta",
  "ja327:0.3": "Kuṭidūsakavagga",
  "ja327:0.4": "7. Kākavatījātaka",
  "ja327:1.1": "“Vāti cāyaṁ tato gandho,",
  "ja327:1.2": "yattha me vasatī piyā;",
  "ja327:1.3": "Dūre ito hi kākavatī,",
  "ja327:1.4": "yattha me nirato mano”.",
  "ja327:2.1": "“Kathaṁ samuddamatarī,",
  "ja327:2.2": "kathaṁ atari kepukaṁ;",
  "ja327:2.3": "Kathaṁ satta samuddāni,",
  "ja327:2.4": "kathaṁ simbalimāruhi”.",
  "ja327:3.1": "“Tayā samuddamatariṁ,",
  "ja327:3.2": "tayā atari kepukaṁ;",
  "ja327:3.3": "Tayā satta samuddāni,",
  "ja327:3.4": "tayā simbalimāruhiṁ”.",
  "ja327:4.1": "“Dhiratthumaṁ mahākāyaṁ,",
  "ja327:4.2": "dhiratthumaṁ acetanaṁ;",
  "ja327:4.3": "Yattha jāyāyahaṁ jāraṁ,",
  "ja327:4.4": "āvahāmi vahāmi cā”ti.",
  "ja327:5.1": "Kākavatījātakaṁ sattamaṁ."
}