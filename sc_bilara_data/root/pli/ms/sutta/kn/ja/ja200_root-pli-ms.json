{
  "ja200:0.1": "Jātaka",
  "ja200:0.2": "Dukanipāta",
  "ja200:0.3": "Ruhakavagga",
  "ja200:0.4": "10. Sādhusīlajātaka",
  "ja200:1.1": "“Sarīradabyaṁ vuḍḍhabyaṁ,",
  "ja200:1.2": "sojaccaṁ sādhusīliyaṁ;",
  "ja200:1.3": "Brāhmaṇaṁ teva pucchāma,",
  "ja200:1.4": "kaṁ nu tesaṁ vanimhase”.",
  "ja200:2.1": "“Attho atthi sarīrasmiṁ,",
  "ja200:2.2": "vuḍḍhabyassa namo kare;",
  "ja200:2.3": "Attho atthi sujātasmiṁ,",
  "ja200:2.4": "sīlaṁ asmāka ruccatī”ti.",
  "ja200:3.1": "Sādhusīlajātakaṁ dasamaṁ.",
  "ja200:3.2": "Ruhakavaggo pañcamo.",
  "ja200:4.1": "Tassuddānaṁ",
  "ja200:5.1": "Apiruhaka rūpavatī musalo,",
  "ja200:5.2": "Pavasanti sapañcamapokkharaṇī;",
  "ja200:5.3": "Atha muttimavāṇija umhayate,",
  "ja200:5.4": "Ciraāgata koṭṭha sarīra dasāti."
}