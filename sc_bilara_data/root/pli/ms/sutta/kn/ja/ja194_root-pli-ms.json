{
  "ja194:0.1": "Jātaka",
  "ja194:0.2": "Dukanipāta",
  "ja194:0.3": "Ruhakavagga",
  "ja194:0.4": "4. Maṇicorajātaka",
  "ja194:1.1": "“Na santi devā pavasanti nūna,",
  "ja194:1.2": "Na hi nūna santi idha lokapālā;",
  "ja194:1.3": "Sahasā karontānamasaññatānaṁ,",
  "ja194:1.4": "Na hi nūna santī paṭisedhitāro”.",
  "ja194:2.1": "“Akāle vassatī tassa,",
  "ja194:2.2": "kāle tassa na vassati;",
  "ja194:2.3": "Saggā ca cavati ṭhānā,",
  "ja194:2.4": "nanu so tāvatā hato”ti.",
  "ja194:3.1": "Maṇicorajātakaṁ catutthaṁ."
}