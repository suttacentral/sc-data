{
  "ja224:0.1": "Jātaka",
  "ja224:0.2": "Dukanipāta",
  "ja224:0.3": "Kāsāvavagga",
  "ja224:0.4": "4. Kumbhilajātaka",
  "ja224:1.1": "“Yassete caturo dhammā,",
  "ja224:1.2": "vānarinda yathā tava;",
  "ja224:1.3": "Saccaṁ dhammo dhiti cāgo,",
  "ja224:1.4": "diṭṭhaṁ so ativattati.",
  "ja224:2.1": "Yassa cete na vijjanti,",
  "ja224:2.2": "guṇā paramabhaddakā;",
  "ja224:2.3": "Saccaṁ dhammo dhiti cāgo,",
  "ja224:2.4": "diṭṭhaṁ so nātivattatī”ti.",
  "ja224:3.1": "Kumbhilajātakaṁ catutthaṁ."
}