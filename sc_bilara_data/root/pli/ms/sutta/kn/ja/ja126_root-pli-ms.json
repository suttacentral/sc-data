{
  "ja126:0.1": "Jātaka",
  "ja126:0.2": "Ekakanipāta",
  "ja126:0.3": "Kusanāḷivagga",
  "ja126:0.4": "6. Asilakkhaṇajātaka",
  "ja126:1.1": "“Tadevekassa kalyāṇaṁ,",
  "ja126:1.2": "tadevekassa pāpakaṁ;",
  "ja126:1.3": "Tasmā sabbaṁ na kalyāṇaṁ,",
  "ja126:1.4": "sabbaṁ vāpi na pāpakan”ti.",
  "ja126:2.1": "Asilakkhaṇajātakaṁ chaṭṭhaṁ."
}