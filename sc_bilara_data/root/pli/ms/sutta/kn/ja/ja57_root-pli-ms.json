{
  "ja57:0.1": "Jātaka",
  "ja57:0.2": "Ekakanipāta",
  "ja57:0.3": "Āsīsavagga",
  "ja57:0.4": "7. Vānarindajātaka",
  "ja57:1.1": "“Yassete caturo dhammā,",
  "ja57:1.2": "vānarinda yathā tava;",
  "ja57:1.3": "Saccaṁ dhammo dhiti cāgo,",
  "ja57:1.4": "diṭṭhaṁ so ativattatī”ti.",
  "ja57:2.1": "Vānarindajātakaṁ sattamaṁ."
}