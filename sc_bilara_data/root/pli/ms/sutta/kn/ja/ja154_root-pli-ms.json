{
  "ja154:0.1": "Jātaka",
  "ja154:0.2": "Dukanipāta",
  "ja154:0.3": "Daḷhavagga",
  "ja154:0.4": "4. Uragajātaka",
  "ja154:1.1": "“Idhūragānaṁ pavaro paviṭṭho,",
  "ja154:1.2": "Selassa vaṇṇena pamokkhamicchaṁ;",
  "ja154:1.3": "Brahmañca vaṇṇaṁ apacāyamāno,",
  "ja154:1.4": "Bubhukkhito no vitarāmi bhottuṁ”.",
  "ja154:2.1": "“So brahmagutto cirameva jīva,",
  "ja154:2.2": "Dibyā ca te pātubhavantu bhakkhā;",
  "ja154:2.3": "Yo brahmavaṇṇaṁ apacāyamāno,",
  "ja154:2.4": "Bubhukkhito no vitarāsi bhottun”ti.",
  "ja154:3.1": "Uragajātakaṁ catutthaṁ."
}