{
  "ja443:0.1": "Jātaka",
  "ja443:0.2": "Dasakanipāta",
  "ja443:0.3": "Catudvāravagga",
  "ja443:0.4": "5. Cūḷabodhijātaka",
  "ja443:1.1": "“Yo te imaṁ visālakkhiṁ,",
  "ja443:1.2": "piyaṁ sammhitabhāsiniṁ;",
  "ja443:1.3": "Ādāya balā gaccheyya,",
  "ja443:1.4": "kiṁ nu kayirāsi brāhmaṇa”.",
  "ja443:2.1": "“Uppajje me na mucceyya,",
  "ja443:2.2": "na me mucceyya jīvato;",
  "ja443:2.3": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:2.4": "khippameva nivāraye”.",
  "ja443:3.1": "“Yannu pubbe vikatthittho,",
  "ja443:3.2": "balamhiva apassito;",
  "ja443:3.3": "Svajja tuṇhikato dāni,",
  "ja443:3.4": "saṅghāṭiṁ sibbamacchasi”.",
  "ja443:4.1": "“Uppajji me na muccittha,",
  "ja443:4.2": "na me muccittha jīvato;",
  "ja443:4.3": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:4.4": "khippameva nivārayiṁ”.",
  "ja443:5.1": "“Kiṁ te uppajji no mucci,",
  "ja443:5.2": "kiṁ te na mucci jīvato;",
  "ja443:5.3": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:5.4": "katamaṁ tvaṁ nivārayi”.",
  "ja443:6.1": "“Yamhi jāte na passati,",
  "ja443:6.2": "ajāte sādhu passati;",
  "ja443:6.3": "So me uppajji no mucci,",
  "ja443:6.4": "kodho dummedhagocaro.",
  "ja443:7.1": "Yena jātena nandanti,",
  "ja443:7.2": "amittā dukkhamesino;",
  "ja443:7.3": "So me uppajji no mucci,",
  "ja443:7.4": "kodho dummedhagocaro.",
  "ja443:8.1": "Yasmiñca jāyamānamhi,",
  "ja443:8.2": "sadatthaṁ nāvabujjhati;",
  "ja443:8.3": "So me uppajji no mucci,",
  "ja443:8.4": "kodho dummedhagocaro.",
  "ja443:9.1": "Yenābhibhūto kusalaṁ jahāti,",
  "ja443:9.2": "Parakkare vipulañcāpi atthaṁ;",
  "ja443:9.3": "Sa bhīmaseno balavā pamaddī,",
  "ja443:9.4": "Kodho mahārāja na me amuccatha.",
  "ja443:10.1": "Kaṭṭhasmiṁ matthamānasmiṁ,",
  "ja443:10.2": "Pāvako nāma jāyati;",
  "ja443:10.3": "Tameva kaṭṭhaṁ ḍahati,",
  "ja443:10.4": "Yasmā so jāyate gini.",
  "ja443:11.1": "Evaṁ mandassa posassa,",
  "ja443:11.2": "bālassa avijānato;",
  "ja443:11.3": "Sārambhā jāyate kodho,",
  "ja443:11.4": "sopi teneva ḍayhati.",
  "ja443:12.1": "Aggīva tiṇakaṭṭhasmiṁ,",
  "ja443:12.2": "kodho yassa pavaḍḍhati;",
  "ja443:12.3": "Nihīyati tassa yaso,",
  "ja443:12.4": "kāḷapakkheva candimā.",
  "ja443:13.1": "Anedho dhūmaketūva,",
  "ja443:13.2": "kodho yassūpasammati;",
  "ja443:13.3": "Āpūrati tassa yaso,",
  "ja443:13.4": "sukkapakkheva candimā”ti.",
  "ja443:14.1": "Cūḷabodhijātakaṁ pañcamaṁ."
}