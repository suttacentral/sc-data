{
  "ja150:0.1": "Jātaka",
  "ja150:0.2": "Ekakanipāta",
  "ja150:0.3": "Kakaṇṭakavagga",
  "ja150:0.4": "10. Sañjīvajātaka",
  "ja150:1.1": "“Asantaṁ yo paggaṇhāti,",
  "ja150:1.2": "asantaṁ cūpasevati;",
  "ja150:1.3": "Tameva ghāsaṁ kurute,",
  "ja150:1.4": "byaggho sañjīvako yathā”ti.",
  "ja150:2.1": "Sañjīvajātakaṁ dasamaṁ.",
  "ja150:2.2": "Kakaṇṭakavaggo pannarasamo.",
  "ja150:3.1": "Tassuddānaṁ",
  "ja150:4.1": "Sukhamedhati daṇḍavaro ca puna,",
  "ja150:4.2": "Lasi vāladhi pañcamarādhavaro;",
  "ja150:4.3": "Samahodadhi kattika bondi puna,",
  "ja150:4.4": "Caturaṅgulabyagghavarena dasāti.",
  "ja150:5.1": "Uparimapaṇṇāsako.",
  "ja150:6.1": "Atha vagguddānaṁ",
  "ja150:7.1": "Apaṇṇakaṁ sīlavaggakuruṅga,",
  "ja150:7.2": "Kulāvakaṁ atthakāmena pañcamaṁ;",
  "ja150:7.3": "Āsīso itthivaruṇaṁ apāyi,",
  "ja150:7.4": "Littavaggena te dasa;",
  "ja150:7.5": "Parosataṁ haṁci kusanāḷi,",
  "ja150:7.6": "Asampadaṁ kakaṇṭakavaggo.",
  "ja150:8.1": "Ekanipātamhilaṅkatanti.",
  "ja150:9.1": "Ekakanipātaṁ niṭṭhitaṁ."
}