{
  "ja433:0.1": "Jātaka",
  "ja433:0.2": "Navakanipāta",
  "ja433:0.3": "Gijjhavagga",
  "ja433:0.4": "7. Lomasakassapajātaka",
  "ja433:1.1": "“Assa indasamo rāja,",
  "ja433:1.2": "accantaṁ ajarāmaro;",
  "ja433:1.3": "Sace tvaṁ yaññaṁ yājeyya,",
  "ja433:1.4": "isiṁ lomasakassapaṁ”.",
  "ja433:2.1": "“Sasamuddapariyāyaṁ,",
  "ja433:2.2": "mahiṁ sāgarakuṇḍalaṁ;",
  "ja433:2.3": "Na icche saha nindāya,",
  "ja433:2.4": "evaṁ seyya vijānahi.",
  "ja433:3.1": "Dhiratthu taṁ yasalābhaṁ,",
  "ja433:3.2": "dhanalābhañca brāhmaṇa;",
  "ja433:3.3": "Yā vutti vinipātena,",
  "ja433:3.4": "adhammacaraṇena vā.",
  "ja433:4.1": "Api ce pattamādāya,",
  "ja433:4.2": "anagāro paribbaje;",
  "ja433:4.3": "Sāyeva jīvikā seyyo,",
  "ja433:4.4": "yā cādhammena esanā.",
  "ja433:5.1": "Api ce pattamādāya,",
  "ja433:5.2": "anagāro paribbaje;",
  "ja433:5.3": "Aññaṁ ahiṁsayaṁ loke,",
  "ja433:5.4": "api rajjena taṁ varaṁ”.",
  "ja433:6.1": "“Balaṁ cando balaṁ suriyo,",
  "ja433:6.2": "balaṁ samaṇabrāhmaṇā;",
  "ja433:6.3": "Balaṁ velā samuddassa,",
  "ja433:6.4": "balātibalamitthiyo.",
  "ja433:7.1": "Yathā uggatapaṁ santaṁ,",
  "ja433:7.2": "isiṁ lomasakassapaṁ;",
  "ja433:7.3": "Pitu atthā candavatī,",
  "ja433:7.4": "vājapeyyaṁ ayājayi”.",
  "ja433:8.1": "“Taṁ lobhapakataṁ kammaṁ,",
  "ja433:8.2": "Kaṭukaṁ kāmahetukaṁ;",
  "ja433:8.3": "Tassa mūlaṁ gavesissaṁ,",
  "ja433:8.4": "Checchaṁ rāgaṁ sabandhanaṁ.",
  "ja433:9.1": "Dhiratthu kāme subahūpi loke,",
  "ja433:9.2": "Tapova seyyo kāmaguṇehi rāja;",
  "ja433:9.3": "Tapo karissāmi pahāya kāme,",
  "ja433:9.4": "Taveva raṭṭhaṁ candavatī ca hotū”ti.",
  "ja433:10.1": "Lomasakassapajātakaṁ sattamaṁ."
}