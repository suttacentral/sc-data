{
  "ja331:0.1": "Jātaka",
  "ja331:0.2": "Catukkanipāta",
  "ja331:0.3": "Kokilavagga",
  "ja331:0.4": "1. Kokilajātaka",
  "ja331:1.1": "“Yo ve kāle asampatte,",
  "ja331:1.2": "ativelaṁ pabhāsati;",
  "ja331:1.3": "Evaṁ so nihato seti,",
  "ja331:1.4": "kokilāyiva atrajo.",
  "ja331:2.1": "Na hi satthaṁ sunisitaṁ,",
  "ja331:2.2": "visaṁ halāhalāmiva;",
  "ja331:2.3": "Evaṁ nikaṭṭhe pāteti,",
  "ja331:2.4": "vācā dubbhāsitā yathā.",
  "ja331:3.1": "Tasmā kāle akāle vā,",
  "ja331:3.2": "vācaṁ rakkheyya paṇḍito;",
  "ja331:3.3": "Nātivelaṁ pabhāseyya,",
  "ja331:3.4": "api attasamamhi vā.",
  "ja331:4.1": "Yo ca kāle mitaṁ bhāse,",
  "ja331:4.2": "matipubbo vicakkhaṇo;",
  "ja331:4.3": "Sabbe amitte ādeti,",
  "ja331:4.4": "supaṇṇo uragāmivā”ti.",
  "ja331:5.1": "Kokilajātakaṁ paṭhamaṁ."
}