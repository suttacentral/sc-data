{
  "ja72:0.1": "Jātaka",
  "ja72:0.2": "Ekakanipāta",
  "ja72:0.3": "Varuṇavagga",
  "ja72:0.4": "2. Sīlavahatthijātaka",
  "ja72:1.1": "“Akataññussa posassa,",
  "ja72:1.2": "niccaṁ vivaradassino;",
  "ja72:1.3": "Sabbañce pathaviṁ dajjā,",
  "ja72:1.4": "neva naṁ abhirādhaye”ti.",
  "ja72:2.1": "Sīlavahatthijātakaṁ dutiyaṁ."
}