{
  "an8.15:0.1": "Aṅguttara Nikāya 8 ",
  "an8.15:0.2": "2. Mahāvagga ",
  "an8.15:0.3": "15. Malasutta ",
  "an8.15:1.1": "“Aṭṭhimāni, bhikkhave, malāni. ",
  "an8.15:1.2": "Katamāni aṭṭha? ",
  "an8.15:1.3": "Asajjhāyamalā, bhikkhave, mantā; ",
  "an8.15:1.4": "anuṭṭhānamalā, bhikkhave, gharā; ",
  "an8.15:1.5": "malaṁ, bhikkhave, vaṇṇassa kosajjaṁ; ",
  "an8.15:1.6": "pamādo, bhikkhave, rakkhato malaṁ; ",
  "an8.15:1.7": "malaṁ, bhikkhave, itthiyā duccaritaṁ; ",
  "an8.15:1.8": "maccheraṁ, bhikkhave, dadato malaṁ; ",
  "an8.15:1.9": "malā, bhikkhave, pāpakā akusalā dhammā asmiṁ loke paramhi ca; ",
  "an8.15:1.10": "tato, bhikkhave, malā malataraṁ avijjā paramaṁ malaṁ. ",
  "an8.15:1.11": "Imāni kho, bhikkhave, aṭṭha malānīti. ",
  "an8.15:2.1": "Asajjhāyamalā mantā, ",
  "an8.15:2.2": "anuṭṭhānamalā gharā; ",
  "an8.15:2.3": "Malaṁ vaṇṇassa kosajjaṁ, ",
  "an8.15:2.4": "pamādo rakkhato malaṁ. ",
  "an8.15:3.1": "Malitthiyā duccaritaṁ, ",
  "an8.15:3.2": "maccheraṁ dadato malaṁ; ",
  "an8.15:3.3": "Malā ve pāpakā dhammā, ",
  "an8.15:3.4": "asmiṁ loke paramhi ca; ",
  "an8.15:3.5": "Tato malā malataraṁ, ",
  "an8.15:3.6": "avijjā paramaṁ malan”ti. ",
  "an8.15:3.7": "Pañcamaṁ. "
}