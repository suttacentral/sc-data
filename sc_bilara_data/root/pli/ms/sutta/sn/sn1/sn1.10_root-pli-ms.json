{
  "sn1.10:0.1": "Saṁyutta Nikāya 1.10 ",
  "sn1.10:0.2": "1. Naḷavagga ",
  "sn1.10:0.3": "Araññasutta ",
  "sn1.10:1.1": "Sāvatthinidānaṁ. ",
  "sn1.10:1.2": "Ekamantaṁ ṭhitā kho sā devatā bhagavantaṁ gāthāya ajjhabhāsi: ",
  "sn1.10:2.1": "“Araññe viharantānaṁ, ",
  "sn1.10:2.2": "santānaṁ brahmacārinaṁ; ",
  "sn1.10:2.3": "Ekabhattaṁ bhuñjamānānaṁ, ",
  "sn1.10:2.4": "kena vaṇṇo pasīdatī”ti. ",
  "sn1.10:3.1": "“Atītaṁ nānusocanti, ",
  "sn1.10:3.2": "nappajappanti nāgataṁ; ",
  "sn1.10:3.3": "Paccuppannena yāpenti, ",
  "sn1.10:3.4": "tena vaṇṇo pasīdati. ",
  "sn1.10:4.1": "Anāgatappajappāya, ",
  "sn1.10:4.2": "atītassānusocanā; ",
  "sn1.10:4.3": "Etena bālā sussanti, ",
  "sn1.10:4.4": "naḷova harito luto”ti. ",
  "sn1.10:4.5": "Naḷavaggo paṭhamo. ",
  "sn1.10:5.0": "Tassuddānaṁ ",
  "sn1.10:5.1": "Oghaṁ nimokkhaṁ upaneyyaṁ, ",
  "sn1.10:5.2": "Accenti katichindi ca; ",
  "sn1.10:5.3": "Jāgaraṁ appaṭividitā, ",
  "sn1.10:5.4": "Susammuṭṭhā mānakāminā; ",
  "sn1.10:5.5": "Araññe dasamo vutto, ",
  "sn1.10:5.6": "Vaggo tena pavuccati. "
}