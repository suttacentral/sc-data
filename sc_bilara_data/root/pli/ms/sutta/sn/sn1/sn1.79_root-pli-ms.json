{
  "sn1.79:0.1": "Saṁyutta Nikāya 1 ",
  "sn1.79:0.2": "8. Chetvāvagga ",
  "sn1.79:0.3": "79. Pātheyyasutta ",
  "sn1.79:1.1": "“Kiṁsu bandhati pātheyyaṁ, ",
  "sn1.79:1.2": "kiṁsu bhogānamāsayo; ",
  "sn1.79:1.3": "Kiṁsu naraṁ parikassati, ",
  "sn1.79:1.4": "kiṁsu lokasmi dujjahaṁ; ",
  "sn1.79:1.5": "Kismiṁ baddhā puthū sattā, ",
  "sn1.79:1.6": "pāsena sakuṇī yathā”ti. ",
  "sn1.79:2.1": "“Saddhā bandhati pātheyyaṁ, ",
  "sn1.79:2.2": "sirī bhogānamāsayo; ",
  "sn1.79:2.3": "Icchā naraṁ parikassati, ",
  "sn1.79:2.4": "icchā lokasmi dujjahā; ",
  "sn1.79:2.5": "Icchābaddhā puthū sattā, ",
  "sn1.79:2.6": "pāsena sakuṇī yathā”ti. "
}