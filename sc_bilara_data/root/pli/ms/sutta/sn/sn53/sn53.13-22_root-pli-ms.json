{
  "sn53.13-22:0.1": "Saṁyutta Nikāya 53 ",
  "sn53.13-22:0.2": "2. Appamādavagga ",
  "sn53.13-22:0.3": "13. Appamādavagga ",
  "sn53.13-22:1.1": "(Appamādavaggo vitthāretabbo.) ",
  "sn53.13-22:2.0": "Tassuddānaṁ",
  "sn53.13-22:2.1": "Tathāgataṁ padaṁ kūṭaṁ, ",
  "sn53.13-22:2.2": "mūlaṁ sāro ca vassikaṁ; ",
  "sn53.13-22:2.3": "Rājā candimasūriyā, ",
  "sn53.13-22:2.4": "vatthena dasamaṁ padanti. "
}