{
  "sn53.23-34:0.1": "Saṁyutta Nikāya 53 ",
  "sn53.23-34:0.2": "3. Balakaraṇīyavagga ",
  "sn53.23-34:0.3": "23. Balakaraṇīyavagga ",
  "sn53.23-34:1.1": "(Balakaraṇīyavaggo vitthāretabbo.) ",
  "sn53.23-34:2.0": "Tassuddānaṁ",
  "sn53.23-34:2.1": "Balaṁ bījañca nāgo ca, ",
  "sn53.23-34:2.2": "rukkho kumbhena sūkiyā; ",
  "sn53.23-34:2.3": "Ākāsena ca dve meghā, ",
  "sn53.23-34:2.4": "nāvā āgantukā nadīti. "
}