{
  "dt1.2:0.1": "Dhātukathā ",
  "dt1.2:0.2": "Uddesa ",
  "dt1.2:0.3": "2. Abbhantaramātikā ",
  "dt1.2:1.1": "Pañcakkhandhā ",
  "dt1.2:2.1": "Dvādasāyatanāni ",
  "dt1.2:3.1": "Aṭṭhārasa dhātuyo ",
  "dt1.2:4.1": "Cattāri saccāni ",
  "dt1.2:5.1": "Bāvīsatindriyāni ",
  "dt1.2:6.1": "Paṭiccasamuppādo ",
  "dt1.2:7.1": "Cattāro satipaṭṭhānā ",
  "dt1.2:8.1": "Cattāro sammappadhānā ",
  "dt1.2:9.1": "Cattāro iddhipādā ",
  "dt1.2:10.1": "Cattāri jhānāni ",
  "dt1.2:11.1": "Catasso appamaññāyo ",
  "dt1.2:12.1": "Pañcindriyāni ",
  "dt1.2:13.1": "Pañca balāni ",
  "dt1.2:14.1": "Satta bojjhaṅgā ",
  "dt1.2:15.1": "Ariyo aṭṭhaṅgiko maggo ",
  "dt1.2:16.1": "Phasso vedanā saññā cetanā cittaṁ adhimokkho manasikāro. "
