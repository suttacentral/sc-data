{
  "pp1.2:0.1": "Puggalapaññatti ",
  "pp1.2:0.2": "Mātikā ",
  "pp1.2:0.3": "2. Dukauddesa ",
  "pp1.2:1.1": "**Dve puggalā—** ",
  "pp1.2:2.1": "Kodhano ca, upanāhī ca. ",
  "pp1.2:3.1": "Makkhī ca, paḷāsī ca. ",
  "pp1.2:4.1": "Issukī ca, maccharī ca. ",
  "pp1.2:5.1": "Saṭho ca, māyāvī ca. ",
  "pp1.2:6.1": "Ahiriko ca, anottappī ca. ",
  "pp1.2:7.1": "Dubbaco ca, pāpamitto ca. ",
  "pp1.2:8.1": "Indriyesu aguttadvāro ca, bhojane amattaññū ca. ",
  "pp1.2:9.1": "Muṭṭhassati ca, asampajāno ca. ",
  "pp1.2:10.1": "Sīlavipanno ca, diṭṭhivipanno ca. ",
  "pp1.2:11.1": "Ajjhattasaṁyojano ca, bahiddhāsaṁyojano ca. ",
  "pp1.2:12.1": "Akkodhano ca, anupanāhī ca. ",
  "pp1.2:13.1": "Amakkhī ca, apaḷāsī ca. ",
  "pp1.2:14.1": "Anissukī ca, amaccharī ca. ",
  "pp1.2:15.1": "Asaṭho ca, amāyāvī ca. ",
  "pp1.2:16.1": "Hirimā ca, ottappī ca. ",
  "pp1.2:17.1": "Suvaco ca, kalyāṇamitto ca. ",
  "pp1.2:18.1": "Indriyesu guttadvāro ca, bhojane mattaññū ca. ",
  "pp1.2:19.1": "Upaṭṭhitassati ca, sampajāno ca. ",
  "pp1.2:20.1": "Sīlasampanno ca, diṭṭhisampanno ca. ",
  "pp1.2:21.1": "Dve puggalā dullabhā lokasmiṁ. ",
  "pp1.2:22.1": "Dve puggalā duttappayā. ",
  "pp1.2:23.1": "Dve puggalā sutappayā. ",
  "pp1.2:24.1": "Dvinnaṁ puggalānaṁ āsavā vaḍḍhanti. ",
  "pp1.2:25.1": "Dvinnaṁ puggalānaṁ āsavā na vaḍḍhanti. ",
  "pp1.2:26.1": "Hīnādhimutto ca, paṇītādhimutto ca. ",
  "pp1.2:27.1": "Titto ca, tappetā ca. ",
  "pp1.2:28.1": "Dukaṁ. "
}