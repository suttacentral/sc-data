{
  "kv15.5:0.1": "Kathāvatthu",
  "kv15.5:0.2": "Mahāpaṇṇāsaka",
  "kv15.5:0.3": "Pannarasamavagga",
  "kv15.5:0.4": "Āsavakathā",
  "kv15.5:1.1": "☑ Cattāro āsavā anāsavāti?",
  "kv15.5:1.2": "Āmantā.",
  "kv15.5:1.3": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.5:1.4": "Na hevaṁ vattabbe …pe….",
  "kv15.5:2.1": "☒ Na vattabbaṁ—",
  "kv15.5:2.2": "“cattāro āsavā anāsavā”ti?",
  "kv15.5:2.3": "Āmantā.",
  "kv15.5:2.4": "Atthaññeva āsavā yehi āsavehi te āsavā sāsavā hontīti?",
  "kv15.5:2.5": "Na hevaṁ vattabbe.",
  "kv15.5:2.6": "Tena hi cattāro āsavā anāsavāti.",
  "kv15.5:2.7": "Āsavakathā niṭṭhitā."
}