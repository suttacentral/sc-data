{
  "kv15.8:0.1": "Kathāvatthu ",
  "kv15.8:0.2": "Mahāpaṇṇāsaka ",
  "kv15.8:0.3": "Pannarasamavagga ",
  "kv15.8:0.4": "Dutiyasaññāvedayitakathā ",
  "kv15.8:1.1": "Saññāvedayitanirodhasamāpatti lokiyāti? ",
  "kv15.8:1.2": "Āmantā. ",
  "kv15.8:1.3": "Rūpanti? ",
  "kv15.8:1.4": "Na hevaṁ vattabbe …pe… ",
  "kv15.8:1.5": "vedanā … ",
  "kv15.8:1.6": "saññā … ",
  "kv15.8:1.7": "saṅkhārā … ",
  "kv15.8:1.8": "viññāṇanti? ",
  "kv15.8:1.9": "Na hevaṁ vattabbe …pe… ",
  "kv15.8:1.10": "kāmāvacarāti? ",
  "kv15.8:1.11": "Na hevaṁ vattabbe …pe… ",
  "kv15.8:1.12": "rūpāvacarāti? ",
  "kv15.8:1.13": "Na hevaṁ vattabbe …pe… ",
  "kv15.8:1.14": "arūpāvacarāti? ",
  "kv15.8:1.15": "Na hevaṁ vattabbe …pe…. ",
  "kv15.8:2.1": "Na vattabbaṁ—",
  "kv15.8:2.2": "“saññāvedayitanirodhasamāpatti lokiyā”ti? ",
  "kv15.8:2.3": "Āmantā. ",
  "kv15.8:2.4": "Lokuttarāti? ",
  "kv15.8:2.5": "Na hevaṁ vattabbe. ",
  "kv15.8:2.6": "Tena hi lokiyāti. ",
  "kv15.8:2.7": "Dutiyasaññāvedayitakathā niṭṭhitā. "
}