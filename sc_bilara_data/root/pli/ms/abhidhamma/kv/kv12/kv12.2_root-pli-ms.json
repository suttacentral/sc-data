{
  "kv12.2:0.1": "Kathāvatthu",
  "kv12.2:0.2": "Mahāpaṇṇāsaka",
  "kv12.2:0.3": "Dvādasamavagga",
  "kv12.2:0.4": "Kammakathā",
  "kv12.2:1.1": "☑ Sabbaṁ kammaṁ savipākanti?",
  "kv12.2:1.2": "Āmantā.",
  "kv12.2:1.3": "Sabbā cetanā savipākāti?",
  "kv12.2:1.4": "Na hevaṁ vattabbe …pe…",
  "kv12.2:1.5": "sabbā cetanā savipākāti?",
  "kv12.2:1.6": "Āmantā.",
  "kv12.2:1.7": "Vipākābyākatā cetanā savipākāti?",
  "kv12.2:1.8": "Na hevaṁ vattabbe …pe…",
  "kv12.2:1.9": "sabbā cetanā savipākāti?",
  "kv12.2:1.10": "Āmantā.",
  "kv12.2:1.11": "Kiriyābyākatā cetanā savipākāti?",
  "kv12.2:1.12": "Na hevaṁ vattabbe …pe….",
  "kv12.2:2.1": "☑ Sabbā cetanā savipākāti?",
  "kv12.2:2.2": "Āmantā.",
  "kv12.2:2.3": "Kāmāvacarā vipākābyākatā cetanā savipākāti?",
  "kv12.2:2.4": "Na hevaṁ vattabbe …pe…",
  "kv12.2:2.5": "sabbā cetanā savipākāti?",
  "kv12.2:2.6": "Āmantā.",
  "kv12.2:2.7": "Rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā savipākāti?",
  "kv12.2:2.8": "Na hevaṁ vattabbe …pe….",
  "kv12.2:3.1": "☑ Sabbā cetanā savipākāti?",
  "kv12.2:3.2": "Āmantā.",
  "kv12.2:3.3": "Kāmāvacarā kiriyābyākatā cetanā savipākāti?",
  "kv12.2:3.4": "Na hevaṁ vattabbe …pe…",
  "kv12.2:3.5": "sabbā cetanā savipākāti?",
  "kv12.2:3.6": "Āmantā.",
  "kv12.2:3.7": "Rūpāvacarā arūpāvacarā kiriyābyākatā cetanā savipākāti?",
  "kv12.2:3.8": "Na hevaṁ vattabbe …pe….",
  "kv12.2:4.1": "☑ Vipākābyākatā cetanā avipākāti?",
  "kv12.2:4.2": "Āmantā.",
  "kv12.2:4.3": "Hañci vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:4.4": "“sabbā cetanā savipākā”ti.",
  "kv12.2:5.1": "☑ Kiriyābyākatā cetanā avipākāti?",
  "kv12.2:5.2": "Āmantā.",
  "kv12.2:5.3": "Hañci kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:5.4": "“sabbā cetanā savipākā”ti.",
  "kv12.2:6.1": "☑ Kāmāvacarā rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā avipākāti?",
  "kv12.2:6.2": "Āmantā.",
  "kv12.2:6.3": "Hañci apariyāpannā vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:6.4": "“sabbā cetanā savipākā”ti.",
  "kv12.2:7.1": "☑ Kāmāvacarā rūpāvacarā arūpāvacarā kiriyābyākatā cetanā avipākāti?",
  "kv12.2:7.2": "Āmantā.",
  "kv12.2:7.3": "Hañci arūpāvacarā kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:7.4": "“sabbā cetanā savipākā”ti.",
  "kv12.2:8.1": "☒ Na vattabbaṁ—",
  "kv12.2:8.2": "“sabbaṁ kammaṁ savipākan”ti?",
  "kv12.2:8.3": "Āmantā.",
  "kv12.2:8.4": "Nanu vuttaṁ bhagavatā—",
  "kv12.2:8.5": "“nāhaṁ, bhikkhave, sañcetanikānaṁ kammānaṁ katānaṁ upacitānaṁ appaṭisaṁviditvā byantibhāvaṁ vadāmi, tañca kho diṭṭheva dhamme upapajje vā apare vā pariyāye”ti.",
  "kv12.2:8.6": "Attheva suttantoti?",
  "kv12.2:8.7": "Āmantā.",
  "kv12.2:8.8": "Tena hi sabbaṁ kammaṁ savipākanti.",
  "kv12.2:8.9": "Kammakathā niṭṭhitā."
}