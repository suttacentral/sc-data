{
  "kv1.8:0.1": "Kathāvatthu ",
  "kv1.8:0.2": "Mahāpaṇṇāsaka ",
  "kv1.8:0.3": "Paṭhamavagga ",
  "kv1.8:0.4": "Ekaccaṁatthītikathā ",
  "kv1.8:1.0": "Atītādiekaccakathā ",
  "kv1.8:1.1": "Atītaṁ atthīti? ",
  "kv1.8:1.2": "Ekaccaṁ atthi, ekaccaṁ natthīti. ",
  "kv1.8:1.3": "Ekaccaṁ niruddhaṁ, ekaccaṁ na niruddhaṁ; ",
  "kv1.8:1.4": "ekaccaṁ vigataṁ, ekaccaṁ avigataṁ; ",
  "kv1.8:1.5": "ekaccaṁ atthaṅgataṁ, ekaccaṁ na atthaṅgataṁ; ",
  "kv1.8:1.6": "ekaccaṁ abbhatthaṅgataṁ, ekaccaṁ na abbhatthaṅgatanti? ",
  "kv1.8:1.7": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:2.1": "Atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:2.2": "Āmantā. ",
  "kv1.8:2.3": "Atītā avipakkavipākā dhammā ekacce atthi, ekacce natthīti? ",
  "kv1.8:2.4": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:2.5": "atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:2.6": "Āmantā. ",
  "kv1.8:2.7": "Atītā vipakkavipākā dhammā ekacce atthi, ekacce natthīti? ",
  "kv1.8:2.8": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:2.9": "atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:2.10": "Āmantā. ",
  "kv1.8:2.11": "Atītā avipākā dhammā ekacce atthi ekacce natthīti? ",
  "kv1.8:2.12": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:3.1": "Atītaṁ ekaccaṁ atthi ekaccaṁ natthīti? ",
  "kv1.8:3.2": "Āmantā. ",
  "kv1.8:3.3": "Kiṁ atthi kiṁ natthīti? ",
  "kv1.8:3.4": "Atītā avipakkavipākā dhammā—",
  "kv1.8:3.5": "te atthi; ",
  "kv1.8:3.6": "atītā vipakkavipākā dhammā—",
  "kv1.8:3.7": "te natthīti. ",
  "kv1.8:3.8": "Atītā avipakkavipākā dhammā—",
  "kv1.8:3.9": "te atthīti? ",
  "kv1.8:3.10": "Āmantā. ",
  "kv1.8:3.11": "Atītā vipakkavipākā dhammā—",
  "kv1.8:3.12": "te atthīti? ",
  "kv1.8:3.13": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:3.14": "atītā avipakkavipākā dhammā—",
  "kv1.8:3.15": "te atthīti? ",
  "kv1.8:3.16": "Āmantā. ",
  "kv1.8:3.17": "Atītā avipākā dhammā—",
  "kv1.8:3.18": "te atthīti? ",
  "kv1.8:3.19": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:4.1": "Atītā vipakkavipākā dhammā—",
  "kv1.8:4.2": "te natthīti? ",
  "kv1.8:4.3": "Āmantā. ",
  "kv1.8:4.4": "Atītā avipakkavipākā dhammā—",
  "kv1.8:4.5": "te natthīti? ",
  "kv1.8:4.6": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:5.1": "Atītā avipākā dhammā—",
  "kv1.8:5.2": "te natthīti? ",
  "kv1.8:5.3": "Āmantā. ",
  "kv1.8:5.4": "Atītā avipakkavipākā dhammā—",
  "kv1.8:5.5": "te natthīti? ",
  "kv1.8:5.6": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:6.1": "Atītā avipakkavipākā dhammā—",
  "kv1.8:6.2": "te atthīti? ",
  "kv1.8:6.3": "Āmantā. ",
  "kv1.8:6.4": "Nanu atītā avipakkavipākā dhammā niruddhāti? ",
  "kv1.8:6.5": "Āmantā. ",
  "kv1.8:6.6": "Hañci atītā avipakkavipākā dhammā niruddhā, no ca vata re vattabbe—",
  "kv1.8:6.7": "“atītā avipakkavipākā dhammā—",
  "kv1.8:6.8": "te atthī”ti. ",
  "kv1.8:7.1": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:7.2": "te atthīti? ",
  "kv1.8:7.3": "Āmantā. ",
  "kv1.8:7.4": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:7.5": "te atthīti? ",
  "kv1.8:7.6": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:7.7": "atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:7.8": "te atthīti? ",
  "kv1.8:7.9": "Āmantā. ",
  "kv1.8:7.10": "Atītā avipākā dhammā niruddhā—",
  "kv1.8:7.11": "te atthīti? ",
  "kv1.8:7.12": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:8.1": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:8.2": "te natthīti? ",
  "kv1.8:8.3": "Āmantā. ",
  "kv1.8:8.4": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:8.5": "te natthīti? ",
  "kv1.8:8.6": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:9.1": "Atītā avipākā dhammā niruddhā—",
  "kv1.8:9.2": "te natthīti? ",
  "kv1.8:9.3": "Āmantā. ",
  "kv1.8:9.4": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:9.5": "te natthīti? ",
  "kv1.8:9.6": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:10.1": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:10.2": "te atthīti? ",
  "kv1.8:10.3": "Āmantā. ",
  "kv1.8:10.4": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:10.5": "te natthīti? ",
  "kv1.8:10.6": "Āmantā. ",
  "kv1.8:10.7": "Atītā ekadesaṁ vipakkavipākā dhammā ekadesaṁ avipakkavipākā dhammā niruddhā—",
  "kv1.8:10.8": "te ekacce atthi ekacce natthīti? ",
  "kv1.8:10.9": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:11.1": "Na vattabbaṁ—",
  "kv1.8:11.2": "“atītā avipakkavipākā dhammā—",
  "kv1.8:11.3": "te atthī”ti? ",
  "kv1.8:11.4": "Āmantā. ",
  "kv1.8:11.5": "Nanu atītā avipakkavipākā dhammā vipaccissantīti? ",
  "kv1.8:11.6": "Āmantā. ",
  "kv1.8:11.7": "Hañci atītā avipakkavipākā dhammā vipaccissanti, tena vata re vattabbe—",
  "kv1.8:11.8": "“atītā avipakkavipākā dhammā—",
  "kv1.8:11.9": "te atthī”ti. ",
  "kv1.8:12.1": "Atītā avipakkavipākā dhammā vipaccissantīti katvā te atthīti? ",
  "kv1.8:12.2": "Āmantā. ",
  "kv1.8:12.3": "Vipaccissantīti katvā paccuppannāti? ",
  "kv1.8:12.4": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:12.5": "vipaccissantīti katvā paccuppannāti? ",
  "kv1.8:12.6": "Āmantā. ",
  "kv1.8:12.7": "Paccuppannā dhammā nirujjhissantīti katvā te natthīti? ",
  "kv1.8:12.8": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:13.0": "Anāgatādiekaccakathā ",
  "kv1.8:13.1": "Anāgataṁ atthīti? ",
  "kv1.8:13.2": "Ekaccaṁ atthi, ekaccaṁ natthīti. ",
  "kv1.8:13.3": "Ekaccaṁ jātaṁ, ekaccaṁ ajātaṁ; ",
  "kv1.8:13.4": "ekaccaṁ sañjātaṁ, ekaccaṁ asañjātaṁ; ",
  "kv1.8:13.5": "ekaccaṁ nibbattaṁ, ekaccaṁ anibbattaṁ; ",
  "kv1.8:13.6": "ekaccaṁ pātubhūtaṁ, ekaccaṁ apātubhūtanti? ",
  "kv1.8:13.7": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:14.1": "Anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:14.2": "Āmantā. ",
  "kv1.8:14.3": "Anāgatā uppādino dhammā ekacce atthi, ekacce natthīti? ",
  "kv1.8:14.4": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:14.5": "anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:14.6": "Āmantā. ",
  "kv1.8:14.7": "Anāgatā anuppādino dhammā ekacce atthi, ekacce natthīti? ",
  "kv1.8:14.8": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:15.1": "Anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti? ",
  "kv1.8:15.2": "Āmantā. ",
  "kv1.8:15.3": "Kiṁ atthi, kiṁ natthīti? ",
  "kv1.8:15.4": "Anāgatā uppādino dhammā—",
  "kv1.8:15.5": "te atthi; ",
  "kv1.8:15.6": "anāgatā anuppādino dhammā—",
  "kv1.8:15.7": "te natthīti. ",
  "kv1.8:15.8": "Anāgatā uppādino dhammā—",
  "kv1.8:15.9": "te atthīti? ",
  "kv1.8:15.10": "Āmantā. ",
  "kv1.8:15.11": "Anāgatā anuppādino dhammā—",
  "kv1.8:15.12": "te atthīti? ",
  "kv1.8:15.13": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:15.14": "anāgatā anuppādino dhammā—",
  "kv1.8:15.15": "te natthīti? ",
  "kv1.8:15.16": "Āmantā. ",
  "kv1.8:15.17": "Anāgatā uppādino dhammā—",
  "kv1.8:15.18": "te natthīti? ",
  "kv1.8:15.19": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:16.1": "Anāgatā uppādino dhammā—",
  "kv1.8:16.2": "te atthīti? ",
  "kv1.8:16.3": "Āmantā. ",
  "kv1.8:16.4": "Nanu anāgatā uppādino dhammā ajātāti? ",
  "kv1.8:16.5": "Āmantā. ",
  "kv1.8:16.6": "Hañci anāgatā uppādino dhammā ajātā, no ca vata re vattabbe—",
  "kv1.8:16.7": "“anāgatā uppādino dhammā—",
  "kv1.8:16.8": "te atthī”ti. ",
  "kv1.8:17.1": "Anāgatā uppādino dhammā ajātā—",
  "kv1.8:17.2": "te atthīti? ",
  "kv1.8:17.3": "Āmantā. ",
  "kv1.8:17.4": "Anāgatā anuppādino dhammā ajātā—",
  "kv1.8:17.5": "te atthīti? ",
  "kv1.8:17.6": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:17.7": "anāgatā anuppādino dhammā ajātā—",
  "kv1.8:17.8": "te natthīti? ",
  "kv1.8:17.9": "Āmantā. ",
  "kv1.8:17.10": "Anāgatā uppādino dhammā ajātā—",
  "kv1.8:17.11": "te natthīti? ",
  "kv1.8:17.12": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:18.1": "Na vattabbaṁ—",
  "kv1.8:18.2": "“anāgatā uppādino dhammā—",
  "kv1.8:18.3": "te atthī”ti? ",
  "kv1.8:18.4": "Āmantā. ",
  "kv1.8:18.5": "Nanu anāgatā uppādino dhammā uppajjissantīti? ",
  "kv1.8:18.6": "Āmantā. ",
  "kv1.8:18.7": "Hañci anāgatā uppādino dhammā uppajjissanti, tena vata re vattabbe—",
  "kv1.8:18.8": "“anāgatā uppādino dhammā—",
  "kv1.8:18.9": "te atthī”ti. ",
  "kv1.8:19.1": "Anāgatā uppādino dhammā uppajjissantīti katvā te atthīti? ",
  "kv1.8:19.2": "Āmantā. ",
  "kv1.8:19.3": "Uppajjissantīti katvā paccuppannāti? ",
  "kv1.8:19.4": "Na hevaṁ vattabbe …pe… ",
  "kv1.8:19.5": "uppajjissantīti katvā paccuppannāti? ",
  "kv1.8:19.6": "Āmantā. ",
  "kv1.8:19.7": "Paccuppannā dhammā nirujjhissantīti katvā te natthīti? ",
  "kv1.8:19.8": "Na hevaṁ vattabbe …pe…. ",
  "kv1.8:19.9": "Ekaccaṁ atthītikathā niṭṭhitā. "
}