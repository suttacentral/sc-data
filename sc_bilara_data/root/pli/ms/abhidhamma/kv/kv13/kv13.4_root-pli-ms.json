{
  "kv13.4:0.1": "Kathāvatthu",
  "kv13.4:0.2": "Mahāpaṇṇāsaka",
  "kv13.4:0.3": "Terasamavagga",
  "kv13.4:0.4": "Niyatassaniyāmakathā",
  "kv13.4:1.1": "☑ Niyato niyāmaṁ okkamatīti?",
  "kv13.4:1.2": "Āmantā.",
  "kv13.4:1.3": "Micchattaniyato sammattaniyāmaṁ okkamati, sammattaniyato micchattaniyāmaṁ okkamatīti?",
  "kv13.4:1.4": "Na hevaṁ vattabbe …pe….",
  "kv13.4:2.1": "☑ Niyato niyāmaṁ okkamatīti?",
  "kv13.4:2.2": "Āmantā.",
  "kv13.4:2.3": "Pubbe maggaṁ bhāvetvā pacchā niyāmaṁ okkamatīti?",
  "kv13.4:2.4": "Na hevaṁ vattabbe …pe…",
  "kv13.4:2.5": "pubbe sotāpattimaggaṁ bhāvetvā pacchā sotāpattiniyāmaṁ okkamatīti?",
  "kv13.4:2.6": "Na hevaṁ vattabbe …pe…",
  "kv13.4:2.7": "pubbe sakadāgāmi …pe…",
  "kv13.4:2.8": "anāgāmi …pe…",
  "kv13.4:2.9": "arahattamaggaṁ bhāvetvā pacchā arahattaniyāmaṁ okkamatīti?",
  "kv13.4:2.10": "Na hevaṁ vattabbe …pe….",
  "kv13.4:3.1": "☑ Pubbe satipaṭṭhānaṁ …pe…",
  "kv13.4:3.2": "sammappadhānaṁ … iddhipādaṁ … indriyaṁ … balaṁ … bojjhaṅgaṁ bhāvetvā pacchā niyāmaṁ okkamatīti?",
  "kv13.4:3.3": "Na hevaṁ vattabbe …pe….",
  "kv13.4:4.1": "☒ Na vattabbaṁ—",
  "kv13.4:4.2": "“niyato niyāmaṁ okkamatī”ti?",
  "kv13.4:4.3": "Āmantā.",
  "kv13.4:4.4": "Bhabbo bodhisatto tāya jātiyā dhammaṁ nābhisametunti?",
  "kv13.4:4.5": "Na hevaṁ vattabbe.",
  "kv13.4:4.6": "Tena hi niyato niyāmaṁ okkamatīti.",
  "kv13.4:4.7": "Niyatassa niyāmakathā niṭṭhitā."
}