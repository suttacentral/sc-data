{
  "kv3.7:0.1": "Kathāvatthu ",
  "kv3.7:0.2": "Mahāpaṇṇāsaka ",
  "kv3.7:0.3": "Tatiyavagga ",
  "kv3.7:0.4": "Dibbacakkhukathā ",
  "kv3.7:1.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:1.2": "Āmantā. ",
  "kv3.7:1.3": "Maṁsacakkhuṁ dibbacakkhuṁ, dibbacakkhuṁ maṁsacakkhunti? ",
  "kv3.7:1.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:2.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:2.2": "Āmantā. ",
  "kv3.7:2.3": "Yādisaṁ maṁsacakkhuṁ tādisaṁ dibbacakkhuṁ, yādisaṁ dibbacakkhuṁ tādisaṁ maṁsacakkhunti? ",
  "kv3.7:2.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:3.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:3.2": "Āmantā. ",
  "kv3.7:3.3": "Taññeva maṁsacakkhuṁ taṁ dibbacakkhuṁ, taṁ dibbacakkhuṁ taṁ maṁsacakkhunti? ",
  "kv3.7:3.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:4.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:4.2": "Āmantā. ",
  "kv3.7:4.3": "Yādiso maṁsacakkhussa visayo ānubhāvo gocaro tādiso dibbassa cakkhussa visayo ānubhāvo gocaroti? ",
  "kv3.7:4.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:5.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:5.2": "Āmantā. ",
  "kv3.7:5.3": "Upādinnaṁ hutvā anupādinnaṁ hotīti? ",
  "kv3.7:5.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:6.1": "Upādinnaṁ hutvā anupādinnaṁ hotīti? ",
  "kv3.7:6.2": "Āmantā. ",
  "kv3.7:6.3": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti? ",
  "kv3.7:6.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:7.1": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti? ",
  "kv3.7:7.2": "Āmantā. ",
  "kv3.7:7.3": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti? ",
  "kv3.7:7.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:8.1": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti? ",
  "kv3.7:8.2": "Āmantā. ",
  "kv3.7:8.3": "Pariyāpannaṁ hutvā apariyāpannaṁ hotīti? ",
  "kv3.7:8.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:9.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:9.2": "Āmantā. ",
  "kv3.7:9.3": "Dibbacakkhuṁ dhammupatthaddhaṁ maṁsacakkhuṁ hotīti? ",
  "kv3.7:9.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:10.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:10.2": "Āmantā. ",
  "kv3.7:10.3": "Dibbacakkhuṁ dhammupatthaddhaṁ paññācakkhuṁ hotīti? ",
  "kv3.7:10.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:11.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:11.2": "Āmantā. ",
  "kv3.7:11.3": "Dibbacakkhuṁ dhammupatthaddhaṁ maṁsacakkhuṁ hotīti? ",
  "kv3.7:11.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.7:12.1": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti? ",
  "kv3.7:12.2": "Āmantā. ",
  "kv3.7:12.3": "Dveva cakkhūnīti? ",
  "kv3.7:12.4": "Na hevaṁ vattabbe …pe… ",
  "kv3.7:12.5": "dveva cakkhūnīti? ",
  "kv3.7:12.6": "Āmantā. ",
  "kv3.7:12.7": "Nanu tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.8": "maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhunti? ",
  "kv3.7:12.9": "Āmantā. ",
  "kv3.7:12.10": "Hañci tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.11": "maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhuṁ, no ca vata re vattabbe—",
  "kv3.7:12.12": "“dveva cakkhūnī”ti. ",
  "kv3.7:13.1": "Dveva cakkhūnīti? ",
  "kv3.7:13.2": "Āmantā. ",
  "kv3.7:13.3": "Nanu vuttaṁ bhagavatā—",
  "kv3.7:13.4": "“tīṇimāni, bhikkhave, cakkhūni. ",
  "kv3.7:13.5": "Katamāni tīṇi? ",
  "kv3.7:13.6": "Maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhunti—",
  "kv3.7:13.7": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti. ",
  "kv3.7:14.1": "“Maṁsacakkhuṁ dibbacakkhuṁ, ",
  "kv3.7:14.2": "Paññācakkhuṁ anuttaraṁ; ",
  "kv3.7:14.3": "Etāni tīṇi cakkhūni, ",
  "kv3.7:14.4": "Akkhāsi purisuttamo. ",
  "kv3.7:15.1": "Maṁsacakkhussa uppādo, ",
  "kv3.7:15.2": "maggo dibbassa cakkhuno; ",
  "kv3.7:15.3": "Yadā ca ñāṇaṁ udapādi, ",
  "kv3.7:15.4": "paññācakkhuṁ anuttaraṁ; ",
  "kv3.7:15.5": "Tassa cakkhussa paṭilābhā, ",
  "kv3.7:15.6": "sabbadukkhā pamuccatī”ti. ",
  "kv3.7:16.1": "Attheva suttantoti? ",
  "kv3.7:16.2": "Āmantā. ",
  "kv3.7:16.3": "Tena hi na vattabbaṁ—",
  "kv3.7:16.4": "“dveva cakkhūnī”ti. ",
  "kv3.7:16.5": "Dibbacakkhukathā niṭṭhitā. "
}