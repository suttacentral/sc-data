{
  "kv3.8:0.1": "Kathāvatthu ",
  "kv3.8:0.2": "Mahāpaṇṇāsaka ",
  "kv3.8:0.3": "Tatiyavagga ",
  "kv3.8:0.4": "Dibbasotakathā ",
  "kv3.8:1.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:1.2": "Āmantā. ",
  "kv3.8:1.3": "Maṁsasotaṁ dibbasotaṁ, dibbasotaṁ maṁsasotanti? ",
  "kv3.8:1.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:2.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:2.2": "Āmantā. ",
  "kv3.8:2.3": "Yādisaṁ maṁsasotaṁ tādisaṁ dibbasotaṁ, yādisaṁ dibbasotaṁ tādisaṁ maṁsasotanti? ",
  "kv3.8:2.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:3.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:3.2": "Āmantā. ",
  "kv3.8:3.3": "Taññeva maṁsasotaṁ taṁ dibbasotaṁ, taṁ dibbasotaṁ taṁ maṁsasotanti? ",
  "kv3.8:3.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:4.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:4.2": "Āmantā. ",
  "kv3.8:4.3": "Yādiso maṁsasotassa visayo ānubhāvo gocaro tādiso dibbassa sotassa visayo ānubhāvo gocaroti? ",
  "kv3.8:4.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:5.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:5.2": "Āmantā. ",
  "kv3.8:5.3": "Upādinnaṁ hutvā anupādinnaṁ hotīti? ",
  "kv3.8:5.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:6.1": "Upādinnaṁ hutvā anupādinnaṁ hotīti? ",
  "kv3.8:6.2": "Āmantā. ",
  "kv3.8:6.3": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti? ",
  "kv3.8:6.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:7.1": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti? ",
  "kv3.8:7.2": "Āmantā. ",
  "kv3.8:7.3": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti? ",
  "kv3.8:7.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:8.1": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti? ",
  "kv3.8:8.2": "Āmantā. ",
  "kv3.8:8.3": "Pariyāpannaṁ hutvā apariyāpannaṁ hotīti? ",
  "kv3.8:8.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:9.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:9.2": "Āmantā. ",
  "kv3.8:9.3": "Dibbasotaṁ dhammupatthaddhaṁ maṁsasotaṁ hotīti? ",
  "kv3.8:9.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:10.1": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti? ",
  "kv3.8:10.2": "Āmantā. ",
  "kv3.8:10.3": "Ekaṁyeva sotanti? ",
  "kv3.8:10.4": "Na hevaṁ vattabbe …pe…. ",
  "kv3.8:11.1": "Ekaṁyeva sotanti? ",
  "kv3.8:11.2": "Āmantā. ",
  "kv3.8:11.3": "Nanu dve sotāni vuttāni bhagavatā—",
  "kv3.8:11.4": "“maṁsasotaṁ, dibbasotan”ti? ",
  "kv3.8:11.5": "Āmantā. ",
  "kv3.8:11.6": "Hañci dve sotāni vuttāni bhagavatā—",
  "kv3.8:11.7": "maṁsasotaṁ, dibbasotaṁ, no ca vata re vattabbe—",
  "kv3.8:11.8": "“ekaññeva sotan”ti. ",
  "kv3.8:11.9": "Dibbasotakathā niṭṭhitā. "
}