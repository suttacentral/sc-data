{
  "kv21.6:0.1": "Kathāvatthu",
  "kv21.6:0.2": "Mahāpaṇṇāsaka",
  "kv21.6:0.3": "Ekavīsatimavagga",
  "kv21.6:0.4": "Sabbadisākathā",
  "kv21.6:1.1": "☑ Sabbā disā buddhā tiṭṭhantīti?",
  "kv21.6:1.2": "Āmantā.",
  "kv21.6:1.3": "Puratthimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:1.4": "Na hevaṁ vattabbe …pe…",
  "kv21.6:1.5": "puratthimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:1.6": "Āmantā.",
  "kv21.6:1.7": "Kiṁnāmo so bhagavā, kiṁjacco, kiṅgotto, kiṁnāmā tassa bhagavato mātāpitaro, kiṁnāmaṁ tassa bhagavato sāvakayugaṁ, konāmo tassa bhagavato upaṭṭhāko, kīdisaṁ cīvaraṁ dhāreti, kīdisaṁ pattaṁ dhāreti, katarasmiṁ gāme vā nigame vā nagare vā raṭṭhe vā janapade vāti?",
  "kv21.6:1.8": "Na hevaṁ vattabbe …pe….",
  "kv21.6:2.1": "☑ Dakkhiṇāya disāya …pe…",
  "kv21.6:2.2": "pacchimāya disāya …pe…",
  "kv21.6:2.3": "uttarāya disāya …pe…",
  "kv21.6:2.4": "heṭṭhimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:2.5": "Na hevaṁ vattabbe …pe…",
  "kv21.6:2.6": "heṭṭhimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:2.7": "Āmantā.",
  "kv21.6:2.8": "Kiṁnāmo so bhagavā …pe…",
  "kv21.6:2.9": "janapade vāti?",
  "kv21.6:2.10": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.1": "☑ Uparimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:3.2": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.3": "Uparimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:3.4": "Āmantā.",
  "kv21.6:3.5": "Cātumahārājike tiṭṭhati …pe…",
  "kv21.6:3.6": "tāvatiṁse tiṭṭhati …pe…",
  "kv21.6:3.7": "yāme tiṭṭhati …pe…",
  "kv21.6:3.8": "tusite tiṭṭhati …pe…",
  "kv21.6:3.9": "nimmānaratiyā tiṭṭhati …pe…",
  "kv21.6:3.10": "paranimmitavasavattiyā tiṭṭhati …pe…",
  "kv21.6:3.11": "brahmaloke tiṭṭhatīti?",
  "kv21.6:3.12": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.13": "Sabbadisākathā niṭṭhitā."
}