{
  "kv21.3:0.1": "Kathāvatthu",
  "kv21.3:0.2": "Mahāpaṇṇāsaka",
  "kv21.3:0.3": "Ekavīsatimavagga",
  "kv21.3:0.4": "Saṁyojanakathā",
  "kv21.3:1.1": "☑ Atthi kiñci saṁyojanaṁ appahāya arahattappattīti?",
  "kv21.3:1.2": "Āmantā.",
  "kv21.3:1.3": "Atthi kiñci sakkāyadiṭṭhiṁ appahāya …pe…",
  "kv21.3:1.4": "vicikicchaṁ appahāya …pe…",
  "kv21.3:1.5": "sīlabbataparāmāsaṁ appahāya …",
  "kv21.3:1.6": "rāgaṁ appahāya …",
  "kv21.3:1.7": "dosaṁ appahāya …",
  "kv21.3:1.8": "mohaṁ appahāya …",
  "kv21.3:1.9": "anottappaṁ appahāya arahattappattīti?",
  "kv21.3:1.10": "Na hevaṁ vattabbe …pe….",
  "kv21.3:2.1": "☑ Atthi kiñci saṁyojanaṁ appahāya arahattappattīti?",
  "kv21.3:2.2": "Āmantā.",
  "kv21.3:2.3": "Arahā sarāgo sadoso samoho samāno samakkho sapaḷāso saupāyāso sakilesoti?",
  "kv21.3:2.4": "Na hevaṁ vattabbe …pe…",
  "kv21.3:2.5": "nanu arahā nirāgo niddoso nimmoho nimmāno nimmakkho nippaḷāso nirupāyāso nikkilesoti?",
  "kv21.3:2.6": "Āmantā.",
  "kv21.3:2.7": "Hañci arahā nirāgo …pe…",
  "kv21.3:2.8": "nikkileso, no ca vata re vattabbe—",
  "kv21.3:2.9": "“atthi kiñci saṁyojanaṁ appahāya arahattappattī”ti.",
  "kv21.3:3.1": "☒ Na vattabbaṁ—",
  "kv21.3:3.2": "“kiñci saṁyojanaṁ appahāya arahattappattī”ti?",
  "kv21.3:3.3": "Āmantā.",
  "kv21.3:3.4": "Arahā sabbaṁ buddhavisayaṁ jānātīti?",
  "kv21.3:3.5": "Na hevaṁ vattabbe.",
  "kv21.3:3.6": "Tena hi atthi kiñci saṁyojanaṁ appahāya arahattappattīti.",
  "kv21.3:3.7": "Saṁyojanakathā niṭṭhitā."
}