{
  "thag1.114:0.1": "Verses of the Senior Monks",
  "thag1.114:0.2": "The Book of the Ones",
  "thag1.114:0.3": "Chapter Twelve",
  "thag1.114:0.4": "Adhimutta (1st)",
  "thag1.114:1.1": "Your body is uncomfortably heavy,",
  "thag1.114:1.2": "and life is running out;",
  "thag1.114:1.3": "greedy for physical pleasure,",
  "thag1.114:1.4": "how can you find happiness as an ascetic?"
}