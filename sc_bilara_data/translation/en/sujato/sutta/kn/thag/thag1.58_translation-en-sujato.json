{
  "thag1.58:0.1": "Verses of the Senior Monks",
  "thag1.58:0.2": "The Book of the Ones",
  "thag1.58:0.3": "Chapter Six",
  "thag1.58:0.4": "Ramaṇīyakuṭika",
  "thag1.58:1.1": "My little hut is pleasing, delightful,",
  "thag1.58:1.2": "a gift given in faith.",
  "thag1.58:1.3": "I’ve no need of girls:",
  "thag1.58:1.4": "go, ladies, to those in need!"
