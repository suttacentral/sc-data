{
  "sn1.64:0.1": "Saṃyutta Nikāya 1",
  "sn1.64:0.2": "7. Oprimido",
  "sn1.64:0.3": "64. Grilhões",
  "sn1.64:1.1": "“O que aprisiona o mundo?",
  "sn1.64:1.2": "O que o explora?",
  "sn1.64:1.3": "Com o abandono do que",
  "sn1.64:1.4": "se pode falar da cessação?”",
  "sn1.64:2.1": "“O deleite aprisiona o mundo.",
  "sn1.64:2.2": "O pensamento o explora.",
  "sn1.64:2.3": "Com o abandono do anseio",
  "sn1.64:2.4": "pode-se falar da cessação.”"
}