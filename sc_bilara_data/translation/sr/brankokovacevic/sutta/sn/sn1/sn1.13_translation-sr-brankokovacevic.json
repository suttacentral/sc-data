{
  "sn1.13:0.1": "'\"\\'Budini govori tematski grupisani 1.13 \\'\"'",
  "sn1.13:0.2": "'\"\\'Nandana \\'\"'",
  "sn1.13:0.3": "'\"\\'Sinu ničeg ravnoga nema \\'\"'",
  "sn1.13:1.1": "'\"\\'Kraj Sāvatthija. \\'\"'",
  "sn1.13:1.2": "'\"\\'Stojeći sa strane, neko božanstvo u prisustvu Blaženog izgovori ove stihove: \\'\"'",
  "sn1.13:2.1": "'\"\\'„Ljubavi prema sinu ravne nema, \\'\"'",
  "sn1.13:2.2": "'\"\\'ni bogatstvu kao što su goveda; \\'\"'",
  "sn1.13:2.3": "'\"\\'sunčevoj svetlosti ravne nema, \\'\"'",
  "sn1.13:2.4": "'\"\\'među vodama okean je prvi.” \\'\"'",
  "sn1.13:3.1": "'\"\\'„Ljubavi prema sebi ravne nema, \\'\"'",
  "sn1.13:3.2": "'\"\\'ni bogatstvu kao što je pirinač. \\'\"'",
  "sn1.13:3.3": "'\"\\'Nema svetla ravnog mudrosti, \\'\"'",
  "sn1.13:3.4": "'\"\\'među vodama kiša je prva.” \\'\"'"
}