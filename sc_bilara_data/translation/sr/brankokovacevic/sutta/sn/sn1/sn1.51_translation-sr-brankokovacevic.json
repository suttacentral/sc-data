{
  "sn1.51:0.1": "Budini govori tematski grupisani 1.51 ",
  "sn1.51:0.2": "6. Starost ",
  "sn1.51:0.3": "Starost ",
  "sn1.51:1.1": "„Šta je dobro sve do starosti? ",
  "sn1.51:1.2": "Šta je dobro kao oslonac? ",
  "sn1.51:1.3": "Šta je dragulj među ljudima? ",
  "sn1.51:1.4": "A šta to ni lopovi ne mogu ukrasti?” ",
  "sn1.51:2.1": "„Vrlina je dobra sve do starosti. ",
  "sn1.51:2.2": "Poverenje jeste dobro kao oslonac. ",
  "sn1.51:2.3": "Mudrost je dragulj među ljudima. ",
  "sn1.51:2.4": "Tvoja dobra dela ni lopovi ne mogu ukrasti.” "
}