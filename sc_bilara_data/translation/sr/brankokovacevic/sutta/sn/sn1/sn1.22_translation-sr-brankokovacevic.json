{
  "sn1.22:0.1": "'\"\\'Budini govori tematski grupisani 1.22 \\'\"'",
  "sn1.22:0.2": "'\"\\'3. Mač \\'\"'",
  "sn1.22:0.3": "'\"\\'Dodiruje \\'\"'",
  "sn1.22:1.1": "'\"\\'„Ne dodiruje onoga ko ne dodiruje, \\'\"'",
  "sn1.22:1.2": "'\"\\'ali hoće dodirnuti onoga ko dodiruje. \\'\"'",
  "sn1.22:1.3": "'\"\\'Zbog toga dodiruje onoga ko dodiruje, \\'\"'",
  "sn1.22:1.4": "'\"\\'ko se ogreši o čoveka nevinoga. \\'\"'",
  "sn1.22:2.1": "'\"\\'Kad čovek napadne nevinoga, \\'\"'",
  "sn1.22:2.2": "'\"\\'ko je miroljubiv, čist i neiskvaren, \\'\"'",
  "sn1.22:2.3": "'\"\\'napad se okreće protiv te budale, \\'\"'",
  "sn1.22:2.4": "'\"\\'kao i prašina kad se uz vetar baci. \\'\"'"
}