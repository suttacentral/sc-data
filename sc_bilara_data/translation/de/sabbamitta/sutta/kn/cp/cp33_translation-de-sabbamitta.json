{
  "cp33:0.1": "'\\'\"\\\\\\'Korb des Verhaltens 33 \\\\\\'\"\\''",
  "cp33:0.2": "'\\'\"\\\\\\'Das Kapitel mit Yudhañjaya \\\\\\'\"\\''",
  "cp33:0.3": "'\\'\"\\\\\\'Die Vollkommenheit der Liebe (1) \\\\\\'\"\\''",
  "cp33:0.4": "'\\'\"\\\\\\'Das Verhalten des Goldschwarz \\\\\\'\"\\''",
  "cp33:1.1": "'\\'\"\\\\\\'„Als ich in einem Wald lebte, war ich Sāma der Schwarze, \\\\\\'\"\\''",
  "cp33:1.2": "'\\'\"\\\\\\'von Sakka erschaffen. \\\\\\'\"\\''",
  "cp33:1.3": "'\\'\"\\\\\\'Ich führte Löwen und Tiger \\\\\\'\"\\''",
  "cp33:1.4": "'\\'\"\\\\\\'im Wald durch Liebe. \\\\\\'\"\\''",
  "cp33:2.1": "'\\'\"\\\\\\'Umgeben von Löwen und Tigern, \\\\\\'\"\\''",
  "cp33:2.2": "'\\'\"\\\\\\'von Leoparden, Bären und Büffeln, \\\\\\'\"\\''",
  "cp33:2.3": "'\\'\"\\\\\\'von gefleckten Hirschen und Wildschweinen, \\\\\\'\"\\''",
  "cp33:2.4": "'\\'\"\\\\\\'so lebte ich im Wald. \\\\\\'\"\\''",
  "cp33:3.1": "'\\'\"\\\\\\'Niemand hatte Angst vor mir, \\\\\\'\"\\''",
  "cp33:3.2": "'\\'\"\\\\\\'noch fürchtete ich mich vor ihnen. \\\\\\'\"\\''",
  "cp33:3.3": "'\\'\"\\\\\\'Von der Macht der Liebe getragen \\\\\\'\"\\''",
  "cp33:3.4": "'\\'\"\\\\\\'erfreute ich mich damals im Wald.“ \\\\\\'\"\\''"
}