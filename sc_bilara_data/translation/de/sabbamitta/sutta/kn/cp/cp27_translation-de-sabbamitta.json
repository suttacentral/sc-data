{
  "cp27:0.1": "'\\'\"\\\\\\'Korb des Verhaltens 27 \\\\\\'\"\\''",
  "cp27:0.2": "'\\'\"\\\\\\'Das Kapitel mit Yudhañjaya \\\\\\'\"\\''",
  "cp27:0.3": "'\\'\"\\\\\\'Die Vollkommenheit der Wahrheit (1) \\\\\\'\"\\''",
  "cp27:0.4": "'\\'\"\\\\\\'Das Verhalten des Affenkönigs \\\\\\'\"\\''",
  "cp27:1.1": "'\\'\"\\\\\\'„Als ich ein Affe \\\\\\'\"\\''",
  "cp27:1.2": "'\\'\"\\\\\\'in einem Einschnitt am Flussufer war, \\\\\\'\"\\''",
  "cp27:1.3": "'\\'\"\\\\\\'wurde ich von einem Krokodil belästigt \\\\\\'\"\\''",
  "cp27:1.4": "'\\'\"\\\\\\'und konnte nicht hinüberkommen. \\\\\\'\"\\''",
  "cp27:2.1": "'\\'\"\\\\\\'Wo ich gewöhnlich landete, \\\\\\'\"\\''",
  "cp27:2.2": "'\\'\"\\\\\\'wenn ich von diesem zum anderen Ufer sprang, \\\\\\'\"\\''",
  "cp27:2.3": "'\\'\"\\\\\\'dort kauerte mein Todfeind, \\\\\\'\"\\''",
  "cp27:2.4": "'\\'\"\\\\\\'der böse Gavial. \\\\\\'\"\\''",
  "cp27:3.1": "'\\'\"\\\\\\'Es wiegte mich in Sicherheit und sagte: ‚Komm!‘ \\\\\\'\"\\''",
  "cp27:3.2": "'\\'\"\\\\\\'‚Ich komme!‘, sagte ich zu ihm. \\\\\\'\"\\''",
  "cp27:3.3": "'\\'\"\\\\\\'Indem ich auf seinen Kopf trat, \\\\\\'\"\\''",
  "cp27:3.4": "'\\'\"\\\\\\'schaffte ich es zum anderen Ufer. \\\\\\'\"\\''",
  "cp27:4.1": "'\\'\"\\\\\\'Ich sprach keine Lüge zu ihm, \\\\\\'\"\\''",
  "cp27:4.2": "'\\'\"\\\\\\'sondern handelte nach meinem Wort. \\\\\\'\"\\''",
  "cp27:4.3": "'\\'\"\\\\\\'Es gibt niemanden, der meiner Wahrhaftigkeit gleichkommt: \\\\\\'\"\\''",
  "cp27:4.4": "'\\'\"\\\\\\'Das ist meine Vollkommenheit der Wahrheit.“ \\\\\\'\"\\''"
}