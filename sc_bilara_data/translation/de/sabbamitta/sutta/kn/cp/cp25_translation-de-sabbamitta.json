{
  "cp25:0.1": "'\\'\"\\\\\\'Korb des Verhaltens 25 \\\\\\'\"\\''",
  "cp25:0.2": "'\\'\"\\\\\\'Das Kapitel mit Yudhañjaya \\\\\\'\"\\''",
  "cp25:0.3": "'\\'\"\\\\\\'Die Vollkommenheit der Entsagung (5) \\\\\\'\"\\''",
  "cp25:0.4": "'\\'\"\\\\\\'Das Verhalten Soṇas des Klugen \\\\\\'\"\\''",
  "cp25:1.1": "'\\'\"\\\\\\'„Und wiederum, als ich \\\\\\'\"\\''",
  "cp25:1.2": "'\\'\"\\\\\\'in der Stadt Brahmavaḍḍhana war, \\\\\\'\"\\''",
  "cp25:1.3": "'\\'\"\\\\\\'war ich dort in einer erlesenen, wohlhabenden \\\\\\'\"\\''",
  "cp25:1.4": "'\\'\"\\\\\\'und bedeutenden Familie wiedergeboren. \\\\\\'\"\\''",
  "cp25:2.1": "'\\'\"\\\\\\'Trotzdem sah ich, \\\\\\'\"\\''",
  "cp25:2.2": "'\\'\"\\\\\\'dass die Welt blind geworden war, in Dunkelheit erstickt. \\\\\\'\"\\''",
  "cp25:2.3": "'\\'\"\\\\\\'Mein Geist schreckte vor Wiedergeburt zurück, \\\\\\'\"\\''",
  "cp25:2.4": "'\\'\"\\\\\\'als würde er heftig von einem Treibstock gestochen. \\\\\\'\"\\''",
  "cp25:3.1": "'\\'\"\\\\\\'Ich sah die vielen Arten von Bosheit \\\\\\'\"\\''",
  "cp25:3.2": "'\\'\"\\\\\\'und dachte für mich: \\\\\\'\"\\''",
  "cp25:3.3": "'\\'\"\\\\\\'‚Wann werde ich das Haus verlassen \\\\\\'\"\\''",
  "cp25:3.4": "'\\'\"\\\\\\'und in den Wald hineingehen?‘ \\\\\\'\"\\''",
  "cp25:4.1": "'\\'\"\\\\\\'Dann luden mich auch meine Verwandten ein, \\\\\\'\"\\''",
  "cp25:4.2": "'\\'\"\\\\\\'die Sinnenfreuden zu genießen. \\\\\\'\"\\''",
  "cp25:4.3": "'\\'\"\\\\\\'Ich sprach zu ihnen von meinem Verlangen: \\\\\\'\"\\''",
  "cp25:4.4": "'\\'\"\\\\\\'‚Ladet mich nicht zu solchen Dingen ein!‘ \\\\\\'\"\\''",
  "cp25:5.1": "'\\'\"\\\\\\'Mein jüngerer Bruder \\\\\\'\"\\''",
  "cp25:5.2": "'\\'\"\\\\\\'mit Namen Nanda der Kluge \\\\\\'\"\\''",
  "cp25:5.3": "'\\'\"\\\\\\'folgte meinen sittlichen Übungen \\\\\\'\"\\''",
  "cp25:5.4": "'\\'\"\\\\\\'und wählte das Fortziehen. \\\\\\'\"\\''",
  "cp25:6.1": "'\\'\"\\\\\\'Ich, Soṇa, und Nanda \\\\\\'\"\\''",
  "cp25:6.2": "'\\'\"\\\\\\'und meine beiden Eltern \\\\\\'\"\\''",
  "cp25:6.3": "'\\'\"\\\\\\'gaben dann unsere Reichtümer auf \\\\\\'\"\\''",
  "cp25:6.4": "'\\'\"\\\\\\'und gingen in den Wald hinein.“ \\\\\\'\"\\''"
}