{
  "cp29:0.1": "'\"\\'Korb des Verhaltens 29 \\'\"'",
  "cp29:0.2": "'\"\\'Das Kapitel mit Yudhañjaya \\'\"'",
  "cp29:0.3": "'\"\\'Die Vollkommenheit der Wahrheit (3) \\'\"'",
  "cp29:0.4": "'\"\\'Das Verhalten der jungen Wachtel \\'\"'",
  "cp29:1.1": "'\"\\'„Und wiederum, als ich \\'\"'",
  "cp29:1.2": "'\"\\'eine junge Wachtel in Magadha war, \\'\"'",
  "cp29:1.3": "'\"\\'da war ich ein Nestling, die Flügel noch nicht gesprossen, \\'\"'",
  "cp29:1.4": "'\"\\'nur ein Fleischfetzen im Nest. \\'\"'",
  "cp29:2.1": "'\"\\'Meine Mutter fütterte mich \\'\"'",
  "cp29:2.2": "'\"\\'mit Futter, das sie im Schnabel brachte. \\'\"'",
  "cp29:2.3": "'\"\\'Ich überlebte durch ihre Berührung, \\'\"'",
  "cp29:2.4": "'\"\\'denn ich hatte keine körperliche Kraft. \\'\"'",
  "cp29:3.1": "'\"\\'In einem Jahr in der heißen Jahreszeit \\'\"'",
  "cp29:3.2": "'\"\\'fing der Wald Feuer. \\'\"'",
  "cp29:3.3": "'\"\\'Es kam auf uns zu, \\'\"'",
  "cp29:3.4": "'\"\\'ein Großbrand mit einer schwarzen Spur. \\'\"'",
  "cp29:4.1": "'\"\\'Das Feuer grollte und dröhnte \\'\"'",
  "cp29:4.2": "'\"\\'mit hohen Flammenkronen \\'\"'",
  "cp29:4.3": "'\"\\'und brannte allmählich \\'\"'",
  "cp29:4.4": "'\"\\'näher zu mir. \\'\"'",
  "cp29:5.1": "'\"\\'Voller Angst und Schrecken vor dem grimmigen Feuer \\'\"'",
  "cp29:5.2": "'\"\\'gaben Mutter und Vater \\'\"'",
  "cp29:5.3": "'\"\\'mich im Nest auf, \\'\"'",
  "cp29:5.4": "'\"\\'um sich selbst zu retten. \\'\"'",
  "cp29:6.1": "'\"\\'Ich schlug mit Füßen und Flügeln, \\'\"'",
  "cp29:6.2": "'\"\\'aber ich hatte keine körperliche Kraft. \\'\"'",
  "cp29:6.3": "'\"\\'Als ich nicht wegkonnte, \\'\"'",
  "cp29:6.4": "'\"\\'hatte ich diesen Gedanken: \\'\"'",
  "cp29:7.1": "'\"\\'‚Die, zu denen ich laufen sollte, \\'\"'",
  "cp29:7.2": "'\"\\'ängstlich und zitternd vor Furcht, \\'\"'",
  "cp29:7.3": "'\"\\'sind entkommen, haben mich zurückgelassen. \\'\"'",
  "cp29:7.4": "'\"\\'Wie soll ich heute handeln? \\'\"'",
  "cp29:8.1": "'\"\\'Es gibt die Eigenschaft der Tugend in der Welt, \\'\"'",
  "cp29:8.2": "'\"\\'der Wahrheit, der Reinheit, der Barmherzigkeit. \\'\"'",
  "cp29:8.3": "'\"\\'Durch diese Wahrheit will ich \\'\"'",
  "cp29:8.4": "'\"\\'eine unübertreffliche Erklärung der Wahrheit machen.‘ \\'\"'",
  "cp29:9.1": "'\"\\'Von der Macht des Dhamma ergriffen, \\'\"'",
  "cp29:9.2": "'\"\\'in Erinnerung an frühere Sieger, \\'\"'",
  "cp29:9.3": "'\"\\'auf die Macht der Wahrheit gestützt, \\'\"'",
  "cp29:9.4": "'\"\\'so machte ich eine Erklärung der Wahrheit: \\'\"'",
  "cp29:10.1": "'\"\\'‚Ich habe Flügel, die nicht fliegen! \\'\"'",
  "cp29:10.2": "'\"\\'Ich habe Füße, die nicht gehen! \\'\"'",
  "cp29:10.3": "'\"\\'Mutter und Vater sind weggegangen! \\'\"'",
  "cp29:10.4": "'\"\\'Feuer: Weiche zurück!‘ \\'\"'",
  "cp29:11.1": "'\"\\'Als ich diese Wahrheit erklärte, \\'\"'",
  "cp29:11.2": "'\"\\'zogen sich die hohen Flammenkronen \\'\"'",
  "cp29:11.3": "'\"\\'sechzehn Meilen zurück, \\'\"'",
  "cp29:11.4": "'\"\\'als wären sie auf Wasser getroffen. \\'\"'",
  "cp29:11.5": "'\"\\'Es gibt niemanden, der meiner Wahrhaftigkeit gleichkommt: \\'\"'",
  "cp29:11.6": "'\"\\'Das ist meine Vollkommenheit der Wahrheit.“ \\'\"'"
}