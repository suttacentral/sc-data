{
  "cp32:0.1": "'\"\\'Korb des Verhaltens 32 \\'\"'",
  "cp32:0.2": "'\"\\'Das Kapitel mit Yudhañjaya \\'\"'",
  "cp32:0.3": "'\"\\'Die Vollkommenheit der Wahrheit (6) \\'\"'",
  "cp32:0.4": "'\"\\'Sutasomas Verhalten \\'\"'",
  "cp32:1.1": "'\"\\'„Und wiederum, als ich \\'\"'",
  "cp32:1.2": "'\"\\'Sutasoma war, der Herr der Erde, \\'\"'",
  "cp32:1.3": "'\"\\'gefangen von einem Menschenfresser, \\'\"'",
  "cp32:1.4": "'\"\\'erinnerte ich mich an das, was ich einem Brahmanen versprochen hatte. \\'\"'",
  "cp32:2.1": "'\"\\'Hundert adlige Krieger \\'\"'",
  "cp32:2.2": "'\"\\'hatte er an den Händen aufgehängt. \\'\"'",
  "cp32:2.3": "'\"\\'Nachdem er ihr Blut entleert hatte, \\'\"'",
  "cp32:2.4": "'\"\\'brachte er mich zum Opfer herbei. \\'\"'",
  "cp32:3.1": "'\"\\'Der Menschenfresser fragte mich: \\'\"'",
  "cp32:3.2": "'\"\\'‚Was wünschst du – Freilassung? \\'\"'",
  "cp32:3.3": "'\"\\'Ich werde tun, was du denkst, \\'\"'",
  "cp32:3.4": "'\"\\'wenn du nur zurückkommst.‘ \\'\"'",
  "cp32:4.1": "'\"\\'Ich versprach ihm, \\'\"'",
  "cp32:4.2": "'\"\\'zur Morgendämmerung zurückzukehren. \\'\"'",
  "cp32:4.3": "'\"\\'Ich ging zu der entzückenden Stadt \\'\"'",
  "cp32:4.4": "'\"\\'und entsagte dann dem Königtum. \\'\"'",
  "cp32:5.1": "'\"\\'Ich besann mich auf den wahren Dhamma, \\'\"'",
  "cp32:5.2": "'\"\\'wie er von früheren Siegern gehegt war, \\'\"'",
  "cp32:5.3": "'\"\\'gab mein Vermögen dem Brahmanen \\'\"'",
  "cp32:5.4": "'\"\\'und kehrte zum Menschenfresser zurück. \\'\"'",
  "cp32:6.1": "'\"\\'Ich zweifelte nicht daran, \\'\"'",
  "cp32:6.2": "'\"\\'dass er mich töten würde. \\'\"'",
  "cp32:6.3": "'\"\\'Indem ich meine wahrhaftige Rede beschützte, \\'\"'",
  "cp32:6.4": "'\"\\'ging ich, um mein Leben aufzugeben. \\'\"'",
  "cp32:6.5": "'\"\\'Es gibt niemanden, der meiner Wahrhaftigkeit gleichkommt: \\'\"'",
  "cp32:6.6": "'\"\\'Das ist meine Vollkommenheit der Wahrheit.“ \\'\"'"
}