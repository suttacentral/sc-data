{
  "cp23:0.1": "'\\'\"\\\\\\'Korb des Verhaltens 23 \\\\\\'\"\\''",
  "cp23:0.2": "'\\'\"\\\\\\'Das Kapitel mit Yudhañjaya \\\\\\'\"\\''",
  "cp23:0.3": "'\\'\"\\\\\\'Die Vollkommenheit der Entsagung (3) \\\\\\'\"\\''",
  "cp23:0.4": "'\\'\"\\\\\\'Ayogharas Verhalten \\\\\\'\"\\''",
  "cp23:1.1": "'\\'\"\\\\\\'„Und wiederum, als ich \\\\\\'\"\\''",
  "cp23:1.2": "'\\'\"\\\\\\'der wahre Sohn des Königs von Kāsi war, \\\\\\'\"\\''",
  "cp23:1.3": "'\\'\"\\\\\\'wuchs ich in einem Haus aus Eisen auf, \\\\\\'\"\\''",
  "cp23:1.4": "'\\'\"\\\\\\'daher wurde ich Ayoghara genannt. \\\\\\'\"\\''",
  "cp23:2.1": "'\\'\"\\\\\\'‚Dein Leben war schmerzhaft, \\\\\\'\"\\''",
  "cp23:2.2": "'\\'\"\\\\\\'da du in Gefangenschaft aufgezogen wurdest. \\\\\\'\"\\''",
  "cp23:2.3": "'\\'\"\\\\\\'Heute, mein Sohn, herrsche \\\\\\'\"\\''",
  "cp23:2.4": "'\\'\"\\\\\\'über die Gesamtheit dieses Landes.‘ \\\\\\'\"\\''",
  "cp23:3.1": "'\\'\"\\\\\\'Ich verbeugte mich vor den Bürgern von Stadt und Land, \\\\\\'\"\\''",
  "cp23:3.2": "'\\'\"\\\\\\'vor dem König und seinen Leuten, \\\\\\'\"\\''",
  "cp23:3.3": "'\\'\"\\\\\\'dann erhob ich meine zusammengelegten Hände \\\\\\'\"\\''",
  "cp23:3.4": "'\\'\"\\\\\\'und sagte Folgendes: \\\\\\'\"\\''",
  "cp23:4.1": "'\\'\"\\\\\\'‚Alle Geschöpfe auf dieser Erde, \\\\\\'\"\\''",
  "cp23:4.2": "'\\'\"\\\\\\'die Niederen, Mittleren oder Hohen, \\\\\\'\"\\''",
  "cp23:4.3": "'\\'\"\\\\\\'sind ungeschützt in ihrem eigenen Heim, \\\\\\'\"\\''",
  "cp23:4.4": "'\\'\"\\\\\\'in dem sie mit ihren Familien aufwuchsen. \\\\\\'\"\\''",
  "cp23:5.1": "'\\'\"\\\\\\'Ich war einzigartig in der Welt, \\\\\\'\"\\''",
  "cp23:5.2": "'\\'\"\\\\\\'da ich in Gefangenschaft aufgezogen wurde. \\\\\\'\"\\''",
  "cp23:5.3": "'\\'\"\\\\\\'Ich wuchs in einem Haus aus Eisen auf, \\\\\\'\"\\''",
  "cp23:5.4": "'\\'\"\\\\\\'von Sonne und Mond schwach erleuchtet. \\\\\\'\"\\''",
  "cp23:6.1": "'\\'\"\\\\\\'Mit faulendem Aas angefüllt, \\\\\\'\"\\''",
  "cp23:6.2": "'\\'\"\\\\\\'so entkam ich dem Leib meiner Mutter \\\\\\'\"\\''",
  "cp23:6.3": "'\\'\"\\\\\\'und wurde dann zu scheußlicherem Leiden \\\\\\'\"\\''",
  "cp23:6.4": "'\\'\"\\\\\\'in ein eisernes Haus gestoßen. \\\\\\'\"\\''",
  "cp23:7.1": "'\\'\"\\\\\\'Da ich in so entsetzlich schreckliches \\\\\\'\"\\''",
  "cp23:7.2": "'\\'\"\\\\\\'Leiden gefallen bin, \\\\\\'\"\\''",
  "cp23:7.3": "'\\'\"\\\\\\'wenn ich da Königtum begehrte, \\\\\\'\"\\''",
  "cp23:7.4": "'\\'\"\\\\\\'wäre ich der Schlimmste der Schlimmen. \\\\\\'\"\\''",
  "cp23:8.1": "'\\'\"\\\\\\'Ich habe genug vom Körper, \\\\\\'\"\\''",
  "cp23:8.2": "'\\'\"\\\\\\'ich brauche kein Königtum. \\\\\\'\"\\''",
  "cp23:8.3": "'\\'\"\\\\\\'Ich werde das Erlöschen suchen, \\\\\\'\"\\''",
  "cp23:8.4": "'\\'\"\\\\\\'wo der Tod mich nicht zermalmen kann.‘ \\\\\\'\"\\''",
  "cp23:9.1": "'\\'\"\\\\\\'Als ich so nachdachte \\\\\\'\"\\''",
  "cp23:9.2": "'\\'\"\\\\\\'und die große Menge jammerte, \\\\\\'\"\\''",
  "cp23:9.3": "'\\'\"\\\\\\'da ging ich in das Waldstück hinein, \\\\\\'\"\\''",
  "cp23:9.4": "'\\'\"\\\\\\'wie ein Elefantenbulle, der seine Stricke zerreißt. \\\\\\'\"\\''",
  "cp23:10.1": "'\\'\"\\\\\\'Es war nicht, weil ich meine Eltern nicht mochte, \\\\\\'\"\\''",
  "cp23:10.2": "'\\'\"\\\\\\'noch weil ich großen Ruhm nicht mochte, \\\\\\'\"\\''",
  "cp23:10.3": "'\\'\"\\\\\\'sondern weil mir Allwissenheit kostbar war, \\\\\\'\"\\''",
  "cp23:10.4": "'\\'\"\\\\\\'darum entsagte ich dem Königtum.“ \\\\\\'\"\\''"
}