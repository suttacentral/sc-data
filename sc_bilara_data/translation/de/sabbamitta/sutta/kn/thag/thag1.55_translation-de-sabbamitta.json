  "thag1.55:0.3": "'\\'\"\\\\\\'6. Das sechste Kapitel \\\\\\'\"\\''",
  "thag1.55:0.4": "'\\'\"\\\\\\'Añjanavaniya \\\\\\'\"\\''",
