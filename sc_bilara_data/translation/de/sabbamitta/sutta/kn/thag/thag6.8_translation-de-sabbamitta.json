{
  "thag6.8:0.1": "'\\'\"\\\\\\'Strophen der altehrwürdigen Mönche 6.8 \\\\\\'\"\\''",
  "thag6.8:0.2": "'\\'\"\\\\\\'Das Sechserbuch \\\\\\'\"\\''",
  "thag6.8:0.3": "'\\'\"\\\\\\'1. Das erste Kapitel \\\\\\'\"\\''",
  "thag6.8:0.4": "'\\'\"\\\\\\'Migajāla \\\\\\'\"\\''",
  "thag6.8:1.1": "'\\'\"\\\\\\'Es wurde vom Klaräugigen gut gelehrt, \\\\\\'\"\\''",
  "thag6.8:1.2": "'\\'\"\\\\\\'dem Buddha, der mit der Sonne verwandt ist, \\\\\\'\"\\''",
  "thag6.8:1.3": "'\\'\"\\\\\\'der alle Fesseln überwunden \\\\\\'\"\\''",
  "thag6.8:1.4": "'\\'\"\\\\\\'und alles Weiterrollen zerstört hat. \\\\\\'\"\\''",
  "thag6.8:2.1": "'\\'\"\\\\\\'Es macht unabhängig, führt hinüber, \\\\\\'\"\\''",
  "thag6.8:2.2": "'\\'\"\\\\\\'trocknet die Wurzel des Verlangens aus, \\\\\\'\"\\''",
  "thag6.8:2.3": "'\\'\"\\\\\\'und wenn es die giftige Wurzel, das Schlachthaus, abgeschnitten hat, \\\\\\'\"\\''",
  "thag6.8:2.4": "'\\'\"\\\\\\'führt es zum Erlöschen. \\\\\\'\"\\''",
  "thag6.8:3.1": "'\\'\"\\\\\\'Indem es die Wurzel des Nichtwissens zerbricht, \\\\\\'\"\\''",
  "thag6.8:3.2": "'\\'\"\\\\\\'zerschlägt es das Triebwerk der Taten \\\\\\'\"\\''",
  "thag6.8:3.3": "'\\'\"\\\\\\'und schleudert den Donnerkeil der Erkenntnis \\\\\\'\"\\''",
  "thag6.8:3.4": "'\\'\"\\\\\\'auf das Aufgreifen von Bewusstsein. \\\\\\'\"\\''",
  "thag6.8:4.1": "'\\'\"\\\\\\'Es klärt uns über unsere Gefühle auf \\\\\\'\"\\''",
  "thag6.8:4.2": "'\\'\"\\\\\\'und erlöst uns vom Ergreifen – \\\\\\'\"\\''",
  "thag6.8:4.3": "'\\'\"\\\\\\'alle Daseinszustände betrachten wir mit Verstehen \\\\\\'\"\\''",
  "thag6.8:4.4": "'\\'\"\\\\\\'als eine Grube mit glühenden Kohlen. \\\\\\'\"\\''",
  "thag6.8:5.1": "'\\'\"\\\\\\'Es ist sehr süß und sehr tief, \\\\\\'\"\\''",
  "thag6.8:5.2": "'\\'\"\\\\\\'hält Geburt und Tod in Schach; \\\\\\'\"\\''",
  "thag6.8:5.3": "'\\'\"\\\\\\'es ist der edle achtfache Pfad – \\\\\\'\"\\''",
  "thag6.8:5.4": "'\\'\"\\\\\\'das Zur-Ruhe-Kommen des Leidens, Seligkeit. \\\\\\'\"\\''",
  "thag6.8:6.1": "'\\'\"\\\\\\'Man erkennt Tat als Tat \\\\\\'\"\\''",
  "thag6.8:6.2": "'\\'\"\\\\\\'und Ergebnis als Ergebnis; \\\\\\'\"\\''",
  "thag6.8:6.3": "'\\'\"\\\\\\'man erkennt abhängig entstandene Erscheinungen, \\\\\\'\"\\''",
  "thag6.8:6.4": "'\\'\"\\\\\\'als stünden sie in hellem Licht; \\\\\\'\"\\''",
  "thag6.8:6.5": "'\\'\"\\\\\\'es führt zum großen Schutzgebiet, zum Frieden \\\\\\'\"\\''",
  "thag6.8:6.6": "'\\'\"\\\\\\'und ist am Ende vortrefflich. \\\\\\'\"\\''"
}