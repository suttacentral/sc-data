{
  "thag2.25:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 2.25 \\'\"'",
  "thag2.25:0.2": "'\"\\'Das Zweierbuch \\'\"'",
  "thag2.25:0.3": "'\"\\'3. Das dritte Kapitel \\'\"'",
  "thag2.25:0.4": "'\"\\'Vītasoka \\'\"'",
  "thag2.25:1.1": "'\"\\'Der Barbier kam, \\'\"'",
  "thag2.25:1.2": "'\"\\'um meinen Kopf zu scheren. \\'\"'",
  "thag2.25:1.3": "'\"\\'Ich griff nach einem Spiegel \\'\"'",
  "thag2.25:1.4": "'\"\\'und betrachtete meinen Körper. \\'\"'",
  "thag2.25:2.1": "'\"\\'Mein Körper erschien hohl; \\'\"'",
  "thag2.25:2.2": "'\"\\'einst war ich blind, doch die Dunkelheit verschwand. \\'\"'",
  "thag2.25:2.3": "'\"\\'Meine schöne Frisur ist abgeschnitten: \\'\"'",
  "thag2.25:2.4": "'\"\\'Künftige Leben wird es jetzt nicht mehr geben. \\'\"'"
}