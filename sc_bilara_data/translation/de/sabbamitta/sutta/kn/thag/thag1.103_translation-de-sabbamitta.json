{
  "thag1.103:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 1.103 \\'\"'",
  "thag1.103:0.2": "'\"\\'Das Einerbuch \\'\"'",
  "thag1.103:0.3": "'\"\\'11. Das elfte Kapitel \\'\"'",
  "thag1.103:0.4": "'\"\\'Bandhura \\'\"'",
  "thag1.103:1.1": "'\"\\'Ich brauche das nicht – \\'\"'",
  "thag1.103:1.2": "'\"\\'mit der süßen Lehre bin ich glücklich und zufrieden. \\'\"'",
  "thag1.103:1.3": "'\"\\'Ich habe das Beste getrunken, den unübertrefflichen Nektar: \\'\"'",
  "thag1.103:1.4": "'\"\\'Ich werde keinem Gift nahekommen. \\'\"'"
