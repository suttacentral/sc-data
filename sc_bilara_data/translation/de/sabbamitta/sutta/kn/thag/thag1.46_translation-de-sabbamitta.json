  "thag1.46:0.4": "'\\'\"\\\\\\'Samiddhi \\\\\\'\"\\''",
  "thag1.46:1.1": "'\\'\"\\\\\\'Ich zog aus Vertrauen fort \\\\\\'\"\\''",
