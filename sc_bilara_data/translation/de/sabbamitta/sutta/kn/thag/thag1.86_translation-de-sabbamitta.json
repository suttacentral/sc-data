{
  "thag1.86:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 1.86 \\'\"'",
  "thag1.86:0.2": "'\"\\'Das Einerbuch \\'\"'",
  "thag1.86:0.3": "'\"\\'9. Das neunte Kapitel \\'\"'",
  "thag1.86:0.4": "'\"\\'Nāgita \\'\"'",
  "thag1.86:1.1": "'\"\\'Woanders gibt es viele andere Doktrinen; \\'\"'",
  "thag1.86:1.2": "'\"\\'diese Pfade führen nicht, wie dieser, zum Erlöschen. \\'\"'",
  "thag1.86:1.3": "'\"\\'Denn der Buddha selbst unterweist den Saṅgha; \\'\"'",
  "thag1.86:1.4": "'\"\\'der Lehrer zeigt die offenen Handflächen. \\'\"'"
}