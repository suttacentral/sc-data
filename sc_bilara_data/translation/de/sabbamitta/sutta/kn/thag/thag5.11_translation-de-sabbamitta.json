{
  "thag5.11:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 5.11 \\'\"'",
  "thag5.11:0.2": "'\"\\'Das Fünferbuch \\'\"'",
  "thag5.11:0.3": "'\"\\'1. Das erste Kapitel \\'\"'",
  "thag5.11:0.4": "'\"\\'Soṇa mit den gespitzten Ohren \\'\"'",
  "thag5.11:1.1": "'\"\\'Ich habe die Ordination erhalten; \\'\"'",
  "thag5.11:1.2": "'\"\\'ich bin befreit, ohne Befleckungen; \\'\"'",
  "thag5.11:1.3": "'\"\\'ich habe den Gesegneten selbst gesehen \\'\"'",
  "thag5.11:1.4": "'\"\\'und sogar bei ihm gewohnt. \\'\"'",
  "thag5.11:2.1": "'\"\\'Der Gesegnete, der Lehrer, \\'\"'",
  "thag5.11:2.2": "'\"\\'verbrachte große Teile der Nacht unter freiem Himmel, \\'\"'",
  "thag5.11:2.3": "'\"\\'dann betrat er, der so geschickt im Meditieren ist, \\'\"'",
  "thag5.11:2.4": "'\"\\'seine Unterkunft. \\'\"'",
  "thag5.11:3.1": "'\"\\'Indem er seine äußere Robe ausbreitete, \\'\"'",
  "thag5.11:3.2": "'\"\\'bereitete Gotama sein Bett, \\'\"'",
  "thag5.11:3.3": "'\"\\'wie ein Löwe in einer Felshöhle, \\'\"'",
  "thag5.11:3.4": "'\"\\'Angst und Schrecken hatte er aufgegeben. \\'\"'",
  "thag5.11:4.1": "'\"\\'Dann sagte Soṇa, ein Schüler des Buddha, \\'\"'",
  "thag5.11:4.2": "'\"\\'mit schöner Aussprache \\'\"'",
  "thag5.11:4.3": "'\"\\'vor dem besten der Buddhas \\'\"'",
  "thag5.11:4.4": "'\"\\'die wahre Lehre auf. \\'\"'",
  "thag5.11:5.1": "'\"\\'Wenn er die fünf Aggregate vollständig verstanden \\'\"'",
  "thag5.11:5.2": "'\"\\'und die direkte Route entwickelt hat \\'\"'",
  "thag5.11:5.3": "'\"\\'und beim höchsten Frieden angekommen ist, \\'\"'",
  "thag5.11:5.4": "'\"\\'wird er vollkommen erlöschen, unbefleckt. \\'\"'"
}