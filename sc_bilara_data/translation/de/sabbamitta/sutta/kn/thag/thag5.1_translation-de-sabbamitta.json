{
  "thag5.1:0.1": "'\\'\"\\\\\\'Strophen der altehrwürdigen Mönche 5.1 \\\\\\'\"\\''",
  "thag5.1:0.2": "'\\'\"\\\\\\'Das Fünferbuch \\\\\\'\"\\''",
  "thag5.1:0.3": "'\\'\"\\\\\\'1. Das erste Kapitel \\\\\\'\"\\''",
  "thag5.1:0.4": "'\\'\"\\\\\\'Rājadatta \\\\\\'\"\\''",
  "thag5.1:1.1": "'\\'\"\\\\\\'Ich war ein Mönch und ging zu einem Leichenfeld, \\\\\\'\"\\''",
  "thag5.1:1.2": "'\\'\"\\\\\\'wo ich den verlassenen Körper einer Frau sah, \\\\\\'\"\\''",
  "thag5.1:1.3": "'\\'\"\\\\\\'weggeworfen auf ein Leichenfeld, \\\\\\'\"\\''",
  "thag5.1:1.4": "'\\'\"\\\\\\'voller Maden, die daran fraßen. \\\\\\'\"\\''",
  "thag5.1:2.1": "'\\'\"\\\\\\'Manche Männer waren angewidert, \\\\\\'\"\\''",
  "thag5.1:2.2": "'\\'\"\\\\\\'als sie sie tot und verfault da sahen; \\\\\\'\"\\''",
  "thag5.1:2.3": "'\\'\"\\\\\\'doch in mir kam sexuelle Begierde auf, \\\\\\'\"\\''",
  "thag5.1:2.4": "'\\'\"\\\\\\'ich war wie blind für ihren triefenden Körper. \\\\\\'\"\\''",
  "thag5.1:3.1": "'\\'\"\\\\\\'Schneller als man Reis kocht \\\\\\'\"\\''",
  "thag5.1:3.2": "'\\'\"\\\\\\'verließ ich diesen Ort! \\\\\\'\"\\''",
  "thag5.1:3.3": "'\\'\"\\\\\\'Achtsam und der Situation bewusst \\\\\\'\"\\''",
  "thag5.1:3.4": "'\\'\"\\\\\\'ließ ich mich abseits nieder. \\\\\\'\"\\''",
  "thag5.1:4.1": "'\\'\"\\\\\\'Da kam die Erkenntnis \\\\\\'\"\\''",
  "thag5.1:4.2": "'\\'\"\\\\\\'in mir auf – \\\\\\'\"\\''",
  "thag5.1:4.3": "'\\'\"\\\\\\'die Gefahr wurde klar \\\\\\'\"\\''",
  "thag5.1:4.4": "'\\'\"\\\\\\'und ich wurde gründlich ernüchtert. \\\\\\'\"\\''",
  "thag5.1:5.1": "'\\'\"\\\\\\'Dann war mein Geist befreit – \\\\\\'\"\\''",
  "thag5.1:5.2": "'\\'\"\\\\\\'sieh’ die Exzellenz der Lehre! \\\\\\'\"\\''",
  "thag5.1:5.3": "'\\'\"\\\\\\'Das dreifache Wissen habe ich verwirklicht \\\\\\'\"\\''",
  "thag5.1:5.4": "'\\'\"\\\\\\'und die Anleitung des Buddha erfüllt. \\\\\\'\"\\''"
}