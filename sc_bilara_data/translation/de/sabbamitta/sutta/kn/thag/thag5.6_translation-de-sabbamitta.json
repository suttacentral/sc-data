{
  "thag5.6:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 5.6 \\'\"'",
  "thag5.6:0.2": "'\"\\'Das Fünferbuch \\'\"'",
  "thag5.6:0.3": "'\"\\'1. Das erste Kapitel \\'\"'",
  "thag5.6:0.4": "'\"\\'Nadīkassapa \\'\"'",
  "thag5.6:1.1": "'\"\\'Es war wahrhaftig zu meinem Vorteil, \\'\"'",
  "thag5.6:1.2": "'\"\\'dass der Buddha zum Fluss Nerañjara ging. \\'\"'",
  "thag5.6:1.3": "'\"\\'Als ich seine Lehre gehört hatte, \\'\"'",
  "thag5.6:1.4": "'\"\\'scheute ich falsche Ansicht. \\'\"'",
  "thag5.6:2.1": "'\"\\'Früher brachte ich viele verschiedene Opfer dar; \\'\"'",
  "thag5.6:2.2": "'\"\\'ich huldigte der heiligen Flamme \\'\"'",
  "thag5.6:2.3": "'\"\\'und bildete mir ein: „Das ist Reinheit.“ \\'\"'",
  "thag5.6:2.4": "'\"\\'Ich war ein blinder gewöhnlicher Mensch, \\'\"'",
  "thag5.6:3.1": "'\"\\'im Dickicht falscher Ansicht gefangen, \\'\"'",
  "thag5.6:3.2": "'\"\\'von falschem Verständnis getäuscht. \\'\"'",
  "thag5.6:3.3": "'\"\\'Unreinheit hielt ich für Reinheit, \\'\"'",
  "thag5.6:3.4": "'\"\\'blind war ich und unwissend. \\'\"'",
  "thag5.6:4.1": "'\"\\'Ich habe falsche Ansicht aufgegeben, \\'\"'",
  "thag5.6:4.2": "'\"\\'alle Wiedergeburten sind zerschmettert. \\'\"'",
  "thag5.6:4.3": "'\"\\'Ich huldige der wahrhaft würdigen Flamme: \\'\"'",
  "thag5.6:4.4": "'\"\\'Vor dem Klargewordenen verneige ich mich. \\'\"'",
  "thag5.6:5.1": "'\"\\'Alle Täuschung habe ich aufgegeben; \\'\"'",
  "thag5.6:5.2": "'\"\\'das Verlangen nach Wiedergeburt ist zerschmettert; \\'\"'",
  "thag5.6:5.3": "'\"\\'das Umherwandern durch Geburten ist beendet; \\'\"'",
  "thag5.6:5.4": "'\"\\'künftige Leben wird es jetzt nicht mehr geben. \\'\"'"
}