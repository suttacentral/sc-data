{
  "thag1.62:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 1.62 \\'\"'",
  "thag1.62:0.2": "'\"\\'Das Einerbuch \\'\"'",
  "thag1.62:0.3": "'\"\\'7. Das siebte Kapitel \\'\"'",
  "thag1.62:0.4": "'\"\\'Vajjiputta (1) \\'\"'",
  "thag1.62:1.1": "'\"\\'Wir leben allein in der Wildnis \\'\"'",
  "thag1.62:1.2": "'\"\\'wie ein weggeworfenes Stück Holz im Wald. \\'\"'",
  "thag1.62:1.3": "'\"\\'Viele Menschen beneiden mich, \\'\"'",
  "thag1.62:1.4": "'\"\\'wie die Wesen in der Hölle die beneiden, die in den Himmel kommen. \\'\"'"
}