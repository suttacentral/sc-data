{
  "thag1.68:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 1.68 \\'\"'",
  "thag1.68:0.2": "'\"\\'Das Einerbuch \\'\"'",
  "thag1.68:0.3": "'\"\\'7. Das siebte Kapitel \\'\"'",
  "thag1.68:0.4": "'\"\\'Ekudāniya \\'\"'",
  "thag1.68:1.1": "'\"\\'Ein Abgeklärter von hohem Bewusstsein, beflissen, \\'\"'",
  "thag1.68:1.2": "'\"\\'der sich in den Wegen der Abgeklärtheit schult: \\'\"'",
  "thag1.68:1.3": "'\"\\'Für einen Solchen, gestillt und stets achtsam, \\'\"'",
  "thag1.68:1.4": "'\"\\'gibt es keinen Kummer. \\'\"'"
}