{
  "thag2.26:0.1": "'\\'\"\\\\\\'Strophen der altehrwürdigen Mönche 2.26 \\\\\\'\"\\''",
  "thag2.26:0.2": "'\\'\"\\\\\\'Das Zweierbuch \\\\\\'\"\\''",
  "thag2.26:0.3": "'\\'\"\\\\\\'3. Das dritte Kapitel \\\\\\'\"\\''",
  "thag2.26:0.4": "'\\'\"\\\\\\'Puṇṇamāsa (2) \\\\\\'\"\\''",
  "thag2.26:1.1": "'\\'\"\\\\\\'Ich gab die fünf Hindernisse auf, \\\\\\'\"\\''",
  "thag2.26:1.2": "'\\'\"\\\\\\'um ein Schutzgebiet zu finden. \\\\\\'\"\\''",
  "thag2.26:1.3": "'\\'\"\\\\\\'Ich nahm die Lehre als Spiegel, \\\\\\'\"\\''",
  "thag2.26:1.4": "'\\'\"\\\\\\'um selbst zu erkennen und zu sehen. \\\\\\'\"\\''",
  "thag2.26:2.1": "'\\'\"\\\\\\'Ich betrachtete diesen Körper, \\\\\\'\"\\''",
  "thag2.26:2.2": "'\\'\"\\\\\\'alles daran, innen und außen. \\\\\\'\"\\''",
  "thag2.26:2.3": "'\\'\"\\\\\\'Im Inneren und im Äußeren \\\\\\'\"\\''",
  "thag2.26:2.4": "'\\'\"\\\\\\'erschien mein Körper hohl. \\\\\\'\"\\''"
}