{
  "thag2.29:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 2.29 \\'\"'",
  "thag2.29:0.2": "'\"\\'Das Zweierbuch \\'\"'",
  "thag2.29:0.3": "'\"\\'3. Das dritte Kapitel \\'\"'",
  "thag2.29:0.4": "'\"\\'Bhāradvāja \\'\"'",
  "thag2.29:1.1": "'\"\\'So brüllen die Weisen: \\'\"'",
  "thag2.29:1.2": "'\"\\'wie Löwen in Berghöhlen, \\'\"'",
  "thag2.29:1.3": "'\"\\'Helden, Sieger in der Schlacht, \\'\"'",
  "thag2.29:1.4": "'\"\\'wenn sie Māra und seine Schwadron bezwungen haben. \\'\"'",
  "thag2.29:2.1": "'\"\\'Ich habe dem Lehrer gedient; \\'\"'",
  "thag2.29:2.2": "'\"\\'ich habe den Dhamma und den Saṅgha geehrt. \\'\"'",
  "thag2.29:2.3": "'\"\\'Ich bin glücklich und froh, \\'\"'",
  "thag2.29:2.4": "'\"\\'weil ich meinen Sohn frei von Befleckungen gesehen habe. \\'\"'"
}