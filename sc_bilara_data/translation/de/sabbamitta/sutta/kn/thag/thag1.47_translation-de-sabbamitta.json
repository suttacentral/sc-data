{
  "thag1.47:0.1": "'Strophen der altehrwürdigen Mönche 1.47 '",
  "thag1.47:0.2": "'Das Einerbuch '",
  "thag1.47:0.3": "'5. Das fünfte Kapitel '",
  "thag1.47:0.4": "'Ujjaya '",
  "thag1.47:1.1": "'Ehre sei dir, Buddha, du Held! '",
