{
  "thag2.4:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 2.4 \\'\"'",
  "thag2.4:0.2": "'\"\\'Das Zweierbuch \\'\"'",
  "thag2.4:0.3": "'\"\\'1. Das erste Kapitel \\'\"'",
  "thag2.4:0.4": "'\"\\'Gaṅgātīriya \\'\"'",
  "thag2.4:1.1": "'\"\\'Meine Hütte am Ufer des Ganges \\'\"'",
  "thag2.4:1.2": "'\"\\'ist aus drei Palmblättern gemacht. \\'\"'",
  "thag2.4:1.3": "'\"\\'Meine Almosenschale ist von einer Leiche genommen, \\'\"'",
  "thag2.4:1.4": "'\"\\'meine Robe besteht aus weggeworfenen Fetzen. \\'\"'",
  "thag2.4:2.1": "'\"\\'In meinen ersten beiden Regenzeiten \\'\"'",
  "thag2.4:2.2": "'\"\\'sprach ich nur ein Wort. \\'\"'",
  "thag2.4:2.3": "'\"\\'In meiner dritten Regenzeit \\'\"'",
  "thag2.4:2.4": "'\"\\'war die Masse der Dunkelheit zerschmettert. \\'\"'"
}