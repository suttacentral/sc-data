{
  "thag1.92:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 1.92 \\'\"'",
  "thag1.92:0.2": "'\"\\'Das Einerbuch \\'\"'",
  "thag1.92:0.3": "'\"\\'10. Das zehnte Kapitel \\'\"'",
  "thag1.92:0.4": "'\"\\'Vijaya \\'\"'",
  "thag1.92:1.1": "'\"\\'Einer, dessen Befleckungen aufgelöst sind, \\'\"'",
  "thag1.92:1.2": "'\"\\'der nicht an Nahrung hängt, \\'\"'",
  "thag1.92:1.3": "'\"\\'dessen Revier die Befreiung \\'\"'",
  "thag1.92:1.4": "'\"\\'des Merkmalslosen und Leeren ist – \\'\"'",
  "thag1.92:1.5": "'\"\\'seine Spur ist schwer zu verfolgen, \\'\"'",
  "thag1.92:1.6": "'\"\\'wie die von Vögeln am Himmel. \\'\"'"
