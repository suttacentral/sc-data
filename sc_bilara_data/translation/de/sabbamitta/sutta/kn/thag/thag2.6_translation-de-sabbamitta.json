{
  "thag2.6:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 2.6 \\'\"'",
  "thag2.6:0.2": "'\"\\'Das Zweierbuch \\'\"'",
  "thag2.6:0.3": "'\"\\'1. Das erste Kapitel \\'\"'",
  "thag2.6:0.4": "'\"\\'Meḷajina \\'\"'",
  "thag2.6:1.1": "'\"\\'Als ich den Dhamma hörte, \\'\"'",
  "thag2.6:1.2": "'\"\\'der vom Lehrer gesprochen wurde, \\'\"'",
  "thag2.6:1.3": "'\"\\'war ich mir keines Zweifels \\'\"'",
  "thag2.6:1.4": "'\"\\'an dem Allwissenden bewusst, dem Unbesiegten, \\'\"'",
  "thag2.6:2.1": "'\"\\'dem Führer der Karawane, dem großen Helden, \\'\"'",
  "thag2.6:2.2": "'\"\\'dem unübertroffenen Wagenlenker. \\'\"'",
  "thag2.6:2.3": "'\"\\'Ich habe keinen Zweifel \\'\"'",
  "thag2.6:2.4": "'\"\\'am Pfad oder der Übung. \\'\"'"
}