{
  "thag2.13:0.1": "'\"\\'Strophen der altehrwürdigen Mönche 2.13 \\'\"'",
  "thag2.13:0.2": "'\"\\'Das Zweierbuch \\'\"'",
  "thag2.13:0.3": "'\"\\'2. Das zweite Kapitel \\'\"'",
  "thag2.13:0.4": "'\"\\'Heraññakāni \\'\"'",
  "thag2.13:1.1": "'\"\\'Die Tage und Nächte eilen vorbei, \\'\"'",
  "thag2.13:1.2": "'\"\\'und dann bricht das Leben ab. \\'\"'",
  "thag2.13:1.3": "'\"\\'Das Leben der Sterblichen schwindet dahin \\'\"'",
  "thag2.13:1.4": "'\"\\'wie Wasser in winzigen Rinnsalen. \\'\"'",
  "thag2.13:2.1": "'\"\\'Aber während sie schlechte Taten tun, \\'\"'",
  "thag2.13:2.2": "'\"\\'merken es die Toren nicht – \\'\"'",
  "thag2.13:2.3": "'\"\\'später wird es bitter werden, \\'\"'",
  "thag2.13:2.4": "'\"\\'denn das Ergebnis wird schlecht für sie sein. \\'\"'"
}