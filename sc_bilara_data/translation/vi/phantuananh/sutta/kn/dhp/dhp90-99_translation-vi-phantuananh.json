{
  "dhp90:0.1": "Tiểu Bộ Kinh ",
  "dhp90:0.2": "Kinh Pháp Cú ",
  "dhp90:0.3": "Phẩm A-la-hán ",
  "dhp90:0.4": "Chuyện ông Jīvaka ",
  "dhp90:1": "Ðích đã đến, không sầu, ",
  "dhp90:2": "Giải thoát ngoài tất cả, ",
  "dhp90:3": "Ðoạn trừ mọi buộc ràng, ",
  "dhp90:4": "Vị ấy không nhiệt não. ",
  "dhp91:0": "Chuyện Trưởng lão Mahā Kassapa (Ca-diếp) ",
  "dhp91:1": "Tự sách tấn chánh niệm, ",
  "dhp91:2": "Không thích cư xá nào, ",
  "dhp91:3": "Như ngỗng trời rời ao, ",
  "dhp91:4": "Bỏ sau mọi trú ẩn. ",
  "dhp92:0": "Chuyện Trưởng lão Belaṭṭhasīsa ",
  "dhp92:1": "Tài sản không chất chứa, ",
  "dhp92:2": "Ăn uống biết liễu trì, ",
  "dhp92:3": "Tự tại trong hành xứ, ",
  "dhp92:4": "Không vô tướng, giải thoát, ",
  "dhp92:5": "Như chim giữa hư không, ",
  "dhp92:6": "Hướng chúng đi khó tìm. ",
  "dhp93:0": "Chuyện Trưởng lão Anuruddha (A-nậu-lâu-đà) ",
  "dhp93:1": "Ai lậu hoặc đoạn sạch, ",
  "dhp93:2": "Ăn uống không tham đắm, ",
  "dhp93:3": "Tự tại trong hành xứ, ",
  "dhp93:4": "Không, vô tướng, giải thoát. ",
  "dhp93:5": "Như chim giữa hư không, ",
  "dhp93:6": "Dấu chân thật khó tìm. ",
  "dhp94:0": "Chuyện Trưởng lão Mahā Kaccāyana (Ca-chiên-diên) ",
  "dhp94:1": "Ai nhiếp phục các căn, ",
  "dhp94:2": "Như đánh xe điều ngự, ",
  "dhp94:3": "Mạn trừ, lậu hoặc dứt, ",
  "dhp94:4": "Người vậy, Chư Thiên mến. ",
  "dhp95:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp95:1": "Như đất không hiềm hận, ",
  "dhp95:2": "Như cột trụ kiên trì, ",
  "dhp95:3": "Như hồ, không bùn nhơ, ",
  "dhp95:4": "Không luân hồi, vị ấy. ",
  "dhp96:0": "Chuyện Trưởng lão Tissa ở xứ Kosambi ",
  "dhp96:1": "Người tâm ý an tịnh, ",
  "dhp96:2": "Lời an, nghiệp cũng an, ",
  "dhp96:3": "Chánh trí, chơn giải thoát, ",
  "dhp96:4": "Tịnh lạc là vị ấy. ",
  "dhp97:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp97:1": "Không tin, hiểu Vô vi. ",
  "dhp97:2": "Người cắt mọi hệ lụy, ",
  "dhp97:3": "Cơ hội tận, xả ly ",
  "dhp97:4": "Vị ấy thật tối thượng. ",
  "dhp98:0": "Chuyện Trưởng lão Revata ở rừng Keo ",
  "dhp98:1": "Làng mạc hay rừng núi ",
  "dhp98:2": "Thung lũng hay đồi cao, ",
  "dhp98:3": "La-hán trú chỗ nào, ",
  "dhp98:4": "Ðất ấy thật khả ái. ",
  "dhp99:0": "Chuyện người nữ quyến rũ sa-môn ",
  "dhp99:1": "Khả ái thay núi rừng, ",
  "dhp99:2": "Chỗ người phàm không ưa, ",
  "dhp99:3": "Vị ly tham ưa thích, ",
  "dhp99:4": "Vì không tìm dục lạc. ",
  "dhp99:5": "Hết phẩm A-la-hán "
}