{
  "dhp116:0.1": "Tiểu Bộ Kinh ",
  "dhp116:0.2": "Kinh Pháp Cú ",
  "dhp116:0.3": "Phẩm Ác ",
  "dhp116:0.4": "Chuyện Bà-la-môn Cūḷekasātaka (Tiểu Trung Y) ",
  "dhp116:1": "Hãy gấp làm điều lành, ",
  "dhp116:2": "Ngăn tâm làm điều ác. ",
  "dhp116:3": "Ai chậm làm việc lành, ",
  "dhp116:4": "Ý ưa thích việc ác. ",
  "dhp117:0": "Chuyện Trưởng lão Seyyasaka ",
  "dhp117:1": "Nếu người làm điều ác, ",
  "dhp117:2": "Chớ tiếp tục làm thêm. ",
  "dhp117:3": "Chớ ước muốn điều ác, ",
  "dhp117:4": "Chứa ác, tất chịu khổ. ",
  "dhp118:0": "Chuyện Thiên nữ Lājā (Cốm Rang) ",
  "dhp118:1": "Nếu người làm điều thiện, ",
  "dhp118:2": "Nên tiếp tục làm thêm. ",
  "dhp118:3": "Hãy ước muốn điều thiện, ",
  "dhp118:4": "Chứa thiện, được an lạc. ",
  "dhp119:0": "Chuyện Trưởng giả Anāthapiṇḍika (Cấp Cô Độc) ",
  "dhp119:1": "Người ác thấy là hiền. ",
  "dhp119:2": "Khi ác chưa chín muồi, ",
  "dhp119:3": "Khi ác nghiệp chín muồi, ",
  "dhp119:4": "Người ác mới thấy ác. ",
  "dhp120:1": "Người hiền thấy là ác, ",
  "dhp120:2": "Khi thiện chưa chín muồi, ",
  "dhp120:3": "Khi thiện nghiệp chín muồi, ",
  "dhp120:4": "Người hiền thấy là thiện. ",
  "dhp121:0": "Chuyện vị Tỳ-khoe khinh giới nhỏ ",
  "dhp121:1": "Chớ chê khinh điều ác, ",
  "dhp121:2": "Cho rằng “chưa đến mình”, ",
  "dhp121:3": "Như nước nhỏ từng giọt, ",
  "dhp121:4": "Rồi bình cũng đầy tràn. ",
  "dhp121:5": "Người ngu chứa đầy ác ",
  "dhp121:6": "Do chất chứa dần dần. ",
  "dhp122:0": "Chuyện Bá hộ Biḷālapādaka (Chân Mèo) ",
  "dhp122:1": "Chớ chê khinh điều thiện ",
  "dhp122:2": "Cho rằng “chưa đến mình”, ",
  "dhp122:3": "Như nước nhỏ từng giọt, ",
  "dhp122:4": "Rồi bình cũng đầy tràn. ",
  "dhp122:5": "Người trí chứa đầy thiện, ",
  "dhp122:6": "Do chất chứa dần dần. ",
  "dhp123:0": "Chuyện phú thương tránh hiểm lộ ",
  "dhp123:1": "Ít bạn đường, nhiều tiền, ",
  "dhp123:2": "Người buôn tránh đường hiểm. ",
  "dhp123:3": "Muốn sống, tránh thuốc độc, ",
  "dhp123:4": "Hãy tránh ác như vậy. ",
  "dhp124:0": "Chuyện thợ săn Kukkuṭamitta (Kê Hữu) ",
  "dhp124:1": "Bàn tay không thương tích, ",
  "dhp124:2": "Có thể cầm thuốc độc. ",
  "dhp124:3": "Không thương tích, tránh độc, ",
  "dhp124:4": "Không làm, không có ác. ",
  "dhp125:0": "Chuyện thợ săn Koka gặp sa-môn ",
  "dhp125:1": "Hại người không ác tâm, ",
  "dhp125:2": "Người thanh tịnh, không uế, ",
  "dhp125:3": "Tội ác đến kẻ ngu, ",
  "dhp125:4": "Như ngược gió tung bụi. ",
  "dhp126:0": "Chuyện Trưởng lão Tissa và người thợ ngọc thị giả ",
  "dhp126:1": "Một số sinh bào thai, ",
  "dhp126:2": "Kẻ ác sinh địa ngục, ",
  "dhp126:3": "Người thiện lên cõi trời, ",
  "dhp126:4": "Vô lậu chứng Niết-bàn. ",
  "dhp127:0": "Chuyện 3 nhóm Tỳ-kheo ",
  "dhp127:1": "Không trên trời, giữa biển, ",
  "dhp127:2": "Không lánh vào động núi, ",
  "dhp127:3": "Không chỗ nào trên đời, ",
  "dhp127:4": "Trốn được quả ác nghiệp. ",
  "dhp128:0": "Chuyện vua Suppabuddhasakka (Thiện Giác) ",
  "dhp128:1": "Không trên trời, giữa biển, ",
  "dhp128:2": "Không lánh vào động núi, ",
  "dhp128:3": "Không chỗ nào trên đời, ",
  "dhp128:4": "Trốn khỏi tay thần chết. ",
  "dhp128:5": "Hết phẩm Ác "
