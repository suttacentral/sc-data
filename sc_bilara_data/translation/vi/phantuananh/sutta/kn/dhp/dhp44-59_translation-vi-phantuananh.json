{
  "dhp44:0.1": "Tiểu Bộ Kinh ",
  "dhp44:0.2": "Kinh Pháp Cú ",
  "dhp44:0.3": "Phẩm Hoa ",
  "dhp44:0.4": "Chuyện 500 vị Tỳ-kheo đàm luận về địa đại ",
  "dhp44:1": "Ai chinh phục đất này ",
  "dhp44:2": "Dạ ma, Thiên giới này? ",
  "dhp44:3": "Ai khéo giảng Pháp cú, ",
  "dhp44:4": "Như người khéo hái hoa? ",
  "dhp45:1": "Hữu học chinh phục đất, ",
  "dhp45:2": "Dạ ma, Thiên giới này. ",
  "dhp45:3": "Hữu học giảng Pháp cú, ",
  "dhp45:4": "Như người khéo hái hoa. ",
  "dhp46:0": "Chuyện vị Tỳ-kheo quán bào ảnh ",
  "dhp46:1": "Biết thân như bọt nước, ",
  "dhp46:2": "Ngộ thân là như huyễn, ",
  "dhp46:3": "Bẻ tên hoa của Ma, ",
  "dhp46:4": "Vượt tầm mắt Thần chết. ",
  "dhp47:0": "Chuyện vua Viḍūḍabha (Lưu Ly) ",
  "dhp47:1": "Người nhặt các loại hoa, ",
  "dhp47:2": "Ý đắm say, tham nhiễm, ",
  "dhp47:3": "Bị Thần chết mang đi, ",
  "dhp47:4": "Như lụt trôi làng ngủ. ",
  "dhp48:0": "Chuyện nàng Patipūjikā (Sự Phụ) ",
  "dhp48:1": "Người nhặt các loại hoa, ",
  "dhp48:2": "Ý đắm say, tham nhiễm, ",
  "dhp48:3": "Các dục chưa thỏa mãn, ",
  "dhp48:4": "Ðã bị chết chinh phục. ",
  "dhp49:0": "Chuyện bá hộ Macchariya (Khan Nang) ",
  "dhp49:1": "Như ong đến với hoa, ",
  "dhp49:2": "Không hại sắc và hương, ",
  "dhp49:3": "Che chở hoa, lấy nhụy. ",
  "dhp49:4": "Bậc Thánh đi vào làng. ",
  "dhp50:0": "Chuyện tu sĩ ngoại đạo Pāṭika (Tiểu Điệp) ",
  "dhp50:1": "Không nên nhìn lỗi người, ",
  "dhp50:2": "Người làm hay không làm. ",
  "dhp50:3": "Nên nhìn tự chính mình, ",
  "dhp50:4": "Có làm hay không làm. ",
  "dhp51:0": "Chuyện thiện nam tử Chattapāṇi (Tản Thủ) ",
  "dhp51:1": "Như bông hoa tươi đẹp, ",
  "dhp51:2": "Có sắc nhưng không hương. ",
  "dhp51:3": "Cũng vậy, lời khéo nói, ",
  "dhp51:4": "Không làm, không kết quả. ",
  "dhp52:1": "Như bông hoa tươi đẹp, ",
  "dhp52:2": "Có sắc lại thêm hương; ",
  "dhp52:3": "Cũng vậy, lời khéo nói, ",
  "dhp52:4": "Có làm, có kết quả. ",
  "dhp53:0": "Chuyện bà Visākhā (Thiện Chi) ",
  "dhp53:1": "Như từ một đống hoa, ",
  "dhp53:2": "Nhiều tràng hoa được kết. ",
  "dhp53:3": "Cũng vậy, thân sanh tử, ",
  "dhp53:4": "Làm được nhiều thiện sự. ",
  "dhp54:0": "Chuyện Trưởng lão Ānanda vấn đạo ",
  "dhp54:1": "Hương các loại hoa thơm ",
  "dhp54:2": "Không ngược bay chiều gió ",
  "dhp54:3": "Nhưng hương người đức hạnh Ngược gió khắp tung bay ",
  "dhp54:4": "Chỉ có bậc chân nhân Toả khắp mọi phương trời. ",
  "dhp55:1": "Hoa chiên đàn, già la, ",
  "dhp55:2": "Hoa sen, hoa vũ quý. ",
  "dhp55:3": "Giữa những hương hoa ấy, ",
  "dhp55:4": "Giới hương là vô thượng. ",
  "dhp56:0": "Chuyện Trưởng lão Mahā Kassapa (Ca-diếp) được Đế-thích để bát cúng dường ",
  "dhp56:1": "Ít giá trị hương này, ",
  "dhp56:2": "Hương già la, chiên đàn; ",
  "dhp56:3": "Chỉ hương người đức hạnh, ",
  "dhp56:4": "Tối thượng tỏa Thiên giới. ",
  "dhp57:0": "Chuyện Trưởng lão Godhika viên tịch ",
  "dhp57:1": "Giữa ai có giới hạnh, ",
  "dhp57:2": "An trú không phóng dật. ",
  "dhp57:3": "Chánh trí, chơn giải thoát, ",
  "dhp57:4": "Ác ma không thấy đường. ",
  "dhp58:0": "Chuyện ông Garahadinna (Oán Trách Thí) ",
  "dhp58:1": "Như giữa đống rác nhớp, ",
  "dhp58:2": "Quăng bỏ trên đường lớn, ",
  "dhp58:3": "Chỗ ấy hoa sen nở, ",
  "dhp58:4": "Thơm sạch, đẹp ý người. ",
  "dhp59:1": "Cũng vậy giữa quần sanh, ",
  "dhp59:2": "Uế nhiễm, mù, phàm tục, ",
  "dhp59:3": "Ðệ tử bậc Chánh Giác, ",
  "dhp59:4": "Sáng ngời với Tuệ Tri. ",
  "dhp59:5": "Hết phẩm Hoa "
}