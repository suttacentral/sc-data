{
  "dhp383:0.1": "Tiểu Bộ Kinh ",
  "dhp383:0.2": "Kinh Pháp Cú ",
  "dhp383:0.3": "Phẩm Bà-la-môn ",
  "dhp383:0.4": "Chuyện vị Bà-la-môn có nhiều đức tin ",
  "dhp383:1": "Hỡi này Bà-la-môn, Hãy tinh tấn đoạn lòng, ",
  "dhp383:2": "Từ bỏ các dục lạc, ",
  "dhp383:3": "Biết được hành đoạn diệt, ",
  "dhp383:4": "Ngươi là bậc vô vi. ",
  "dhp384:0": "Chuyện chư Tỳ-kheo ",
  "dhp384:1": "Nhờ thường trú hai pháp ",
  "dhp384:2": "Ðến được bờ bên kia. ",
  "dhp384:3": "Bà-la-môn có trí, ",
  "dhp384:4": "Mọi kiết sử dứt sạch. ",
  "dhp385:0": "Chuyện Ma Vương ",
  "dhp385:1": "Không bờ này, bờ kia ",
  "dhp385:2": "Cả hai bờ không có, ",
  "dhp385:3": "Lìa khổ, không trói buộc ",
  "dhp385:4": "Ta gọi Bà-la-môn. ",
  "dhp386:0": "Chuyện người Bà-la-môn ",
  "dhp386:1": "Tu thiền, trú ly trần ",
  "dhp386:2": "Phận sự xong, vô lậu, ",
  "dhp386:3": "Ðạt được đích tối thượng, ",
  "dhp386:4": "Ta gọi Bà-la-môn. ",
  "dhp387:0": "Chuyện Trưởng lão Ānanda ",
  "dhp387:1": "Mặt trời sáng ban ngày, ",
  "dhp387:2": "Mặt trăng sáng ban đêm. ",
  "dhp387:3": "Khí giới sáng Sát-lỵ, ",
  "dhp387:4": "Thiền định sáng Phạm chí. ",
  "dhp387:5": "Còn hào quang đức Phật, ",
  "dhp387:6": "Chói sáng cả ngày đêm. ",
  "dhp388:0": "Chuyện một người xuất gia ",
  "dhp388:1": "Dứt ác gọi Phạm chí, ",
  "dhp388:2": "Tịnh hạnh gọi Sa-môn, ",
  "dhp388:3": "Tự mình xuất cấu uế, ",
  "dhp388:4": "Nên gọi bậc xuất gia. ",
  "dhp389:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp389:1": "Chớ có đập Phạm chí! ",
  "dhp389:2": "Phạm chí chớ đập lại! ",
  "dhp389:3": "Xấu thay đập Phạm chí ",
  "dhp389:4": "Đập trả lại xấu hơn! ",
  "dhp390:1": "Đối vị Bà-la-môn, Đây không lợi ích nhỏ. ",
  "dhp390:2": "Khi ý không ái luyến, ",
  "dhp390:3": "Tâm hại được chận đứng, ",
  "dhp390:4": "Chỉ khi ấy khổ diệt, ",
  "dhp391:0": "Chuyện Trưởng lão ni Mahā Pajāpatigotamī ",
  "dhp391:1": "Với người thân miệng ý, ",
  "dhp391:2": "Không làm các ác hạnh ",
  "dhp391:3": "Ba nghiệp được phòng hộ, ",
  "dhp391:4": "Ta gọi Bà-la-môn. ",
  "dhp392:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp392:1": "Từ ai, biết chánh pháp ",
  "dhp392:2": "Bậc Chánh Giác thuyết giảng, ",
  "dhp392:3": "Hãy kính lễ vị ấy, ",
  "dhp392:4": "Như phạm chí thờ lửa. ",
  "dhp393:0": "Chuyện Bà-la-môn Jātila ",
  "dhp393:1": "Được gọi Bà-la-môn, Không vì đầu bện tóc, ",
  "dhp393:2": "Không chủng tộc, thọ sanh, ",
  "dhp393:3": "Ai thật chân, chánh, tịnh, ",
  "dhp393:4": "Mới gọi Bà-la-môn. ",
  "dhp394:0": "Chuyện Bà-la-môn Kuhaka ",
  "dhp394:1": "Kẻ ngu, có ích gì ",
  "dhp394:2": "Bện tóc với da dê, ",
  "dhp394:3": "Nội tâm toàn phiền não, ",
  "dhp394:4": "Ngoài mặt đánh bóng suông. ",
  "dhp395:0": "Chuyện nàng Kisāgotamī ",
  "dhp395:1": "Người mặc áo đống rác, ",
  "dhp395:2": "Gầy ốm, lộ mạch gân, ",
  "dhp395:3": "Độc thân thiền trong rừng. ",
  "dhp395:4": "Ta gọi Bà-la-môn. ",
  "dhp396:0": "Chuyện một vị Bà-la-môn ",
  "dhp396:1": "Ta không gọi Phạm chí, ",
  "dhp396:2": "Vì chỗ sanh, mẹ sanh. ",
  "dhp396:3": "Chỉ được gọi tên suông ",
  "dhp396:4": "Nếu tâm còn phiền não. ",
  "dhp396:5": "Không phiền não, chấp trước ",
  "dhp396:6": "Ta gọi Bà-la-môn. ",
  "dhp397:0": "Chuyện con trai Trưởng giả Uggasena ",
  "dhp397:1": "Đoạn hết các kiết sử, ",
  "dhp397:2": "Không còn gì lo sợ ",
  "dhp397:3": "Không đắm trước buộc ràng ",
  "dhp397:4": "Ta gọi Bà-la-môn ",
  "dhp398:0": "Chuyện 2 vị Bà-la-môn ",
  "dhp398:1": "Bỏ đai da, bỏ cương ",
  "dhp398:2": "Bỏ dây, đồ sở thuộc, ",
  "dhp398:3": "Bỏ then chốt, sáng suốt, ",
  "dhp398:4": "Ta gọi Bà-la-môn. ",
  "dhp399:0": "Chuyện Bà-la-môn Akkosaka Bhāradvāja ",
  "dhp399:1": "Không ác ý, nhẫn chịu, ",
  "dhp399:2": "Phỉ báng, đánh, phạt hình, ",
  "dhp399:3": "Lấy nhẫn làm quân lực, ",
  "dhp399:4": "Ta gọi Bà-la-môn. ",
  "dhp400:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp400:1": "Không hận, hết bổn phận, ",
  "dhp400:2": "Trì giới, không tham ái, ",
  "dhp400:3": "Nhiếp phục, thân cuối cùng, ",
  "dhp400:4": "Ta gọi Bà-la-môn. ",
  "dhp401:0": "Chuyện Trưởng lão ni Uppalavaṇṇā ",
  "dhp401:1": "Như nước trên lá sen, ",
  "dhp401:2": "Như hột cải đầu kim, ",
  "dhp401:3": "Người không nhiễm ái dục, ",
  "dhp401:4": "Ta gọi Bà-la-môn. ",
  "dhp402:0": "Chuyện người Bà-la-môn ",
  "dhp402:1": "Ai tự trên đời này, ",
  "dhp402:2": "Giác khổ, diệt trừ khổ, ",
  "dhp402:3": "Bỏ gánh nặng, giải thoát, ",
  "dhp402:4": "Ta gọi Bà-la-môn. ",
  "dhp403:0": "Chuyện Tỳ-kheo-ni Khemā ",
  "dhp403:1": "Người trí tuệ sâu xa, ",
  "dhp403:2": "Khéo biết đạo, phi đạo ",
  "dhp403:3": "Chứng đạt đích vô thượng, ",
  "dhp403:4": "Ta gọi Bà-la-môn. ",
  "dhp404:0": "Chuyện Trưởng lão Tissa ngụ trong núi ",
  "dhp404:1": "Không liên hệ cả hai, ",
  "dhp404:2": "Xuất gia và thế tục, ",
  "dhp404:3": "Sống độc thân, ít dục, ",
  "dhp404:4": "Ta gọi Bà-la-môn. ",
  "dhp405:0": "Chuyện chư Tỳ-kheo ",
  "dhp405:1": "Bỏ trượng, đối chúng sanh, ",
  "dhp405:2": "Yếu kém hay kiên cường, ",
  "dhp405:3": "Không giết, không bảo giết, ",
  "dhp405:4": "Ta gọi Bà-la-môn. ",
  "dhp406:0": "Chuyện các Sa-di ",
  "dhp406:1": "Thân thiện giữa thù địch ",
  "dhp406:2": "Ôn hòa giữa hung hăng. ",
  "dhp406:3": "Không nhiễm, giữa nhiễm trước, ",
  "dhp406:4": "Ta gọi Bà-la-môn. ",
  "dhp407:0": "Chuyện Trưởng lão Mahā Panthaka ",
  "dhp407:1": "Người bỏ rơi tham sân, ",
  "dhp407:2": "Không mạn không ganh tị, ",
  "dhp407:3": "Như hột cải đầu kim, ",
  "dhp407:4": "Ta gọi Bà-la-môn. ",
  "dhp408:0": "Chuyện Trưởng lão Pilindavaccha ",
  "dhp408:1": "Nói lên lời ôn hòa, ",
  "dhp408:2": "Lợi ích và chân thật, ",
  "dhp408:3": "Không mất lòng một ai, ",
  "dhp408:4": "Ta gọi Bà-la-môn. ",
  "dhp409:0": "Chuyện một vị Tỳ-kheo ",
  "dhp409:1": "Ở đời, vật dài, ngắn, ",
  "dhp409:2": "Nhỏ, lớn, đẹp hay xấu ",
  "dhp409:3": "Phàm không cho không lấy, ",
  "dhp409:4": "Ta gọi Bà-la-môn. ",
  "dhp410:0": "Chuyện Trưởng lão Sāriputta (Xá-lợi-phất) ",
  "dhp410:1": "Người không có hy cầu, ",
  "dhp410:2": "Đời này và đời sau, ",
  "dhp410:3": "Không hy cầu, giải thoát, ",
  "dhp410:4": "Ta gọi Bà-la-môn. ",
  "dhp411:0": "Chuyện Trưởng lão Mahā Moggallāna (Mục-kiền-liên) ",
  "dhp411:1": "Người không còn tham ái, ",
  "dhp411:2": "Có trí, không nghi hoặc, ",
  "dhp411:3": "Thể nhập vào bất tử, ",
  "dhp411:4": "Ta gọi Bà-la-môn. ",
  "dhp412:0": "Chuyện Trưởng lão Revata ",
  "dhp412:1": "Người sống ở đời này ",
  "dhp412:2": "Không nhiễm cả thiện ác, ",
  "dhp412:3": "Không sầu, sạch không bụi ",
  "dhp412:4": "Ta gọi Bà-la-môn. ",
  "dhp413:0": "Chuyện Trưởng lão Candābha ",
  "dhp413:1": "Như trăng, sạch không uế ",
  "dhp413:2": "Sáng trong và tịnh lặng, ",
  "dhp413:3": "Hữu ái, được đoạn tận, ",
  "dhp413:4": "Ta gọi Bà-la-môn. ",
  "dhp414:0": "Chuyện Trưởng lão Sīvali ",
  "dhp414:1": "Vượt đường nguy hiểm này, ",
  "dhp414:2": "Nhiếp phục luân hồi, si, ",
  "dhp414:3": "Đến bờ kia thiền định ",
  "dhp414:4": "Không dục ái, không nghi, ",
  "dhp414:5": "Không chấp trước, tịch tịnh, ",
  "dhp414:6": "Ta gọi Bà-la-môn. ",
  "dhp415:0": "Chuyện Trưởng lão Sundarasamudda ",
  "dhp415:1": "Ai ở đời, đoạn dục, ",
  "dhp415:2": "Bỏ nhà, sống xuất gia, ",
  "dhp415:3": "Dục hữu được đoạn tận, ",
  "dhp415:4": "Ta gọi Bà-la-môn. ",
  "dhp416:0": "Chuyện Trưởng lão Jaṭila ",
  "dhp416:1": "Ai ở đời đoạn ái ",
  "dhp416:2": "Bỏ nhà, sống xuất gia, ",
  "dhp416:3": "Ái hữu được đoạn tận, ",
  "dhp416:4": "Ta gọi Bà-la-môn. ",
  "dhp416:5.0": "Chuyện Trưởng lão Jotika ",
  "dhp416:5.1": "(dị bản) ",
  "dhp416:6": "(dị bản) ",
  "dhp416:7": "(dị bản) ",
  "dhp416:8": "(dị bản) ",
  "dhp417:0": "Chuyện vị Tỳ-kheo đã từng khiêu vũ ",
  "dhp417:1": "Bỏ trói buộc loài người, ",
  "dhp417:2": "Vượt trói buộc cõi trời. ",
  "dhp417:3": "Giải thoát mọi buộc ràng, ",
  "dhp417:4": "Ta gọi Bà-la-môn. ",
  "dhp418:0": "Chuyện vị Tỳ-kheo đã từng khiêu vũ ",
  "dhp418:1": "Bỏ điều ưa, điều ghét, ",
  "dhp418:2": "Mát lạnh, diệt sanh y ",
  "dhp418:3": "Bậc anh hùng chiến thắng, Nhiếp phục mọi thế giới, ",
  "dhp418:4": "Ta gọi Bà-la-môn. ",
  "dhp419:0": "Chuyện Trưởng lão Vaṅgīsa ",
  "dhp419:1": "Ai hiểu rõ hoàn toàn ",
  "dhp419:2": "Sanh tử các chúng sanh, ",
  "dhp419:3": "Không nhiễm, khéo vượt qua, Sáng suốt chân giác ngộ, ",
  "dhp419:4": "Ta gọi Bà-la-môn. ",
  "dhp420:1": "Với ai, loài trời, người Cùng với Càn-thát-bà, ",
  "dhp420:2": "Không biết chỗ thọ sanh ",
  "dhp420:3": "Lậu tận bậc La-hán. ",
  "dhp420:4": "Ta gọi Bà-la-môn. ",
  "dhp421:0": "Chuyện Trưởng lão ni Dhammadinnā ",
  "dhp421:1": "Ai quá, hiện, vị lai ",
  "dhp421:2": "Không một sở hữu gì, ",
  "dhp421:3": "Không sở hữu không nắm, ",
  "dhp421:4": "Ta gọi Bà-la-môn. ",
  "dhp422:0": "Chuyện Trưởng lão Angulimāla ",
  "dhp422:1": "Bậc trâu chúa, thù thắng Bậc anh hùng, đại sĩ, ",
  "dhp422:2": "Bậc chiến thắng, không nhiễm, ",
  "dhp422:3": "Bậc tẩy sạch, giác ngộ, ",
  "dhp422:4": "Ta gọi Bà-la-môn. ",
  "dhp423:0": "Chuyện Bà-la-môn Devahita ",
  "dhp423:1": "Ai biết được đời trước, ",
  "dhp423:2": "Thấy thiên giới, đọa xứ, ",
  "dhp423:3": "Đạt được sanh diệt tận ",
  "dhp423:4": "Thắng trí, tự viên thành Bậc mâu-ni đạo sĩ. ",
  "dhp423:5": "Viên mãn mọi thành tựu ",
  "dhp423:6": "Ta gọi Bà-la-môn. ",
  "dhp423:7": "Hết phẩm Bà-la-môn ",
  "dhp423:57": "Hết kinh Pháp Cú "
}