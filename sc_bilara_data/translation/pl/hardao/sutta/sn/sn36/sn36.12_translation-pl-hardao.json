{
  "sn36.12:0.1": "'\"\\'Zbiór połączonych mów - 36 \\'\"'",
  "sn36.12:0.2": "'\"\\'2. Rozdział o tym, co w prywatności \\'\"'",
  "sn36.12:0.3": "'\"\\'Pierwsza mowa o przestrzeni \\'\"'",
  "sn36.12:1.1": "'\"\\'Tak jak, mnisi, jak wiatry w przestworzach wieją z różnorakich stron. \\'\"'",
  "sn36.12:1.2": "'\"\\'Wieją wschodnie wiatry, wieją zachodnie wiatry, wieją północne wiatry, wieją południowe wiatry, wieją wiatry niosące kurz, wieją wiatry bez kurzu, wieją zimne wiatry, wieją gorące wiatry, wieją niewielkie wiatry, wieją mocne wiatry. \\'\"'",
  "sn36.12:1.3": "'\"\\'Tak samo, mnisi, w tym ciele powstają różnorakie odczucia, powstają przyjemne odczucia, powstają bolesne odczucia, powstają ani przyjemne, ani bolesne odczucia. \\'\"'",
  "sn36.12:2.1": "'\"\\'Jak wiatr, co w przestworzach, \\'\"'",
  "sn36.12:2.2": "'\"\\'wieje ze stron różnorakich, \\'\"'",
  "sn36.12:2.3": "'\"\\'Wschodni czy zachodni, \\'\"'",
  "sn36.12:2.4": "'\"\\'północny czy też południowy, \\'\"'",
  "sn36.12:3.1": "'\"\\'Niosący kurz czy bez kurzu, \\'\"'",
  "sn36.12:3.2": "'\"\\'raz zimny, raz gorący, \\'\"'",
  "sn36.12:3.3": "'\"\\'Mocny czy niewielki, \\'\"'",
  "sn36.12:3.4": "'\"\\'wieją podmuchy niejednakie. \\'\"'",
  "sn36.12:4.1": "'\"\\'Tak samo w tym ciele \\'\"'",
  "sn36.12:4.2": "'\"\\'powstają odczucia, \\'\"'",
  "sn36.12:4.3": "'\"\\'Przyjemne czy bolesne, \\'\"'",
  "sn36.12:4.4": "'\"\\'są także i neutralne. \\'\"'",
  "sn36.12:5.1": "'\"\\'Tedy mnich taki, pilny, \\'\"'",
  "sn36.12:5.2": "'\"\\'z przejrzystym pojmowaniem,  niezrezygnowany, \\'\"'",
  "sn36.12:5.3": "'\"\\'wszystkie odczucia ma \\'\"'",
  "sn36.12:5.4": "'\"\\'mędrzec ten, poznane. \\'\"'",
  "sn36.12:6.1": "'\"\\'W pełni odczucia rozumiejący, \\'\"'",
  "sn36.12:6.2": "'\"\\'w doczesności już bez skażeń, \\'\"'",
  "sn36.12:6.3": "'\"\\'po rozpadzie ciała, w Prawdzie ustanowiony, \\'\"'",
  "sn36.12:6.4": "'\"\\'znawca, nie może być odnaleziony”. \\'\"'",
  "sn36.12:6.5": "'\"\\'Druga. \\'\"'"
}