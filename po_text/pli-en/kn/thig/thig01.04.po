#. lang="pli"><section class="sutta" id="thig1.4"><article><div
#. class="hgroup"><p class="division">
msgctxt "thig1.4:1.1"
msgid "Therīgāthā"
msgstr "Verses of the Senior Nuns"

#. HTML: </p><p>
msgctxt "thig1.4:2.1"
msgid "Ekakanipāta"
msgstr "The Book of the Ones"

#. HTML: </p><h1>
msgctxt "thig1.4:3.1"
msgid "4. Tissātherīgāthā"
msgstr "1.4. Tissā"

#. HTML: </h1></div><blockquote class="gatha"><p><a class="sc" id="1"></a><a
#. class="vns" id="vns4"></a><a class="vnp" id="vnp4"></a>
msgctxt "thig1.4:4.1"
msgid "“Tisse sikkhassu sikkhāya,"
msgstr "Tissā, train in the trainings—"

#. HTML: <br>
msgctxt "thig1.4:4.2"
msgid "mā taṃ yogā upaccaguṃ;"
msgstr "don’t let the practice pass you by."

#. HTML: <br>
msgctxt "thig1.4:4.3"
msgid "Sabbayogavisaṃyuttā,"
msgstr "Detached from all attachments,"

#. HTML: <br>
msgctxt "thig1.4:4.4"
msgid "cara loke anāsavā”ti."
msgstr "live in the world free of defilements."

#. HTML: </p></blockquote><p class="endsutta"><a class="sc" id="2"></a>
msgctxt "thig1.4:5.1"
msgid "…"
msgstr ""

msgctxt "thig1.4:5.2"
msgid "Tissā therī …."
msgstr ""
