#. HTML: <section class="sutta" id="pli-tv-pvr2.8"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr2.8:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.8:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.8:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.8:4.1"
msgid "8. Samuccayavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.8:5.1"
msgid "Avassutā bhikkhunī avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantī kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.8:5.2"
msgid "Avassutā bhikkhunī avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantī tisso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.8:5.3"
msgid "Adhakkhakaṃ ubbhajāṇumaṇḍalaṃ gahaṇaṃ sādiyati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr2.8:5.4"
msgid "ubbhakkhakaṃ adhojāṇumaṇḍalaṃ gahaṇaṃ sādiyati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr2.8:5.5"
msgid "kāyapaṭibaddhaṃ gahaṇaṃ sādiyati, āpatti dukkaṭassa—"
msgstr ""

msgctxt "pli-tv-pvr2.8:5.6"
msgid "avassutā bhikkhunī avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantī imā tisso āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.8:6.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.6"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.7"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.8:6.8"
msgid "Sattannaṃ āpattikkhandhānaṃ tīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.9"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr2.8:6.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.11"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr2.8:6.12"
msgid "Catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.8:6.13"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.14"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr2.8:6.15"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.8:6.16"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr2.8:7.1"
msgid "Dadhiṃ viññāpetvā bhuñjantī kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr2.8:7.2"
msgid "Dadhiṃ viññāpetvā bhuñjantī dve āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr2.8:7.3"
msgid "Bhuñjissāmīti paṭiggaṇhāti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr2.8:7.4"
msgid "ajjhohāre ajjhohāre āpatti pāṭidesanīyassa—"
msgstr ""

msgctxt "pli-tv-pvr2.8:7.5"
msgid "dadhiṃ viññāpetvā bhuñjantī imā dve āpattiyo āpajjati."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr2.8:8.1"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.2"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.3"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.4"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.5"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.6"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.7"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.8:8.8"
msgid "Sattannaṃ āpattikkhandhānaṃ dvīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.9"
msgid "siyā pāṭidesanīyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr2.8:8.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ catūhi samuṭṭhānehi samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.11"
msgid "siyā kāyato samuṭṭhanti na vācato na cittato, siyā kāyato ca vācato ca samuṭṭhanti na cittato, siyā kāyato ca cittato ca samuṭṭhanti na vācato, siyā kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr2.8:8.12"
msgid "Catunnaṃ adhikaraṇānaṃ—"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.13"
msgid "āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.8:8.14"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.8:8.15"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr2.8:8.16"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-pvr2.8:9.1"
msgid "Samuccayavāro niṭṭhito aṭṭhamo."
msgstr ""
