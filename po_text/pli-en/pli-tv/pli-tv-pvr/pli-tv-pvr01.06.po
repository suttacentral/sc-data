#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.6:5.1"
msgid "Methunaṃ dhammaṃ paṭisevantassa āpattiyo catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.6:5.2"
msgid "Methunaṃ dhammaṃ paṭisevantassa āpattiyo catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ …"
msgstr ""

msgctxt "pli-tv-pvr1.6:5.3"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.6:5.4"
