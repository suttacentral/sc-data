msgctxt "pli-tv-pvr1.4:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.4:2.1"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.4:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.4:4.1"
msgid "4. Saṅgahitavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.4:5.1"
msgid "Methunaṃ dhammaṃ paṭisevantassa āpattiyo sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr1.4:5.2"
msgid "Methunaṃ dhammaṃ paṭisevantassa āpattiyo sattannaṃ āpattikkhandhānaṃ tīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.4:5.3"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā dukkaṭāpattikkhandhena …"
msgstr ""

msgctxt "pli-tv-pvr1.4:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.4:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli5.47
msgctxt "pli-tv-pvr1.4:6.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karontassa āpatti sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr1.4:6.2"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karontassa āpatti sattannaṃ āpattikkhandhānaṃ ekena āpattikkhandhena saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.4:6.3"
msgid "dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr1.4:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr1.4:7.1"
msgid "Saṅgahitavāro niṭṭhito catuttho."
msgstr ""
