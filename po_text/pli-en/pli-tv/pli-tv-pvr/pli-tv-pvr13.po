#. HTML: <section class="sutta" id="pli-tv-pvr13"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr13:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr13:2.1"
msgid "Codanākaṇḍa"
msgstr ""

#. HTML: </h1></div><h2>
msgctxt "pli-tv-pvr13:3.1"
msgid "1. Anuvijjakaanuyoga"
msgstr ""

#. HTML: </h2><p>
#. REF: sc1, pts-vp-pli5.160
msgctxt "pli-tv-pvr13:4.1"
msgid "Anuvijjakena codako pucchitabbo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.2"
msgid "“yaṃ kho tvaṃ, āvuso, imaṃ bhikkhuṃ codesi, kimhi naṃ codesi, sīlavipattiyā vā codesi, ācāravipattiyā vā codesi, diṭṭhivipattiyā vā codesī”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.3"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.4"
msgid "“sīlavipattiyā vā codemi, ācāravipattiyā vā codemi, diṭṭhivipattiyā vā codemī”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.5"
msgid "“jānāsi panāyasmā sīlavipattiṃ, jānāsi ācāravipattiṃ, jānāsi diṭṭhivipattin”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.6"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.7"
msgid "“jānāmi kho ahaṃ, āvuso, sīlavipattiṃ, jānāmi ācāravipattiṃ, jānāmi diṭṭhivipattin”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.8"
msgid "“katamā panāvuso, sīlavipatti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.9"
msgid "Katamā ācāravipatti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.10"
msgid "Katamā diṭṭhivipattī”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.11"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.12"
msgid "“cattāri ca pārājikāni, terasa ca saṃghādisesā—"
msgstr ""

msgctxt "pli-tv-pvr13:4.13"
msgid "ayaṃ sīlavipatti."
msgstr ""

msgctxt "pli-tv-pvr13:4.14"
msgid "Thullaccayaṃ, pācittiyaṃ, pāṭidesanīyaṃ, dukkaṭaṃ, dubbhāsitaṃ, ayaṃ ācāravipatti."
msgstr ""

msgctxt "pli-tv-pvr13:4.15"
msgid "Micchādiṭṭhi, antaggāhikā diṭṭhi—"
msgstr ""

msgctxt "pli-tv-pvr13:4.16"
msgid "ayaṃ diṭṭhivipattī”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.17"
msgid "“yaṃ kho tvaṃ, āvuso, imaṃ bhikkhuṃ codesi, diṭṭhena vā codesi sutena vā codesi parisaṅkāya vā codesī”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.18"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.19"
msgid "“diṭṭhena vā codemi sutena vā codemi parisaṅkāya vā codemī”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.20"
msgid "“yaṃ kho tvaṃ, āvuso, imaṃ bhikkhuṃ diṭṭhena codesi, kiṃ te diṭṭhaṃ kinti te diṭṭhaṃ, kadā te diṭṭhaṃ, kattha te diṭṭhaṃ pārājikaṃ ajjhāpajjanto diṭṭho, saṃghādisesaṃ ajjhāpajjanto diṭṭho, thullaccayaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.21"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.22"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.23"
msgid "dukkaṭaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.24"
msgid "dubbhāsitaṃ ajjhāpajjanto diṭṭho, kattha ca tvaṃ ahosi, kattha cāyaṃ bhikkhu ahosi, kiñca tvaṃ karosi, kiṃ cāyaṃ bhikkhu karotī”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.25"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.26"
msgid "“na kho ahaṃ, āvuso, imaṃ bhikkhuṃ diṭṭhena codemi, api ca sutena codemī”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.27"
msgid "“yaṃ kho tvaṃ, āvuso, imaṃ bhikkhuṃ sutena codesi, kiṃ te sutaṃ, kinti te sutaṃ, kadā te sutaṃ, kattha te sutaṃ, pārājikaṃ ajjhāpannoti sutaṃ, saṃghādisesaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.28"
msgid "thullaccayaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.29"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.30"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.31"
msgid "dukkaṭaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.32"
msgid "dubbhāsitaṃ ajjhāpannoti sutaṃ, bhikkhussa sutaṃ, bhikkhuniyā sutaṃ, sikkhamānāya sutaṃ, sāmaṇerassa sutaṃ, sāmaṇeriyā sutaṃ, upāsakassa sutaṃ, upāsikāya sutaṃ, rājūnaṃ sutaṃ, rājamahāmattānaṃ sutaṃ, titthiyānaṃ sutaṃ, titthiyasāvakānaṃ sutan”ti?"
msgstr ""

msgctxt "pli-tv-pvr13:4.33"
msgid "So ce evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-pvr13:4.34"
msgid "“na kho ahaṃ, āvuso, imaṃ bhikkhuṃ sutena codemi, api ca parisaṅkāya codemī”ti, so evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-pvr13:4.35"
msgid "“yaṃ kho tvaṃ, āvuso, imaṃ bhikkhuṃ parisaṅkāya codesi, kiṃ parisaṅkasi, kinti parisaṅkasi, kadā parisaṅkasi, kattha parisaṅkasi, pārājikaṃ ajjhāpannoti parisaṅkasi, saṃghādisesaṃ ajjhāpannoti parisaṅkasi, thullaccayaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.36"
msgid "pācittiyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.37"
msgid "pāṭidesanīyaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.38"
msgid "dukkaṭaṃ …"
msgstr ""

msgctxt "pli-tv-pvr13:4.39"
msgid "dubbhāsitaṃ ajjhāpannoti parisaṅkasi, bhikkhussa sutvā parisaṅkasi, bhikkhuniyā sutvā parisaṅkasi, sikkhamānāya sutvā parisaṅkasi, sāmaṇerassa sutvā parisaṅkasi, sāmaṇeriyā sutvā parisaṅkasi, upāsakassa sutvā parisaṅkasi, upāsikāya sutvā parisaṅkasi, rājūnaṃ sutvā parisaṅkasi, rājamahāmattānaṃ sutvā parisaṅkasi, titthiyānaṃ sutvā parisaṅkasi, titthiyasāvakānaṃ sutvā parisaṅkasī”ti?"
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc2
msgctxt "pli-tv-pvr13:5.1"
msgid "Diṭṭhaṃ diṭṭhena sameti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:5.2"
msgid "diṭṭhena saṃsandate diṭṭhaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:5.3"
msgid "Diṭṭhaṃ paṭicca na upeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:5.4"
msgid "asuddhaparisaṅkito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:5.5"
msgid "So puggalo paṭiññāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:5.6"
msgid "kātabbo tenuposatho."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-pvr13:6.1"
msgid "Sutaṃ sutena sameti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:6.2"
msgid "sutena saṃsandate sutaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:6.3"
msgid "Sutaṃ paṭicca na upeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:6.4"
msgid "asuddhaparisaṅkito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:6.5"
msgid "So puggalo paṭiññāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:6.6"
msgid "kātabbo tenuposatho."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-pvr13:7.1"
msgid "Mutaṃ mutena sameti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:7.2"
msgid "mutena saṃsandate mutaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:7.3"
msgid "Mutaṃ paṭicca na upeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:7.4"
msgid "asuddhaparisaṅkito;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:7.5"
msgid "So puggalo paṭiññāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:7.6"
msgid "kātabbo tenuposatho."
msgstr ""

#. HTML: </p></blockquote><p>
#. REF: sc5, pts-vp-pli5.161
msgctxt "pli-tv-pvr13:8.1"
msgid "Codanāya ko ādi, kiṃ majjhe, kiṃ pariyosānaṃ?"
msgstr ""

msgctxt "pli-tv-pvr13:8.2"
msgid "Codanāya okāsakammaṃ ādi, kiriyā majjhe, samatho pariyosānaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:8.3"
msgid "Codanāya kati mūlāni, kati vatthūni, kati bhūmiyo, katihākārehi codeti?"
msgstr ""

msgctxt "pli-tv-pvr13:8.4"
msgid "Codanāya dve mūlāni, tīṇi vatthūni, pañca bhūmiyo, dvīhākārehi codeti."
msgstr ""

msgctxt "pli-tv-pvr13:8.5"
msgid "Codanāya katamāni dve mūlāni?"
msgstr ""

msgctxt "pli-tv-pvr13:8.6"
msgid "Samūlikā vā amūlikā vā—"
msgstr ""

msgctxt "pli-tv-pvr13:8.7"
msgid "codanāya imāni dve mūlāni."
msgstr ""

msgctxt "pli-tv-pvr13:8.8"
msgid "Codanāya katamāni tīṇi vatthūni?"
msgstr ""

msgctxt "pli-tv-pvr13:8.9"
msgid "Diṭṭhena sutena parisaṅkāya—"
msgstr ""

msgctxt "pli-tv-pvr13:8.10"
msgid "codanāya imāni tīṇi vatthūni."
msgstr ""

msgctxt "pli-tv-pvr13:8.11"
msgid "Codanāya katamā pañca bhūmiyo?"
msgstr ""

msgctxt "pli-tv-pvr13:8.12"
msgid "Kālena vakkhāmi no akālena, bhūtena vakkhāmi no abhūtena, saṇhena vakkhāmi no pharusena, atthasaṃhitena vakkhāmi no anatthasaṃhitena, mettācitto vakkhāmi no dosantaroti—"
msgstr ""

msgctxt "pli-tv-pvr13:8.13"
msgid "codanāya imā pañca bhūmiyo."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-pvr13:9.1"
msgid "Katamehi dvīhākārehi codeti?"
msgstr ""

msgctxt "pli-tv-pvr13:9.2"
msgid "Kāyena vā codeti vācāya vā codeti—"
msgstr ""

msgctxt "pli-tv-pvr13:9.3"
msgid "imehi dvīhākārehi codeti."
msgstr ""

#. HTML: </p><h2>
msgctxt "pli-tv-pvr13:10.1"
msgid "2. Codakādipaṭipatti"
msgstr ""

#. HTML: </h2><p>
#. REF: sc7
msgctxt "pli-tv-pvr13:11.1"
msgid "Codakena kathaṃ paṭipajjitabbaṃ?"
msgstr ""

msgctxt "pli-tv-pvr13:11.2"
msgid "Cuditakena kathaṃ paṭipajjitabbaṃ?"
msgstr ""

msgctxt "pli-tv-pvr13:11.3"
msgid "Saṃghena kathaṃ paṭipajjitabbaṃ?"
msgstr ""

msgctxt "pli-tv-pvr13:11.4"
msgid "Anuvijjakena kathaṃ paṭipajjitabbaṃ?"
msgstr ""

msgctxt "pli-tv-pvr13:11.5"
msgid "Codakena kathaṃ paṭipajjitabbanti?"
msgstr ""

msgctxt "pli-tv-pvr13:11.6"
msgid "Codakena pañcasu dhammesu patiṭṭhāya paro codetabbo."
msgstr ""

msgctxt "pli-tv-pvr13:11.7"
msgid "Kālena vakkhāmi no akālena, bhūtena vakkhāmi no abhūtena, saṇhena vakkhāmi no pharusena, atthasaṃhitena vakkhāmi no anatthasaṃhitena, mettācitto vakkhāmi no dosantaroti—"
msgstr ""

msgctxt "pli-tv-pvr13:11.8"
msgid "codakena evaṃ paṭipajjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.9"
msgid "Cuditakena kathaṃ paṭipajjitabbanti?"
msgstr ""

msgctxt "pli-tv-pvr13:11.10"
msgid "Cuditakena dvīsu dhammesu paṭipajjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.11"
msgid "Sacce ca akuppe ca—"
msgstr ""

msgctxt "pli-tv-pvr13:11.12"
msgid "cuditakena evaṃ paṭipajjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.13"
msgid "Saṃghena kathaṃ paṭipajjitabbanti?"
msgstr ""

msgctxt "pli-tv-pvr13:11.14"
msgid "Saṃghena otiṇṇānotiṇṇaṃ jānitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.15"
msgid "Saṃghena evaṃ paṭipajjitabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.16"
msgid "Anuvijjakena kathaṃ paṭipajjitabbanti?"
msgstr ""

msgctxt "pli-tv-pvr13:11.17"
msgid "Anuvijjakena yena dhammena yena vinayena yena satthusāsanena taṃ adhikaraṇaṃ vūpasammati tathā taṃ adhikaraṇaṃ vūpasametabbaṃ."
msgstr ""

msgctxt "pli-tv-pvr13:11.18"
msgid "Anuvijjakena evaṃ paṭipajjitabbaṃ."
msgstr ""

#. HTML: </p><blockquote class="gatha"><p>
#. REF: sc8
msgctxt "pli-tv-pvr13:12.1"
msgid "Uposatho kimatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:12.2"
msgid "Pavāraṇā kissa kāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:12.3"
msgid "Parivāso kimatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:12.4"
msgid "Mūlāyapaṭikassanā kissa kāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:12.5"
msgid "Mānattaṃ kimatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:12.6"
msgid "Abbhānaṃ kissa kāraṇā."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-pvr13:13.1"
msgid "Uposatho sāmaggatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:13.2"
msgid "Visuddhatthāya pavāraṇā;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:13.3"
msgid "Parivāso mānattatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:13.4"
msgid "Mūlāyapaṭikassanā niggahatthāya;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:13.5"
msgid "Mānattaṃ abbhānatthāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:13.6"
msgid "Visuddhatthāya abbhānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-pvr13:14.1"
msgid "Chandā dosā bhayā mohā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:14.2"
msgid "there ca paribhāsati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:14.3"
msgid "Kāyassa bhedā duppañño,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:14.4"
msgid "khato upahatindriyo;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:14.5"
msgid "Nirayaṃ gacchati dummedho,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:14.6"
msgid "na ca sikkhāya gāravo."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-pvr13:15.1"
msgid "Na ca āmisaṃ nissāya,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:15.2"
msgid "na ca nissāya puggalaṃ;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:15.3"
msgid "Ubho ete vivajjetvā,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:15.4"
msgid "yathādhammo tathā kare."
msgstr ""

#. HTML: </p></blockquote><h2>
msgctxt "pli-tv-pvr13:16.1"
msgid "3. Codakassaattajhāpana"
msgstr ""

#. HTML: </h2><blockquote class="gatha"><p>
#. REF: sc12
msgctxt "pli-tv-pvr13:17.1"
msgid "Kodhano upanāhī ca,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:17.2"
msgid "Caṇḍo ca paribhāsako;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:17.3"
msgid "Anāpattiyā āpattīti ropeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:17.4"
msgid "Tādiso codako jhāpeti attānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc13, pts-vp-pli5.162
msgctxt "pli-tv-pvr13:18.1"
msgid "Upakaṇṇakaṃ jappati jimhaṃ pekkhati,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:18.2"
msgid "Vītiharati kummaggaṃ paṭisevati;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:18.3"
msgid "Anāpattiyā āpattīti ropeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:18.4"
msgid "Tādiso codako jhāpeti attānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-pvr13:19.1"
msgid "Akālena codeti abhūtena,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:19.2"
msgid "Pharusena anatthasaṃhitena;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:19.3"
msgid "Dosantaro codeti no mettācitto,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:19.4"
msgid "Anāpattiyā āpattīti ropeti;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:19.5"
msgid "Tādiso codako jhāpeti attānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-pvr13:20.1"
msgid "Dhammādhammaṃ na jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:20.2"
msgid "Dhammādhammassa akovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:20.3"
msgid "Anāpattiyā āpattīti ropeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:20.4"
msgid "Tādiso codako jhāpeti attānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-pvr13:21.1"
msgid "Vinayāvinayaṃ na jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:21.2"
msgid "Vinayāvinayassa akovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:21.3"
msgid "Anāpattiyā āpattīti ropeti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:21.4"
msgid "Tādiso codako jhāpeti attānaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-pvr13:22.1"
msgid "Bhāsitābhāsitaṃ na jānāti,"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:22.2"
msgid "Bhāsitābhāsitassa akovido;"
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr13:22.3"
msgid "Anāpattiyā āpattīti ropeti,"
msgstr ""

