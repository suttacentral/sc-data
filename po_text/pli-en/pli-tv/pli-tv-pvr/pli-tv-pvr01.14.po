#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr1.14:5.1"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.14:5.2"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ āpattādhikaraṇaṃ …"
msgstr ""

msgctxt "pli-tv-pvr1.14:5.3"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.14:5.4"
