msgctxt "pli-tv-pvr2.4:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.4:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.4:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.4:4.1"
msgid "4. Saṅgahavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.4:5.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.4:5.2"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo sattannaṃ āpattikkhandhānaṃ tīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.4:5.3"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā dukkaṭāpattikkhandhena …"
msgstr ""

msgctxt "pli-tv-pvr2.4:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.4:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.4:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr2.4:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo sattannaṃ āpattikkhandhānaṃ dvīhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr2.4:6.3"
msgid "siyā pāṭidesanīyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr2.4:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.4:7.1"
msgid "Saṅgahavāro niṭṭhito catuttho."
msgstr ""
