msgctxt "pli-tv-pvr2.5:1.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.5:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr2.5:3.1"
msgid "Paṭhamabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.5:4.1"
msgid "5. Samuṭṭhānavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.5:5.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.5:5.2"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.5:5.3"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato …"
msgstr ""

msgctxt "pli-tv-pvr2.5:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.5:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.5:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr2.5:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo channaṃ āpattisamuṭṭhānānaṃ catūhi samuṭṭhānehi samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr2.5:6.3"
msgid "siyā kāyato samuṭṭhanti, na vācato na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.5:6.4"
msgid "siyā kāyato ca vācato ca samuṭṭhanti, na cittato;"
msgstr ""

msgctxt "pli-tv-pvr2.5:6.5"
msgid "siyā kāyato ca cittato ca samuṭṭhanti, na vācato;"
msgstr ""

msgctxt "pli-tv-pvr2.5:6.6"
msgid "siyā kāyato ca vācato ca cittato ca samuṭṭhanti."
msgstr ""

msgctxt "pli-tv-pvr2.5:6.7"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.5:7.1"
msgid "Samuṭṭhānavāro niṭṭhito pañcamo."
msgstr ""
