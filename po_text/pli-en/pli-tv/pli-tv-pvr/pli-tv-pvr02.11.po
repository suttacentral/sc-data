msgctxt "pli-tv-pvr2.11:4.1"
msgid "3. Vipattivāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.11:5.1"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.11:5.2"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.11:5.3"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ …"
msgstr ""

msgctxt "pli-tv-pvr2.11:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.11:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.11:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.11:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā āpattiyo catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.11:6.3"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.11:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.11:7.1"
msgid "Vipattivāro niṭṭhito tatiyo."
msgstr ""
