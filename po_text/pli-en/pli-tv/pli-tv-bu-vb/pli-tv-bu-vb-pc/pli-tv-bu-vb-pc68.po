#. HTML: <section class="sutta" id="pli-tv-bu-vb-pc68"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-pc68:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-pc68:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p></p><p>
msgctxt "pli-tv-bu-vb-pc68:0.3"
msgid "Pācittiyakaṇḍa"
msgstr "The chapter on confession"

msgctxt "pli-tv-bu-vb-pc68:0.4"
msgid "Sappāṇakavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-pc68:0.5"
msgid "68. Ariṭṭhasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.134
msgctxt "pli-tv-bu-vb-pc68:1.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.2"
msgid "Tena kho pana samayena ariṭṭhassa nāma bhikkhuno gaddhabādhipubbassa evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ hoti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.3"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.4"
msgid "Assosuṃ kho sambahulā bhikkhū—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.5"
msgid "“ariṭṭhassa kira nāma bhikkhuno gaddhabādhipubbassa evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.6"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.7"
msgid "Atha kho te bhikkhū yena ariṭṭho bhikkhu gaddhabādhipubbo tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.8"
msgid "upasaṅkamitvā ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.9"
msgid "“saccaṃ kira te, āvuso ariṭṭha, evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.10"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā’”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.11"
msgid "“Evaṃ byā kho ahaṃ, āvuso, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.12"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bu-vb-pc68:1.13"
msgid "“Mā, āvuso ariṭṭha, evaṃ avaca."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.14"
msgid "Mā bhagavantaṃ abbhācikkhi."
msgstr ""

#. VAR: abbhakkhānaṃ → abbhācikkhanaṃ (itipi)
msgctxt "pli-tv-bu-vb-pc68:1.15"
msgid "Na hi sādhu bhagavato abbhakkhānaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.16"
msgid "Na hi bhagavā evaṃ vadeyya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.17"
msgid "Anekapariyāyenāvuso ariṭṭha, antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.18"
msgid "Alañca pana te paṭisevato antarāyāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.19"
msgid "Appassādā kāmā vuttā bhagavatā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.20"
msgid "Aṭṭhikaṅkalūpamā kāmā vuttā bhagavatā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.21"
msgid "Maṃsapesūpamā kāmā vuttā bhagavatā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.22"
msgid "tiṇukkūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.23"
msgid "aṅgārakāsūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.24"
msgid "supinakūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.25"
msgid "yācitakūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.26"
msgid "rukkhaphalūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.27"
msgid "asisūnūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.28"
msgid "sattisūlūpamā kāmā vuttā bhagavatā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.29"
msgid "sappasirūpamā kāmā vuttā bhagavatā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3, pts-vp-pli4.135
msgctxt "pli-tv-bu-vb-pc68:1.30"
msgid "Evampi kho ariṭṭho bhikkhu gaddhabādhipubbo tehi bhikkhūhi vuccamāno tatheva taṃ pāpakaṃ diṭṭhigataṃ thāmasā parāmāsā abhinivissa voharati—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.31"
msgid "“evaṃ byā kho ahaṃ, āvuso, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.32"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.33"
msgid "Yato ca kho te bhikkhū nāsakkhiṃsu ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ etasmā pāpakā diṭṭhigatā vivecetuṃ, atha kho te bhikkhū yena bhagavā tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.34"
msgid "upasaṅkamitvā bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.35"
msgid "Atha kho bhagavā etasmiṃ nidāne etasmiṃ pakaraṇe bhikkhusaṃghaṃ sannipātāpetvā ariṭṭhaṃ bhikkhuṃ gaddhabādhipubbaṃ paṭipucchi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.36"
msgid "“saccaṃ kira te, ariṭṭha, evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.37"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.38"
msgid "“Evaṃ byā kho ahaṃ, bhante, bhagavatā dhammaṃ desitaṃ ājānāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.39"
msgid "‘yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-pc68:1.40"
msgid "Kassa nu kho nāma tvaṃ, moghapurisa, mayā evaṃ dhammaṃ desitaṃ ājānāsi?"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.41"
msgid "Nanu mayā, moghapurisa, anekapariyāyena antarāyikā dhammā antarāyikā vuttā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.42"
msgid "Alañca pana te paṭisevato antarāyāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.43"
msgid "Appassādā kāmā vuttā mayā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.44"
msgid "Aṭṭhikaṅkalūpamā kāmā vuttā mayā … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.45"
msgid "maṃsapesūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.46"
msgid "tiṇukkūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.47"
msgid "aṅgārakāsūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.48"
msgid "supinakūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.49"
msgid "yācitakūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.50"
msgid "rukkhaphalūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.51"
msgid "asisūnūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.52"
msgid "sattisūlūpamā kāmā vuttā mayā …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.53"
msgid "sappasirūpamā kāmā vuttā mayā bahudukkhā bahupāyāsā, ādīnavo ettha bhiyyo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.54"
msgid "Atha ca pana tvaṃ, moghapurisa, attanā duggahitena amhe ceva abbhācikkhasi, attānañca khaṇasi, bahuñca apuññaṃ pasavasi."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.55"
msgid "Tañhi te, moghapurisa, bhavissati dīgharattaṃ ahitāya dukkhāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.56"
msgid "Netaṃ, moghapurisa, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.57"
msgid "evañca pana, bhikkhave, imaṃ sikkhāpadaṃ uddiseyyātha—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc5
msgctxt "pli-tv-bu-vb-pc68:1.58"
msgid "“Yo pana bhikkhu evaṃ vadeyya—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.59"
msgid "‘tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā’ti so bhikkhu bhikkhūhi evamassa vacanīyo—‘"
msgstr ""

#. VAR: māyasmā → mā āyasmā (s1-3) 
#. VAR:  dhammā antarāyikā vuttā →  dhammā vuttā (syā1-3)
msgctxt "pli-tv-bu-vb-pc68:1.60"
msgid "māyasmā evaṃ avaca, mā bhagavantaṃ abbhācikkhi, na hi sādhu bhagavato abbhakkhānaṃ, na hi bhagavā evaṃ vadeyya, anekapariyāyenāvuso, antarāyikā dhammā antarāyikā vuttā bhagavatā, alañca pana te paṭisevato antarāyāyā’ti."
msgstr ""

#. VAR: Evañca → evañca pana (pts1, mr)
msgctxt "pli-tv-bu-vb-pc68:1.61"
msgid "Evañca so bhikkhu bhikkhūhi vuccamāno tatheva paggaṇheyya, so bhikkhu bhikkhūhi yāvatatiyaṃ samanubhāsitabbo tassa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:1.62"
msgid "Yāvatatiyañce samanubhāsiyamāno taṃ paṭinissajjeyya, iccetaṃ kusalaṃ, no ce paṭinissajjeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc6
msgctxt "pli-tv-bu-vb-pc68:2.1.1"
msgid "Yo panāti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc68:2.1.2"
msgid "yo yādiso … pe …"
msgstr ""

#. HTML: </b><b class="term">
msgctxt "pli-tv-bu-vb-pc68:2.1.3"
msgid "bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc68:2.1.4"
msgid "… pe …"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc68:2.1.5"
msgid "ayaṃ imasmiṃ atthe adhippeto bhikkhūti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bu-vb-pc68:2.1.6"
msgid "Evaṃ vadeyyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc68:2.1.7"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

#. HTML: </p><p><b class="term">
#. REF: sc8
msgctxt "pli-tv-bu-vb-pc68:2.1.8"
msgid "So bhikkhūti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc68:2.1.9"
msgid "yo so evaṃvādī bhikkhu."
msgstr ""

#. HTML: </b></p><p><b class="term">
#. REF: sc9, pts-vp-pli4.136
msgctxt "pli-tv-bu-vb-pc68:2.1.10"
msgid "Bhikkhūhīti"
msgstr ""

#. HTML: </b><b class="gloss">
msgctxt "pli-tv-bu-vb-pc68:2.1.11"
msgid "aññehi bhikkhūhi."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-pc68:2.1.12"
msgid "Ye passanti ye suṇanti tehi vattabbo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.13"
msgid "“māyasmā evaṃ avaca, mā bhagavantaṃ abbhācikkhi, na hi sādhu bhagavato abbhakkhānaṃ, na hi bhagavā evaṃ vadeyya, anekapariyāyenāvuso antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.14"
msgid "Alañca pana te paṭisevato antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.15"
msgid "Dutiyampi vattabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.16"
msgid "Tatiyampi vattabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.17"
msgid "Sace paṭinissajjati, iccetaṃ kusalaṃ;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.18"
msgid "no ce paṭinissajjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.19"
msgid "Sutvā na vadanti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.20"
msgid "So bhikkhu saṃghamajjhampi ākaḍḍhitvā vattabbo—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.21"
msgid "“māyasmā evaṃ avaca, mā bhagavantaṃ abbhācikkhi, na hi sādhu bhagavato abbhakkhānaṃ, na hi bhagavā evaṃ vadeyya, anekapariyāyenāvuso antarāyikā dhammā antarāyikā vuttā bhagavatā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.22"
msgid "Alañca pana te paṭisevato antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.23"
msgid "Dutiyampi vattabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.24"
msgid "Tatiyampi vattabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.25"
msgid "Sace paṭinissajjati, iccetaṃ kusalaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.26"
msgid "No ce paṭinissajjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.27"
msgid "So bhikkhu samanubhāsitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.28"
msgid "“Evañca pana, bhikkhave, samanubhāsitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.29"
msgid "Byattena bhikkhunā paṭibalena saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bu-vb-pc68:2.1.30"
msgid "‘Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.31"
msgid "Itthannāmassa bhikkhuno evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.32"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.33"
msgid "So taṃ diṭṭhiṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.34"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuṃ samanubhāseyya—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.35"
msgid "tassā diṭṭhiyā paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.36"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bu-vb-pc68:2.1.37"
msgid "Suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.38"
msgid "Itthannāmassa bhikkhuno evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.39"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi, yathā yeme antarāyikā dhammā vuttā bhagavatā, te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.40"
msgid "So taṃ diṭṭhiṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.41"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ samanubhāsati tassā diṭṭhiyā paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.42"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno samanubhāsanā, tassā diṭṭhiyā paṭinissaggāya, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.43"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bu-vb-pc68:2.1.44"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.45"
msgid "tatiyampi etamatthaṃ vadāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.46"
msgid "suṇātu me, bhante, saṃgho."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.47"
msgid "Itthannāmassa bhikkhuno evarūpaṃ pāpakaṃ diṭṭhigataṃ uppannaṃ—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.48"
msgid "“tathāhaṃ bhagavatā dhammaṃ desitaṃ ājānāmi yathā yeme antarāyikā dhammā vuttā bhagavatā te paṭisevato nālaṃ antarāyāyā”ti."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.49"
msgid "So taṃ diṭṭhiṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.50"
msgid "Saṃgho itthannāmaṃ bhikkhuṃ samanubhāsati tassā diṭṭhiyā paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.51"
msgid "Yassāyasmato khamati itthannāmassa bhikkhuno samanubhāsanā, tassā diṭṭhiyā paṭinissaggāya, so tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.52"
msgid "yassa nakkhamati, so bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bu-vb-pc68:2.1.53"
msgid "Samanubhaṭṭho saṃghena itthannāmo bhikkhu, tassā diṭṭhiyā paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.54"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bu-vb-pc68:2.1.55"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.56"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.1.57"
msgid "Kammavācāpariyosāne āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-bu-vb-pc68:2.2.1"
msgid "Dhammakamme dhammakammasaññī na paṭinissajjati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.2.2"
msgid "Dhammakamme vematiko na paṭinissajjati, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.2.3"
msgid "Dhammakamme adhammakammasaññī na paṭinissajjati, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-bu-vb-pc68:2.2.4"
msgid "Adhammakamme dhammakammasaññī, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.2.5"
msgid "Adhammakamme vematiko, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.2.6"
msgid "Adhammakamme adhammakammasaññī, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-bu-vb-pc68:2.3.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-pc68:2.3.2"
msgid "asamanubhāsantassa, paṭinissajjantassa, ummattakassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc18
msgctxt "pli-tv-bu-vb-pc68:2.3.3"
msgid "Ariṭṭhasikkhāpadaṃ niṭṭhitaṃ aṭṭhamaṃ."
msgstr ""
