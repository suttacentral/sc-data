msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk70:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk70:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk70:5.1"
msgid "70. Ṭhitasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk70:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk70:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū ṭhitā nisinnassa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk70:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk70:7.1"
msgid "“Na ṭhito nisinnassa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk70:8.1"
msgid "Na ṭhitena nisinnassa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk70:8.2"
msgid "Yo anādariyaṃ paṭicca ṭhito nisinnassa agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk70:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk70:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk70:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk70:10.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
