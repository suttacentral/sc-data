msgctxt "pli-tv-bu-vb-sk15:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk15:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk15:4.1"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk15:5.1"
msgid "15. Kāyappacālakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.188
msgctxt "pli-tv-bu-vb-sk15:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū kāyappacālakaṃ antaraghare gacchanti kāyaṃ olambentā …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk15:7.1"
msgid "“Na kāyappacālakaṃ antaraghare gamissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk15:8.1"
msgid "Na kāyappacālakaṃ antaraghare gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:8.2"
msgid "Kāyaṃ paggahetvā gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:8.3"
msgid "Yo anādariyaṃ paṭicca kāyappacālakaṃ antaraghare gacchati kāyaṃ olambento, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk15:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk15:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk15:10.1"
msgid "Pañcamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
