msgctxt "pli-tv-bu-vb-sk14:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk14:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk14:4.1"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk14:5.1"
msgid "14. Dutiyauccasaddasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk14:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk14:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū uccāsaddaṃ mahāsaddaṃ karontā antaraghare nisīdanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk14:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk14:7.1"
msgid "“Appasaddo antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk14:8.1"
msgid "Appasaddena antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk14:8.2"
msgid "Yo anādariyaṃ paṭicca uccāsaddaṃ mahāsaddaṃ karonto antaraghare nisīdati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk14:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk14:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk14:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk14:10.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
