#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk61"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk61:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk61:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk61:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
#. VAR: Pādukavagga → pādukāvaggo (s1-3)
msgctxt "pli-tv-bu-vb-sk61:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk61:5.1"
msgid "61. Pādukasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk61:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk61:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū pādukāruḷhassa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk61:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
#. VAR: pādukāruḷhassa → pādukārūḷhassa (bj, s1-3, pts1)
msgctxt "pli-tv-bu-vb-sk61:7.1"
msgid "“Na pādukāruḷhassa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk61:8.1"
msgid "Na pādukāruḷhassa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk61:8.2"
msgid "Yo anādariyaṃ paṭicca akkantassa vā paṭimukkassa vā omukkassa vā agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk61:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk61:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk61:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk61:10.1"
msgid "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
