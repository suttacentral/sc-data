msgctxt "pli-tv-bu-vb-sk24:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk24:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk24:4.1"
msgid "Khambhakatavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk24:5.1"
msgid "24. Dutiyaoguṇṭhitasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk24:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk24:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sasīsaṃ pārupitvā antaraghare nisīdanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk24:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk24:7.1"
msgid "“Na oguṇṭhito antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk24:8.1"
msgid "Na oguṇṭhitena antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk24:8.2"
msgid "Yo anādariyaṃ paṭicca sasīsaṃ pārupitvā antaraghare nisīdati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk24:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk24:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, vāsūpagatassa, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk24:10.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
