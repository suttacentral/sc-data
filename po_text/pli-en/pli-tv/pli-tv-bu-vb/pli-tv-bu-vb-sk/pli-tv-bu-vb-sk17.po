msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk17:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk17:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk17:4.1"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk17:5.1"
msgid "17. Bāhuppacālakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk17:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū bāhuppacālakaṃ antaraghare gacchanti bāhuṃ olambentā …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk17:7.1"
msgid "“Na bāhuppacālakaṃ antaraghare gamissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk17:8.1"
msgid "Na bāhuppacālakaṃ antaraghare gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:8.2"
msgid "Bāhuṃ paggahetvā gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:8.3"
msgid "Yo anādariyaṃ paṭicca bāhuppacālakaṃ antaraghare gacchati bāhuṃ olambento, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk17:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk17:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk17:10.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
