msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk42:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk42:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk42:4.1"
msgid "Kabaḷavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk42:5.1"
msgid "42. Dutiyaanāhaṭasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk42:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk42:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū bhuñjamānā sabbaṃ hatthaṃ mukhe pakkhipanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk42:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk42:7.1"
msgid "“Na bhuñjamāno sabbaṃ hatthaṃ mukhe pakkhipissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk42:8.1"
msgid "Na bhuñjamānena sabbo hattho mukhe pakkhipitabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk42:8.2"
msgid "Yo anādariyaṃ paṭicca bhuñjamāno sabbaṃ hatthaṃ mukhe pakkhipati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk42:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk42:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk42:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk42:10.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
