msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk74:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk74:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk74:5.1"
msgid "74. Hariteuccārasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk74:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk74:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū harite uccārampi passāvampi kheḷampi karonti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk74:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk74:7.1"
msgid "“Na harite agilāno uccāraṃ vā passāvaṃ vā kheḷaṃ vā karissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk74:8.1"
msgid "Na harite agilānena uccāro vā passāvo vā kheḷo vā kātabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk74:8.2"
msgid "Yo anādariyaṃ paṭicca harite agilāno uccāraṃ vā passāvaṃ vā kheḷaṃ vā karoti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk74:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk74:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, appaharite kato haritaṃ ottharati, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk74:10.1"
msgid "Cuddasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
