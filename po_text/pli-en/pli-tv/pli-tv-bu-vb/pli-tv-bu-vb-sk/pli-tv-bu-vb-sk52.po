#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk52:5.1"
msgid "52. Hatthanillehakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk52:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk52:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū hatthanillehakaṃ bhuñjanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk52:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk52:7.1"
msgid "“Na hatthanillehakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk52:8.1"
msgid "Na hatthanillehakaṃ bhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk52:8.2"
msgid "Yo anādariyaṃ paṭicca hatthanillehakaṃ bhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk52:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk52:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk52:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk52:10.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
