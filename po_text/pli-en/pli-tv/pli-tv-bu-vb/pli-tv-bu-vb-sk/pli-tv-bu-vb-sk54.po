#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk54:5.1"
msgid "54. Oṭṭhanillehakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk54:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk54:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū oṭṭhanillehakaṃ bhuñjanti … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk54:6.3"
msgid "."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk54:7.1"
msgid "“Na oṭṭhanillehakaṃ bhuñjissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk54:8.1"
msgid "Na oṭṭhanillehakaṃ bhuñjitabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk54:8.2"
msgid "Yo anādariyaṃ paṭicca oṭṭhanillehakaṃ bhuñjati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk54:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk54:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk54:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk54:10.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
