#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc60"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc60:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc60:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc60:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc60:4.1"
msgid "Ārāmavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc60:5.1"
msgid "60. Pasākhejātasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.316
msgctxt "pli-tv-bi-vb-pc60:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:6.2"
msgid "Tena kho pana samayena aññatarā bhikkhunī pasākhe jātaṃ gaṇḍaṃ purisena saddhiṃ ekenekā bhedāpesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:6.3"
msgid "Atha kho so puriso taṃ bhikkhuniṃ dūsetuṃ upakkami."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:6.4"
msgid "Sā vissaramakāsi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:6.5"
msgid "Bhikkhuniyo upadhāvitvā taṃ bhikkhuniṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:6.6"
msgid "“kissa tvaṃ, ayye, vissaramakāsī”ti?"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc60:7.1"
msgid "Atha kho sā bhikkhunī bhikkhunīnaṃ etamatthaṃ ārocesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.4"
msgid "“kathañhi nāma bhikkhunī pasākhe jātaṃ gaṇḍaṃ purisena saddhiṃ ekenekā bhedāpessatī”ti … pe …"
msgstr ""

#. VAR: bhedāpetī”ti → bhedāpesīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-pc60:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī pasākhe jātaṃ gaṇḍaṃ purisena saddhiṃ ekenekā bhedāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhunī pasākhe jātaṃ gaṇḍaṃ purisena saddhiṃ ekenekā bhedāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: rudhitaṃ → ruhitaṃ (bj, s1-3) | rūhitaṃ (pts1)
msgctxt "pli-tv-bi-vb-pc60:8.1"
msgid "“Yā pana bhikkhunī pasākhe jātaṃ gaṇḍaṃ vā rudhitaṃ vā anapaloketvā saṃghaṃ vā gaṇaṃ vā purisena saddhiṃ ekenekā bhedāpeyya vā phālāpeyya vā dhovāpeyya vā ālimpāpeyya vā bandhāpeyya vā mocāpeyya vā, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc60:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc60:10.1"
msgid "Pasākhaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.2"
msgid "adhonābhi ubbhajāṇumaṇḍalaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.3"
msgid "Jātanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.4"
msgid "tattha jātaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.5"
msgid "Gaṇḍo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.6"
msgid "yo koci gaṇḍo."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.7"
msgid "Rudhitaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.8"
msgid "yaṃ kiñci vaṇaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.9"
msgid "Anapaloketvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.10"
msgid "anāpucchā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.11"
msgid "Saṃgho nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.12"
msgid "bhikkhunisaṃgho vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.13"
msgid "Gaṇo nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.14"
msgid "sambahulā bhikkhuniyo vuccanti."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.15"
msgid "Puriso nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.16"
msgid "manussapuriso, na yakkho, na peto, na tiracchānagato, viññū paṭibalo dūsetuṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.17"
msgid "Saddhinti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.18"
msgid "ekato."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc60:10.19"
msgid "Ekenekāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc60:10.20"
msgid "puriso ceva hoti bhikkhunī ca."
msgstr ""

#. HTML: </p><p>
#. REF: sc6, pts-vp-pli4.317
msgctxt "pli-tv-bi-vb-pc60:11.1"
msgid "“Bhindā”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.2"
msgid "Bhinne āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.3"
msgid "“Phālehī”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.4"
msgid "Phālite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.5"
msgid "“Dhovā”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

#. VAR: Dhovite → dhote (s1-3, pts1)
msgctxt "pli-tv-bi-vb-pc60:11.6"
msgid "Dhovite āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.7"
msgid "“Ālimpā”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

#. VAR: Litte → ālitte (s1-3)
msgctxt "pli-tv-bi-vb-pc60:11.8"
msgid "Litte āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.9"
msgid "“Bandhāhī”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.10"
msgid "Baddhe āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.11"
msgid "“Mocehī”ti āṇāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc60:11.12"
msgid "Mutte āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc60:12.1"
msgid "Anāpatti—"
msgstr ""

#. VAR: bandhāpeti vā mocāpeti vā, yā kāci viññū dutiyikā → yā kāci viññū dutiyā (s1-3) | yo koci viññū dutiyo (pts1, mr)
msgctxt "pli-tv-bi-vb-pc60:12.2"
msgid "apaloketvā bhedāpeti vā phālāpeti vā dhovāpeti vā ālimpāpeti vā bandhāpeti vā mocāpeti vā, yā kāci viññū dutiyikā hoti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc60:13.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc60:13.2"
msgid "Ārāmavaggo chaṭṭho."
msgstr ""
