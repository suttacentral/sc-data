#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc50"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc50:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc50:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc50:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc50:4.1"
msgid "Cittāgāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc50:5.1"
msgid "50. Tiracchānavijjāvācanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.306
msgctxt "pli-tv-bi-vb-pc50:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo tiracchānavijjaṃ vācenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:6.3"
msgid "Manussā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:6.4"
msgid "“kathañhi nāma bhikkhuniyo tiracchānavijjaṃ vācessanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc50:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.4"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo tiracchānavijjaṃ vācessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.5"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo tiracchānavijjaṃ vācentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.8"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo tiracchānavijjaṃ vācessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc50:8.1"
msgid "“Yā pana bhikkhunī tiracchānavijjaṃ vāceyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc50:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc50:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc50:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc50:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
#. VAR: Tiracchānavijjā → tiracchānavijjaṃ (bj, mr)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc50:10.1"
msgid "Tiracchānavijjā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc50:10.2"
msgid "yaṃ kiñci bāhirakaṃ anatthasaṃhitaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc50:11.1"
msgid "Vāceyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc50:11.2"
msgid "padena vāceti, pade pade āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:11.3"
msgid "Akkharāya vāceti, akkharakkharāya āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc50:12.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc50:12.2"
msgid "lekhaṃ vāceti, dhāraṇaṃ vāceti, guttatthāya parittaṃ vāceti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc50:13.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc50:13.2"
msgid "Cittāgāravaggo pañcamo."
msgstr ""
