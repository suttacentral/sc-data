#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc74"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc74:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc74:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc74:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc74:4.1"
msgid "Kumāribhūtavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc74:5.1"
msgid "74. Ūnadvādasavassasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc74:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:6.2"
msgid "Tena kho pana samayena bhikkhuniyo ūnadvādasavassā vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:6.3"
msgid "Tā bālā honti abyattā na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:6.4"
msgid "Saddhivihāriniyopi bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:6.5"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc74:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.3"
msgid "“kathañhi nāma bhikkhuniyo ūnadvādasavassā vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo ūnadvādasavassā vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo ūnadvādasavassā vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc74:8.1"
msgid "“Yā pana bhikkhunī ūnadvādasavassā vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc74:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc74:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc74:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc74:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc74:10.1"
msgid "Ūnadvādasavassā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc74:10.2"
msgid "appattadvādasavassā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc74:11.1"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc74:11.2"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc74:12.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:12.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:12.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:12.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:12.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc74:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc74:13.2"
msgid "paripuṇṇadvādasavassā vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc74:14.1"
msgid "Catutthasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
