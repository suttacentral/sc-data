#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc70"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc70:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc70:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc70:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc70:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc70:5.1"
msgid "70. Dutiyasahajīvinīsikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc70:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā neva vūpakāseti na vūpakāsāpeti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:6.3"
msgid "Sāmiko aggahesi."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc70:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.3"
msgid "“kathañhi nāma ayyā thullanandā sahajīviniṃ vuṭṭhāpetvā neva vūpakāsessati na vūpakāsāpessati, sāmiko aggahesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.4"
msgid "Sacāyaṃ bhikkhunī pakkantā assa, na ca sāmiko gaṇheyyā”ti … pe …"
msgstr ""

#. VAR: vūpakāsāpeti → ... si (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-pc70:7.5"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā neva vūpakāseti na vūpakāsāpeti, sāmiko aggahesī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.8"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā neva vūpakāsessati na vūpakāsāpessati, sāmiko aggahesi."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc70:8.1"
msgid "“Yā pana bhikkhunī sahajīviniṃ vuṭṭhāpetvā neva vūpakāseyya na vūpakāsāpeyya antamaso chappañcayojanānipi, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc70:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc70:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5, pts-vp-pli4.327
msgctxt "pli-tv-bi-vb-pc70:10.1"
msgid "Sahajīvinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:10.2"
msgid "saddhivihārinī vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc70:10.3"
msgid "Vuṭṭhāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:10.4"
msgid "upasampādetvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc70:10.5"
msgid "Neva vūpakāseyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:10.6"
msgid "na sayaṃ vūpakāseyya."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc70:10.7"
msgid "Na vūpakāsāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc70:10.8"
msgid "na aññaṃ āṇāpeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:10.9"
msgid "“Neva vūpakāsessāmi na vūpakāsāpessāmi antamaso chappañcayojanānipī”ti dhuraṃ nikkhittamatte āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc70:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc70:11.2"
msgid "sati antarāye, pariyesitvā dutiyikaṃ bhikkhuniṃ na labhati, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc70:12.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-pc70:12.2"
msgid "Gabbhinivaggo sattamo."
msgstr ""
