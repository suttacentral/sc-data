#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc68"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc68:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc68:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc68:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc68:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc68:5.1"
msgid "68. Sahajīvinīsikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc68:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā dve vassāni neva anuggaṇhāti na anuggaṇhāpeti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:6.3"
msgid "Tā bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:6.4"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli4.325
msgctxt "pli-tv-bi-vb-pc68:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.3"
msgid "“kathañhi nāma ayyā thullanandā sahajīviniṃ vuṭṭhāpetvā dve vassāni neva anuggaṇhissati na anuggaṇhāpessatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.4"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā dve vassāni neva anuggaṇhāti na anuggaṇhāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.7"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī sahajīviniṃ vuṭṭhāpetvā dve vassāni neva anuggaṇhissati na anuggaṇhāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc68:8.1"
msgid "“Yā pana bhikkhunī sahajīviniṃ vuṭṭhāpetvā dve vassāni neva anuggaṇheyya na anuggaṇhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc68:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc68:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc68:10.1"
msgid "Sahajīvinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:10.2"
msgid "saddhivihārinī vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc68:10.3"
msgid "Vuṭṭhāpetvāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:10.4"
msgid "upasampādetvā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc68:10.5"
msgid "Dve vassānīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:10.6"
msgid "dve saṃvaccharāni."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc68:11.1"
msgid "Neva anuggaṇheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:11.2"
msgid "na sayaṃ anuggaṇheyya uddesena paripucchāya ovādena anusāsaniyā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc68:12.1"
msgid "Na anuggaṇhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc68:12.2"
msgid "na aññaṃ āṇāpeyya “dve vassāni neva anuggaṇhissāmi na anuggaṇhāpessāmī”ti dhuraṃ nikkhittamatte āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc68:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc68:13.2"
msgid "sati antarāye, pariyesitvā na labhati, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc68:14.1"
msgid "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
