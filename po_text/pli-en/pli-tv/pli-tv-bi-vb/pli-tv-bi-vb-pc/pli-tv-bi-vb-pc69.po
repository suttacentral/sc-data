#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc69"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc69:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc69:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc69:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc69:4.1"
msgid "Gabbhinivagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc69:5.1"
msgid "69. Pavattinīnānubandhanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc69:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:6.2"
msgid "Tena kho pana samayena bhikkhuniyo vuṭṭhāpitaṃ pavattiniṃ dve vassāni nānubandhanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:6.3"
msgid "Tā bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:6.4"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc69:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.3"
msgid "“kathañhi nāma bhikkhuniyo vuṭṭhāpitaṃ pavattiniṃ dve vassāni nānubandhissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo vuṭṭhāpitaṃ pavattiniṃ dve vassāni nānubandhantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo vuṭṭhāpitaṃ pavattiniṃ dve vassāni nānubandhissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3, pts-vp-pli4.326
msgctxt "pli-tv-bi-vb-pc69:8.1"
msgid "“Yā pana bhikkhunī vuṭṭhāpitaṃ pavattiniṃ dve vassāni nānubandheyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc69:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc69:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc69:10.1"
msgid "Vuṭṭhāpitanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:10.2"
msgid "upasampāditaṃ."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc69:10.3"
msgid "Pavattinī nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:10.4"
msgid "upajjhāyā vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc69:10.5"
msgid "Dve vassānīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:10.6"
msgid "dve saṃvaccharāni."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc69:10.7"
msgid "Nānubandheyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc69:10.8"
msgid "na sayaṃ upaṭṭhaheyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:10.9"
msgid "Dve vassāni nānubandhissāmīti dhuraṃ nikkhittamatte āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc69:11.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc69:11.2"
msgid "upajjhāyā bālā vā hoti alajjinī vā, gilānāya, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc69:12.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
