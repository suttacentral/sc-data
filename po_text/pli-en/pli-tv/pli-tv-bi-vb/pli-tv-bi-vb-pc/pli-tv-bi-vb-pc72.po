#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc72"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc72:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc72:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc72:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc72:4.1"
msgid "Kumāribhūtavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc72:5.1"
msgid "72. Dutiyakumāribhūtasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.328
msgctxt "pli-tv-bi-vb-pc72:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:6.2"
msgid "Tena kho pana samayena bhikkhuniyo paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu asikkhitasikkhaṃ vuṭṭhāpenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:6.3"
msgid "Tā bālā honti abyattā;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:6.4"
msgid "na jānanti kappiyaṃ vā akappiyaṃ vā."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc72:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.3"
msgid "“kathañhi nāma bhikkhuniyo paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu asikkhitasikkhaṃ vuṭṭhāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu asikkhitasikkhaṃ vuṭṭhāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu asikkhitasikkhaṃ vuṭṭhāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.9"
msgid "vigarahitvā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.10"
msgid "dhammiṃ kathaṃ katvā bhikkhū āmantesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.11"
msgid "“anujānāmi, bhikkhave, aṭṭhārasavassāya kumāribhūtāya dve vassāni chasu dhammesu sikkhāsammutiṃ dātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.12"
msgid "Evañca pana, bhikkhave, dātabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.13"
msgid "Tāya aṭṭhārasavassāya kumāribhūtāya saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.14"
msgid "‘ahaṃ, ayye, itthannāmā itthannāmāya ayyāya aṭṭhārasavassā kumāribhūtā saṃghaṃ dve vassāni chasu dhammesu sikkhāsammutiṃ yācāmī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.15"
msgid "Dutiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.16"
msgid "Tatiyampi yācitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:7.17"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc72:8.1"
msgid "‘Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:8.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya aṭṭhārasavassā kumāribhūtā saṃghaṃ dve vassāni chasu dhammesu sikkhāsammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:8.3"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmāya aṭṭhārasavassāya kumāribhūtāya dve vassāni chasu dhammesu sikkhāsammutiṃ dadeyya."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:8.4"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc72:9.1"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:9.2"
msgid "Ayaṃ itthannāmā itthannāmāya ayyāya aṭṭhārasavassā kumāribhūtā saṃghaṃ dve vassāni chasu dhammesu sikkhāsammutiṃ yācati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:9.3"
msgid "Saṃgho itthannāmāya aṭṭhārasavassāya kumāribhūtāya dve vassāni chasu dhammesu sikkhāsammutiṃ deti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:9.4"
msgid "Yassā ayyāya khamati itthannāmāya aṭṭhārasavassāya kumāribhūtāya dve vassāni chasu dhammesu sikkhāsammutiyā dānaṃ, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:9.5"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc72:10.1"
msgid "Dinnā saṃghena itthannāmāya aṭṭhārasavassāya kumāribhūtāya dve vassāni chasu dhammesu sikkhāsammuti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:10.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc72:11.1"
msgid "Sā aṭṭhārasavassā kumāribhūtā evaṃ vadehīti vattabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:11.2"
msgid "“pāṇātipātā veramaṇiṃ dve vassāni avītikkamma samādānaṃ samādiyāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:11.3"
msgid "vikālabhojanā veramaṇiṃ dve vassāni avītikkamma samādānaṃ samādiyāmī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc72:12.1"
msgid "Atha kho bhagavā tā bhikkhuniyo anekapariyāyena vigarahitvā dubbharatāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:12.2"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc72:13.1"
msgid "“Yā pana bhikkhunī paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu asikkhitasikkhaṃ vuṭṭhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc72:14.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:14.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc72:14.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:14.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:14.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc72:15.1"
msgid "Paripuṇṇavīsativassā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:15.2"
msgid "pattavīsativassā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc72:15.3"
msgid "Kumāribhūtā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:15.4"
msgid "sāmaṇerī vuccati."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc72:15.5"
msgid "Dve vassānīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:15.6"
msgid "dve saṃvaccharāni."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc72:15.7"
msgid "Asikkhitasikkhā nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:15.8"
msgid "sikkhā vā na dinnā hoti, dinnā vā sikkhā kupitā."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc72:15.9"
msgid "Vuṭṭhāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc72:15.10"
msgid "upasampādeyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc72:16.1"
msgid "“Vuṭṭhāpessāmī”ti gaṇaṃ vā ācariniṃ vā pattaṃ vā cīvaraṃ vā pariyesati, sīmaṃ vā sammannati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:16.2"
msgid "Ñattiyā dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:16.3"
msgid "Dvīhi kammavācāhi dukkaṭā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:16.4"
msgid "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:16.5"
msgid "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-pc72:17.1"
msgid "Dhammakamme dhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:17.2"
msgid "Dhammakamme vematikā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:17.3"
msgid "Dhammakamme adhammakammasaññā vuṭṭhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc13
msgctxt "pli-tv-bi-vb-pc72:18.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:18.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:18.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-pc72:19.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc72:19.2"
msgid "paripuṇṇavīsativassaṃ kumāribhūtaṃ dve vassāni chasu dhammesu sikkhitasikkhaṃ vuṭṭhāpeti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc15
msgctxt "pli-tv-bi-vb-pc72:20.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
