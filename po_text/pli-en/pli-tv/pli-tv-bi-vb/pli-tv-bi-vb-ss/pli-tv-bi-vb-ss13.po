#. HTML: <section class="sutta" id="pli-tv-bi-vb-ss13"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-ss13:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss13:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-ss13:3.1"
msgid "Saṃghādisesakaṇḍa"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-ss13:4.1"
msgid "Saṅghādisesā 13: Dutiyapāpasamācārasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.241
msgctxt "pli-tv-bi-vb-ss13:5.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.3"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.4"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.5"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.6"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

#. VAR: Tumhaññeva → tuyhaññeva (si) | tumheyeva (s1-3)
#. VAR: vebhassiyā → vebhassā (bj, s1-3)
msgctxt "pli-tv-bi-vb-ss13:5.7"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.8"
msgid "‘bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.9"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:5.10"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-ss13:6.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.3"
msgid "“kathañhi nāma ayyā thullanandā saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.4"
msgid "saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.5"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.6"
msgid "Santi saṃghe aññāpi bhikkhuniyo … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.7"
msgid "viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.8"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetīti” … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.9"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.10"
msgid "saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.11"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.12"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.13"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.14"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.15"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.16"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.17"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.18"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.19"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.20"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vakkhati—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.21"
msgid "saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.22"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.23"
msgid "Santi saṃghe aññāpi bhikkhuniyo … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.24"
msgid "viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.25"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetīti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.26"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:6.27"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-ss13:7.1"
msgid "“Yā pana bhikkhunī evaṃ vadeyya—"
msgstr ""

#. VAR: ayye → māyye (bj, s1-3, pts1)
msgctxt "pli-tv-bi-vb-ss13:7.2"
msgid "‘saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.3"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.4"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.5"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

#. VAR: Tumhaññeva → tumheyeva (s1-3)
#. VAR: vebhassiyā → vebhassā (bj, s1-3)
msgctxt "pli-tv-bi-vb-ss13:7.6"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.7"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.8"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.9"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.10"
msgid "Sā bhikkhunī bhikkhunīhi evamassa vacanīyā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.11"
msgid "‘mā, ayye, evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.12"
msgid "saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.13"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.14"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.15"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.16"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.17"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.18"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.19"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī’ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.20"
msgid "Evañca sā bhikkhunī bhikkhunīhi vuccamānā tatheva paggaṇheyya, sā bhikkhunī bhikkhunīhi yāvatatiyaṃ samanubhāsitabbā tassa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.21"
msgid "Yāvatatiyañce samanubhāsīyamānā taṃ paṭinissajjeyya, iccetaṃ kusalaṃ;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:7.22"
msgid "no ce paṭinissajjeyya, ayampi bhikkhunī yāvatatiyakaṃ dhammaṃ āpannā nissāraṇīyaṃ saṃghādisesan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-ss13:8.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:8.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-ss13:8.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:8.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:8.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-ss13:9.1"
msgid "Evaṃ vadeyyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:9.2"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:9.3"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:9.4"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:9.5"
msgid "Tā saṃgho na kiñci āha”."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-ss13:10.1"
msgid "Tumhaññeva saṃgho uññāyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:10.2"
msgid "avaññāya."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-ss13:11.1"
msgid "Paribhavenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:11.2"
msgid "pāribhabyatā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-ss13:12.1"
msgid "Akkhantiyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:12.2"
msgid "kopena."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
#. VAR: vibhassīkatā → vibhassikatāya (bj) | vibhassikatā (pts1)+. HTML: <b>
msgctxt "pli-tv-bi-vb-ss13:13.1"
msgid "Vebhassiyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:13.2"
msgid "vibhassīkatā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc10, pts-vp-pli4.242
msgctxt "pli-tv-bi-vb-ss13:14.1"
msgid "Dubbalyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:14.2"
msgid "apakkhatā."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-ss13:15.1"
msgid "Evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:15.2"
msgid "“bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:15.3"
msgid "Viciccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:15.4"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc12
msgctxt "pli-tv-bi-vb-ss13:16.1"
msgid "Sā bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:16.2"
msgid "yā sā evaṃvādinī bhikkhunī."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc13
msgctxt "pli-tv-bi-vb-ss13:17.1"
msgid "Bhikkhunīhīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:17.2"
msgid "aññāhi bhikkhunīhi."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-ss13:18.1"
msgid "Yā passanti yā suṇanti tāhi vattabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.2"
msgid "“māyye, evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.3"
msgid "‘saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.4"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.5"
msgid "Santi saṃghe aññāpi bhikkhuniyo … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.6"
msgid "viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.7"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī’”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.8"
msgid "Dutiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.9"
msgid "Tatiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.10"
msgid "Sace paṭinissajjati, iccetaṃ kusalaṃ;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.11"
msgid "no ce paṭinissajjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.12"
msgid "Sutvā na vadanti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.13"
msgid "Sā bhikkhunī saṃghamajjhampi ākaḍḍhitvā vattabbā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.14"
msgid "“māyye, evaṃ avaca—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.15"
msgid "‘saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.16"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.17"
msgid "Santi saṃghe aññāpi bhikkhuniyo … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.18"
msgid "viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.19"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī’”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.20"
msgid "Dutiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.21"
msgid "Tatiyampi vattabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.22"
msgid "Sace paṭinissajjati, iccetaṃ kusalaṃ;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.23"
msgid "no ce paṭinissajjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.24"
msgid "Sā bhikkhunī samanubhāsitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.25"
msgid "“Evañca pana, bhikkhave, samanubhāsitabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:18.26"
msgid "Byattāya bhikkhuniyā paṭibalāya saṃgho ñāpetabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-bi-vb-ss13:19.1"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.2"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.3"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.4"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.5"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.6"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.7"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.8"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.9"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.10"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuniṃ samanubhāseyya tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.11"
msgid "Esā ñatti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.12"
msgid "‘Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.13"
msgid "Ayaṃ itthannāmā bhikkhunī saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.14"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.15"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.16"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.17"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.18"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.19"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.20"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.21"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.22"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.23"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho itthannāmaṃ bhikkhuniṃ samanubhāseyya tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:19.24"
msgid "Esā ñatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-bi-vb-ss13:20.1"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.2"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.3"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.4"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.5"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.6"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.7"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.8"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.9"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.10"
msgid "Saṃgho itthannāmaṃ bhikkhuniṃ samanubhāsati tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.11"
msgid "Yassā ayyāya khamati itthannāmāya bhikkhuniyā samanubhāsanā tassa vatthussa paṭinissaggāya, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.12"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.13"
msgid "Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.14"
msgid "Ayaṃ itthannāmā bhikkhunī saṃghena samanubhaṭṭhā bhikkhuniyo evaṃ vadeti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.15"
msgid "“saṃsaṭṭhāva, ayye, tumhe viharatha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.16"
msgid "Mā tumhe nānā viharittha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.17"
msgid "Santi saṃghe aññāpi bhikkhuniyo evācārā evaṃsaddā evaṃsilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.18"
msgid "Tā saṃgho na kiñci āha."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.19"
msgid "Tumhaññeva saṃgho uññāya paribhavena akkhantiyā vebhassiyā dubbalyā evamāha—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.20"
msgid "bhaginiyo kho saṃsaṭṭhā viharanti pāpācārā pāpasaddā pāpasilokā, bhikkhunisaṃghassa vihesikā, aññamaññissā vajjappaṭicchādikā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.21"
msgid "Viviccathāyye."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.22"
msgid "Vivekaññeva bhaginīnaṃ saṃgho vaṇṇetī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.23"
msgid "Sā taṃ vatthuṃ na paṭinissajjati."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.24"
msgid "Saṃgho itthannāmaṃ bhikkhuniṃ samanubhāsati tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.25"
msgid "Yassā ayyāya khamati itthannāmāya bhikkhuniyā samanubhāsanā tassa vatthussa paṭinissaggāya, sā tuṇhassa;"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:20.26"
msgid "yassā nakkhamati, sā bhāseyya."
msgstr ""

#. HTML: </p><p>
#. REF: sc17
msgctxt "pli-tv-bi-vb-ss13:21.1"
msgid "Dutiyampi etamatthaṃ vadāmi … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:21.2"
msgid "tatiyampi etamatthaṃ vadāmi …"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:21.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p>
#. REF: sc18
msgctxt "pli-tv-bi-vb-ss13:22.1"
msgid "Samanubhaṭṭhā saṃghena itthannāmā bhikkhunī tassa vatthussa paṭinissaggāya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:22.2"
msgid "Khamati saṃghassa, tasmā tuṇhī, evametaṃ dhārayāmī’”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc19
msgctxt "pli-tv-bi-vb-ss13:23.1"
msgid "Ñattiyā dukkaṭaṃ, dvīhi kammavācāhi thullaccayā, kammavācāpariyosāne āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:23.2"
msgid "Saṃghādisesaṃ ajjhāpajjantiyā ñattiyā dukkaṭaṃ, dvīhi kammavācāhi thullaccayā paṭippassambhanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc20
msgctxt "pli-tv-bi-vb-ss13:24.1"
msgid "Ayampīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:24.2"
msgid "purimāyo upādāya vuccati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc21
msgctxt "pli-tv-bi-vb-ss13:25.1"
msgid "Yāvatatiyakanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:25.2"
msgid "yāvatatiyaṃ samanubhāsanāya āpajjati, na sahavatthujjhācārā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc22
msgctxt "pli-tv-bi-vb-ss13:26.1"
msgid "Nissāraṇīyanti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:26.2"
msgid "saṃghamhā nissārīyati."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc23
msgctxt "pli-tv-bi-vb-ss13:27.1"
msgid "Saṃghādisesoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-ss13:27.2"
msgid "saṃghova tassā āpattiyā mānattaṃ deti, mūlāya paṭikassati, abbheti, na sambahulā na ekā bhikkhunī."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:27.3"
msgid "Tena vuccati “saṃghādiseso”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:27.4"
msgid "Tasseva āpattinikāyassa nāmakammaṃ adhivacanaṃ, tenapi vuccati “saṃghādiseso”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc24
msgctxt "pli-tv-bi-vb-ss13:28.1"
msgid "Dhammakamme dhammakammasaññā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:28.2"
msgid "Dhammakamme vematikā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:28.3"
msgid "Dhammakamme adhammakammasaññā na paṭinissajjati, āpatti saṃghādisesassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc25
msgctxt "pli-tv-bi-vb-ss13:29.1"
msgid "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:29.2"
msgid "Adhammakamme vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:29.3"
msgid "Adhammakamme adhammakammasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc26
msgctxt "pli-tv-bi-vb-ss13:30.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:30.2"
msgid "asamanubhāsantiyā, paṭinissajjantiyā, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc27
msgctxt "pli-tv-bi-vb-ss13:31.1"
msgid "Dasamasaṃghādisesasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: </p><h2>
#. VAR: 17 Kuladūsakasikkhāpada → ^pi-tv-bu-vb-ss10#7 [Saṅghādisesā 10: Saṃghabhedasikkhāpada] (10:14)
msgctxt "pli-tv-bi-vb-ss13:32.1"
msgid "17 Kuladūsakasikkhāpada"
msgstr ""

#. HTML: </h2><p>
#. REF: sc28
msgctxt "pli-tv-bi-vb-ss13:33.1"
msgid "Uddiṭṭhā kho, ayyāyo, sattarasa saṃghādisesā dhammā—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:33.2"
msgid "nava paṭhamāpattikā, aṭṭha yāvatatiyakā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:33.3"
msgid "Yesaṃ bhikkhunī aññataraṃ vā aññataraṃ vā āpajjati, tāya bhikkhuniyā ubhatosaṃghe pakkhamānattaṃ caritabbaṃ."
msgstr ""

#. VAR:  sā bhikkhunī →  etthantare pāṭho katthaci
msgctxt "pli-tv-bi-vb-ss13:33.4"
msgid "Ciṇṇamānattā bhikkhunī yattha siyā vīsatigaṇo bhikkhunisaṃgho tattha sā bhikkhunī abbhetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:33.5"
msgid "Ekāyapi ce ūno vīsatigaṇo bhikkhunisaṃgho taṃ bhikkhuniṃ abbheyya."
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:33.6"
msgid "Sā ca bhikkhunī anabbhitā, tā ca bhikkhuniyo gārayhā, ayaṃ tattha sāmīci."
msgstr ""

#. HTML: </p><p>
#. REF: sc29
msgctxt "pli-tv-bi-vb-ss13:34.1"
msgid "Tatthāyyāyo pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.2"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.3"
msgid "Dutiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.4"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.5"
msgid "Tatiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.6"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-ss13:34.7"
msgid "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc30
msgctxt "pli-tv-bi-vb-ss13:35.1"
msgid "Sattarasakaṃ niṭṭhitaṃ."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-bi-vb-ss13:35.2"
msgid "Bhikkhunivibhaṅge saṃghādisesakaṇḍaṃ niṭṭhitaṃ."
msgstr ""
