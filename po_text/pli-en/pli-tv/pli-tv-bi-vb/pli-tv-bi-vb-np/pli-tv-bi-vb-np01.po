#. HTML: <section class="sutta" id="pli-tv-bi-vb-np1"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np1:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np1:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np1:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np1:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np1:5.1"
msgid "1. Pattasannicayasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.243
msgctxt "pli-tv-bi-vb-np1:6.1"
msgid "Ime kho panāyyāyo tiṃsa nissaggiyā pācittiyā dhammā uddesaṃ āgacchanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np1:7.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:7.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo bahū patte sannicayaṃ karonti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:7.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:7.4"
msgid "“kathañhi nāma bhikkhuniyo bahū patte sannicayaṃ karissanti, pattavāṇijjaṃ vā bhikkhuniyo karissanti, āmattikāpaṇaṃ vā pasāressantī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-np1:8.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.4"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo pattasannicayaṃ karissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.5"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo pattasannicayaṃ karontī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.8"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo pattasannicayaṃ karissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:8.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc4
msgctxt "pli-tv-bi-vb-np1:9.1"
msgid "“Yā pana bhikkhunī pattasannicayaṃ kareyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np1:10.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:10.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np1:10.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:10.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:10.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6, pts-vp-pli4.244
msgctxt "pli-tv-bi-vb-np1:11.1"
msgid "Patto nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:11.2"
msgid "dve pattā—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:11.3"
msgid "ayopatto, mattikāpatto."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:11.4"
msgid "Tayo pattassa vaṇṇā—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:11.5"
msgid "ukkaṭṭho patto, majjhimo patto, omako patto."
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np1:11.6"
msgid "Ukkaṭṭho nāma patto"
msgstr ""

#. HTML: </b><b>
msgctxt "pli-tv-bi-vb-np1:11.7"
msgid "aḍḍhāḷhakodanaṃ gaṇhāti catubhāgaṃ khādanaṃ tadupiyaṃ byañjanaṃ."
msgstr ""

#. HTML: </b><b>
msgctxt "pli-tv-bi-vb-np1:11.8"
msgid "Majjhimo nāma patto"
msgstr ""

#. HTML: </b><b>
msgctxt "pli-tv-bi-vb-np1:11.9"
msgid "nāḷikodanaṃ gaṇhāti catubhāgaṃ khādanaṃ tadupiyaṃ byañjanaṃ."
msgstr ""

#. HTML: </b><b>
msgctxt "pli-tv-bi-vb-np1:11.10"
msgid "Omako nāma patto"
msgstr ""

#. HTML: </b><b>
msgctxt "pli-tv-bi-vb-np1:11.11"
msgid "patthodanaṃ gaṇhāti catubhāgaṃ khādanaṃ tadupiyaṃ byañjanaṃ."
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:11.12"
msgid "Tato ukkaṭṭho apatto omako apatto."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np1:12.1"
msgid "Sannicayaṃ kareyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:12.2"
msgid "anadhiṭṭhito avikappito."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np1:13.1"
msgid "Nissaggiyo hotīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np1:13.2"
msgid "saha aruṇuggamanā nissaggiyo hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.3"
msgid "Nissajjitabbo saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.4"
msgid "Evañca pana, bhikkhave, nissajjitabbo."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.5"
msgid "Tāya bhikkhuniyā saṃghaṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyo—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.6"
msgid "“ayaṃ me, ayye, patto rattātikkanto nissaggiyo, imāhaṃ saṃghassa nissajjāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.7"
msgid "Nissajjitvā āpatti desetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.8"
msgid "Byattāya bhikkhuniyā paṭibalāya āpatti paṭiggahetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:13.9"
msgid "Nissaṭṭhapatto dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np1:14.1"
msgid "“Suṇātu me, ayye, saṃgho."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:14.2"
msgid "Ayaṃ patto itthannāmāya bhikkhuniyā nissaggiyo saṃghassa nissaṭṭho."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:14.3"
msgid "Yadi saṃghassa pattakallaṃ, saṃgho imaṃ pattaṃ itthannāmāya bhikkhuniyā dadeyyā”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-np1:15.1"
msgid "Tāya bhikkhuniyā sambahulā bhikkhuniyo upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā vuḍḍhānaṃ bhikkhunīnaṃ pāde vanditvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassu vacanīyā—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:15.2"
msgid "“ayaṃ me, ayyāyo, patto rattātikkanto nissaggiyo, imāhaṃ ayyānaṃ nissajjāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:15.3"
msgid "Nissajjitvā āpatti desetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:15.4"
msgid "Byattāya bhikkhuniyā paṭibalāya āpatti paṭiggahetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:15.5"
msgid "Nissaṭṭhapatto dātabbo—"
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-np1:16.1"
msgid "“Suṇantu me, ayyāyo."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:16.2"
msgid "Ayaṃ patto itthannāmāya bhikkhuniyā nissaggiyo ayyānaṃ nissaṭṭho."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:16.3"
msgid "Yadi ayyānaṃ pattakallaṃ, ayyāyo, imaṃ pattaṃ itthannāmāya bhikkhuniyā dadeyyun”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc12
msgctxt "pli-tv-bi-vb-np1:17.1"
msgid "Tāya bhikkhuniyā ekaṃ bhikkhuniṃ upasaṅkamitvā ekaṃsaṃ uttarāsaṅgaṃ karitvā ukkuṭikaṃ nisīditvā añjaliṃ paggahetvā evamassa vacanīyā—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.2"
msgid "“ayaṃ me, ayye, patto rattātikkanto nissaggiyo."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.3"
msgid "Imāhaṃ ayyāya nissajjāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.4"
msgid "Nissajjitvā āpatti desetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.5"
msgid "Tāya bhikkhuniyā āpatti paṭiggahetabbā."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.6"
msgid "Nissaṭṭhapatto dātabbo—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:17.7"
msgid "“imaṃ pattaṃ ayyāya dammī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc13, pts-vp-pli4.245
msgctxt "pli-tv-bi-vb-np1:18.1"
msgid "Rattātikkante atikkantasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.2"
msgid "Rattātikkante vematikā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.3"
msgid "Rattātikkante anatikkantasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.4"
msgid "Anadhiṭṭhite adhiṭṭhitasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.5"
msgid "Avikappite vikappitasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.6"
msgid "Avissajjite vissajjitasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.7"
msgid "Anaṭṭhe naṭṭhasaññā …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.8"
msgid "avinaṭṭhe vinaṭṭhasaññā …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.9"
msgid "abhinne bhinnasaññā …"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:18.10"
msgid "avilutte viluttasaññā, nissaggiyaṃ pācittiyaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc14
msgctxt "pli-tv-bi-vb-np1:19.1"
msgid "Nissaggiyaṃ pattaṃ anissajjitvā paribhuñjati, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:19.2"
msgid "Rattānatikkante atikkantasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:19.3"
msgid "Rattānatikkante vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:19.4"
msgid "Rattānatikkante anatikkantasaññā anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc15
msgctxt "pli-tv-bi-vb-np1:20.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np1:20.2"
msgid "antoaruṇe adhiṭṭheti, vikappeti, vissajjeti, nassati, vinassati, bhijjati, acchinditvā gaṇhanti, vissāsaṃ gaṇhanti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p>
#. REF: sc16
msgctxt "pli-tv-bi-vb-np1:21.1"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo nissaṭṭhapattaṃ na denti."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:21.2"
msgid "Bhagavato etamatthaṃ ārocesuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:21.3"
msgid "“Na, bhikkhave, nissaṭṭhapatto na dātabbo."
msgstr ""

msgctxt "pli-tv-bi-vb-np1:21.4"
msgid "Yā na dadeyya, āpatti dukkaṭassā”ti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc17
msgctxt "pli-tv-bi-vb-np1:22.1"
msgid "Paṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
