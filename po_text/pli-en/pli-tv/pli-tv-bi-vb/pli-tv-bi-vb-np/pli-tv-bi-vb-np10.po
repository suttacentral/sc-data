#. HTML: <section class="sutta" id="pli-tv-bi-vb-np10"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np10:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np10:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np10:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np10:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np10:5.1"
msgid "10. Puggalikacetāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.254
msgctxt "pli-tv-bi-vb-np10:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī bahussutā hoti bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.3"
msgid "Bahū manussā thullanandaṃ bhikkhuniṃ payirupāsanti."
msgstr ""

#. VAR: undriyati → uddīyati (bj) | udrīyati (s1-3) | udriyati (pts1)
msgctxt "pli-tv-bi-vb-np10:6.4"
msgid "Tena kho pana samayena thullanandāya bhikkhuniyā pariveṇaṃ undriyati."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.5"
msgid "Manussā thullanandaṃ bhikkhuniṃ etadavocuṃ—"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.6"
msgid "“kissidaṃ te, ayye, pariveṇaṃ undriyatī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.7"
msgid "“Natthāvuso, dāyakā, natthi kārakā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.8"
msgid "Atha kho te manussā thullanandāya bhikkhuniyā pariveṇatthāya chandakaṃ saṅgharitvā thullanandāya bhikkhuniyā parikkhāraṃ adaṃsu."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:6.9"
msgid "Thullanandā bhikkhunī tena ca parikkhārena sayampi yācitvā bhesajjaṃ cetāpetvā paribhuñji."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np10:7.1"
msgid "Manussā jānitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.2"
msgid "“kathañhi nāma ayyā thullanandā aññadatthikena parikkhārena aññuddisikena puggalikena saññācikena aññaṃ cetāpessatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.3"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī aññadatthikena parikkhārena aññuddisikena puggalikena saññācikena aññaṃ cetāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.5"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.6"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī aññadatthikena parikkhārena aññuddisikena puggalikena saññācikena aññaṃ cetāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.7"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:7.8"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-np10:8.1"
msgid "“Yā pana bhikkhunī aññadatthikena parikkhārena aññuddisikena puggalikena saññācikena aññaṃ cetāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np10:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np10:9.2"
msgid "yā yādisā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:9.3"
msgid "bhikkhunīti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:9.4"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np10:10.1"
msgid "Aññadatthikena parikkhārena aññuddisikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np10:10.2"
msgid "aññassatthāya dinnena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-np10:11.1"
msgid "Puggalikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np10:11.2"
msgid "ekāya bhikkhuniyā, na saṃghassa, na gaṇassa."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np10:12.1"
msgid "Saññācikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np10:12.2"
msgid "sayaṃ yācitvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np10:13.1"
msgid "Aññaṃ cetāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np10:13.2"
msgid "yaṃatthāya dinnaṃ taṃ ṭhapetvā aññaṃ cetāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.3"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.4"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.5"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.6"
msgid "“idaṃ me, ayye, aññadatthikena parikkhārena aññuddisikena puggalikena saññācikena aññaṃ cetāpitaṃ nissaggiyaṃ, imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.7"
msgid "dadeyyā”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.8"
msgid "dadeyyun”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:13.9"
msgid "ayyāya dammī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np10:14.1"
msgid "Aññadatthike aññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:14.2"
msgid "Aññadatthike vematikā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:14.3"
msgid "Aññadatthike anaññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:14.4"
msgid "Nissaṭṭhaṃ paṭilabhitvā yathādāne upanetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-np10:15.1"
msgid "Anaññadatthike aññadatthikasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:15.2"
msgid "Anaññadatthike vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np10:15.3"
msgid "Anaññadatthike anaññadatthikasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-np10:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np10:16.2"
msgid "sesakaṃ upaneti, sāmike apaloketvā upaneti, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-np10:17.1"
msgid "Dasamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
