#. HTML: <section class="sutta" id="pli-tv-bi-vb-np9"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-np9:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np9:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np9:3.1"
msgid "Nissaggiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-np9:4.1"
msgid "Pattavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-np9:5.1"
msgid "9. Dutiyagaṇikacetāpanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-np9:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:6.2"
msgid "Tena kho pana samayena aññatarassa pūgassa pariveṇavāsikā bhikkhuniyo yāguyā kilamanti."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:6.3"
msgid "Atha kho so pūgo bhikkhunīnaṃ yāguatthāya chandakaṃ saṅgharitvā aññatarassa āpaṇikassa ghare parikkhāraṃ nikkhipitvā bhikkhuniyo upasaṅkamitvā etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:6.4"
msgid "“amukassa, ayye, āpaṇikassa ghare yāguatthāya parikkhāro nikkhitto."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:6.5"
msgid "Tato taṇḍule āharāpetvā yāguṃ pacāpetvā paribhuñjathā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:6.6"
msgid "Bhikkhuniyo tena ca parikkhārena sayampi yācitvā bhesajjaṃ cetāpetvā paribhuñjiṃsu."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-np9:7.1"
msgid "So pūgo jānitvā ujjhāyati khiyyati vipāceti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.2"
msgid "“kathañhi nāma bhikkhuniyo aññadatthikena parikkhārena aññuddisikena mahājanikena saññācikena aññaṃ cetāpessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.3"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo aññadatthikena parikkhārena aññuddisikena mahājanikena saññācikena aññaṃ cetāpentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.4"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.5"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.6"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo aññadatthikena parikkhārena aññuddisikena mahājanikena saññācikena aññaṃ cetāpessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.7"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:7.8"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-np9:8.1"
msgid "“Yā pana bhikkhunī aññadatthikena parikkhārena aññuddisikena mahājanikena saññācikena aññaṃ cetāpeyya, nissaggiyaṃ pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-np9:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-np9:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-np9:10.1"
msgid "Aññadatthikena parikkhārena aññuddisikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:10.2"
msgid "aññassatthāya dinnena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-np9:11.1"
msgid "Mahājanikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:11.2"
msgid "gaṇassa, na saṃghassa, na ekabhikkhuniyā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-np9:12.1"
msgid "Saññācikenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:12.2"
msgid "sayaṃ yācitvā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc8
msgctxt "pli-tv-bi-vb-np9:13.1"
msgid "Aññaṃ cetāpeyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-np9:13.2"
msgid "yaṃatthāya dinnaṃ taṃ ṭhapetvā aññaṃ cetāpeti, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.3"
msgid "Paṭilābhena nissaggiyaṃ hoti."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.4"
msgid "Nissajjitabbaṃ saṃghassa vā gaṇassa vā ekabhikkhuniyā vā."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.5"
msgid "Evañca pana, bhikkhave, nissajjitabbaṃ … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.6"
msgid "“idaṃ me, ayye, aññadatthikena parikkhārena aññuddisikena mahājanikena saññācikena aññaṃ cetāpitaṃ nissaggiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.7"
msgid "Imāhaṃ saṃghassa nissajjāmī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.8"
msgid "dadeyyā”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.9"
msgid "dadeyyun”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:13.10"
msgid "ayyāya dammī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-np9:14.1"
msgid "Aññadatthike aññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:14.2"
msgid "Aññadatthike vematikā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:14.3"
msgid "Aññadatthike anaññadatthikasaññā aññaṃ cetāpeti, nissaggiyaṃ pācittiyaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:14.4"
msgid "Nissaṭṭhaṃ paṭilabhitvā yathādāne upanetabbaṃ."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-np9:15.1"
msgid "Anaññadatthike aññadatthikasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:15.2"
msgid "Anaññadatthike vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-np9:15.3"
msgid "Anaññadatthike anaññadatthikasaññā anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc11
msgctxt "pli-tv-bi-vb-np9:16.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-np9:16.2"
msgid "sesakaṃ upaneti, sāmike apaloketvā upaneti, āpadāsu, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc12
msgctxt "pli-tv-bi-vb-np9:17.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
