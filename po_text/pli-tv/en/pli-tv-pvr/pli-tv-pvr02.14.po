#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.14:5.1"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.14:5.2"
msgid "Kāyasaṃsaggaṃ sādiyanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ …"
msgstr ""

msgctxt "pli-tv-pvr2.14:5.3"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.14:5.4"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.14:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr2.14:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjanapaccayā āpattiyo catunnaṃ adhikaraṇānaṃ—"
msgstr ""

msgctxt "pli-tv-pvr2.14:6.3"
msgid "āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr2.14:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.14:7.1"
msgid "Adhikaraṇavāro niṭṭhito chaṭṭho."
