#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk66"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk66:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk66:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk66:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk66:4.1"
msgid "Pādukavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk66:5.1"
msgid "66. Veṭhitasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk66:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk66:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū veṭhitasīsassa dhammaṃ desenti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk66:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk66:7.1"
msgid "“Na veṭhitasīsassa agilānassa dhammaṃ desessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk66:8.1"
msgid "Veṭhitasīso nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bu-vb-sk66:8.2"
msgid "kesantaṃ na dassāpetvā veṭhito hoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk66:9.1"
msgid "Na veṭhitasīsassa agilānassa dhammo desetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk66:9.2"
msgid "Yo anādariyaṃ paṭicca veṭhitasīsassa agilānassa dhammaṃ deseti, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk66:10.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk66:10.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, kesantaṃ vivarāpetvā deseti, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc6
msgctxt "pli-tv-bu-vb-sk66:11.1"
msgid "Chaṭṭhasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
