msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk27:5.1"
msgid "27. Sakkaccapaṭiggahaṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.190
msgctxt "pli-tv-bu-vb-sk27:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk27:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū asakkaccaṃ piṇḍapātaṃ paṭiggaṇhanti chaḍḍetukāmā viya …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk27:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk27:7.1"
msgid "“Sakkaccaṃ piṇḍapātaṃ paṭiggahessāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk27:8.1"
msgid "Sakkaccaṃ piṇḍapāto paṭiggahetabbo."
msgstr ""

msgctxt "pli-tv-bu-vb-sk27:8.2"
msgid "Yo anādariyaṃ paṭicca asakkaccaṃ piṇḍapātaṃ paṭiggaṇhāti chaḍḍetukāmo viya, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk27:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk27:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk27:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk27:10.1"
msgid "Sattamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
