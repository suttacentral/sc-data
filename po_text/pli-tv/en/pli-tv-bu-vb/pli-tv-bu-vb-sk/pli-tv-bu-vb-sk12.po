#. HTML: <section class="sutta" id="pli-tv-bu-vb-sk12"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-sk12:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk12:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk12:3.1"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk12:4.1"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk12:5.1"
msgid "12. Dutiyaujjagghikasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk12:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk12:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū mahāhasitaṃ hasantā antaraghare nisīdanti …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk12:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk12:7.1"
msgid "“Na ujjagghikāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk12:8.1"
msgid "Na ujjagghikāya antaraghare nisīditabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk12:8.2"
msgid "Yo anādariyaṃ paṭicca mahāhasitaṃ hasanto antaraghare nisīdati, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk12:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk12:9.2"
msgid "asañcicca, assatiyā, ajānantassa, gilānassa, hasanīyasmiṃ vatthusmiṃ mihitamattaṃ karoti, āpadāsu, ummattakassa, ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk12:10.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
