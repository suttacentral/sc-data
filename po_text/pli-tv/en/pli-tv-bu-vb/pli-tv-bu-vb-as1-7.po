#. HTML: <section class="sutta" id="pli-tv-bu-vb-as1-7"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bu-vb-as1-7:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-as1-7:2.1"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-as1-7:3.1"
msgid "Adhikaraṇasamatha"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-as1-7:4.1"
msgid "Adhikaraṇasamathā 1: 7 Tiṇavatthāraka"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.207
msgctxt "pli-tv-bu-vb-as1-7:5.1"
msgid "Ime kho panāyasmanto satta adhikaraṇasamathā dhammā uddesaṃ āgacchanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
#. VAR: tassapāpiyasikā → tassapāpiyyasikā (bj, pts1) | tassa pāpiyasikā (s2, s3)
msgctxt "pli-tv-bu-vb-as1-7:6.1"
msgid "Uppannuppannānaṃ adhikaraṇānaṃ samathāya vūpasamāya sammukhāvinayo dātabbo, sativinayo dātabbo, amūḷhavinayo dātabbo, paṭiññāya kāretabbaṃ, yebhuyyasikā, tassapāpiyasikā, tiṇavatthārakoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-as1-7:7.1"
msgid "Uddiṭṭhā kho, āyasmanto, satta adhikaraṇasamathā dhammā."
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.2"
msgid "Tatthāyasmante pucchāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.3"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.4"
msgid "Dutiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.5"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.6"
msgid "Tatiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.7"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:7.8"
msgid "Parisuddhetthāyasmanto, tasmā tuṇhī, evametaṃ dhārayāmīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc4
msgctxt "pli-tv-bu-vb-as1-7:8.1"
msgid "Adhikaraṇasamathā niṭṭhitā."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-as1-7:9.1"
msgid "Uddiṭṭhaṃ kho, āyasmanto, nidānaṃ;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.2"
msgid "uddiṭṭhā cattāro pārājikā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.3"
msgid "uddiṭṭhā terasa saṃghādisesā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.4"
msgid "uddiṭṭhā dve aniyatā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.5"
msgid "uddiṭṭhā tiṃsa nissaggiyā pācittiyā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.6"
msgid "uddiṭṭhā dvenavuti pācittiyā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.7"
msgid "uddiṭṭhā cattāro pāṭidesanīyā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.8"
msgid "uddiṭṭhā sekhiyā dhammā;"
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.9"
msgid "uddiṭṭhā satta adhikaraṇasamathā dhammā."
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.10"
msgid "Ettakaṃ tassa bhagavato suttāgataṃ suttapariyāpannaṃ anvaddhamāsaṃ uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-bu-vb-as1-7:9.11"
msgid "Tattha sabbeheva samaggehi sammodamānehi avivadamānehi sikkhitabbanti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc6
msgctxt "pli-tv-bu-vb-as1-7:10.1"
msgid "Mahāvibhaṅgo niṭṭhito."
msgstr ""
