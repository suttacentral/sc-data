#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc18"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc18:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc18:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc18:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc18:4.1"
msgid "Rattandhakāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc18:5.1"
msgid "18. Paraujjhāpanakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc18:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:6.2"
msgid "Tena kho pana samayena bhaddāya kāpilāniyā antevāsinī bhikkhunī bhaddaṃ kāpilāniṃ sakkaccaṃ upaṭṭheti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:6.3"
msgid "Bhaddā kāpilānī bhikkhuniyo etadavoca—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:6.4"
msgid "“ayaṃ maṃ, ayye, bhikkhunī sakkaccaṃ upaṭṭheti, imissāhaṃ cīvaraṃ dassāmī”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:6.5"
msgid "Atha kho sā bhikkhunī duggahitena dūpadhāritena paraṃ ujjhāpesi—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:6.6"
msgid "“ahaṃ kirāyye, ayyaṃ na sakkaccaṃ upaṭṭhemi, na kira me ayyā cīvaraṃ dassatī”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc18:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.3"
msgid "“kathañhi nāma bhikkhunī duggahitena dūpadhāritena paraṃ ujjhāpessatī”ti … pe …"
msgstr ""

#. VAR: ujjhāpetī”ti → ujjhāpesīti (bj, pts1, mr)
msgctxt "pli-tv-bi-vb-pc18:7.4"
msgid "“saccaṃ kira, bhikkhave, bhikkhunī duggahitena dūpadhāritena paraṃ ujjhāpetī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.7"
msgid "kathañhi nāma, bhikkhave, bhikkhunī duggahitena dūpadhāritena paraṃ ujjhāpessati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc18:8.1"
msgid "“Yā pana bhikkhunī duggahitena dūpadhāritena paraṃ ujjhāpeyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc18:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc18:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc18:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc18:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
#. VAR: gahitena → uggahitena (pts1, mr)+. HTML: <b>
msgctxt "pli-tv-bi-vb-pc18:10.1"
msgid "Duggahitenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc18:10.2"
msgid "aññathā gahitena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc18:11.1"
msgid "Dūpadhāritenāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc18:11.2"
msgid "aññathā upadhāritena."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc18:12.1"
msgid "Paranti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc18:12.2"
msgid "upasampannaṃ ujjhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli4.276
msgctxt "pli-tv-bi-vb-pc18:13.1"
msgid "Upasampannāya upasampannasaññā ujjhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:13.2"
msgid "Upasampannāya vematikā ujjhāpeti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:13.3"
msgid "Upasampannāya anupasampannasaññā ujjhāpeti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc18:14.1"
msgid "Anupasampannaṃ ujjhāpeti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:14.2"
msgid "Anupasampannāya upasampannasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:14.3"
msgid "Anupasampannāya vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:14.4"
msgid "Anupasampannāya anupasampannasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc18:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc18:15.2"
msgid "ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc18:16.1"
msgid "Aṭṭhamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
