#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc33"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc33:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc33:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc33:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc33:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc33:5.1"
msgid "33. Aphāsukaraṇasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.290
msgctxt "pli-tv-bi-vb-pc33:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.2"
msgid "Tena kho pana samayena thullanandā bhikkhunī bahussutā hoti bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.3"
msgid "Bhaddāpi kāpilānī bahussutā hoti bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ uḷārasambhāvitā."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.4"
msgid "Manussā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.5"
msgid "“ayyā bhaddā kāpilānī bahussutā bhāṇikā visāradā paṭṭā dhammiṃ kathaṃ kātuṃ uḷārasambhāvitā”ti bhaddaṃ kāpilāniṃ paṭhamaṃ payirupāsitvā, pacchā thullanandaṃ bhikkhuniṃ payirupāsanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.6"
msgid "Thullanandā bhikkhunī issāpakatā—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:6.7"
msgid "“imā kira appicchā santuṭṭhā pavivittā asaṃsaṭṭhā yā imā saññattibahulā viññattibahulā viharantī”ti bhaddāya kāpilāniyā purato caṅkamatipi tiṭṭhatipi nisīdatipi seyyampi kappeti uddisatipi uddisāpetipi sajjhāyampi karoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc33:7.1"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.2"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.3"
msgid "“kathañhi nāma ayyā thullanandā ayyāya bhaddāya kāpilāniyā sañcicca aphāsuṃ karissatī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.4"
msgid "“saccaṃ kira, bhikkhave, thullanandā bhikkhunī bhaddāya kāpilāniyā sañcicca aphāsuṃ karotī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.5"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.6"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.7"
msgid "kathañhi nāma, bhikkhave, thullanandā bhikkhunī bhaddāya kāpilāniyā sañcicca aphāsuṃ karissati."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.8"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:7.9"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc33:8.1"
msgid "“Yā pana bhikkhunī bhikkhuniyā sañcicca aphāsuṃ kareyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc33:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc33:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc33:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc33:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc33:10.1"
msgid "Bhikkhuniyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc33:10.2"
msgid "aññāya bhikkhuniyā."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc33:11.1"
msgid "Sañciccāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc33:11.2"
msgid "jānantī sañjānantī cecca abhivitaritvā vītikkamo."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc33:12.1"
msgid "Aphāsuṃ kareyyāti—"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc33:12.2"
msgid "“iminā imissā aphāsu bhavissatī”ti anāpucchā purato caṅkamati vā tiṭṭhati vā nisīdati vā seyyaṃ vā kappeti uddisati vā uddisāpeti vā sajjhāyaṃ vā karoti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc8, pts-vp-pli4.291
msgctxt "pli-tv-bi-vb-pc33:13.1"
msgid "Upasampannāya upasampannasaññā sañcicca aphāsuṃ karoti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:13.2"
msgid "Upasampannāya vematikā sañcicca aphāsuṃ karoti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:13.3"
msgid "Upasampannāya anupasampannasaññā sañcicca aphāsuṃ karoti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc33:14.1"
msgid "Anupasampannāya sañcicca aphāsuṃ karoti, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:14.2"
msgid "Anupasampannāya upasampannasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:14.3"
msgid "Anupasampannāya vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:14.4"
msgid "Anupasampannāya anupasampannasaññā, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc10
msgctxt "pli-tv-bi-vb-pc33:15.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc33:15.2"
msgid "na aphāsuṃ kattukāmā āpucchā purato caṅkamati vā tiṭṭhati vā nisīdati vā seyyaṃ vā kappeti uddisati vā uddisāpeti vā sajjhāyaṃ vā karoti, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc11
msgctxt "pli-tv-bi-vb-pc33:16.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
