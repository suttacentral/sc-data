#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc32"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc32:1.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc32:2.1"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc32:3.1"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc32:4.1"
msgid "Tuvaṭṭavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc32:5.1"
msgid "32. Ekattharaṇatuvaṭṭanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bi-vb-pc32:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:6.2"
msgid "Tena kho pana samayena bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭenti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:6.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:6.4"
msgid "“kathañhi nāma bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc32:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.4"
msgid "“kathañhi nāma bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.5"
msgid "“saccaṃ kira, bhikkhave, bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭentī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.8"
msgid "kathañhi nāma, bhikkhave, bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
#. VAR: ekattharaṇapāvuraṇā → ekattharaṇapāpuraṇā (bj, s1-3)
msgctxt "pli-tv-bi-vb-pc32:8.1"
msgid "“Yā pana bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭeyyuṃ, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc32:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc32:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc32:9.3"
msgid "bhikkhuniyoti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc32:9.4"
msgid "upasampannāyo vuccanti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc32:10.1"
msgid "Dve ekattharaṇapāvuraṇā tuvaṭṭeyyunti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc32:10.2"
msgid "taññeva attharitvā taññeva pārupanti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc32:11.1"
msgid "Ekattharaṇapāvuraṇe ekattharaṇapāvuraṇasaññā tuvaṭṭenti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:11.2"
msgid "Ekattharaṇapāvuraṇe vematikā tuvaṭṭenti, āpatti pācittiyassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:11.3"
msgid "Ekattharaṇapāvuraṇe nānattharaṇapāvuraṇasaññā tuvaṭṭenti, āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
#. VAR: nānāpāvuraṇe → nānāpāpuraṇasaññā (bj) | nānāpāpuraṇe (s1-3) | nānāpāvuraṇasaññā (pts1, mr)
msgctxt "pli-tv-bi-vb-pc32:12.1"
msgid "Ekattharaṇe nānāpāvuraṇe, āpatti dukkaṭassa."
msgstr ""

#. VAR: ekapāvuraṇe → ekapāpuraṇasaññā (bj) | ekapāpuraṇe (s1-3) | ekattharaṇapāvuraṇasaññā (pts1) | ekapāvuraṇasaññā (mr)
msgctxt "pli-tv-bi-vb-pc32:12.2"
msgid "Nānattharaṇe ekapāvuraṇe, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:12.3"
msgid "Nānattharaṇapāvuraṇe ekattharaṇapāvuraṇasaññā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:12.4"
msgid "Nānattharaṇapāvuraṇe vematikā, āpatti dukkaṭassa."
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:12.5"
msgid "Nānattharaṇapāvuraṇe nānattharaṇapāvuraṇasaññā, anāpatti."
msgstr ""

#. HTML: </p><p>
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc32:13.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc32:13.2"
msgid "vavatthānaṃ dassetvā nipajjanti, ummattikānaṃ, ādikammikānanti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc9
msgctxt "pli-tv-bi-vb-pc32:14.1"
msgid "Dutiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""
