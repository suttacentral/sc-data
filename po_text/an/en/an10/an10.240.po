msgstr "giving up …"

msgctxt "an10.240:1.4"
msgid "khayāya …"
msgstr "ending …"

msgctxt "an10.240:1.5"
msgid "vayāya …"
msgstr "vanishing …"

msgctxt "an10.240:1.6"
msgid "virāgāya …"
msgstr "fading away …"

msgctxt "an10.240:1.7"
msgid "nirodhāya …"
msgstr "cessation …"

