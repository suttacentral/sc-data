{
  "kv15.2:0.1": "Kathāvatthu",
  "kv15.2:0.2": "Mahāpaṇṇāsaka",
  "kv15.2:0.3": "Pannarasamavagga",
  "kv15.2:0.4": "Aññamaññapaccayakathā",
  "kv15.2:1.1": "☑ Avijjāpaccayāva saṅkhārā, na vattabbaṁ—",
  "kv15.2:1.2": "“saṅkhārapaccayāpi avijjā”ti?",
  "kv15.2:1.3": "Āmantā.",
  "kv15.2:1.4": "Nanu avijjā saṅkhārena sahajātāti?",
  "kv15.2:1.5": "Āmantā.",
  "kv15.2:1.6": "Hañci avijjā saṅkhārena sahajātā, tena vata re vattabbe—",
  "kv15.2:1.7": "“avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā”ti.",
  "kv15.2:2.1": "☑ Taṇhāpaccayāva upādānaṁ, na vattabbaṁ—",
  "kv15.2:2.2": "“upādānapaccayāpi taṇhā”ti?",
  "kv15.2:2.3": "Āmantā.",
  "kv15.2:2.4": "Nanu taṇhā upādānena sahajātāti?",
  "kv15.2:2.5": "Āmantā.",
  "kv15.2:2.6": "Hañci taṇhā upādānena sahajātā, tena vata re vattabbe—",
  "kv15.2:2.7": "“taṇhāpaccayāpi upādānaṁ, upādānapaccayāpi taṇhā”ti.",
  "kv15.2:3.1": "☒ “Jarāmaraṇapaccayā, bhikkhave, jāti, jātipaccayā bhavo”ti—",
  "kv15.2:3.2": "attheva suttantoti?",
  "kv15.2:3.3": "Natthi.",
  "kv15.2:3.4": "Tena hi avijjāpaccayāva saṅkhārā, na vattabbaṁ—",
  "kv15.2:3.5": "“saṅkhārapaccayāpi avijjā”ti.",
  "kv15.2:3.6": "Taṇhāpaccayāva upādānaṁ, na vattabbaṁ—",
  "kv15.2:3.7": "“upādānapaccayāpi taṇhā”ti.",
  "kv15.2:4.1": "☑ “Viññāṇapaccayā, bhikkhave, nāmarūpaṁ, nāmarūpapaccayāpi viññāṇan”ti—",
  "kv15.2:4.2": "attheva suttantoti?",
  "kv15.2:4.3": "Āmantā.",
  "kv15.2:4.4": "Tena hi avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā;",
  "kv15.2:4.5": "taṇhāpaccayāpi upādānaṁ, upādānapaccayāpi taṇhāti.",
  "kv15.2:4.6": "Aññamaññapaccayakathā niṭṭhitā."
}