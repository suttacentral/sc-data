{
  "kv15.6:0.1": "Kathāvatthu",
  "kv15.6:0.2": "Mahāpaṇṇāsaka",
  "kv15.6:0.3": "Pannarasamavagga",
  "kv15.6:0.4": "Jarāmaraṇakathā",
  "kv15.6:1.1": "☑ Lokuttarānaṁ dhammānaṁ jarāmaraṇaṁ lokuttaranti?",
  "kv15.6:1.2": "Āmantā.",
  "kv15.6:1.3": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.6:1.4": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.5": "sotāpattimaggassa jarāmaraṇaṁ sotāpattimaggoti?",
  "kv15.6:1.6": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.7": "sotāpattimaggassa jarāmaraṇaṁ sotāpattimaggoti?",
  "kv15.6:1.8": "Āmantā.",
  "kv15.6:1.9": "Sotāpattiphalassa jarāmaraṇaṁ sotāpattiphalanti?",
  "kv15.6:1.10": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.11": "sakadāgāmimaggassa …pe…",
  "kv15.6:1.12": "sakadāgāmiphalassa …pe…",
  "kv15.6:1.13": "anāgāmimaggassa …pe…",
  "kv15.6:1.14": "anāgāmiphalassa …pe…",
  "kv15.6:1.15": "arahattamaggassa jarāmaraṇaṁ arahattamaggoti?",
  "kv15.6:1.16": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.17": "arahattamaggassa jarāmaraṇaṁ arahattamaggoti?",
  "kv15.6:1.18": "Āmantā.",
  "kv15.6:1.19": "Arahattaphalassa jarāmaraṇaṁ arahattaphalanti?",
  "kv15.6:1.20": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.21": "satipaṭṭhānānaṁ …",
  "kv15.6:1.22": "sammappadhānānaṁ …",
  "kv15.6:1.23": "iddhipādānaṁ …",
  "kv15.6:1.24": "indriyānaṁ …",
  "kv15.6:1.25": "balānaṁ …",
  "kv15.6:1.26": "bojjhaṅgānaṁ jarāmaraṇaṁ bojjhaṅgoti?",
  "kv15.6:1.27": "Na hevaṁ vattabbe …pe….",
  "kv15.6:2.1": "☒ Na vattabbaṁ—",
  "kv15.6:2.2": "“lokuttarānaṁ dhammānaṁ jarāmaraṇaṁ lokuttaran”ti?",
  "kv15.6:2.3": "Āmantā.",
  "kv15.6:2.4": "Lokiyanti?",
  "kv15.6:2.5": "Na hevaṁ vattabbe.",
  "kv15.6:2.6": "Tena hi lokuttaranti.",
  "kv15.6:2.7": "Jarāmaraṇakathā niṭṭhitā."
}