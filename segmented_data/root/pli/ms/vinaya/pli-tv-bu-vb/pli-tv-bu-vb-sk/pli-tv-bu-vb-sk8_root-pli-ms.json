{
  "pli-tv-bu-vb-sk8:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk8:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk8:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk8:0.4": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk8:0.5": "8. Dutiyaokkhittacakkhusikkhāpada",
  "pli-tv-bu-vb-sk8:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk8:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṁ tahaṁ olokentā antaraghare nisīdanti …pe….",
  "pli-tv-bu-vb-sk8:1.3.1": "“Okkhittacakkhu antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk8:1.4": "Okkhittacakkhunā antaraghare nisīditabbaṁ yugamattaṁ pekkhantena.",
  "pli-tv-bu-vb-sk8:1.5": "Yo anādariyaṁ paṭicca tahaṁ tahaṁ olokento antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk8:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk8:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk8:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk8:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk8:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk8:1.11": "āpadāsu,",
  "pli-tv-bu-vb-sk8:1.12": "ummattakassa,",
  "pli-tv-bu-vb-sk8:1.13": "ādikammikassāti.",
  "pli-tv-bu-vb-sk8:1.14": "Aṭṭhamasikkhāpadaṁ niṭṭhitaṁ."
}