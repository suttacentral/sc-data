{
  "pli-tv-bu-vb-sk39:0.1": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk39:0.2": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk39:0.3": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk39:0.4": "Sakkaccavagga",
  "pli-tv-bu-vb-sk39:0.5": "39. Kabaḷasikkhāpada",
  "pli-tv-bu-vb-sk39:1.1": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk39:1.2": "Tena kho pana samayena chabbaggiyā bhikkhū mahantaṁ kabaḷaṁ karonti …pe…",
  "pli-tv-bu-vb-sk39:1.3.1": "“Nātimahantaṁ kabaḷaṁ karissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk39:1.4": "Nātimahanto kabaḷo kātabbo.",
  "pli-tv-bu-vb-sk39:1.5": "Yo anādariyaṁ paṭicca mahantaṁ kabaḷaṁ karoti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk39:1.6.1": "Anāpatti—",
  "pli-tv-bu-vb-sk39:1.7": "asañcicca,",
  "pli-tv-bu-vb-sk39:1.8": "assatiyā,",
  "pli-tv-bu-vb-sk39:1.9": "ajānantassa,",
  "pli-tv-bu-vb-sk39:1.10": "gilānassa,",
  "pli-tv-bu-vb-sk39:1.11": "khajjake,",
  "pli-tv-bu-vb-sk39:1.12": "phalāphale,",
  "pli-tv-bu-vb-sk39:1.13": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk39:1.14": "āpadāsu,",
  "pli-tv-bu-vb-sk39:1.15": "ummattakassa,",
  "pli-tv-bu-vb-sk39:1.16": "ādikammikassāti.",
  "pli-tv-bu-vb-sk39:1.17": "Navamasikkhāpadaṁ niṭṭhitaṁ."
}