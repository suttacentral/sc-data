{
  "sn32.1:0.1": "Saṁyutta Nikāya 32 ",
  "sn32.1:0.2": "1. Valāhakavagga ",
  "sn32.1:0.3": "1. Suddhikasutta ",
  "sn32.1:1.1": "Sāvatthinidānaṁ. ",
  "sn32.1:1.2": "“Valāhakakāyike vo, bhikkhave, deve desessāmi. ",
  "sn32.1:1.3": "Taṁ suṇātha. ",
  "sn32.1:1.4": "Katame ca, bhikkhave, valāhakakāyikā devā? ",
  "sn32.1:1.5": "Santi, bhikkhave, sītavalāhakā devā; ",
  "sn32.1:1.6": "santi uṇhavalāhakā devā; ",
  "sn32.1:1.7": "santi abbhavalāhakā devā; ",
  "sn32.1:1.8": "santi vātavalāhakā devā; ",
  "sn32.1:1.9": "santi vassavalāhakā devā—",
  "sn32.1:1.10": "ime vuccanti, bhikkhave, ‘valāhakakāyikā devā’”ti. ",
  "sn32.1:1.11": "Paṭhamaṁ. "
}