{
  "sn35.156:0.1": "Saṁyutta Nikāya 35 ",
  "sn35.156:0.2": "16. Nandikkhayavagga ",
  "sn35.156:0.3": "156. Ajjhattanandikkhayasutta ",
  "sn35.156:1.1": "“Aniccaṁyeva, bhikkhave, bhikkhu cakkhuṁ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.2": "Sammā passaṁ nibbindati. ",
  "sn35.156:1.3": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.4": "rāgakkhayā nandikkhayo. ",
  "sn35.156:1.5": "Nandirāgakkhayā cittaṁ suvimuttanti vuccati …pe… ",
  "sn35.156:1.6": "aniccaṁyeva, bhikkhave, bhikkhu jivhaṁ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.7": "Sammā passaṁ nibbindati. ",
  "sn35.156:1.8": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.9": "rāgakkhayā …pe… ",
  "sn35.156:1.10": "cittaṁ suvimuttanti vuccati …pe… ",
  "sn35.156:1.11": "aniccaṁyeva, bhikkhave, bhikkhu manaṁ aniccanti passati, sāssa hoti sammādiṭṭhi. ",
  "sn35.156:1.12": "Sammā passaṁ nibbindati. ",
  "sn35.156:1.13": "Nandikkhayā rāgakkhayo; ",
  "sn35.156:1.14": "rāgakkhayā nandikkhayo. ",
  "sn35.156:1.15": "Nandirāgakkhayā cittaṁ suvimuttanti vuccatī”ti. ",
  "sn35.156:1.16": "Paṭhamaṁ. "
}