{
  "sn50.35-44:0.1": "Saṁyutta Nikāya 50 ",
  "sn50.35-44:0.2": "4. Esanāvagga ",
  "sn50.35-44:0.3": "35. Esanāvagga ",
  "sn50.35-44:1.1": "(Esanāvaggo vitthāretabbo.) ",
  "sn50.35-44:2.0": "Tassuddānaṁ",
  "sn50.35-44:2.1": "Esanā vidhā āsavo, ",
  "sn50.35-44:2.2": "Bhavo ca dukkhatā tisso; ",
  "sn50.35-44:2.3": "Khilaṁ malañca nīgho ca, ",
  "sn50.35-44:2.4": "Vedanā taṇhā tasinā cāti. "
}