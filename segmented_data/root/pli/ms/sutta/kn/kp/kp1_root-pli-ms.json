{
  "kp1:0.1": "Khuddakapāṭha",
  "kp1:0.2": "1. Saraṇattaya",
  "kp1:1.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "kp1:2.1": "Buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:2.2": "Dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:2.3": "Saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:3.1": "Dutiyampi buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:3.2": "Dutiyampi dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:3.3": "Dutiyampi saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:4.1": "Tatiyampi buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:4.2": "Tatiyampi dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:4.3": "Tatiyampi saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:5.1": "Saraṇattayaṁ."
}