{
  "tha-ap71:0.1": "Therāpadāna",
  "tha-ap71:0.2": "Sakacintaniyavagga",
  "tha-ap71:0.3": "9. Sumanattheraapadāna",
  "tha-ap71:1.1": "“Sumano nāma nāmena,",
  "tha-ap71:1.2": "mālākāro ahaṁ tadā;",
  "tha-ap71:1.3": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap71:1.4": "lokāhutipaṭiggahaṁ.",
  "tha-ap71:2.1": "Ubho hatthehi paggayha,",
  "tha-ap71:2.2": "sumanaṁ pupphamuttamaṁ;",
  "tha-ap71:2.3": "Buddhassa abhiropesiṁ,",
  "tha-ap71:2.4": "sikhino lokabandhuno.",
  "tha-ap71:3.1": "Imāya pupphapūjāya,",
  "tha-ap71:3.2": "cetanāpaṇidhīhi ca;",
  "tha-ap71:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap71:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap71:4.1": "Ekattiṁse ito kappe,",
  "tha-ap71:4.2": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap71:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap71:4.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap71:5.1": "Chabbīsatimhi kappamhi,",
  "tha-ap71:5.2": "cattārosuṁ mahāyasā;",
  "tha-ap71:5.3": "Sattaratanasampannā,",
  "tha-ap71:5.4": "rājāno cakkavattino.",
  "tha-ap71:6.1": "Paṭisambhidā catasso,",
  "tha-ap71:6.2": "…pe…",
  "tha-ap71:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap71:7.1": "Itthaṁ sudaṁ āyasmā sumano thero imā gāthāyo abhāsitthāti.",
  "tha-ap71:8.1": "Sumanattherassāpadānaṁ navamaṁ."
}