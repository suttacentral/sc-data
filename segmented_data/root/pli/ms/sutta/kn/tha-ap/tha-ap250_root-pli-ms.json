{
  "tha-ap250:0.1": "Therāpadāna",
  "tha-ap250:0.2": "Tuvaradāyakavagga",
  "tha-ap250:0.3": "8 Sattalipupphapūjakattheraapadāna",
  "tha-ap250:1.1": "“Satta sattalipupphāni,",
  "tha-ap250:1.2": "sīse katvānahaṁ tadā;",
  "tha-ap250:1.3": "Buddhassa abhiropesiṁ,",
  "tha-ap250:1.4": "vessabhumhi naruttame.",
  "tha-ap250:2.1": "Ekattiṁse ito kappe,",
  "tha-ap250:2.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap250:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap250:2.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap250:3.1": "Paṭisambhidā catasso,",
  "tha-ap250:3.2": "…pe…",
  "tha-ap250:3.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap250:4.1": "Itthaṁ sudaṁ āyasmā sattalipupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap250:5.1": "Sattalipupphapūjakattherassāpadānaṁ aṭṭhamaṁ."
}