{
  "tha-ap278:0.1": "Therāpadāna",
  "tha-ap278:0.2": "Suvaṇṇabibbohanavagga",
  "tha-ap278:0.3": "6 Potthakadāyakattheraapadāna",
  "tha-ap278:1.1": "“Satthāraṁ dhammamārabbha,",
  "tha-ap278:1.2": "saṅghañcāpi mahesinaṁ;",
  "tha-ap278:1.3": "Potthadānaṁ mayā dinnaṁ,",
  "tha-ap278:1.4": "dakkhiṇeyye anuttare.",
  "tha-ap278:2.1": "Ekanavutito kappe,",
  "tha-ap278:2.2": "yaṁ kammamakariṁ tadā;",
  "tha-ap278:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap278:2.4": "potthadānassidaṁ phalaṁ.",
  "tha-ap278:3.1": "Paṭisambhidā catasso,",
  "tha-ap278:3.2": "…pe…",
  "tha-ap278:3.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap278:4.1": "Itthaṁ sudaṁ āyasmā potthakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap278:5.1": "Potthakadāyakattherassāpadānaṁ chaṭṭhaṁ."
}