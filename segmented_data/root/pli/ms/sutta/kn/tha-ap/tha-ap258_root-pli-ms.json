{
  "tha-ap258:0.1": "Therāpadāna",
  "tha-ap258:0.2": "Thomakavagga",
  "tha-ap258:0.3": "6. Upāhanadāyakattheraapadāna",
  "tha-ap258:1.1": "“Ahosiṁ candano nāma,",
  "tha-ap258:1.2": "sambuddhassatrajo tadā;",
  "tha-ap258:1.3": "Ekopāhano mayā dinno,",
  "tha-ap258:1.4": "bodhiṁ sampajja me tuvaṁ.",
  "tha-ap258:2.1": "Ekanavutito kappe,",
  "tha-ap258:2.2": "yaṁ pānadhiṁ dadiṁ tadā;",
  "tha-ap258:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap258:2.4": "upāhanassidaṁ phalaṁ.",
  "tha-ap258:3.1": "Paṭisambhidā catasso,",
  "tha-ap258:3.2": "…pe…",
  "tha-ap258:3.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap258:4.1": "Itthaṁ sudaṁ āyasmā upāhanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap258:5.1": "Upāhanadāyakattherassāpadānaṁ chaṭṭhaṁ."
}