{
  "tha-ap111:0.1": "Therāpadāna",
  "tha-ap111:0.2": "Bhikkhadāyivagga",
  "tha-ap111:0.3": "9. Biḷālidāyakattheraapadāna",
  "tha-ap111:1.1": "“Himavantassāvidūre,",
  "tha-ap111:1.2": "vasāmi paṇṇasanthare;",
  "tha-ap111:1.3": "Ghāsesu gedhamāpanno,",
  "tha-ap111:1.4": "seyyasīlo cahaṃ tadā.",
  "tha-ap111:2.1": "Khaṇantālu kalambāni,",
  "tha-ap111:2.2": "biḷālitakkalāni ca;",
  "tha-ap111:2.3": "Kolaṃ bhallātakaṃ billaṃ,",
  "tha-ap111:2.4": "āhatvā paṭiyāditaṃ.",
  "tha-ap111:3.1": "Padumuttaro lokavidū,",
  "tha-ap111:3.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap111:3.3": "Mama saṅkappamaññāya,",
  "tha-ap111:3.4": "āgacchi mama santikaṃ.",
  "tha-ap111:4.1": "Upāgataṃ mahānāgaṃ,",
  "tha-ap111:4.2": "devadevaṃ narāsabhaṃ;",
  "tha-ap111:4.3": "Biḷāliṃ paggahetvāna,",
  "tha-ap111:4.4": "pattamhi okiriṃ ahaṃ.",
  "tha-ap111:5.1": "Paribhuñji mahāvīro,",
  "tha-ap111:5.2": "tosayanto mamaṃ tadā;",
  "tha-ap111:5.3": "Paribhuñjitvāna sabbaññū,",
  "tha-ap111:5.4": "imaṃ gāthaṃ abhāsatha.",
  "tha-ap111:6.1": "‘Sakaṃ cittaṃ pasādetvā,",
  "tha-ap111:6.2": "biḷāliṃ me adā tuvaṃ;",
  "tha-ap111:6.3": "Kappānaṃ satasahassaṃ,",
  "tha-ap111:6.4": "duggatiṃ nupapajjasi’.",
  "tha-ap111:7.1": "Carimaṃ vattate mayhaṃ,",
  "tha-ap111:7.2": "bhavā sabbe samūhatā;",
  "tha-ap111:7.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap111:7.4": "sammāsambuddhasāsane.",
  "tha-ap111:8.1": "Catupaññāsito kappe,",
  "tha-ap111:8.2": "sumekhaliyasavhayo;",
  "tha-ap111:8.3": "Sattaratanasampanno,",
  "tha-ap111:8.4": "cakkavattī mahabbalo.",
  "tha-ap111:9.1": "Paṭisambhidā catasso,",
  "tha-ap111:9.2": "…pe…",
  "tha-ap111:9.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap111:10.1": "Itthaṃ sudaṃ āyasmā biḷālidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap111:11.1": "Biḷālidāyakattherassāpadānaṃ navamaṃ."
}