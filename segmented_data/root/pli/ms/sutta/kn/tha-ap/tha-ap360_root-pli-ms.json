{
  "tha-ap360:0.1": "Therāpadāna",
  "tha-ap360:0.2": "Saddasaññakavagga",
  "tha-ap360:0.3": "8 Mañcadāyakattheraapadāna",
  "tha-ap360:1.1": "“Vipassino bhagavato,",
  "tha-ap360:1.2": "lokajeṭṭhassa tādino;",
  "tha-ap360:1.3": "Ekaṃ mañcaṃ mayā dinnaṃ,",
  "tha-ap360:1.4": "pasannena sapāṇinā.",
  "tha-ap360:2.1": "Hatthiyānaṃ assayānaṃ,",
  "tha-ap360:2.2": "dibbayānaṃ samajjhagaṃ;",
  "tha-ap360:2.3": "Tena mañcakadānena,",
  "tha-ap360:2.4": "pattomhi āsavakkhayaṃ.",
  "tha-ap360:3.1": "Ekanavutito kappe,",
  "tha-ap360:3.2": "yaṃ mañcamadadiṃ tadā;",
  "tha-ap360:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap360:3.4": "mañcadānassidaṃ phalaṃ.",
  "tha-ap360:4.1": "Paṭisambhidā catasso,",
  "tha-ap360:4.2": "…pe…",
  "tha-ap360:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap360:5.1": "Itthaṃ sudaṃ āyasmā mañcadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap360:6.1": "Mañcadāyakattherassāpadānaṃ aṭṭhamaṃ."
}