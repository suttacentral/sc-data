{
  "tha-ap84:0.1": "Therāpadāna",
  "tha-ap84:0.2": "Timiravagga",
  "tha-ap84:0.3": "2. Gatasaññakattheraapadāna",
  "tha-ap84:1.1": "“Jātiyā sattavassohaṁ,",
  "tha-ap84:1.2": "pabbajiṁ anagāriyaṁ;",
  "tha-ap84:1.3": "Avandiṁ satthuno pāde,",
  "tha-ap84:1.4": "vippasannena cetasā.",
  "tha-ap84:2.1": "Sattanaṅgalakīpupphe,",
  "tha-ap84:2.2": "ākāse ukkhipiṁ ahaṁ;",
  "tha-ap84:2.3": "Tissaṁ buddhaṁ samuddissa,",
  "tha-ap84:2.4": "anantaguṇasāgaraṁ.",
  "tha-ap84:3.1": "Sugatānugataṁ maggaṁ,",
  "tha-ap84:3.2": "pūjetvā haṭṭhamānaso;",
  "tha-ap84:3.3": "Añjaliñca tadākāsiṁ,",
  "tha-ap84:3.4": "pasanno sehi pāṇibhi.",
  "tha-ap84:4.1": "Dvenavute ito kappe,",
  "tha-ap84:4.2": "yaṁ kammamakariṁ tadā;",
  "tha-ap84:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap84:4.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap84:5.1": "Ito aṭṭhamake kappe,",
  "tha-ap84:5.2": "tayo aggisikhā ahu;",
  "tha-ap84:5.3": "Sattaratanasampannā,",
  "tha-ap84:5.4": "cakkavattī mahabbalā.",
  "tha-ap84:6.1": "Paṭisambhidā catasso,",
  "tha-ap84:6.2": "…pe…",
  "tha-ap84:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap84:7.1": "Itthaṁ sudaṁ āyasmā gatasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap84:8.1": "Gatasaññakattherassāpadānaṁ dutiyaṁ."
}