{
  "tha-ap475:0.1": "Therāpadāna",
  "tha-ap475:0.2": "Naḷamālivagga",
  "tha-ap475:0.3": "3. Ukkāsatikattheraapadāna",
  "tha-ap475:1.1": "“Kosiyo nāma bhagavā,",
  "tha-ap475:1.2": "cittakūṭe vasī tadā;",
  "tha-ap475:1.3": "Jhāyī jhānarato buddho,",
  "tha-ap475:1.4": "vivekābhirato muni.",
  "tha-ap475:2.1": "Ajjhogāhetvā himavantaṃ,",
  "tha-ap475:2.2": "Nārīgaṇapurakkhato;",
  "tha-ap475:2.3": "Addasaṃ kosiyaṃ buddhaṃ,",
  "tha-ap475:2.4": "Puṇṇamāyeva candimaṃ.",
  "tha-ap475:3.1": "Ukkāsate gahetvāna,",
  "tha-ap475:3.2": "parivāresahaṃ tadā;",
  "tha-ap475:3.3": "Sattarattindivaṃ ṭhatvā,",
  "tha-ap475:3.4": "aṭṭhamena agacchahaṃ.",
  "tha-ap475:4.1": "Vuṭṭhitaṃ kosiyaṃ buddhaṃ,",
  "tha-ap475:4.2": "sayambhuṃ aparājitaṃ;",
  "tha-ap475:4.3": "Pasannacitto vanditvā,",
  "tha-ap475:4.4": "ekaṃ bhikkhaṃ adāsahaṃ.",
  "tha-ap475:5.1": "Tena kammena dvipadinda,",
  "tha-ap475:5.2": "lokajeṭṭha narāsabha;",
  "tha-ap475:5.3": "Uppajjiṃ tusite kāye,",
  "tha-ap475:5.4": "ekabhikkhāyidaṃ phalaṃ.",
  "tha-ap475:6.1": "Divasañceva rattiñca,",
  "tha-ap475:6.2": "āloko hoti me sadā;",
  "tha-ap475:6.3": "Samantā yojanasataṃ,",
  "tha-ap475:6.4": "obhāsena pharāmahaṃ.",
  "tha-ap475:7.1": "Pañcapaññāsakappamhi,",
  "tha-ap475:7.2": "cakkavattī ahosahaṃ;",
  "tha-ap475:7.3": "Cāturanto vijitāvī,",
  "tha-ap475:7.4": "jambumaṇḍassa issaro.",
  "tha-ap475:8.1": "Tadā me nagaraṃ āsi,",
  "tha-ap475:8.2": "iddhaṃ phītaṃ sunimmitaṃ;",
  "tha-ap475:8.3": "Tiṃsayojanamāyāmaṃ,",
  "tha-ap475:8.4": "vitthārena ca vīsati.",
  "tha-ap475:9.1": "Sobhanaṃ nāma nagaraṃ,",
  "tha-ap475:9.2": "vissakammena māpitaṃ;",
  "tha-ap475:9.3": "Dasasaddāvivittaṃ taṃ,",
  "tha-ap475:9.4": "sammatāḷasamāhitaṃ.",
  "tha-ap475:10.1": "Na tamhi nagare atthi,",
  "tha-ap475:10.2": "vallikaṭṭhañca mattikā;",
  "tha-ap475:10.3": "Sabbasoṇṇamayaṃyeva,",
  "tha-ap475:10.4": "jotate niccakālikaṃ.",
  "tha-ap475:11.1": "Catupākāraparikkhittaṃ,",
  "tha-ap475:11.2": "tayo āsuṃ maṇimayā;",
  "tha-ap475:11.3": "Vemajjhe tālapantī ca,",
  "tha-ap475:11.4": "vissakammena māpitā.",
  "tha-ap475:12.1": "Dasasahassapokkharañño,",
  "tha-ap475:12.2": "padumuppalachāditā;",
  "tha-ap475:12.3": "Puṇḍarīkehi sañchannā,",
  "tha-ap475:12.4": "nānāgandhasamīritā.",
  "tha-ap475:13.1": "Catunnavutito kappe,",
  "tha-ap475:13.2": "yaṃ ukkaṃ dhārayiṃ ahaṃ;",
  "tha-ap475:13.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap475:13.4": "ukkādhārassidaṃ phalaṃ.",
  "tha-ap475:14.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap475:14.2": "…pe…",
  "tha-ap475:14.3": "viharāmi anāsavo.",
  "tha-ap475:15.1": "Svāgataṃ vata me āsi,",
  "tha-ap475:15.2": "…pe…",
  "tha-ap475:15.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap475:16.1": "Paṭisambhidā catasso,",
  "tha-ap475:16.2": "…pe…",
  "tha-ap475:16.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap475:17.1": "Itthaṃ sudaṃ āyasmā ukkāsatiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap475:18.1": "Ukkāsatikattherassāpadānaṃ tatiyaṃ."
}