{
  "tha-ap350:0.1": "Therāpadāna",
  "tha-ap350:0.2": "Ekapadumiyavagga",
  "tha-ap350:0.3": "8. Tiṇamuṭṭhidāyakattheraapadāna",
  "tha-ap350:1.1": "“Himavantassāvidūre,",
  "tha-ap350:1.2": "lambako nāma pabbato;",
  "tha-ap350:1.3": "Upatisso nāma sambuddho,",
  "tha-ap350:1.4": "abbhokāsamhi caṅkami.",
  "tha-ap350:2.1": "Migaluddo tadā āsiṃ,",
  "tha-ap350:2.2": "araññe kānane ahaṃ;",
  "tha-ap350:2.3": "Disvāna taṃ devadevaṃ,",
  "tha-ap350:2.4": "sayambhuṃ aparājitaṃ.",
  "tha-ap350:3.1": "Vippasannena cittena,",
  "tha-ap350:3.2": "tadā tassa mahesino;",
  "tha-ap350:3.3": "Nisīdanatthaṃ buddhassa,",
  "tha-ap350:3.4": "tiṇamuṭṭhimadāsahaṃ.",
  "tha-ap350:4.1": "Datvāna devadevassa,",
  "tha-ap350:4.2": "bhiyyo cittaṃ pasādayiṃ;",
  "tha-ap350:4.3": "Sambuddhaṃ abhivādetvā,",
  "tha-ap350:4.4": "pakkāmiṃ uttarāmukho.",
  "tha-ap350:5.1": "Aciraṃ gatamattaṃ maṃ,",
  "tha-ap350:5.2": "migarājā apothayi;",
  "tha-ap350:5.3": "Sīhena pothito santo,",
  "tha-ap350:5.4": "tattha kālaṅkato ahaṃ.",
  "tha-ap350:6.1": "Āsanne me kataṃ kammaṃ,",
  "tha-ap350:6.2": "buddhaseṭṭhe anāsave;",
  "tha-ap350:6.3": "Sumutto saravegova,",
  "tha-ap350:6.4": "devalokaṃ agañchahaṃ.",
  "tha-ap350:7.1": "Yūpo tattha subho āsi,",
  "tha-ap350:7.2": "puññakammābhinimmito;",
  "tha-ap350:7.3": "Sahassakaṇḍo satabheṇḍu,",
  "tha-ap350:7.4": "dhajālu haritāmayo.",
  "tha-ap350:8.1": "Pabhā niddhāvate tassa,",
  "tha-ap350:8.2": "sataraṃsīva uggato;",
  "tha-ap350:8.3": "Ākiṇṇo devakaññāhi,",
  "tha-ap350:8.4": "āmodiṃ kāmakāmahaṃ.",
  "tha-ap350:9.1": "Devalokā cavitvāna,",
  "tha-ap350:9.2": "sukkamūlena codito;",
  "tha-ap350:9.3": "Āgantvāna manussattaṃ,",
  "tha-ap350:9.4": "pattomhi āsavakkhayaṃ.",
  "tha-ap350:10.1": "Catunnavutito kappe,",
  "tha-ap350:10.2": "nisīdanamadāsahaṃ;",
  "tha-ap350:10.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap350:10.4": "tiṇamuṭṭhiyidaṃ phalaṃ.",
  "tha-ap350:11.1": "Paṭisambhidā catasso,",
  "tha-ap350:11.2": "…pe…",
  "tha-ap350:11.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap350:12.1": "Itthaṃ sudaṃ āyasmā tiṇamuṭṭhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap350:13.1": "Tiṇamuṭṭhidāyakattherassāpadānaṃ aṭṭhamaṃ."
}