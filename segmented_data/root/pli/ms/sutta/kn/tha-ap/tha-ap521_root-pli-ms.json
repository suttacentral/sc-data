{
  "tha-ap521:0.1": "Therāpadāna",
  "tha-ap521:0.2": "Phaladāyakavagga",
  "tha-ap521:0.3": "9. Sabbaphaladāyakattheraapadāna",
  "tha-ap521:1.1": "“Varuṇo nāma nāmena,",
  "tha-ap521:1.2": "brāhmaṇo mantapāragū;",
  "tha-ap521:1.3": "Chaḍḍetvā dasa puttāni,",
  "tha-ap521:1.4": "vanamajjhogahiṃ tadā.",
  "tha-ap521:2.1": "Assamaṃ sukataṃ katvā,",
  "tha-ap521:2.2": "suvibhattaṃ manoramaṃ;",
  "tha-ap521:2.3": "Paṇṇasālaṃ karitvāna,",
  "tha-ap521:2.4": "vasāmi vipine ahaṃ.",
  "tha-ap521:3.1": "Padumuttaro lokavidū,",
  "tha-ap521:3.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap521:3.3": "Mamuddharitukāmo so,",
  "tha-ap521:3.4": "āgacchi mama assamaṃ.",
  "tha-ap521:4.1": "Yāvatā vanasaṇḍamhi,",
  "tha-ap521:4.2": "obhāso vipulo ahu;",
  "tha-ap521:4.3": "Buddhassa ānubhāvena,",
  "tha-ap521:4.4": "pajjalī vipinaṃ tadā.",
  "tha-ap521:5.1": "Disvāna taṃ pāṭihīraṃ,",
  "tha-ap521:5.2": "buddhaseṭṭhassa tādino;",
  "tha-ap521:5.3": "Pattapuṭaṃ gahetvāna,",
  "tha-ap521:5.4": "phalena pūjayiṃ ahaṃ.",
  "tha-ap521:6.1": "Upagantvāna sambuddhaṃ,",
  "tha-ap521:6.2": "sahakhārimadāsahaṃ;",
  "tha-ap521:6.3": "Anukampāya me buddho,",
  "tha-ap521:6.4": "idaṃ vacanamabravi.",
  "tha-ap521:7.1": "‘Khāribhāraṃ gahetvāna,",
  "tha-ap521:7.2": "pacchato ehi me tuvaṃ;",
  "tha-ap521:7.3": "Paribhutte ca saṃghamhi,",
  "tha-ap521:7.4": "puññaṃ tava bhavissati’.",
  "tha-ap521:8.1": "Puṭakantaṃ gahetvāna,",
  "tha-ap521:8.2": "bhikkhusaṃghassadāsahaṃ;",
  "tha-ap521:8.3": "Tattha cittaṃ pasādetvā,",
  "tha-ap521:8.4": "tusitaṃ upapajjahaṃ.",
  "tha-ap521:9.1": "Tattha dibbehi naccehi,",
  "tha-ap521:9.2": "gītehi vāditehi ca;",
  "tha-ap521:9.3": "Puññakammena saṃyuttaṃ,",
  "tha-ap521:9.4": "anubhomi sadā sukhaṃ.",
  "tha-ap521:10.1": "Yaṃ yaṃ yonupapajjāmi,",
  "tha-ap521:10.2": "devattaṃ atha mānusaṃ;",
  "tha-ap521:10.3": "Bhoge me ūnatā natthi,",
  "tha-ap521:10.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:11.1": "Yāvatā caturo dīpā,",
  "tha-ap521:11.2": "sasamuddā sapabbatā;",
  "tha-ap521:11.3": "Phalaṃ buddhassa datvāna,",
  "tha-ap521:11.4": "issaraṃ kārayāmahaṃ.",
  "tha-ap521:12.1": "Yāvatā me pakkhigaṇā,",
  "tha-ap521:12.2": "ākāse uppatanti ce;",
  "tha-ap521:12.3": "Tepi maṃ vasamanventi,",
  "tha-ap521:12.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:13.1": "Yāvatā vanasaṇḍamhi,",
  "tha-ap521:13.2": "yakkhā bhūtā ca rakkhasā;",
  "tha-ap521:13.3": "Kumbhaṇḍā garuḷā cāpi,",
  "tha-ap521:13.4": "pāricariyaṃ upenti me.",
  "tha-ap521:14.1": "Kummā soṇā madhukārā,",
  "tha-ap521:14.2": "ḍaṃsā ca makasā ubho;",
  "tha-ap521:14.3": "Tepi maṃ vasamanventi,",
  "tha-ap521:14.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:15.1": "Supaṇṇā nāma sakuṇā,",
  "tha-ap521:15.2": "pakkhijātā mahabbalā;",
  "tha-ap521:15.3": "Tepi maṃ saraṇaṃ yanti,",
  "tha-ap521:15.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:16.1": "Yepi dīghāyukā nāgā,",
  "tha-ap521:16.2": "iddhimanto mahāyasā;",
  "tha-ap521:16.3": "Tepi maṃ vasamanventi,",
  "tha-ap521:16.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:17.1": "Sīhā byagghā ca dīpī ca,",
  "tha-ap521:17.2": "acchakokataracchakā;",
  "tha-ap521:17.3": "Tepi maṃ vasamanventi,",
  "tha-ap521:17.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:18.1": "Osadhītiṇavāsī ca,",
  "tha-ap521:18.2": "ye ca ākāsavāsino;",
  "tha-ap521:18.3": "Sabbe maṃ saraṇaṃ yanti,",
  "tha-ap521:18.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:19.1": "Sududdasaṃ sunipuṇaṃ,",
  "tha-ap521:19.2": "gambhīraṃ suppakāsitaṃ;",
  "tha-ap521:19.3": "Phassayitvā viharāmi,",
  "tha-ap521:19.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:20.1": "Vimokkhe aṭṭha phusitvā,",
  "tha-ap521:20.2": "viharāmi anāsavo;",
  "tha-ap521:20.3": "Ātāpī nipako cāhaṃ,",
  "tha-ap521:20.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:21.1": "Ye phalaṭṭhā buddhaputtā,",
  "tha-ap521:21.2": "khīṇadosā mahāyasā;",
  "tha-ap521:21.3": "Ahamaññataro tesaṃ,",
  "tha-ap521:21.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:22.1": "Abhiññāpāramiṃ gantvā,",
  "tha-ap521:22.2": "sukkamūlena codito;",
  "tha-ap521:22.3": "Sabbāsave pariññāya,",
  "tha-ap521:22.4": "viharāmi anāsavo.",
  "tha-ap521:23.1": "Tevijjā iddhipattā ca,",
  "tha-ap521:23.2": "buddhaputtā mahāyasā;",
  "tha-ap521:23.3": "Dibbasotasamāpannā,",
  "tha-ap521:23.4": "tesaṃ aññataro ahaṃ.",
  "tha-ap521:24.1": "Satasahassito kappe,",
  "tha-ap521:24.2": "yaṃ phalaṃ adadiṃ tadā;",
  "tha-ap521:24.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap521:24.4": "phaladānassidaṃ phalaṃ.",
  "tha-ap521:25.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap521:25.2": "…pe…",
  "tha-ap521:25.3": "viharāmi anāsavo.",
  "tha-ap521:26.1": "Svāgataṃ vata me āsi,",
  "tha-ap521:26.2": "…pe…",
  "tha-ap521:26.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap521:27.1": "Paṭisambhidā catasso,",
  "tha-ap521:27.2": "…pe…",
  "tha-ap521:27.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap521:28.1": "Itthaṃ sudaṃ āyasmā sabbaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap521:29.1": "Sabbaphaladāyakattherassāpadānaṃ navamaṃ."
}