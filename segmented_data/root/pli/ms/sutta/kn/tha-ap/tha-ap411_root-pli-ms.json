{
  "tha-ap411:0.1": "Therāpadāna",
  "tha-ap411:0.2": "Metteyyavagga",
  "tha-ap411:0.3": "9. Jatukaṇṇittheraapadāna",
  "tha-ap411:1.1": "“Nagare haṁsavatiyā,",
  "tha-ap411:1.2": "seṭṭhiputto ahosahaṁ;",
  "tha-ap411:1.3": "Samappito kāmaguṇe,",
  "tha-ap411:1.4": "paricāremahaṁ tadā.",
  "tha-ap411:2.1": "Tato pāsādamāruyha,",
  "tha-ap411:2.2": "mahābhoge valañjako;",
  "tha-ap411:2.3": "Tattha naccehi gītehi,",
  "tha-ap411:2.4": "paricāremahaṁ tadā.",
  "tha-ap411:3.1": "Tūriyā āhatā mayhaṁ,",
  "tha-ap411:3.2": "sammatāḷasamāhitā;",
  "tha-ap411:3.3": "Naccantā itthiyo sabbā,",
  "tha-ap411:3.4": "harantiyeva me mano.",
  "tha-ap411:4.1": "Celāpikā vāmanikā,",
  "tha-ap411:4.2": "kuñjavāsī timajjhikā;",
  "tha-ap411:4.3": "Laṅghikā sokajjhāyī ca,",
  "tha-ap411:4.4": "parivārenti maṁ sadā.",
  "tha-ap411:5.1": "Vetāḷino kumbhathūnī,",
  "tha-ap411:5.2": "naṭā ca naccakā bahū;",
  "tha-ap411:5.3": "Naṭakā nāṭakā ceva,",
  "tha-ap411:5.4": "parivārenti maṁ sadā.",
  "tha-ap411:6.1": "Kappakā nhāpakā sūdā,",
  "tha-ap411:6.2": "mālākārā supāsakā;",
  "tha-ap411:6.3": "Jallā mallā ca te sabbe,",
  "tha-ap411:6.4": "parivārenti maṁ sadā.",
  "tha-ap411:7.1": "Etesu kīḷamānesu,",
  "tha-ap411:7.2": "sikkhite katupāsane;",
  "tha-ap411:7.3": "Rattindivaṁ na jānāmi,",
  "tha-ap411:7.4": "indova tidasaṅgaṇe.",
  "tha-ap411:8.1": "Addhikā pathikā sabbe,",
  "tha-ap411:8.2": "yācakā varakā bahū;",
  "tha-ap411:8.3": "Upagacchanti te niccaṁ,",
  "tha-ap411:8.4": "bhikkhayantā mamaṁ gharaṁ.",
  "tha-ap411:9.1": "Samaṇā brāhmaṇā ceva,",
  "tha-ap411:9.2": "puññakkhettā anuttarā;",
  "tha-ap411:9.3": "Vaḍḍhayantā mamaṁ puññaṁ,",
  "tha-ap411:9.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:10.1": "Paṭagā laṭukā sabbe,",
  "tha-ap411:10.2": "nigaṇṭhā pupphasāṭakā;",
  "tha-ap411:10.3": "Tedaṇḍikā ekasikhā,",
  "tha-ap411:10.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:11.1": "Ājīvakā viluttāvī,",
  "tha-ap411:11.2": "godhammā devadhammikā;",
  "tha-ap411:11.3": "Rajojalladharā ete,",
  "tha-ap411:11.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:12.1": "Parittakā santipattā,",
  "tha-ap411:12.2": "kodhapugganikā bahū;",
  "tha-ap411:12.3": "Tapassī vanacārī ca,",
  "tha-ap411:12.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:13.1": "Oḍḍakā damiḷā ceva,",
  "tha-ap411:13.2": "sākuḷā malavāḷakā;",
  "tha-ap411:13.3": "Savarā yonakā ceva,",
  "tha-ap411:13.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:14.1": "Andhakā muṇḍakā sabbe,",
  "tha-ap411:14.2": "koṭalā hanuvindakā;",
  "tha-ap411:14.3": "Ārāvacīnaraṭṭhā ca,",
  "tha-ap411:14.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:15.1": "Alasandakā pallavakā,",
  "tha-ap411:15.2": "dhammarā niggamānusā;",
  "tha-ap411:15.3": "Gehikā cetaputtā ca,",
  "tha-ap411:15.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:16.1": "Mādhurakā kosalakā,",
  "tha-ap411:16.2": "kaliṅgā hatthiporikā;",
  "tha-ap411:16.3": "Isiṇḍā makkalā ceva,",
  "tha-ap411:16.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:17.1": "Celāvakā ārabbhā ca,",
  "tha-ap411:17.2": "oghuḷhā meghalā bahū;",
  "tha-ap411:17.3": "Khuddakā suddakā ceva,",
  "tha-ap411:17.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:18.1": "Rohaṇā sindhavā ceva,",
  "tha-ap411:18.2": "citakā ekakaṇṇikā;",
  "tha-ap411:18.3": "Suraṭṭhā aparantā ca,",
  "tha-ap411:18.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:19.1": "Suppārakā kumārā ca,",
  "tha-ap411:19.2": "mallasovaṇṇabhūmikā;",
  "tha-ap411:19.3": "Vajjītaṅgā ca te sabbe,",
  "tha-ap411:19.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:20.1": "Naḷakārā pesakārā,",
  "tha-ap411:20.2": "cammakārā ca tacchakā;",
  "tha-ap411:20.3": "Kammārā kumbhakārā ca,",
  "tha-ap411:20.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:21.1": "Maṇikārā lohakārā,",
  "tha-ap411:21.2": "soṇṇakārā ca dussikā;",
  "tha-ap411:21.3": "Tipukārā ca te sabbe,",
  "tha-ap411:21.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:22.1": "Usukārā bhamakārā,",
  "tha-ap411:22.2": "pesakārā ca gandhikā;",
  "tha-ap411:22.3": "Rajakā tunnavāyā ca,",
  "tha-ap411:22.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:23.1": "Telikā kaṭṭhahārā ca,",
  "tha-ap411:23.2": "udahārā ca pessikā;",
  "tha-ap411:23.3": "Sūpikā sūparakkhā ca,",
  "tha-ap411:23.4": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:24.1": "Dovārikā anīkaṭṭhā,",
  "tha-ap411:24.2": "Bandhikā pupphachaḍḍakā;",
  "tha-ap411:24.3": "Hatthāruhā hatthipālā,",
  "tha-ap411:24.4": "Āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:25.1": "Ānandassa mahārañño,",
  "tha-ap411:25.2": "mamatthassa adāsahaṁ;",
  "tha-ap411:25.3": "Sattavaṇṇena ratanena,",
  "tha-ap411:25.4": "ūnatthaṁ pūrayāmahaṁ.",
  "tha-ap411:26.1": "Ye mayā kittitā sabbe,",
  "tha-ap411:26.2": "nānāvaṇṇā bahū janā;",
  "tha-ap411:26.3": "Tesāhaṁ cittamaññāya,",
  "tha-ap411:26.4": "tappayiṁ ratanenahaṁ.",
  "tha-ap411:27.1": "Vaggūsu bhāsamānāsu,",
  "tha-ap411:27.2": "vajjamānāsu bherisu;",
  "tha-ap411:27.3": "Saṅkhesu dhamayantesu,",
  "tha-ap411:27.4": "sakagehe ramāmahaṁ.",
  "tha-ap411:28.1": "Bhagavā tamhi samaye,",
  "tha-ap411:28.2": "padumuttaranāyako;",
  "tha-ap411:28.3": "Vasīsatasahassehi,",
  "tha-ap411:28.4": "parikkhīṇāsavehi so.",
  "tha-ap411:29.1": "Bhikkhūhi sahito vīthiṁ,",
  "tha-ap411:29.2": "paṭipajjittha cakkhumā;",
  "tha-ap411:29.3": "Obhāsento disā sabbā,",
  "tha-ap411:29.4": "dīparukkhova jotati.",
  "tha-ap411:30.1": "Vajjanti bheriyo sabbā,",
  "tha-ap411:30.2": "gacchante lokanāyake;",
  "tha-ap411:30.3": "Pabhā niddhāvate tassa,",
  "tha-ap411:30.4": "sataraṁsīva uggato.",
  "tha-ap411:31.1": "Kavāṭantarikāyāpi,",
  "tha-ap411:31.2": "paviṭṭhena ca rasminā;",
  "tha-ap411:31.3": "Antogharesu vipulo,",
  "tha-ap411:31.4": "āloko āsi tāvade.",
  "tha-ap411:32.1": "Pabhaṁ disvāna buddhassa,",
  "tha-ap411:32.2": "pārisajje avocahaṁ;",
  "tha-ap411:32.3": "‘Nissaṁsayaṁ buddhaseṭṭho,",
  "tha-ap411:32.4": "imaṁ vīthimupāgato’.",
  "tha-ap411:33.1": "Khippaṁ oruyha pāsādā,",
  "tha-ap411:33.2": "agamiṁ antarāpaṇaṁ;",
  "tha-ap411:33.3": "Sambuddhaṁ abhivādetvā,",
  "tha-ap411:33.4": "idaṁ vacanamabraviṁ.",
  "tha-ap411:34.1": "‘Anukampatu me buddho,",
  "tha-ap411:34.2": "jalajuttamanāyako;",
  "tha-ap411:34.3": "Vasīsatasahassehi,",
  "tha-ap411:34.4": "adhivāsesi so muni’.",
  "tha-ap411:35.1": "Nimantetvāna sambuddhaṁ,",
  "tha-ap411:35.2": "abhinesiṁ sakaṁ gharaṁ;",
  "tha-ap411:35.3": "Tattha annena pānena,",
  "tha-ap411:35.4": "santappesiṁ mahāmuniṁ.",
  "tha-ap411:36.1": "Bhuttāviṁ kālamaññāya,",
  "tha-ap411:36.2": "buddhaseṭṭhassa tādino;",
  "tha-ap411:36.3": "Sataṅgikena tūriyena,",
  "tha-ap411:36.4": "buddhaseṭṭhaṁ upaṭṭhahiṁ.",
  "tha-ap411:37.1": "Padumuttaro lokavidū,",
  "tha-ap411:37.2": "āhutīnaṁ paṭiggaho;",
  "tha-ap411:37.3": "Antoghare nisīditvā,",
  "tha-ap411:37.4": "imā gāthā abhāsatha.",
  "tha-ap411:38.1": "‘Yo maṁ tūriyehupaṭṭhāsi,",
  "tha-ap411:38.2": "annapānañcadāsi me;",
  "tha-ap411:38.3": "Tamahaṁ kittayissāmi,",
  "tha-ap411:38.4": "suṇātha mama bhāsato.",
  "tha-ap411:39.1": "Pahūtabhakkho hutvāna,",
  "tha-ap411:39.2": "sahirañño sabhojano;",
  "tha-ap411:39.3": "Catudīpe ekarajjaṁ,",
  "tha-ap411:39.4": "kārayissatiyaṁ naro.",
  "tha-ap411:40.1": "Pañca sīle samādāya,",
  "tha-ap411:40.2": "dasakammapathe tato;",
  "tha-ap411:40.3": "Samādāya pavattento,",
  "tha-ap411:40.4": "parisaṁ sikkhāpayissati.",
  "tha-ap411:41.1": "Tūriyasatasahassāni,",
  "tha-ap411:41.2": "bheriyo samalaṅkatā;",
  "tha-ap411:41.3": "Vajjayissantimaṁ niccaṁ,",
  "tha-ap411:41.4": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap411:42.1": "Tiṁsakappasahassāni,",
  "tha-ap411:42.2": "devaloke ramissati;",
  "tha-ap411:42.3": "Catusaṭṭhikkhattuṁ devindo,",
  "tha-ap411:42.4": "devarajjaṁ karissati.",
  "tha-ap411:43.1": "Catusaṭṭhikkhattuṁ rājā,",
  "tha-ap411:43.2": "cakkavattī bhavissati;",
  "tha-ap411:43.3": "Padesarajjaṁ vipulaṁ,",
  "tha-ap411:43.4": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap411:44.1": "Kappasatasahassamhi,",
  "tha-ap411:44.2": "okkākakulasambhavo;",
  "tha-ap411:44.3": "Gotamo nāma gottena,",
  "tha-ap411:44.4": "satthā loke bhavissati.",
  "tha-ap411:45.1": "Upapajjati yaṁ yoniṁ,",
  "tha-ap411:45.2": "devattaṁ atha mānusaṁ;",
  "tha-ap411:45.3": "Anūnabhogo hutvāna,",
  "tha-ap411:45.4": "manussattaṁ gamissati.",
  "tha-ap411:46.1": "Ajjhāyako bhavitvāna,",
  "tha-ap411:46.2": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap411:46.3": "Uttamatthaṁ gavesanto,",
  "tha-ap411:46.4": "carissati mahiṁ imaṁ.",
  "tha-ap411:47.1": "So pacchā pabbajitvāna,",
  "tha-ap411:47.2": "sukkamūlena codito;",
  "tha-ap411:47.3": "Gotamassa bhagavato,",
  "tha-ap411:47.4": "sāsanebhiramissati.",
  "tha-ap411:48.1": "Ārādhayitvāna sambuddhaṁ,",
  "tha-ap411:48.2": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap411:48.3": "Kilese jhāpayitvāna,",
  "tha-ap411:48.4": "arahāyaṁ bhavissati’.",
  "tha-ap411:49.1": "Vipine byaggharājāva,",
  "tha-ap411:49.2": "migarājāva kesarī;",
  "tha-ap411:49.3": "Abhīto viharāmajja,",
  "tha-ap411:49.4": "sakyaputtassa sāsane.",
  "tha-ap411:50.1": "Devaloke manusse vā,",
  "tha-ap411:50.2": "dalidde duggatimhi vā;",
  "tha-ap411:50.3": "Nibbattiṁ me na passāmi,",
  "tha-ap411:50.4": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap411:51.1": "Vivekamanuyuttomhi,",
  "tha-ap411:51.2": "upasanto nirūpadhi;",
  "tha-ap411:51.3": "Nāgova bandhanaṁ chetvā,",
  "tha-ap411:51.4": "viharāmi anāsavo.",
  "tha-ap411:52.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap411:52.2": "…pe…",
  "tha-ap411:52.3": "viharāmi anāsavo.",
  "tha-ap411:53.1": "Svāgataṁ vata me āsi,",
  "tha-ap411:53.2": "…pe…",
  "tha-ap411:53.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap411:54.1": "Paṭisambhidā catasso,",
  "tha-ap411:54.2": "…pe…",
  "tha-ap411:54.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap411:55.1": "Itthaṁ sudaṁ āyasmā jatukaṇṇitthero imā gāthāyo abhāsitthāti.",
  "tha-ap411:56.1": "Jatukaṇṇittherassāpadānaṁ navamaṁ."
}