{
  "tha-ap174:0.1": "Therāpadāna",
  "tha-ap174:0.2": "Kumudavagga",
  "tha-ap174:0.3": "2 Nisseṇidāyakattheraapadāna",
  "tha-ap174:1.1": "“Koṇḍaññassa bhagavato,",
  "tha-ap174:1.2": "lokajeṭṭhassa tādino;",
  "tha-ap174:1.3": "Ārohatthāya pāsādaṁ,",
  "tha-ap174:1.4": "nisseṇī kāritā mayā.",
  "tha-ap174:2.1": "Tena cittappasādena,",
  "tha-ap174:2.2": "anubhotvāna sampadā;",
  "tha-ap174:2.3": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap174:2.4": "sammāsambuddhasāsane.",
  "tha-ap174:3.1": "Ekattiṁsamhi kappānaṁ,",
  "tha-ap174:3.2": "sahassamhi tayo ahuṁ;",
  "tha-ap174:3.3": "Sambahulā nāma rājāno,",
  "tha-ap174:3.4": "cakkavattī mahabbalā.",
  "tha-ap174:4.1": "Paṭisambhidā catasso,",
  "tha-ap174:4.2": "…pe…",
  "tha-ap174:4.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap174:5.1": "Itthaṁ sudaṁ āyasmā nisseṇidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap174:6.1": "Nisseṇidāyakattherassāpadānaṁ dutiyaṁ."
}