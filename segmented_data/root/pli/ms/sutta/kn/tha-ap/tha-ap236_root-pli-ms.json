{
  "tha-ap236:0.1": "Therāpadāna",
  "tha-ap236:0.2": "Udakāsanavagga",
  "tha-ap236:0.3": "4. Kilañjadāyakattheraapadāna",
  "tha-ap236:1.1": "“Tivarāyaṁ pure ramme,",
  "tha-ap236:1.2": "naḷakāro ahaṁ tadā;",
  "tha-ap236:1.3": "Siddhatthe lokapajjote,",
  "tha-ap236:1.4": "pasannā janatā tahiṁ.",
  "tha-ap236:2.1": "Pūjatthaṁ lokanāthassa,",
  "tha-ap236:2.2": "kilañjaṁ pariyesati;",
  "tha-ap236:2.3": "Buddhapūjaṁ karontānaṁ,",
  "tha-ap236:2.4": "kilañjaṁ adadiṁ ahaṁ.",
  "tha-ap236:3.1": "Catunnavutito kappe,",
  "tha-ap236:3.2": "yaṁ kammamakariṁ tadā;",
  "tha-ap236:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap236:3.4": "kilañjassa idaṁ phalaṁ.",
  "tha-ap236:4.1": "Sattasattatikappamhi,",
  "tha-ap236:4.2": "rājā āsiṁ jaladdharo;",
  "tha-ap236:4.3": "Sattaratanasampanno,",
  "tha-ap236:4.4": "cakkavattī mahabbalo.",
  "tha-ap236:5.1": "Paṭisambhidā catasso,",
  "tha-ap236:5.2": "…pe…",
  "tha-ap236:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap236:6.1": "Itthaṁ sudaṁ āyasmā kilañjadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap236:7.1": "Kilañjadāyakattherassāpadānaṁ catutthaṁ."
}