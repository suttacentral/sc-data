{
  "tha-ap87:0.1": "Therāpadāna",
  "tha-ap87:0.2": "Timiravagga",
  "tha-ap87:0.3": "5. Raṃsisaññakattheraapadāna",
  "tha-ap87:1.1": "“Pabbate himavantamhi,",
  "tha-ap87:1.2": "vāsaṃ kappesahaṃ pure;",
  "tha-ap87:1.3": "Ajinuttaravāsohaṃ,",
  "tha-ap87:1.4": "vasāmi pabbatantare.",
  "tha-ap87:2.1": "Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
  "tha-ap87:2.2": "Sataraṃsiṃva bhāṇumaṃ;",
  "tha-ap87:2.3": "Vanantaragataṃ disvā,",
  "tha-ap87:2.4": "Sālarājaṃva pupphitaṃ.",
  "tha-ap87:3.1": "Raṃsyā cittaṃ pasādetvā,",
  "tha-ap87:3.2": "vipassissa mahesino;",
  "tha-ap87:3.3": "Paggayha añjaliṃ vandiṃ,",
  "tha-ap87:3.4": "sirasā ukkuṭī ahaṃ.",
  "tha-ap87:4.1": "Ekanavutito kappe,",
  "tha-ap87:4.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap87:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap87:4.4": "raṃsisaññāyidaṃ phalaṃ.",
  "tha-ap87:5.1": "Paṭisambhidā catasso,",
  "tha-ap87:5.2": "…pe…",
  "tha-ap87:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap87:6.1": "Itthaṃ sudaṃ āyasmā raṃsisaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap87:7.1": "Raṃsisaññakattherassāpadānaṃ pañcamaṃ."
}