{
  "tha-ap7:0.1": "Therāpadāna",
  "tha-ap7:0.2": "Buddhavagga",
  "tha-ap7:0.3": "7 Puṇṇamantāṇiputtattheraapadāna",
  "tha-ap7:1.1": "“Ajjhāyako mantadharo,",
  "tha-ap7:1.2": "tiṇṇaṃ vedāna pāragū;",
  "tha-ap7:1.3": "Purakkhatomhi sissehi,",
  "tha-ap7:1.4": "upagacchiṃ naruttamaṃ.",
  "tha-ap7:2.1": "Padumuttaro lokavidū,",
  "tha-ap7:2.2": "āhutīnaṃ paṭiggaho;",
  "tha-ap7:2.3": "Mama kammaṃ pakittesi,",
  "tha-ap7:2.4": "saṃkhittena mahāmuni.",
  "tha-ap7:3.1": "Tāhaṃ dhammaṃ suṇitvāna,",
  "tha-ap7:3.2": "Abhivādetvāna satthuno;",
  "tha-ap7:3.3": "Añjaliṃ paggahetvāna,",
  "tha-ap7:3.4": "Pakkamiṃ dakkhiṇāmukho.",
  "tha-ap7:4.1": "Saṃkhittena suṇitvāna,",
  "tha-ap7:4.2": "vitthārena abhāsayiṃ;",
  "tha-ap7:4.3": "Sabbe sissā attamanā,",
  "tha-ap7:4.4": "sutvāna mama bhāsato;",
  "tha-ap7:4.5": "Sakaṃ diṭṭhiṃ vinodetvā,",
  "tha-ap7:4.6": "buddhe cittaṃ pasādayuṃ.",
  "tha-ap7:5.1": "Saṃkhittenapi desemi,",
  "tha-ap7:5.2": "vitthārena tathevahaṃ;",
  "tha-ap7:5.3": "Abhidhammanayaññūhaṃ,",
  "tha-ap7:5.4": "kathāvatthuvisuddhiyā;",
  "tha-ap7:5.5": "Sabbesaṃ viññāpetvāna,",
  "tha-ap7:5.6": "viharāmi anāsavo.",
  "tha-ap7:6.1": "Ito pañcasate kappe,",
  "tha-ap7:6.2": "caturo suppakāsakā;",
  "tha-ap7:6.3": "Sattaratanasampannā,",
  "tha-ap7:6.4": "catudīpamhi issarā.",
  "tha-ap7:7.1": "Paṭisambhidā catasso,",
  "tha-ap7:7.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap7:7.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap7:7.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap7:8.1": "Itthaṃ sudaṃ āyasmā puṇṇo mantāṇiputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap7:9.1": "Puṇṇamantāṇiputtattherassāpadānaṃ pañcamaṃ."
}