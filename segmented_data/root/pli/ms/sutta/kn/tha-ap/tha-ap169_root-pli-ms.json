{
  "tha-ap169:0.1": "Therāpadāna",
  "tha-ap169:0.2": "Supāricariyavagga",
  "tha-ap169:0.3": "7 Yūthikapupphiyattheraapadāna",
  "tha-ap169:1.1": "“Candabhāgānadītīre,",
  "tha-ap169:1.2": "anusotaṁ vajāmahaṁ;",
  "tha-ap169:1.3": "Sayambhuṁ addasaṁ tattha,",
  "tha-ap169:1.4": "sālarājaṁva phullitaṁ.",
  "tha-ap169:2.1": "Pupphaṁ yūthikamādāya,",
  "tha-ap169:2.2": "upagacchiṁ mahāmuniṁ;",
  "tha-ap169:2.3": "Pasannacitto sumano,",
  "tha-ap169:2.4": "buddhassa abhiropayiṁ.",
  "tha-ap169:3.1": "Catunnavutito kappe,",
  "tha-ap169:3.2": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap169:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap169:3.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap169:4.1": "Sattasaṭṭhimhito kappe,",
  "tha-ap169:4.2": "eko sāmuddharo ahuṁ;",
  "tha-ap169:4.3": "Sattaratanasampanno,",
  "tha-ap169:4.4": "cakkavattī mahabbalo.",
  "tha-ap169:5.1": "Paṭisambhidā catasso,",
  "tha-ap169:5.2": "…pe…",
  "tha-ap169:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap169:6.1": "Itthaṁ sudaṁ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap169:7.1": "Yūthikapupphiyattherassāpadānaṁ sattamaṁ."
}