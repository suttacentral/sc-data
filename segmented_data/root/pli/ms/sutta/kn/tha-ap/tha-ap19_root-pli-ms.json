{
  "tha-ap19:0.1": "Therāpadāna",
  "tha-ap19:0.2": "Sīhāsaniyavagga",
  "tha-ap19:0.3": "7 Upasenavaṅgantaputtattheraapadāna",
  "tha-ap19:1.1": "“Padumuttaraṃ bhagavantaṃ,",
  "tha-ap19:1.2": "lokajeṭṭhaṃ narāsabhaṃ;",
  "tha-ap19:1.3": "Pabbhāramhi nisīdantaṃ,",
  "tha-ap19:1.4": "upagacchiṃ naruttamaṃ.",
  "tha-ap19:2.1": "Kaṇikārapupphaṃ disvā,",
  "tha-ap19:2.2": "vaṇṭe chetvānahaṃ tadā;",
  "tha-ap19:2.3": "Alaṅkaritvā chattamhi,",
  "tha-ap19:2.4": "buddhassa abhiropayiṃ.",
  "tha-ap19:3.1": "Piṇḍapātañca pādāsiṃ,",
  "tha-ap19:3.2": "paramannaṃ subhojanaṃ;",
  "tha-ap19:3.3": "Buddhena navame tattha,",
  "tha-ap19:3.4": "samaṇe aṭṭha bhojayiṃ.",
  "tha-ap19:4.1": "Anumodi mahāvīro,",
  "tha-ap19:4.2": "sayambhū aggapuggalo;",
  "tha-ap19:4.3": "Iminā chattadānena,",
  "tha-ap19:4.4": "paramannapavecchanā.",
  "tha-ap19:5.1": "Tena cittappasādena,",
  "tha-ap19:5.2": "sampattimanubhossasi;",
  "tha-ap19:5.3": "Chattiṃsakkhattuṃ devindo,",
  "tha-ap19:5.4": "devarajjaṃ karissati.",
  "tha-ap19:6.1": "Ekavīsatikkhattuñca,",
  "tha-ap19:6.2": "cakkavattī bhavissati;",
  "tha-ap19:6.3": "Padesarajjaṃ vipulaṃ,",
  "tha-ap19:6.4": "gaṇanāto asaṅkhiyaṃ.",
  "tha-ap19:7.1": "Satasahassito kappe,",
  "tha-ap19:7.2": "okkākakulasambhavo;",
  "tha-ap19:7.3": "Gotamo nāma gottena,",
  "tha-ap19:7.4": "satthā loke bhavissati.",
  "tha-ap19:8.1": "Sāsane dibbamānamhi,",
  "tha-ap19:8.2": "manussattaṃ gamissati;",
  "tha-ap19:8.3": "Tassa dhammesu dāyādo,",
  "tha-ap19:8.4": "oraso dhammanimmito.",
  "tha-ap19:9.1": "Upasenoti nāmena,",
  "tha-ap19:9.2": "hessati satthu sāvako;",
  "tha-ap19:9.3": "Samantapāsādikattā,",
  "tha-ap19:9.4": "aggaṭṭhāne ṭhapessati.",
  "tha-ap19:10.1": "Carimaṃ vattate mayhaṃ,",
  "tha-ap19:10.2": "bhavā sabbe samūhatā;",
  "tha-ap19:10.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap19:10.4": "jetvā māraṃ savāhiniṃ.",
  "tha-ap19:11.1": "Paṭisambhidā catasso,",
  "tha-ap19:11.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap19:11.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap19:11.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap19:12.1": "Itthaṃ sudaṃ āyasmā upaseno vaṅgantaputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap19:13.1": "Upasenavaṅgantaputtattherassāpadānaṃ sattamaṃ.",
  "tha-ap19:13.2": "Tatiyabhāṇavāraṃ."
}