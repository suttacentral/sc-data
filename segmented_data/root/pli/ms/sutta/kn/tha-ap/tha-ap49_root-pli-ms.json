{
  "tha-ap49:0.1": "Therāpadāna",
  "tha-ap49:0.2": "Upālivagga",
  "tha-ap49:0.3": "7. Sayanadāyakattheraapadāna",
  "tha-ap49:1.1": "“Siddhatthassa bhagavato,",
  "tha-ap49:1.2": "mettacittassa tādino;",
  "tha-ap49:1.3": "Sayanaggaṁ mayā dinnaṁ,",
  "tha-ap49:1.4": "dussabhaṇḍehi atthataṁ.",
  "tha-ap49:2.1": "Paṭiggahesi bhagavā,",
  "tha-ap49:2.2": "kappiyaṁ sayanāsanaṁ;",
  "tha-ap49:2.3": "Uṭṭhāya sayanā tamhā,",
  "tha-ap49:2.4": "vehāsaṁ uggamī jino.",
  "tha-ap49:3.1": "Catunnavutito kappe,",
  "tha-ap49:3.2": "yaṁ sayanamadāsahaṁ;",
  "tha-ap49:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap49:3.4": "sayanassa idaṁ phalaṁ.",
  "tha-ap49:4.1": "Ekapaññāsito kappe,",
  "tha-ap49:4.2": "varako devasavhayo;",
  "tha-ap49:4.3": "Sattaratanasampanno,",
  "tha-ap49:4.4": "cakkavattī mahabbalo.",
  "tha-ap49:5.1": "Paṭisambhidā catasso,",
  "tha-ap49:5.2": "…pe…",
  "tha-ap49:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap49:6.1": "Itthaṁ sudaṁ āyasmā sayanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap49:7.1": "Sayanadāyakattherassāpadānaṁ sattamaṁ."
}