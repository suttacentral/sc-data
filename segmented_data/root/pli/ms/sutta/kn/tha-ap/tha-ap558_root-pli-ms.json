{
  "tha-ap558:0.1": "Therāpadāna",
  "tha-ap558:0.2": "Yasavagga",
  "tha-ap558:0.3": "6. Uttarattheraapadāna",
  "tha-ap558:1.1": "“Sumedho nāma sambuddho,",
  "tha-ap558:1.2": "bāttiṃsavaralakkhaṇo;",
  "tha-ap558:1.3": "Vivekakāmo bhagavā,",
  "tha-ap558:1.4": "himavantamupāgami.",
  "tha-ap558:2.1": "Ajjhogāhetvā himavantaṃ,",
  "tha-ap558:2.2": "aggo kāruṇiko muni;",
  "tha-ap558:2.3": "Pallaṅkaṃ ābhujitvāna,",
  "tha-ap558:2.4": "nisīdi purisuttamo.",
  "tha-ap558:3.1": "Vijjādharo tadā āsiṃ,",
  "tha-ap558:3.2": "antalikkhacaro ahaṃ;",
  "tha-ap558:3.3": "Tisūlaṃ sukataṃ gayha,",
  "tha-ap558:3.4": "gacchāmi ambare tadā.",
  "tha-ap558:4.1": "Pabbatagge yathā aggi,",
  "tha-ap558:4.2": "puṇṇamāyeva candimā;",
  "tha-ap558:4.3": "Vanaṃ obhāsate buddho,",
  "tha-ap558:4.4": "sālarājāva phullito.",
  "tha-ap558:5.1": "Vanaggā nikkhamitvāna,",
  "tha-ap558:5.2": "buddharaṃsībhidhāvare;",
  "tha-ap558:5.3": "Naḷaggivaṇṇasaṅkāsā,",
  "tha-ap558:5.4": "disvā cittaṃ pasādayiṃ.",
  "tha-ap558:6.1": "Vicinaṃ addasaṃ pupphaṃ,",
  "tha-ap558:6.2": "kaṇikāraṃ devagandhikaṃ;",
  "tha-ap558:6.3": "Tīṇi pupphāni ādāya,",
  "tha-ap558:6.4": "buddhaseṭṭhamapūjayiṃ.",
  "tha-ap558:7.1": "Buddhassa ānubhāvena,",
  "tha-ap558:7.2": "tīṇi pupphāni me tadā;",
  "tha-ap558:7.3": "Uddhaṃ vaṇṭā adhopattā,",
  "tha-ap558:7.4": "chāyaṃ kubbanti satthuno.",
  "tha-ap558:8.1": "Tena kammena sukatena,",
  "tha-ap558:8.2": "cetanāpaṇidhīhi ca;",
  "tha-ap558:8.3": "Jahitvā mānusaṃ dehaṃ,",
  "tha-ap558:8.4": "tāvatiṃsamagacchahaṃ.",
  "tha-ap558:9.1": "Tattha me sukataṃ byamhaṃ,",
  "tha-ap558:9.2": "kaṇikārīti ñāyati;",
  "tha-ap558:9.3": "Saṭṭhiyojanamubbedhaṃ,",
  "tha-ap558:9.4": "tiṃsayojanavitthataṃ.",
  "tha-ap558:10.1": "Sahassakaṇḍaṃ satabheṇḍu,",
  "tha-ap558:10.2": "dhajālu haritāmayaṃ;",
  "tha-ap558:10.3": "Satasahassaniyyūhā,",
  "tha-ap558:10.4": "byamhe pātubhaviṃsu me.",
  "tha-ap558:11.1": "Soṇṇamayā maṇimayā,",
  "tha-ap558:11.2": "lohitaṅkamayāpi ca;",
  "tha-ap558:11.3": "Phalikāpi ca pallaṅkā,",
  "tha-ap558:11.4": "yenicchakā yadicchakā.",
  "tha-ap558:12.1": "Mahārahañca sayanaṃ,",
  "tha-ap558:12.2": "tūlikā vikatīyutaṃ;",
  "tha-ap558:12.3": "Uddhalomiñca ekantaṃ,",
  "tha-ap558:12.4": "bimbohanasamāyutaṃ.",
  "tha-ap558:13.1": "Bhavanā nikkhamitvāna,",
  "tha-ap558:13.2": "caranto devacārikaṃ;",
  "tha-ap558:13.3": "Yathā icchāmi gamanaṃ,",
  "tha-ap558:13.4": "devasaṅghapurakkhato.",
  "tha-ap558:14.1": "Pupphassa heṭṭhā tiṭṭhāmi,",
  "tha-ap558:14.2": "uparicchadanaṃ mama;",
  "tha-ap558:14.3": "Samantā yojanasataṃ,",
  "tha-ap558:14.4": "kaṇikārehi chāditaṃ.",
  "tha-ap558:15.1": "Saṭṭhituriyasahassāni,",
  "tha-ap558:15.2": "sāyapātaṃ upaṭṭhahuṃ;",
  "tha-ap558:15.3": "Parivārenti maṃ niccaṃ,",
  "tha-ap558:15.4": "rattindivamatanditā.",
  "tha-ap558:16.1": "Tattha naccehi gītehi,",
  "tha-ap558:16.2": "tālehi vāditehi ca;",
  "tha-ap558:16.3": "Ramāmi khiḍḍā ratiyā,",
  "tha-ap558:16.4": "modāmi kāmakāmahaṃ.",
  "tha-ap558:17.1": "Tattha bhutvā pivitvā ca,",
  "tha-ap558:17.2": "modāmi tidase tadā;",
  "tha-ap558:17.3": "Nārīgaṇehi sahito,",
  "tha-ap558:17.4": "modāmi byamhamuttame.",
  "tha-ap558:18.1": "Satānaṃ pañcakkhattuñca,",
  "tha-ap558:18.2": "devarajjamakārayiṃ;",
  "tha-ap558:18.3": "Satānaṃ tīṇikkhattuñca,",
  "tha-ap558:18.4": "cakkavattī ahosahaṃ;",
  "tha-ap558:18.5": "Padesarajjaṃ vipulaṃ,",
  "tha-ap558:18.6": "gaṇanāto asaṅkhiyaṃ.",
  "tha-ap558:19.1": "Bhave bhave saṃsaranto,",
  "tha-ap558:19.2": "mahābhogaṃ labhāmahaṃ;",
  "tha-ap558:19.3": "Bhoge me ūnatā natthi,",
  "tha-ap558:19.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:20.1": "Duve bhave saṃsarāmi,",
  "tha-ap558:20.2": "devatte atha mānuse;",
  "tha-ap558:20.3": "Aññaṃ gatiṃ na jānāmi,",
  "tha-ap558:20.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:21.1": "Duve kule pajāyāmi,",
  "tha-ap558:21.2": "khattiye cāpi brāhmaṇe;",
  "tha-ap558:21.3": "Nīce kule na jānāmi,",
  "tha-ap558:21.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:22.1": "Hatthiyānaṃ assayānaṃ,",
  "tha-ap558:22.2": "sivikaṃ sandamānikaṃ;",
  "tha-ap558:22.3": "Labhāmi sabbamevetaṃ,",
  "tha-ap558:22.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:23.1": "Dāsīgaṇaṃ dāsagaṇaṃ,",
  "tha-ap558:23.2": "nāriyo samalaṅkatā;",
  "tha-ap558:23.3": "Labhāmi sabbamevetaṃ,",
  "tha-ap558:23.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:24.1": "Koseyyakambaliyāni,",
  "tha-ap558:24.2": "khomakappāsikāni ca;",
  "tha-ap558:24.3": "Labhāmi sabbamevetaṃ,",
  "tha-ap558:24.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:25.1": "Navavatthaṃ navaphalaṃ,",
  "tha-ap558:25.2": "navaggarasabhojanaṃ;",
  "tha-ap558:25.3": "Labhāmi sabbamevetaṃ,",
  "tha-ap558:25.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:26.1": "Imaṃ khāda imaṃ bhuñja,",
  "tha-ap558:26.2": "imamhi sayane saya;",
  "tha-ap558:26.3": "Labhāmi sabbamevetaṃ,",
  "tha-ap558:26.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:27.1": "Sabbattha pūjito homi,",
  "tha-ap558:27.2": "yaso accuggato mama;",
  "tha-ap558:27.3": "Mahāpakkho sadā homi,",
  "tha-ap558:27.4": "abhejjapariso sadā;",
  "tha-ap558:27.5": "Ñātīnaṃ uttamo homi,",
  "tha-ap558:27.6": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:28.1": "Sītaṃ uṇhaṃ na jānāmi,",
  "tha-ap558:28.2": "pariḷāho na vijjati;",
  "tha-ap558:28.3": "Atho cetasikaṃ dukkhaṃ,",
  "tha-ap558:28.4": "hadaye me na vijjati.",
  "tha-ap558:29.1": "Suvaṇṇavaṇṇo hutvāna,",
  "tha-ap558:29.2": "saṃsarāmi bhavābhave;",
  "tha-ap558:29.3": "Vevaṇṇiyaṃ na jānāmi,",
  "tha-ap558:29.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:30.1": "Devalokā cavitvāna,",
  "tha-ap558:30.2": "sukkamūlena codito;",
  "tha-ap558:30.3": "Sāvatthiyaṃ pure jāto,",
  "tha-ap558:30.4": "mahāsālesu aḍḍhake.",
  "tha-ap558:31.1": "Pañca kāmaguṇe hitvā,",
  "tha-ap558:31.2": "pabbajiṃ anagāriyaṃ;",
  "tha-ap558:31.3": "Jātiyā sattavassohaṃ,",
  "tha-ap558:31.4": "arahattamapāpuṇiṃ.",
  "tha-ap558:32.1": "Upasampadāyī buddho,",
  "tha-ap558:32.2": "guṇamaññāya cakkhumā;",
  "tha-ap558:32.3": "Taruṇo pūjanīyohaṃ,",
  "tha-ap558:32.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:33.1": "Dibbacakkhuvisuddhaṃ me,",
  "tha-ap558:33.2": "samādhikusalo ahaṃ;",
  "tha-ap558:33.3": "Abhiññāpāramippatto,",
  "tha-ap558:33.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:34.1": "Paṭisambhidā anuppatto,",
  "tha-ap558:34.2": "iddhipādesu kovido;",
  "tha-ap558:34.3": "Dhammesu pāramippatto,",
  "tha-ap558:34.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:35.1": "Tiṃsakappasahassamhi,",
  "tha-ap558:35.2": "yaṃ buddhamabhipūjayiṃ;",
  "tha-ap558:35.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap558:35.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap558:36.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap558:36.2": "…pe…",
  "tha-ap558:36.3": "viharāmi anāsavo.",
  "tha-ap558:37.1": "Svāgataṃ vata me āsi,",
  "tha-ap558:37.2": "…pe…",
  "tha-ap558:37.3": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap558:38.1": "Paṭisambhidā catasso,",
  "tha-ap558:38.2": "…pe…",
  "tha-ap558:38.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap558:39.1": "Itthaṃ sudaṃ āyasmā uttaro thero imā gāthāyo abhāsitthāti.",
  "tha-ap558:40.1": "Uttarattherassāpadānaṃ chaṭṭhaṃ."
}