{
  "tha-ap506:0.1": "Therāpadāna",
  "tha-ap506:0.2": "Kaṇikāravagga",
  "tha-ap506:0.3": "4. Avaṭaphaliyattheraapadāna",
  "tha-ap506:1.1": "“Sahassaraṁsī bhagavā,",
  "tha-ap506:1.2": "sayambhū aparājito;",
  "tha-ap506:1.3": "Vivekā vuṭṭhahitvāna,",
  "tha-ap506:1.4": "gocarāyābhinikkhami.",
  "tha-ap506:2.1": "Phalahattho ahaṁ disvā,",
  "tha-ap506:2.2": "upagacchiṁ narāsabhaṁ;",
  "tha-ap506:2.3": "Pasannacitto sumano,",
  "tha-ap506:2.4": "avaṭaṁ adadiṁ phalaṁ.",
  "tha-ap506:3.1": "Catunnavutito kappe,",
  "tha-ap506:3.2": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap506:3.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap506:3.4": "phaladānassidaṁ phalaṁ.",
  "tha-ap506:4.1": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap506:4.2": "…pe…",
  "tha-ap506:4.3": "viharāmi anāsavo.",
  "tha-ap506:5.1": "Svāgataṁ vata me āsi,",
  "tha-ap506:5.2": "…pe…",
  "tha-ap506:5.3": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap506:6.1": "Paṭisambhidā catasso,",
  "tha-ap506:6.2": "…pe…",
  "tha-ap506:6.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap506:7.1": "Itthaṁ sudaṁ āyasmā avaṭaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap506:8.1": "Avaṭaphaliyattherassāpadānaṁ catutthaṁ."
}