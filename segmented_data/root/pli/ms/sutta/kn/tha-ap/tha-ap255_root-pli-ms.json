{
  "tha-ap255:0.1": "Therāpadāna",
  "tha-ap255:0.2": "Thomakavagga",
  "tha-ap255:0.3": "3. Citakapūjakattheraapadāna",
  "tha-ap255:1.1": "“Ānando nāma sambuddho,",
  "tha-ap255:1.2": "sayambhū aparājito;",
  "tha-ap255:1.3": "Araññe parinibbāyi,",
  "tha-ap255:1.4": "amanussamhi kānane.",
  "tha-ap255:2.1": "Devalokā idhāgantvā,",
  "tha-ap255:2.2": "citaṃ katvānahaṃ tadā;",
  "tha-ap255:2.3": "Sarīraṃ tattha jhāpesiṃ,",
  "tha-ap255:2.4": "sakkārañca akāsahaṃ.",
  "tha-ap255:3.1": "Ekanavutito kappe,",
  "tha-ap255:3.2": "yaṃ kammamakariṃ tadā;",
  "tha-ap255:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap255:3.4": "buddhapūjāyidaṃ phalaṃ.",
  "tha-ap255:4.1": "Paṭisambhidā catasso,",
  "tha-ap255:4.2": "…pe…",
  "tha-ap255:4.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap255:5.1": "Itthaṃ sudaṃ āyasmā citakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap255:6.1": "Citakapūjakattherassāpadānaṃ tatiyaṃ."
}