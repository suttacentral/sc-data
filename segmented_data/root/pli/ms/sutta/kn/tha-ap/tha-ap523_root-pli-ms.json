{
  "tha-ap523:0.1": "Therāpadāna",
  "tha-ap523:0.2": "Tiṇadāyakavagga",
  "tha-ap523:0.3": "1. Tiṇamuṭṭhidāyakattheraapadāna",
  "tha-ap523:1.1": "“Himavantassāvidūre,",
  "tha-ap523:1.2": "lambako nāma pabbato;",
  "tha-ap523:1.3": "Tattheva tisso sambuddho,",
  "tha-ap523:1.4": "abbhokāsamhi caṅkami.",
  "tha-ap523:2.1": "Migaluddo pure āsiṃ,",
  "tha-ap523:2.2": "araññe kānane ahaṃ;",
  "tha-ap523:2.3": "Disvāna taṃ devadevaṃ,",
  "tha-ap523:2.4": "tiṇamuṭṭhimadāsahaṃ.",
  "tha-ap523:3.1": "Nisīdanatthaṃ buddhassa,",
  "tha-ap523:3.2": "datvā cittaṃ pasādayiṃ;",
  "tha-ap523:3.3": "Sambuddhaṃ abhivādetvā,",
  "tha-ap523:3.4": "pakkāmiṃ uttarāmukho.",
  "tha-ap523:4.1": "Aciraṃ gatamattassa,",
  "tha-ap523:4.2": "migarājā apothayi;",
  "tha-ap523:4.3": "Sīhena pothito santo,",
  "tha-ap523:4.4": "tattha kālaṅkato ahaṃ.",
  "tha-ap523:5.1": "Āsanne me kataṃ kammaṃ,",
  "tha-ap523:5.2": "buddhaseṭṭhe anāsave;",
  "tha-ap523:5.3": "Sumutto saravegova,",
  "tha-ap523:5.4": "devalokamagañchahaṃ.",
  "tha-ap523:6.1": "Yūpo tattha subho āsi,",
  "tha-ap523:6.2": "puññakammābhinimmito;",
  "tha-ap523:6.3": "Sahassakaṇḍo satabheṇḍu,",
  "tha-ap523:6.4": "dhajālu haritāmayo.",
  "tha-ap523:7.1": "Pabhā niddhāvate tassa,",
  "tha-ap523:7.2": "sataraṃsīva uggato;",
  "tha-ap523:7.3": "Ākiṇṇo devakaññāhi,",
  "tha-ap523:7.4": "āmodiṃ kāmakāmihaṃ.",
  "tha-ap523:8.1": "Devalokā cavitvāna,",
  "tha-ap523:8.2": "sukkamūlena codito;",
  "tha-ap523:8.3": "Āgantvāna manussattaṃ,",
  "tha-ap523:8.4": "pattomhi āsavakkhayaṃ.",
  "tha-ap523:9.1": "Catunnavutito kappe,",
  "tha-ap523:9.2": "nisīdanamadāsahaṃ;",
  "tha-ap523:9.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap523:9.4": "tiṇamuṭṭhe idaṃ phalaṃ.",
  "tha-ap523:10.1": "Kilesā jhāpitā mayhaṃ,",
  "tha-ap523:10.2": "bhavā sabbe samūhatā;",
  "tha-ap523:10.3": "Nāgova bandhanaṃ chetvā,",
  "tha-ap523:10.4": "viharāmi anāsavo.",
  "tha-ap523:11.1": "Svāgataṃ vata me āsi,",
  "tha-ap523:11.2": "mama buddhassa santike;",
  "tha-ap523:11.3": "Tisso vijjā anuppattā,",
  "tha-ap523:11.4": "kataṃ buddhassa sāsanaṃ.",
  "tha-ap523:12.1": "Paṭisambhidā catasso,",
  "tha-ap523:12.2": "vimokkhāpi ca aṭṭhime;",
  "tha-ap523:12.3": "Chaḷabhiññā sacchikatā,",
  "tha-ap523:12.4": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap523:13.1": "Itthaṃ sudaṃ āyasmā tiṇamuṭṭhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap523:14.1": "Tiṇamuṭṭhidāyakattherassāpadānaṃ paṭhamaṃ."
}