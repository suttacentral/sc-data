{
  "tha-ap95:0.1": "Therāpadāna",
  "tha-ap95:0.2": "Sudhāvagga",
  "tha-ap95:0.3": "3 Aḍḍhaceḷakattheraapadāna",
  "tha-ap95:1.1": "“Tissassāhaṃ bhagavato,",
  "tha-ap95:1.2": "upaḍḍhadussamadāsahaṃ;",
  "tha-ap95:1.3": "Paramakāpaññapattomhi,",
  "tha-ap95:1.4": "duggatena samappito.",
  "tha-ap95:2.1": "Upaḍḍhadussaṃ datvāna,",
  "tha-ap95:2.2": "kappaṃ saggamhi modahaṃ;",
  "tha-ap95:2.3": "Avasesesu kappesu,",
  "tha-ap95:2.4": "kusalaṃ kāritaṃ mayā.",
  "tha-ap95:3.1": "Dvenavute ito kappe,",
  "tha-ap95:3.2": "yaṃ dussamadadiṃ tadā;",
  "tha-ap95:3.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap95:3.4": "dussadānassidaṃ phalaṃ.",
  "tha-ap95:4.1": "Ekūnapaññāsakappamhi,",
  "tha-ap95:4.2": "rājāno cakkavattino;",
  "tha-ap95:4.3": "Samantacchadanā nāma,",
  "tha-ap95:4.4": "bāttiṃsāsuṃ janādhipā.",
  "tha-ap95:5.1": "Paṭisambhidā catasso,",
  "tha-ap95:5.2": "…pe…",
  "tha-ap95:5.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap95:6.1": "Itthaṃ sudaṃ āyasmā aḍḍhaceḷako thero imā gāthāyo abhāsitthāti.",
  "tha-ap95:7.1": "Aḍḍhaceḷakattherassāpadānaṃ tatiyaṃ."
}