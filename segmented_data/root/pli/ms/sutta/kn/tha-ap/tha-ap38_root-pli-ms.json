{
  "tha-ap38:0.1": "Therāpadāna",
  "tha-ap38:0.2": "Kuṇḍadhānavagga",
  "tha-ap38:0.3": "6 Adhimuttattheraapadāna",
  "tha-ap38:1.1": "“Nibbute lokanāthamhi,",
  "tha-ap38:1.2": "atthadassīnaruttame;",
  "tha-ap38:1.3": "Upaṭṭhahiṁ bhikkhusaṅghaṁ,",
  "tha-ap38:1.4": "vippasannena cetasā.",
  "tha-ap38:2.1": "Nimantetvā bhikkhusaṅghaṁ,",
  "tha-ap38:2.2": "Ujubhūtaṁ samāhitaṁ;",
  "tha-ap38:2.3": "Ucchunā maṇḍapaṁ katvā,",
  "tha-ap38:2.4": "Bhojesiṁ saṅghamuttamaṁ.",
  "tha-ap38:3.1": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap38:3.2": "devattaṁ atha mānusaṁ;",
  "tha-ap38:3.3": "Sabbe satte abhibhomi,",
  "tha-ap38:3.4": "puññakammassidaṁ phalaṁ.",
  "tha-ap38:4.1": "Aṭṭhārase kappasate,",
  "tha-ap38:4.2": "yaṁ dānamadadiṁ tadā;",
  "tha-ap38:4.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap38:4.4": "ucchudānassidaṁ phalaṁ.",
  "tha-ap38:5.1": "Paṭisambhidā catasso,",
  "tha-ap38:5.2": "…pe…",
  "tha-ap38:5.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap38:6.1": "Itthaṁ sudaṁ āyasmā adhimutto thero imā gāthāyo abhāsitthāti.",
  "tha-ap38:7.1": "Adhimuttattherassāpadānaṁ chaṭṭhaṁ."
}