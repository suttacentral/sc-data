{
  "tha-ap207:0.1": "Therāpadāna",
  "tha-ap207:0.2": "Kaṇikārapupphiyavagga",
  "tha-ap207:0.3": "5. Nigguṇḍipupphiyattheraapadāna",
  "tha-ap207:1.1": "“Vipassissa bhagavato,",
  "tha-ap207:1.2": "āsimārāmiko ahaṁ;",
  "tha-ap207:1.3": "Nigguṇḍipupphaṁ paggayha,",
  "tha-ap207:1.4": "buddhassa abhiropayiṁ.",
  "tha-ap207:2.1": "Ekanavutito kappe,",
  "tha-ap207:2.2": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap207:2.3": "Duggatiṁ nābhijānāmi,",
  "tha-ap207:2.4": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap207:3.1": "Pañcavīse ito kappe,",
  "tha-ap207:3.2": "eko āsiṁ janādhipo;",
  "tha-ap207:3.3": "Mahāpatāpanāmena,",
  "tha-ap207:3.4": "cakkavattī mahabbalo.",
  "tha-ap207:4.1": "Paṭisambhidā catasso,",
  "tha-ap207:4.2": "…pe…",
  "tha-ap207:4.3": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap207:5.1": "Itthaṁ sudaṁ āyasmā nigguṇḍipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap207:6.1": "Nigguṇḍipupphiyattherassāpadānaṁ pañcamaṁ."
}