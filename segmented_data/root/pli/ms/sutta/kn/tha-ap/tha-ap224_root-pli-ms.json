{
  "tha-ap224:0.1": "Therāpadāna",
  "tha-ap224:0.2": "Ālambaṇadāyakavagga",
  "tha-ap224:0.3": "2. Ajinadāyakattheraapadāna",
  "tha-ap224:1.1": "“Ekattiṃse ito kappe,",
  "tha-ap224:1.2": "gaṇasatthārako ahaṃ;",
  "tha-ap224:1.3": "Addasaṃ virajaṃ buddhaṃ,",
  "tha-ap224:1.4": "āhutīnaṃ paṭiggahaṃ.",
  "tha-ap224:2.1": "Cammakhaṇḍaṃ mayā dinnaṃ,",
  "tha-ap224:2.2": "sikhino lokabandhuno;",
  "tha-ap224:2.3": "Tena kammena dvipadinda,",
  "tha-ap224:2.4": "lokajeṭṭha narāsabha.",
  "tha-ap224:3.1": "Sampattiṃ anubhotvāna,",
  "tha-ap224:3.2": "kilese jhāpayiṃ ahaṃ;",
  "tha-ap224:3.3": "Dhāremi antimaṃ dehaṃ,",
  "tha-ap224:3.4": "sammāsambuddhasāsane.",
  "tha-ap224:4.1": "Ekattiṃse ito kappe,",
  "tha-ap224:4.2": "ajinaṃ yaṃ adāsahaṃ;",
  "tha-ap224:4.3": "Duggatiṃ nābhijānāmi,",
  "tha-ap224:4.4": "ajinassa idaṃ phalaṃ.",
  "tha-ap224:5.1": "Ito pañcamake kappe,",
  "tha-ap224:5.2": "rājā āsiṃ sudāyako;",
  "tha-ap224:5.3": "Sattaratanasampanno,",
  "tha-ap224:5.4": "cakkavattī mahabbalo.",
  "tha-ap224:6.1": "Paṭisambhidā catasso,",
  "tha-ap224:6.2": "…pe…",
  "tha-ap224:6.3": "kataṃ buddhassa sāsanaṃ”.",
  "tha-ap224:7.1": "Itthaṃ sudaṃ āyasmā ajinadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap224:8.1": "Ajinadāyakattherassāpadānaṃ dutiyaṃ."
}