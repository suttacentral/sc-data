{
  "thag1.6:0.1": "Theragāthā",
  "thag1.6:0.2": "Ekakanipāta",
  "thag1.6:0.3": "Paṭhamavagga",
  "thag1.6:0.4": "1.6. Sītavaniyattheragāthā",
  "thag1.6:1.1": "“Yo sītavanaṁ upagā bhikkhu,",
  "thag1.6:1.2": "Eko santusito samāhitatto;",
  "thag1.6:1.3": "Vijitāvī apetalomahaṁso,",
  "thag1.6:1.4": "Rakkhaṁ kāyagatāsatiṁ dhitimā”ti.",
  "thag1.6:1.5": "Itthaṁ sudaṁ āyasmā sītavaniyo thero gāthaṁ abhāsitthāti."
}