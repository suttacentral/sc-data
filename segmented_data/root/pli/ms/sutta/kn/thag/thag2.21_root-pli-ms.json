{
  "thag2.21:0.1": "Theragāthā",
  "thag2.21:0.2": "Dukanipāta",
  "thag2.21:0.3": "Tatiyavagga",
  "thag2.21:0.4": "2.21. Uttarattheragāthā",
  "thag2.21:1.1": "“Khandhā mayā pariññātā,",
  "thag2.21:1.2": "taṇhā me susamūhatā;",
  "thag2.21:1.3": "Bhāvitā mama bojjhaṅgā,",
  "thag2.21:1.4": "patto me āsavakkhayo.",
  "thag2.21:2.1": "Sohaṁ khandhe pariññāya,",
  "thag2.21:2.2": "abbahitvāna jāliniṁ;",
  "thag2.21:2.3": "Bhāvayitvāna bojjhaṅge,",
  "thag2.21:2.4": "nibbāyissaṁ anāsavo”ti.",
  "thag2.21:2.5": "… Uttaro thero …."
}