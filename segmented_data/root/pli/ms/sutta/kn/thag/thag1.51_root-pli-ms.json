{
  "thag1.51:0.1": "Theragāthā",
  "thag1.51:0.2": "Ekakanipāta",
  "thag1.51:0.3": "Chaṭṭhavagga",
  "thag1.51:0.4": "1.51. Godhikattheragāthā",
  "thag1.51:1.1": "“Vassati devo yathā sugītaṁ,",
  "thag1.51:1.2": "Channā me kuṭikā sukhā nivātā;",
  "thag1.51:1.3": "Cittaṁ susamāhitañca mayhaṁ,",
  "thag1.51:1.4": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.51:1.5": "… Godhiko thero …."
}