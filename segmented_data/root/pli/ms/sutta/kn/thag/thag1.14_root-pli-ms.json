{
  "thag1.14:0.1": "Theragāthā",
  "thag1.14:0.2": "Ekakanipāta",
  "thag1.14:0.3": "Dutiyavagga",
  "thag1.14:0.4": "1.14. Sivakasāmaṇeragāthā",
  "thag1.14:1.1": "“Upajjhāyo maṁ avaca,",
  "thag1.14:1.2": "ito gacchāma sīvaka;",
  "thag1.14:1.3": "Gāme me vasati kāyo,",
  "thag1.14:1.4": "araññaṁ me gato mano;",
  "thag1.14:1.5": "Semānakopi gacchāmi,",
  "thag1.14:1.6": "natthi saṅgo vijānatan”ti.",
  "thag1.14:1.7": "… Vanavacchassa therassa (siviko) sāmaṇero …."
}