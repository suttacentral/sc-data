{
  "snp3.12:0.1": "Sutta Nipāta 3.12",
  "snp3.12:0.2": "Dvayatānupassanāsutta",
  "snp3.12:1.1": "Evaṃ me sutaṃ—",
  "snp3.12:1.2": " ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati pubbārāme migāramātupāsāde.",
  "snp3.12:1.3": "Tena kho pana samayena bhagavā tadahuposathe pannarase puṇṇāya puṇṇamāya rattiyā bhikkhusaṅghaparivuto abbhokāse nisinno hoti.",
  "snp3.12:1.4": "Atha kho bhagavā tuṇhībhūtaṃ tuṇhībhūtaṃ bhikkhusaṅghaṃ anuviloketvā bhikkhū āmantesi:",
  "snp3.12:2.1": "“‘Ye te, bhikkhave, kusalā dhammā ariyā niyyānikā sambodhagāmino, tesaṃ vo, bhikkhave, kusalānaṃ dhammānaṃ ariyānaṃ niyyānikānaṃ sambodhagāmīnaṃ kā upanisā savanāyā’ti, iti ce, bhikkhave, pucchitāro assu, te evamassu vacanīyā:",
  "snp3.12:2.2": "‘yāvadeva dvayatānaṃ dhammānaṃ yathābhūtaṃ ñāṇāyā’ti.",
  "snp3.12:2.3": "Kiñca dvayataṃ vadetha?",
  "snp3.12:3.1": "Idaṃ dukkhaṃ, ayaṃ dukkhasamudayoti ayamekānupassanā.",
  "snp3.12:3.2": "Ayaṃ dukkhanirodho, ayaṃdukkhanirodhagāminī paṭipadāti, ayaṃ dutiyānupassanā.",
  "snp3.12:3.3": "Evaṃ sammādvayatānupassino kho, bhikkhave, bhikkhuno appamattassa ātāpino pahitattassaviharato dvinnaṃ phalānaṃ aññataraṃ phalaṃ pāṭikaṅkhaṃ—",
  "snp3.12:3.4": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
  "snp3.12:4.1": "Idamavoca bhagavā.",
  "snp3.12:4.2": "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:",
  "snp3.12:5.1": "“Ye dukkhaṃ nappajānanti,",
  "snp3.12:5.2": "atho dukkhassa sambhavaṃ;",
  "snp3.12:5.3": "Yattha ca sabbaso dukkhaṃ,",
  "snp3.12:5.4": "asesaṃ uparujjhati;",
  "snp3.12:5.5": "Tañca maggaṃ na jānanti,",
  "snp3.12:5.6": "dukkhūpasamagāminaṃ.",
  "snp3.12:6.1": "Cetovimuttihīnā te,",
  "snp3.12:6.2": "Atho paññāvimuttiyā;",
  "snp3.12:6.3": "Abhabbā te antakiriyāya,",
  "snp3.12:6.4": "Te ve jātijarūpagā.",
  "snp3.12:7.1": "Ye ca dukkhaṃ pajānanti,",
  "snp3.12:7.2": "atho dukkhassa sambhavaṃ;",
  "snp3.12:7.3": "Yattha ca sabbaso dukkhaṃ,",
  "snp3.12:7.4": "asesaṃ uparujjhati;",
  "snp3.12:7.5": "Tañca maggaṃ pajānanti,",
  "snp3.12:7.6": "dukkhūpasamagāminaṃ.",
  "snp3.12:8.1": "Cetovimuttisampannā,",
  "snp3.12:8.2": "Atho paññāvimuttiyā;",
  "snp3.12:8.3": "Bhabbā te antakiriyāya,",
  "snp3.12:8.4": "Na te jātijarūpagāti.",
  "snp3.12:9.1": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:9.2": "‘siyā’tissu vacanīyā.",
  "snp3.12:9.3": "Kathañca siyā?",
  "snp3.12:9.4": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ upadhipaccayāti, ayamekānupassanā.",
  "snp3.12:9.5": "Upadhīnaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:9.6": "Evaṃ sammā …pe…",
  "snp3.12:9.7": "athāparaṃ etadavoca satthā:",
  "snp3.12:10.1": "“Upadhinidānā pabhavanti dukkhā,",
  "snp3.12:10.2": "Ye keci lokasmimanekarūpā;",
  "snp3.12:10.3": "Yo ve avidvā upadhiṃ karoti,",
  "snp3.12:10.4": "Punappunaṃ dukkhamupeti mando;",
  "snp3.12:10.5": "Tasmā pajānaṃ upadhiṃ na kayirā,",
  "snp3.12:10.6": "Dukkhassa jātippabhavānupassīti.",
  "snp3.12:11.1": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:11.2": "‘siyā’tissu vacanīyā.",
  "snp3.12:11.3": "Kathañca siyā?",
  "snp3.12:11.4": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ avijjāpaccayāti, ayamekānupassanā.",
  "snp3.12:11.5": "Avijjāya tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:11.6": "Evaṃ sammā …pe…",
  "snp3.12:11.7": "athāparaṃ etadavoca satthā:",
  "snp3.12:12.1": "“Jātimaraṇasaṃsāraṃ,",
  "snp3.12:12.2": "ye vajanti punappunaṃ;",
  "snp3.12:12.3": "Itthabhāvaññathābhāvaṃ,",
  "snp3.12:12.4": "avijjāyeva sā gati.",
  "snp3.12:13.1": "Avijjā hāyaṃ mahāmoho,",
  "snp3.12:13.2": "Yenidaṃ saṃsitaṃ ciraṃ;",
  "snp3.12:13.3": "Vijjāgatā ca ye sattā,",
  "snp3.12:13.4": "Na te gacchanti punabbhavanti.",
  "snp3.12:14.1": "‘Siyā aññenapi …pe…",
  "snp3.12:14.2": "kathañca siyā?",
  "snp3.12:14.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ saṅkhārapaccayāti, ayamekānupassanā.",
  "snp3.12:14.4": "Saṅkhārānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:14.5": "Evaṃ sammā …pe…",
  "snp3.12:14.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:15.1": "“Yaṃ kiñci dukkhaṃ sambhoti,",
  "snp3.12:15.2": "sabbaṃ saṅkhārapaccayā;",
  "snp3.12:15.3": "Saṅkhārānaṃ nirodhena,",
  "snp3.12:15.4": "natthi dukkhassa sambhavo.",
  "snp3.12:16.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:16.2": "dukkhaṃ saṅkhārapaccayā;",
  "snp3.12:16.3": "Sabbasaṅkhārasamathā,",
  "snp3.12:16.4": "saññānaṃ uparodhanā;",
  "snp3.12:16.5": "Evaṃ dukkhakkhayo hoti,",
  "snp3.12:16.6": "etaṃ ñatvā yathātathaṃ.",
  "snp3.12:17.1": "Sammaddasā vedaguno,",
  "snp3.12:17.2": "Sammadaññāya paṇḍitā;",
  "snp3.12:17.3": "Abhibhuyya mārasaṃyogaṃ,",
  "snp3.12:17.4": "Na gacchanti punabbhavanti.",
  "snp3.12:18.1": "‘Siyā aññenapi …pe…",
  "snp3.12:18.2": "kathañca siyā?",
  "snp3.12:18.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ viññāṇapaccayāti, ayamekānupassanā.",
  "snp3.12:18.4": "Viññāṇassa tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:18.5": "Evaṃ sammā …pe…",
  "snp3.12:18.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:19.1": "“Yaṃ kiñci dukkhaṃ sambhoti,",
  "snp3.12:19.2": "Sabbaṃ viññāṇapaccayā;",
  "snp3.12:19.3": "Viññāṇassa nirodhena,",
  "snp3.12:19.4": "Natthi dukkhassa sambhavo.",
  "snp3.12:20.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:20.2": "Dukkhaṃ viññāṇapaccayā;",
  "snp3.12:20.3": "Viññāṇūpasamā bhikkhu,",
  "snp3.12:20.4": "Nicchāto parinibbutoti.",
  "snp3.12:21.1": "‘Siyā aññenapi …pe…",
  "snp3.12:21.2": "kathañca siyā?",
  "snp3.12:21.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ phassapaccayāti, ayamekānupassanā.",
  "snp3.12:21.4": "Phassassa tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:21.5": "Evaṃ sammā …pe…",
  "snp3.12:21.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:22.1": "“Tesaṃ phassaparetānaṃ,",
  "snp3.12:22.2": "Bhavasotānusārinaṃ;",
  "snp3.12:22.3": "Kummaggapaṭipannānaṃ,",
  "snp3.12:22.4": "Ārā saṃyojanakkhayo.",
  "snp3.12:23.1": "Ye ca phassaṃ pariññāya,",
  "snp3.12:23.2": "Aññāyupasame ratā;",
  "snp3.12:23.3": "Te ve phassābhisamayā,",
  "snp3.12:23.4": "Nicchātā parinibbutāti.",
  "snp3.12:24.1": "‘Siyā aññenapi …pe…",
  "snp3.12:24.2": "kathañca siyā?",
  "snp3.12:24.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ vedanāpaccayāti, ayamekānupassanā.",
  "snp3.12:24.4": "Vedanānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:24.5": "Evaṃ sammā …pe…",
  "snp3.12:24.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:25.1": "“Sukhaṃ vā yadi vā dukkhaṃ,",
  "snp3.12:25.2": "Adukkhamasukhaṃ saha;",
  "snp3.12:25.3": "Ajjhattañca bahiddhā ca,",
  "snp3.12:25.4": "Yaṃ kiñci atthi veditaṃ.",
  "snp3.12:26.1": "Etaṃ dukkhanti ñatvāna,",
  "snp3.12:26.2": "Mosadhammaṃ palokinaṃ;",
  "snp3.12:26.3": "Phussa phussa vayaṃ passaṃ,",
  "snp3.12:26.4": "Evaṃ tattha vijānati;",
  "snp3.12:26.5": "Vedanānaṃ khayā bhikkhu,",
  "snp3.12:26.6": "Nicchāto parinibbutoti.",
  "snp3.12:27.1": "‘Siyā aññenapi …pe…",
  "snp3.12:27.2": "kathañca siyā?",
  "snp3.12:27.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ taṇhāpaccayāti, ayamekānupassanā.",
  "snp3.12:27.4": "Taṇhāya tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:27.5": "Evaṃ sammā …pe…",
  "snp3.12:27.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:28.1": "“Taṇhādutiyo puriso,",
  "snp3.12:28.2": "Dīghamaddhāna saṃsaraṃ;",
  "snp3.12:28.3": "Itthabhāvaññathābhāvaṃ,",
  "snp3.12:28.4": "Saṃsāraṃ nātivattati.",
  "snp3.12:29.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:29.2": "Taṇhaṃ dukkhassa sambhavaṃ;",
  "snp3.12:29.3": "Vītataṇho anādāno,",
  "snp3.12:29.4": "Sato bhikkhu paribbajeti.",
  "snp3.12:30.1": "‘Siyā aññenapi …pe…",
  "snp3.12:30.2": "kathañca siyā?",
  "snp3.12:30.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ upādānapaccayāti, ayamekānupassanā.",
  "snp3.12:30.4": "Upādānānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:30.5": "Evaṃ sammā …pe…",
  "snp3.12:30.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:31.1": "“Upādānapaccayā bhavo,",
  "snp3.12:31.2": "Bhūto dukkhaṃ nigacchati;",
  "snp3.12:31.3": "Jātassa maraṇaṃ hoti,",
  "snp3.12:31.4": "Eso dukkhassa sambhavo.",
  "snp3.12:32.1": "Tasmā upādānakkhayā,",
  "snp3.12:32.2": "Sammadaññāya paṇḍitā;",
  "snp3.12:32.3": "Jātikkhayaṃ abhiññāya,",
  "snp3.12:32.4": "Na gacchanti punabbhavanti.",
  "snp3.12:33.1": "‘Siyā aññenapi …pe…",
  "snp3.12:33.2": "kathañca siyā?",
  "snp3.12:33.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ ārambhapaccayāti, ayamekānupassanā.",
  "snp3.12:33.4": "Ārambhānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:33.5": "Evaṃ sammā …pe…",
  "snp3.12:33.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:34.1": "“Yaṃ kiñci dukkhaṃ sambhoti,",
  "snp3.12:34.2": "Sabbaṃ ārambhapaccayā;",
  "snp3.12:34.3": "Ārambhānaṃ nirodhena,",
  "snp3.12:34.4": "Natthi dukkhassa sambhavo.",
  "snp3.12:35.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:35.2": "Dukkhaṃ ārambhapaccayā;",
  "snp3.12:35.3": "Sabbārambhaṃ paṭinissajja,",
  "snp3.12:35.4": "Anārambhe vimuttino.",
  "snp3.12:36.1": "Ucchinnabhavataṇhassa,",
  "snp3.12:36.2": "Santacittassa bhikkhuno;",
  "snp3.12:36.3": "Vikkhīṇo jātisaṃsāro,",
  "snp3.12:36.4": "Natthi tassa punabbhavoti.",
  "snp3.12:37.1": "‘Siyā aññenapi …pe…",
  "snp3.12:37.2": "kathañca siyā?",
  "snp3.12:37.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ āhārapaccayāti, ayamekānupassanā.",
  "snp3.12:37.4": "Āhārānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:37.5": "Evaṃ sammā …pe…",
  "snp3.12:37.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:38.1": "“Yaṃ kiñci dukkhaṃ sambhoti,",
  "snp3.12:38.2": "Sabbaṃ āhārapaccayā;",
  "snp3.12:38.3": "Āhārānaṃ nirodhena,",
  "snp3.12:38.4": "Natthi dukkhassa sambhavo.",
  "snp3.12:39.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:39.2": "Dukkhaṃ āhārapaccayā;",
  "snp3.12:39.3": "Sabbāhāraṃ pariññāya,",
  "snp3.12:39.4": "Sabbāhāramanissito.",
  "snp3.12:40.1": "Ārogyaṃ sammadaññāya,",
  "snp3.12:40.2": "Āsavānaṃ parikkhayā;",
  "snp3.12:40.3": "Saṅkhāya sevī dhammaṭṭho,",
  "snp3.12:40.4": "Saṅkhyaṃ nopeti vedagūti.",
  "snp3.12:41.1": "‘Siyā aññenapi …pe…",
  "snp3.12:41.2": "kathañca siyā?",
  "snp3.12:41.3": "Yaṃ kiñci dukkhaṃ sambhoti sabbaṃ iñjitapaccayāti, ayamekānupassanā.",
  "snp3.12:41.4": "Iñjitānaṃ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṃ dutiyānupassanā.",
  "snp3.12:41.5": "Evaṃ sammā …pe…",
  "snp3.12:41.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:42.1": "“Yaṃ kiñci dukkhaṃ sambhoti,",
  "snp3.12:42.2": "Sabbaṃ iñjitapaccayā;",
  "snp3.12:42.3": "Iñjitānaṃ nirodhena,",
  "snp3.12:42.4": "Natthi dukkhassa sambhavo.",
  "snp3.12:43.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:43.2": "Dukkhaṃ iñjitapaccayā;",
  "snp3.12:43.3": "Tasmā hi ejaṃ vossajja,",
  "snp3.12:43.4": "Saṅkhāre uparundhiya;",
  "snp3.12:43.5": "Anejo anupādāno,",
  "snp3.12:43.6": "Sato bhikkhu paribbajeti.",
  "snp3.12:44.1": "‘Siyā aññenapi …pe…",
  "snp3.12:44.2": "kathañca siyā?",
  "snp3.12:44.3": "Nissitassa calitaṃ hotīti, ayamekānupassanā.",
  "snp3.12:44.4": "Anissito na calatīti, ayaṃ dutiyānupassanā.",
  "snp3.12:44.5": "Evaṃ sammā …pe…",
  "snp3.12:44.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:45.1": "“Anissito na calati,",
  "snp3.12:45.2": "Nissito ca upādiyaṃ;",
  "snp3.12:45.3": "Itthabhāvaññathābhāvaṃ,",
  "snp3.12:45.4": "Saṃsāraṃ nātivattati.",
  "snp3.12:46.1": "Etamādīnavaṃ ñatvā,",
  "snp3.12:46.2": "Nissayesu mahabbhayaṃ;",
  "snp3.12:46.3": "Anissito anupādāno,",
  "snp3.12:46.4": "Sato bhikkhu paribbajeti.",
  "snp3.12:47.1": "‘Siyā aññenapi …pe…",
  "snp3.12:47.2": "kathañca siyā?",
  "snp3.12:47.3": "Rūpehi, bhikkhave, arūpā santatarāti, ayamekānupassanā.",
  "snp3.12:47.4": "Arūpehi nirodho santataroti, ayaṃ dutiyānupassanā.",
  "snp3.12:47.5": "Evaṃ sammā …pe…",
  "snp3.12:47.6": "athāparaṃ etadavoca satthā:",
  "snp3.12:48.1": "“Ye ca rūpūpagā sattā,",
  "snp3.12:48.2": "Ye ca arūpaṭṭhāyino;",
  "snp3.12:48.3": "Nirodhaṃ appajānantā,",
  "snp3.12:48.4": "Āgantāro punabbhavaṃ.",
  "snp3.12:49.1": "Ye ca rūpe pariññāya,",
  "snp3.12:49.2": "Arūpesu asaṇṭhitā;",
  "snp3.12:49.3": "Nirodhe ye vimuccanti,",
  "snp3.12:49.4": "Te janā maccuhāyinoti.",
  "snp3.12:50.1": "‘Siyā aññenapi …pe…",
  "snp3.12:50.2": "kathañca siyā?",
  "snp3.12:50.3": "Yaṃ, bhikkhave, sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā pajāya sadevamanussāya idaṃ saccanti upanijjhāyitaṃ tadamariyānaṃ etaṃ musāti yathābhūtaṃ sammappaññāya sudiṭṭhaṃ, ayamekānupassanā.",
  "snp3.12:50.4": "Yaṃ, bhikkhave, sadevakassa …pe…",
  "snp3.12:50.5": "sadevamanussāya idaṃ musāti upanijjhāyitaṃ, tadamariyānaṃ etaṃ saccanti yathābhūtaṃ sammappaññāya sudiṭṭhaṃ, ayaṃ dutiyānupassanā.",
  "snp3.12:50.6": "Evaṃ sammā …pe…",
  "snp3.12:50.7": "athāparaṃ etadavoca satthā:",
  "snp3.12:51.1": "“Anattani attamāniṃ,",
  "snp3.12:51.2": "Passa lokaṃ sadevakaṃ;",
  "snp3.12:51.3": "Niviṭṭhaṃ nāmarūpasmiṃ,",
  "snp3.12:51.4": "Idaṃ saccanti maññati.",
  "snp3.12:52.1": "Yena yena hi maññanti,",
  "snp3.12:52.2": "Tato taṃ hoti aññathā;",
  "snp3.12:52.3": "Tañhi tassa musā hoti,",
  "snp3.12:52.4": "Mosadhammañhi ittaraṃ.",
  "snp3.12:53.1": "Amosadhammaṃ nibbānaṃ,",
  "snp3.12:53.2": "Tadariyā saccato vidū;",
  "snp3.12:53.3": "Te ve saccābhisamayā,",
  "snp3.12:53.4": "Nicchātā parinibbutāti.",
  "snp3.12:54.1": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:54.2": "‘siyā’tissu vacanīyā.",
  "snp3.12:54.3": "Kathañca siyā?",
  "snp3.12:54.4": "Yaṃ, bhikkhave, sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā pajāyasadevamanussāya idaṃ sukhanti upanijjhāyitaṃ, tadamariyānaṃ etaṃ dukkhantiyathābhūtaṃ sammappaññāya sudiṭṭhaṃ, ayamekānupassanā.",
  "snp3.12:54.5": "Yaṃ, bhikkhave, sadevakassa …pe… sadevamanussāya idaṃ dukkhanti upanijjhāyitaṃ tadamariyānaṃetaṃ sukhanti yathābhūtaṃ sammappaññāya sudiṭṭhaṃ, ayaṃ dutiyānupassanā.",
  "snp3.12:54.6": "Evaṃsammā dvayatānupassino kho, bhikkhave, bhikkhuno appamattassa ātāpinopahitattassa viharato dvinnaṃ phalānaṃ aññataraṃ phalaṃ pāṭikaṅkhaṃ—",
  "snp3.12:54.7": "diṭṭhevadhamme aññā, sati vā upādisese anāgāmitāti.",
  "snp3.12:54.8": "Idamavoca bhagavā.",
  "snp3.12:54.9": "Idaṃ vatvānasugato athāparaṃ etadavoca satthā:",
  "snp3.12:55.1": "“Rūpā saddā rasā gandhā,",
  "snp3.12:55.2": "Phassā dhammā ca kevalā;",
  "snp3.12:55.3": "Iṭṭhā kantā manāpā ca,",
  "snp3.12:55.4": "Yāvatatthīti vuccati.",
  "snp3.12:56.1": "Sadevakassa lokassa,",
  "snp3.12:56.2": "Ete vo sukhasammatā;",
  "snp3.12:56.3": "Yattha cete nirujjhanti,",
  "snp3.12:56.4": "Taṃ nesaṃ dukkhasammataṃ.",
  "snp3.12:57.1": "Sukhanti diṭṭhamariyehi,",
  "snp3.12:57.2": "Sakkāyassuparodhanaṃ;",
  "snp3.12:57.3": "Paccanīkamidaṃ hoti,",
  "snp3.12:57.4": "Sabbalokena passataṃ.",
  "snp3.12:58.1": "Yaṃ pare sukhato āhu,",
  "snp3.12:58.2": "Tadariyā āhu dukkhato;",
  "snp3.12:58.3": "Yaṃ pare dukkhato āhu,",
  "snp3.12:58.4": "Tadariyā sukhato vidū.",
  "snp3.12:59.1": "Passa dhammaṃ durājānaṃ,",
  "snp3.12:59.2": "Sampamūḷhetthaviddasu;",
  "snp3.12:59.3": "Nivutānaṃ tamo hoti,",
  "snp3.12:59.4": "Andhakāro apassataṃ.",
  "snp3.12:60.1": "Satañca vivaṭaṃ hoti,",
  "snp3.12:60.2": "Āloko passatāmiva;",
  "snp3.12:60.3": "Santike na vijānanti,",
  "snp3.12:60.4": "Magā dhammassa kovidā.",
  "snp3.12:61.1": "Bhavarāgaparetehi,",
  "snp3.12:61.2": "Bhavasotānusāribhi;",
  "snp3.12:61.3": "Māradheyyānupannehi,",
  "snp3.12:61.4": "Nāyaṃ dhammo susambudho.",
  "snp3.12:62.1": "Ko nu aññatra mariyehi,",
  "snp3.12:62.2": "Padaṃ sambuddhumarahati;",
  "snp3.12:62.3": "Yaṃ padaṃ sammadaññāya,",
  "snp3.12:62.4": "Parinibbanti anāsavā”ti.",
  "snp3.12:63.1": "Idamavoca bhagavā.",
  "snp3.12:63.2": "Attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti.",
  "snp3.12:63.3": "Imasmiñca pana veyyākaraṇasmiṃ bhaññamāne saṭṭhimattānaṃ bhikkhūnaṃ anupādāya āsavehi cittāni vimucciṃsūti.",
  "snp3.12:63.4": "Dvayatānupassanāsuttaṃ dvādasamaṃ.",
  "snp3.12:64.1": "Saccaṃ upadhi avijjā ca,",
  "snp3.12:64.2": "saṅkhāre viññāṇapañcamaṃ;",
  "snp3.12:64.3": "Phassavedaniyā taṇhā,",
  "snp3.12:64.4": "upādānārambhaāhārā;",
  "snp3.12:64.5": "Iñjitaṃ calitaṃ rūpaṃ,",
  "snp3.12:64.6": "saccaṃ dukkhena soḷasāti.",
  "snp3.12:64.7": "Mahāvaggo tatiyo.",
  "snp3.12:65.1": "Pabbajjā ca padhānañca,",
  "snp3.12:65.2": "subhāsitañca sundari;",
  "snp3.12:65.3": "Māghasuttaṃ sabhiyo ca,",
  "snp3.12:65.4": "selo sallañca vuccati.",
  "snp3.12:66.1": "Vāseṭṭho cāpi kokāli,",
  "snp3.12:66.2": "Nālako dvayatānupassanā;",
  "snp3.12:66.3": "Dvādasetāni suttāni,",
  "snp3.12:66.4": "Mahāvaggoti vuccatīti."
}