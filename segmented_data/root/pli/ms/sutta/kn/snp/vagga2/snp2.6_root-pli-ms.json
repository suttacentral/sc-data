{
  "snp2.6:0.1": "Sutta Nipāta 2.6",
  "snp2.6:0.2": "Kapilasutta (dhammacariyasutta)",
  "snp2.6:1.1": "Dhammacariyaṁ brahmacariyaṁ,",
  "snp2.6:1.2": "Etadāhu vasuttamaṁ;",
  "snp2.6:1.3": "Pabbajitopi ce hoti,",
  "snp2.6:1.4": "Agārā anagāriyaṁ.",
  "snp2.6:2.1": "So ce mukharajātiko,",
  "snp2.6:2.2": "vihesābhirato mago;",
  "snp2.6:2.3": "Jīvitaṁ tassa pāpiyo,",
  "snp2.6:2.4": "rajaṁ vaḍḍheti attano.",
  "snp2.6:3.1": "Kalahābhirato bhikkhu,",
  "snp2.6:3.2": "Mohadhammena āvuto;",
  "snp2.6:3.3": "Akkhātampi na jānāti,",
  "snp2.6:3.4": "Dhammaṁ buddhena desitaṁ.",
  "snp2.6:4.1": "Vihesaṁ bhāvitattānaṁ,",
  "snp2.6:4.2": "avijjāya purakkhato;",
  "snp2.6:4.3": "Saṅkilesaṁ na jānāti,",
  "snp2.6:4.4": "maggaṁ nirayagāminaṁ.",
  "snp2.6:5.1": "Vinipātaṁ samāpanno,",
  "snp2.6:5.2": "gabbhā gabbhaṁ tamā tamaṁ;",
  "snp2.6:5.3": "Sa ve tādisako bhikkhu,",
  "snp2.6:5.4": "pecca dukkhaṁ nigacchati.",
  "snp2.6:6.1": "Gūthakūpo yathā assa,",
  "snp2.6:6.2": "sampuṇṇo gaṇavassiko;",
  "snp2.6:6.3": "Yo ca evarūpo assa,",
  "snp2.6:6.4": "dubbisodho hi sāṅgaṇo.",
  "snp2.6:7.1": "Yaṁ evarūpaṁ jānātha,",
  "snp2.6:7.2": "bhikkhavo gehanissitaṁ;",
  "snp2.6:7.3": "Pāpicchaṁ pāpasaṅkappaṁ,",
  "snp2.6:7.4": "pāpaācāragocaraṁ.",
  "snp2.6:8.1": "Sabbe samaggā hutvāna,",
  "snp2.6:8.2": "abhinibbajjiyātha naṁ;",
  "snp2.6:8.3": "Kāraṇḍavaṁ niddhamatha,",
  "snp2.6:8.4": "kasambuṁ apakassatha.",
  "snp2.6:9.1": "Tato palāpe vāhetha,",
  "snp2.6:9.2": "Assamaṇe samaṇamānine;",
  "snp2.6:9.3": "Niddhamitvāna pāpicche,",
  "snp2.6:9.4": "Pāpaācāragocare.",
  "snp2.6:10.1": "Suddhā suddhehi saṁvāsaṁ,",
  "snp2.6:10.2": "Kappayavho patissatā;",
  "snp2.6:10.3": "Tato samaggā nipakā,",
  "snp2.6:10.4": "Dukkhassantaṁ karissathāti.",
  "snp2.6:10.5": "Dhammacariyasuttaṁ chaṭṭhaṁ."
}