{
  "snp1.1:0.1": "Sutta Nipāta 1.1",
  "snp1.1:0.2": "Uragasutta",
  "snp1.1:1.1": "Yo uppatitaṃ vineti kodhaṃ,",
  "snp1.1:1.2": "Visaṭaṃ sappavisaṃva osadhehi;",
  "snp1.1:1.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:1.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:2.1": "Yo rāgamudacchidā asesaṃ,",
  "snp1.1:2.2": "Bhisapupphaṃva saroruhaṃ vigayha;",
  "snp1.1:2.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:2.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:3.1": "Yo taṇhamudacchidā asesaṃ,",
  "snp1.1:3.2": "Saritaṃ sīghasaraṃ visosayitvā;",
  "snp1.1:3.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:3.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:4.1": "Yo mānamudabbadhī asesaṃ,",
  "snp1.1:4.2": "Naḷasetuṃva sudubbalaṃ mahogho;",
  "snp1.1:4.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:4.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:5.1": "Yo nājjhagamā bhavesu sāraṃ,",
  "snp1.1:5.2": "Vicinaṃ pupphamivā udumbaresu;",
  "snp1.1:5.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:5.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:6.1": "Yassantarato na santi kopā,",
  "snp1.1:6.2": "Itibhavābhavatañca vītivatto;",
  "snp1.1:6.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:6.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:7.1": "Yassa vitakkā vidhūpitā,",
  "snp1.1:7.2": "Ajjhattaṃ suvikappitā asesā;",
  "snp1.1:7.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:7.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:8.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:8.2": "Sabbaṃ accagamā imaṃ papañcaṃ;",
  "snp1.1:8.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:8.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:9.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:9.2": "Sabbaṃ vitathamidanti ñatva loke;",
  "snp1.1:9.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:9.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:10.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:10.2": "Sabbaṃ vitathamidanti vītalobho;",
  "snp1.1:10.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:10.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:11.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:11.2": "Sabbaṃ vitathamidanti vītarāgo;",
  "snp1.1:11.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:11.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:12.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:12.2": "Sabbaṃ vitathamidanti vītadoso;",
  "snp1.1:12.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:12.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:13.1": "Yo nāccasārī na paccasārī,",
  "snp1.1:13.2": "Sabbaṃ vitathamidanti vītamoho;",
  "snp1.1:13.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:13.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:14.1": "Yassānusayā na santi keci,",
  "snp1.1:14.2": "Mūlā ca akusalā samūhatāse;",
  "snp1.1:14.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:14.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:15.1": "Yassa darathajā na santi keci,",
  "snp1.1:15.2": "Oraṃ āgamanāya paccayāse;",
  "snp1.1:15.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:15.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:16.1": "Yassa vanathajā na santi keci,",
  "snp1.1:16.2": "Vinibandhāya bhavāya hetukappā;",
  "snp1.1:16.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:16.4": "Urago jiṇṇamivattacaṃ purāṇaṃ.",
  "snp1.1:17.1": "Yo nīvaraṇe pahāya pañca,",
  "snp1.1:17.2": "Anigho tiṇṇakathaṅkatho visallo;",
  "snp1.1:17.3": "So bhikkhu jahāti orapāraṃ,",
  "snp1.1:17.4": "Urago jiṇṇamivattacaṃ purāṇanti.",
  "snp1.1:17.5": "Uragasuttaṃ paṭhamaṃ."
}