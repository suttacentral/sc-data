{
  "snp1.9:0.1": "Sutta Nipāta 1.9",
  "snp1.9:0.2": "Hemavatasutta",
  "snp1.9:1.1": "“Ajja pannaraso uposatho,",
  "snp1.9:1.2": "Dibbā ratti upaṭṭhitā;",
  "snp1.9:1.3": "Anomanāmaṃ satthāraṃ,",
  "snp1.9:1.4": "Handa passāma gotamaṃ”.",
  "snp1.9:2.1": "“Kacci mano supaṇihito,",
  "snp1.9:2.2": "Sabbabhūtesu tādino;",
  "snp1.9:2.3": "Kacci iṭṭhe aniṭṭhe ca,",
  "snp1.9:2.4": "Saṅkappassa vasīkatā”.",
  "snp1.9:3.1": "“Mano cassa supaṇihito,",
  "snp1.9:3.2": "Sabbabhūtesu tādino;",
  "snp1.9:3.3": "Atho iṭṭhe aniṭṭhe ca,",
  "snp1.9:3.4": "Saṅkappassa vasīkatā”.",
  "snp1.9:4.1": "“Kacci adinnaṃ nādiyati,",
  "snp1.9:4.2": "Kacci pāṇesu saññato;",
  "snp1.9:4.3": "Kacci ārā pamādamhā,",
  "snp1.9:4.4": "Kacci jhānaṃ na riñcati”.",
  "snp1.9:5.1": "“Na so adinnaṃ ādiyati,",
  "snp1.9:5.2": "Atho pāṇesu saññato;",
  "snp1.9:5.3": "Atho ārā pamādamhā,",
  "snp1.9:5.4": "Buddho jhānaṃ na riñcati”.",
  "snp1.9:6.1": "“Kacci musā na bhaṇati,",
  "snp1.9:6.2": "Kacci na khīṇabyappatho;",
  "snp1.9:6.3": "Kacci vebhūtiyaṃ nāha,",
  "snp1.9:6.4": "Kacci samphaṃ na bhāsati”.",
  "snp1.9:7.1": "“Musā ca so na bhaṇati,",
  "snp1.9:7.2": "Atho na khīṇabyappatho;",
  "snp1.9:7.3": "Atho vebhūtiyaṃ nāha,",
  "snp1.9:7.4": "Mantā atthañca bhāsati”.",
  "snp1.9:8.1": "“Kacci na rajjati kāmesu,",
  "snp1.9:8.2": "Kacci cittaṃ anāvilaṃ;",
  "snp1.9:8.3": "Kacci mohaṃ atikkanto,",
  "snp1.9:8.4": "Kacci dhammesu cakkhumā”.",
  "snp1.9:9.1": "“Na so rajjati kāmesu,",
  "snp1.9:9.2": "Atho cittaṃ anāvilaṃ;",
  "snp1.9:9.3": "Sabbamohaṃ atikkanto,",
  "snp1.9:9.4": "Buddho dhammesu cakkhumā”.",
  "snp1.9:10.1": "“Kacci vijjāya sampanno,",
  "snp1.9:10.2": "Kacci saṃsuddhacāraṇo;",
  "snp1.9:10.3": "Kaccissa āsavā khīṇā,",
  "snp1.9:10.4": "Kacci natthi punabbhavo”.",
  "snp1.9:11.1": "“Vijjāya ceva sampanno,",
  "snp1.9:11.2": "Atho saṃsuddhacāraṇo;",
  "snp1.9:11.3": "Sabbassa āsavā khīṇā,",
  "snp1.9:11.4": "Natthi tassa punabbhavo”.",
  "snp1.9:12.1": "“Sampannaṃ munino cittaṃ,",
  "snp1.9:12.2": "Kammunā byappathena ca;",
  "snp1.9:12.3": "Vijjācaraṇasampannaṃ,",
  "snp1.9:12.4": "Dhammato naṃ pasaṃsati”.",
  "snp1.9:13.1": "“Sampannaṃ munino cittaṃ,",
  "snp1.9:13.2": "Kammunā byappathena ca;",
  "snp1.9:13.3": "Vijjācaraṇasampannaṃ,",
  "snp1.9:13.4": "Dhammato anumodasi.",
  "snp1.9:14.1": "Sampannaṃ munino cittaṃ,",
  "snp1.9:14.2": "Kammunā byappathena ca;",
  "snp1.9:14.3": "Vijjācaraṇasampannaṃ,",
  "snp1.9:14.4": "Handa passāma gotamaṃ”.",
  "snp1.9:15.1": "“Eṇijaṅghaṃ kisaṃ vīraṃ,",
  "snp1.9:15.2": "Appāhāraṃ alolupaṃ;",
  "snp1.9:15.3": "Muniṃ vanasmiṃ jhāyantaṃ,",
  "snp1.9:15.4": "Ehi passāma gotamaṃ.",
  "snp1.9:16.1": "Sīhaṃvekacaraṃ nāgaṃ,",
  "snp1.9:16.2": "Kāmesu anapekkhinaṃ;",
  "snp1.9:16.3": "Upasaṅkamma pucchāma,",
  "snp1.9:16.4": "Maccupāsappamocanaṃ.",
  "snp1.9:17.1": "Akkhātāraṃ pavattāraṃ,",
  "snp1.9:17.2": "Sabbadhammāna pāraguṃ;",
  "snp1.9:17.3": "Buddhaṃ verabhayātītaṃ,",
  "snp1.9:17.4": "Mayaṃ pucchāma gotamaṃ”.",
  "snp1.9:18.1": "“Kismiṃ loko samuppanno,",
  "snp1.9:18.2": "Kismiṃ kubbati santhavaṃ;",
  "snp1.9:18.3": "Kissa loko upādāya,",
  "snp1.9:18.4": "Kismiṃ loko vihaññati”.",
  "snp1.9:19.1": "“Chasu loko samuppanno,",
  "snp1.9:19.2": "Chasu kubbati santhavaṃ;",
  "snp1.9:19.3": "Channameva upādāya,",
  "snp1.9:19.4": "Chasu loko vihaññati”.",
  "snp1.9:20.1": "“Katamaṃ taṃ upādānaṃ,",
  "snp1.9:20.2": "Yattha loko vihaññati;",
  "snp1.9:20.3": "Niyyānaṃ pucchito brūhi,",
  "snp1.9:20.4": "Kathaṃ dukkhā pamuccati”.",
  "snp1.9:21.1": "“Pañca kāmaguṇā loke,",
  "snp1.9:21.2": "Manochaṭṭhā paveditā;",
  "snp1.9:21.3": "Ettha chandaṃ virājetvā,",
  "snp1.9:21.4": "Evaṃ dukkhā pamuccati.",
  "snp1.9:22.1": "Etaṃ lokassa niyyānaṃ,",
  "snp1.9:22.2": "Akkhātaṃ vo yathātathaṃ;",
  "snp1.9:22.3": "Etaṃ vo ahamakkhāmi,",
  "snp1.9:22.4": "Evaṃ dukkhā pamuccati”.",
  "snp1.9:23.1": "“Ko sūdha tarati oghaṃ,",
  "snp1.9:23.2": "Kodha tarati aṇṇavaṃ;",
  "snp1.9:23.3": "Appatiṭṭhe anālambe,",
  "snp1.9:23.4": "Ko gambhīre na sīdati”.",
  "snp1.9:24.1": "“Sabbadā sīlasampanno,",
  "snp1.9:24.2": "Paññavā susamāhito;",
  "snp1.9:24.3": "Ajjhattacintī satimā,",
  "snp1.9:24.4": "Oghaṃ tarati duttaraṃ.",
  "snp1.9:25.1": "Virato kāmasaññāya,",
  "snp1.9:25.2": "Sabbasaṃyojanātigo;",
  "snp1.9:25.3": "Nandībhavaparikkhīṇo,",
  "snp1.9:25.4": "So gambhīre na sīdati”.",
  "snp1.9:26.1": "“Gambhīrapaññaṃ nipuṇatthadassiṃ,",
  "snp1.9:26.2": "Akiñcanaṃ kāmabhave asattaṃ;",
  "snp1.9:26.3": "Taṃ passatha sabbadhi vippamuttaṃ,",
  "snp1.9:26.4": "Dibbe pathe kamamānaṃ mahesiṃ.",
  "snp1.9:27.1": "Anomanāmaṃ nipuṇatthadassiṃ,",
  "snp1.9:27.2": "Paññādadaṃ kāmālaye asattaṃ;",
  "snp1.9:27.3": "Taṃ passatha sabbaviduṃ sumedhaṃ,",
  "snp1.9:27.4": "Ariye pathe kamamānaṃ mahesiṃ.",
  "snp1.9:28.1": "Sudiṭṭhaṃ vata no ajja,",
  "snp1.9:28.2": "Suppabhātaṃ suhuṭṭhitaṃ;",
  "snp1.9:28.3": "Yaṃ addasāma sambuddhaṃ,",
  "snp1.9:28.4": "Oghatiṇṇamanāsavaṃ.",
  "snp1.9:29.1": "Ime dasasatā yakkhā,",
  "snp1.9:29.2": "Iddhimanto yasassino;",
  "snp1.9:29.3": "Sabbe taṃ saraṇaṃ yanti,",
  "snp1.9:29.4": "Tvaṃ no satthā anuttaro.",
  "snp1.9:30.1": "Te mayaṃ vicarissāma,",
  "snp1.9:30.2": "Gāmā gāmaṃ nagā nagaṃ;",
  "snp1.9:30.3": "Namassamānā sambuddhaṃ,",
  "snp1.9:30.4": "Dhammassa ca sudhammatan”ti.",
  "snp1.9:30.5": "Hemavatasuttaṃ navamaṃ."
}