{
  "snp5.15:0.1": "Sutta Nipāta 5.15",
  "snp5.15:0.2": "Posālamāṇavapucchā",
  "snp5.15:1.1": "“Yo atītaṁ ādisati,",
  "snp5.15:1.2": "(iccāyasmā posālo)",
  "snp5.15:1.3": "Anejo chinnasaṁsayo;",
  "snp5.15:1.4": "Pāraguṁ sabbadhammānaṁ,",
  "snp5.15:1.5": "Atthi pañhena āgamaṁ.",
  "snp5.15:2.1": "Vibhūtarūpasaññissa,",
  "snp5.15:2.2": "sabbakāyappahāyino;",
  "snp5.15:2.3": "Ajjhattañca bahiddhā ca,",
  "snp5.15:2.4": "natthi kiñcīti passato;",
  "snp5.15:2.5": "Ñāṇaṁ sakkānupucchāmi,",
  "snp5.15:2.6": "kathaṁ neyyo tathāvidho”.",
  "snp5.15:3.1": "“Viññāṇaṭṭhitiyo sabbā,",
  "snp5.15:3.2": "(posālāti bhagavā)",
  "snp5.15:3.3": "Abhijānaṁ tathāgato;",
  "snp5.15:3.4": "Tiṭṭhantamenaṁ jānāti,",
  "snp5.15:3.5": "Vimuttaṁ tapparāyaṇaṁ.",
  "snp5.15:4.1": "Ākiñcaññasambhavaṁ ñatvā,",
  "snp5.15:4.2": "Nandī saṁyojanaṁ iti;",
  "snp5.15:4.3": "Evametaṁ abhiññāya,",
  "snp5.15:4.4": "Tato tattha vipassati;",
  "snp5.15:4.5": "Etaṁ ñāṇaṁ tathaṁ tassa,",
  "snp5.15:4.6": "Brāhmaṇassa vusīmato”ti.",
  "snp5.15:4.7": "Posālamāṇavapucchā cuddasamā."
}