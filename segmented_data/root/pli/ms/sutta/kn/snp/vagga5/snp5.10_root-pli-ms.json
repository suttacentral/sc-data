{
  "snp5.10:0.1": "Sutta Nipāta 5.10",
  "snp5.10:0.2": "Todeyyamāṇavapucchā",
  "snp5.10:1.1": "“Yasmiṁ kāmā na vasanti,",
  "snp5.10:1.2": "(iccāyasmā todeyyo)",
  "snp5.10:1.3": "Taṇhā yassa na vijjati;",
  "snp5.10:1.4": "Kathaṅkathā ca yo tiṇṇo,",
  "snp5.10:1.5": "Vimokkho tassa kīdiso”.",
  "snp5.10:2.1": "“Yasmiṁ kāmā na vasanti,",
  "snp5.10:2.2": "(todeyyāti bhagavā)",
  "snp5.10:2.3": "Taṇhā yassa na vijjati;",
  "snp5.10:2.4": "Kathaṅkathā ca yo tiṇṇo,",
  "snp5.10:2.5": "Vimokkho tassa nāparo”.",
  "snp5.10:3.1": "“Nirāsaso so uda āsasāno,",
  "snp5.10:3.2": "Paññāṇavā so uda paññakappī;",
  "snp5.10:3.3": "Muniṁ ahaṁ sakka yathā vijaññaṁ,",
  "snp5.10:3.4": "Taṁ me viyācikkha samantacakkhu”.",
  "snp5.10:4.1": "“Nirāsaso so na ca āsasāno,",
  "snp5.10:4.2": "Paññāṇavā so na ca paññakappī;",
  "snp5.10:4.3": "Evampi todeyya muniṁ vijāna,",
  "snp5.10:4.4": "Akiñcanaṁ kāmabhave asattan”ti.",
  "snp5.10:4.5": "Todeyyamāṇavapucchā navamā."
}