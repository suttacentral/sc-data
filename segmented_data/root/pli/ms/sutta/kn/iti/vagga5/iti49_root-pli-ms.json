{
  "iti49:0.1": "Itivuttaka 49",
  "iti49:0.2": "Dukanipāta",
  "iti49:0.3": "Dutiyavagga",
  "iti49:0.4": "Diṭṭhigatasutta",
  "iti49:1.1": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti49:2.1": "“Dvīhi, bhikkhave, diṭṭhigatehi pariyuṭṭhitā devamanussā olīyanti eke, atidhāvanti eke;",
  "iti49:2.2": "cakkhumanto ca passanti.",
  "iti49:3.1": "Kathañca, bhikkhave, olīyanti eke?",
  "iti49:3.2": "Bhavārāmā, bhikkhave, devamanussā bhavaratā bhavasammuditā tesaṁ bhavanirodhāya dhamme desiyamāne cittaṁ na pakkhandati na pasīdati na santiṭṭhati nādhimuccati.",
  "iti49:3.3": "Evaṁ kho, bhikkhave, olīyanti eke.",
  "iti49:4.1": "Kathañca, bhikkhave, atidhāvanti eke?",
  "iti49:4.2": "Bhaveneva kho paneke aṭṭīyamānā harāyamānā jigucchamānā vibhavaṁ abhinandanti—",
  "iti49:4.3": "yato kira, bho, ayaṁ attā kāyassa bhedā paraṁ maraṇā ucchijjati vinassati na hoti paraṁ maraṇā;",
  "iti49:4.4": "etaṁ santaṁ etaṁ paṇītaṁ etaṁ yāthāvanti.",
  "iti49:4.5": "Evaṁ kho, bhikkhave, atidhāvanti eke.",
  "iti49:5.1": "Kathañca, bhikkhave, cakkhumanto passanti?",
  "iti49:5.2": "Idha bhikkhu bhūtaṁ bhūtato passati;",
  "iti49:5.3": "bhūtaṁ bhūtato disvā bhūtassa nibbidāya virāgāya nirodhāya paṭipanno hoti.",
  "iti49:5.4": "Evaṁ kho, bhikkhave, cakkhumanto passantī”ti.",
  "iti49:5.5": "Etamatthaṁ bhagavā avoca.",
  "iti49:5.6": "Tatthetaṁ iti vuccati:",
  "iti49:6.1": "“Ye bhūtaṁ bhūtato disvā,",
  "iti49:6.2": "bhūtassa ca atikkamaṁ;",
  "iti49:6.3": "Yathābhūte vimuccanti,",
  "iti49:6.4": "bhavataṇhā parikkhayā.",
  "iti49:7.1": "Sa ve bhūtapariñño so,",
  "iti49:7.2": "vītataṇho bhavābhave;",
  "iti49:7.3": "Bhūtassa vibhavā bhikkhu,",
  "iti49:7.4": "nāgacchati punabbhavan”ti.",
  "iti49:8.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti49:8.2": "Dvādasamaṁ.",
  "iti49:8.3": "Dutiyo vaggo.",
  "iti49:9.0": "Tassuddānaṁ",
  "iti49:9.1": "Dve indriyā dve tapanīyā,",
  "iti49:9.2": "sīlena apare duve;",
  "iti49:9.3": "Anottāpī kuhanā dve ca,",
  "iti49:9.4": "saṁvejanīyena te dasa.",
  "iti49:10.1": "Vitakkā desanā vijjā,",
  "iti49:10.2": "paññā dhammena pañcamaṁ;",
  "iti49:10.3": "Ajātaṁ dhātusallānaṁ,",
  "iti49:10.4": "sikkhā jāgariyena ca;",
  "iti49:10.5": "Apāyadiṭṭhiyā ceva,",
  "iti49:10.6": "bāvīsati pakāsitāti.",
  "iti49:10.7": "Dukanipāto niṭṭhito."
}