{
  "dhp33:0.1": "Khuddakanikāya",
  "dhp33:0.2": "Dhammapada",
  "dhp33:0.3": "Cittavagga",
  "dhp33:0.4": "Meghiyattheravatthu",
  "dhp33:1": "Phandanaṃ capalaṃ cittaṃ,",
  "dhp33:2": "dūrakkhaṃ dunnivārayaṃ;",
  "dhp33:3": "Ujuṃ karoti medhāvī,",
  "dhp33:4": "usukārova tejanaṃ.",
  "dhp34:1": "Vārijova thale khitto,",
  "dhp34:2": "okamokataubbhato;",
  "dhp34:3": "Pariphandatidaṃ cittaṃ,",
  "dhp34:4": "māradheyyaṃ pahātave.",
  "dhp35:0": "Aññatarabhikkhuvatthu",
  "dhp35:1": "Dunniggahassa lahuno,",
  "dhp35:2": "yatthakāmanipātino;",
  "dhp35:3": "Cittassa damatho sādhu,",
  "dhp35:4": "cittaṃ dantaṃ sukhāvahaṃ.",
  "dhp36:0": "Aññataraukkaṇṭhitabhikkhuvatthu",
  "dhp36:1": "Sududdasaṃ sunipuṇaṃ,",
  "dhp36:2": "yatthakāmanipātinaṃ;",
  "dhp36:3": "Cittaṃ rakkhetha medhāvī,",
  "dhp36:4": "cittaṃ guttaṃ sukhāvahaṃ.",
  "dhp37:0": "Saṅgharakkhitabhāgineyyattheravatthu",
  "dhp37:1": "Dūraṅgamaṃ ekacaraṃ,",
  "dhp37:2": "asarīraṃ guhāsayaṃ;",
  "dhp37:3": "Ye cittaṃ saṃyamissanti,",
  "dhp37:4": "mokkhanti mārabandhanā.",
  "dhp38:0": "Cittahatthattheravatthu",
  "dhp38:1": "Anavaṭṭhitacittassa,",
  "dhp38:2": "saddhammaṃ avijānato;",
  "dhp38:3": "Pariplavapasādassa,",
  "dhp38:4": "paññā na paripūrati.",
  "dhp39:1": "Anavassutacittassa,",
  "dhp39:2": "ananvāhatacetaso;",
  "dhp39:3": "Puññapāpapahīnassa,",
  "dhp39:4": "natthi jāgarato bhayaṃ.",
  "dhp40:0": "Pañcasatabhikkhuvatthu",
  "dhp40:1": "Kumbhūpamaṃ kāyamimaṃ viditvā,",
  "dhp40:2": "Nagarūpamaṃ cittamidaṃ ṭhapetvā;",
  "dhp40:3": "Yodhetha māraṃ paññāvudhena,",
  "dhp40:4": "Jitañca rakkhe anivesano siyā.",
  "dhp41:0": "Pūtigattatissattheravatthu",
  "dhp41:1": "Aciraṃ vatayaṃ kāyo,",
  "dhp41:2": "pathaviṃ adhisessati;",
  "dhp41:3": "Chuddho apetaviññāṇo,",
  "dhp41:4": "niratthaṃva kaliṅgaraṃ.",
  "dhp42:0": "Nandagopālakavatthu",
  "dhp42:1": "Diso disaṃ yaṃ taṃ kayirā,",
  "dhp42:2": "verī vā pana verinaṃ;",
  "dhp42:3": "Micchāpaṇihitaṃ cittaṃ,",
  "dhp42:4": "pāpiyo naṃ tato kare.",
  "dhp43:0": "Soreyyavatthu",
  "dhp43:1": "Na taṃ mātā pitā kayirā,",
  "dhp43:2": "aññe vāpi ca ñātakā;",
  "dhp43:3": "Sammāpaṇihitaṃ cittaṃ,",
  "dhp43:4": "seyyaso naṃ tato kare.",
  "dhp43:5": "Cittavaggo tatiyo."
}