{
  "thig1.10:0.1": "Therīgāthā",
  "thig1.10:0.2": "Ekakanipāta",
  "thig1.10:0.3": "10. Upasamātherīgāthā",
  "thig1.10:1.1": "“Upasame tare oghaṁ,",
  "thig1.10:1.2": "maccudheyyaṁ suduttaraṁ;",
  "thig1.10:1.3": "Dhārehi antimaṁ dehaṁ,",
  "thig1.10:1.4": "jetvā māraṁ savāhinin”ti.",
  "thig1.10:2.1": "… Upasamā therī …."
}