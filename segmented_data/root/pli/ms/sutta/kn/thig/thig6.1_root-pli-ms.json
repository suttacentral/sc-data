{
  "thig6.1:0.1": "Therīgāthā",
  "thig6.1:0.2": "Chakkanipāta",
  "thig6.1:0.3": "1. Pañcasatamattātherīgāthā",
  "thig6.1:1.1": "“Yassa maggaṃ na jānāsi,",
  "thig6.1:1.2": "āgatassa gatassa vā;",
  "thig6.1:1.3": "Taṃ kuto cāgataṃ sattaṃ,",
  "thig6.1:1.4": "‘mama putto’ti rodasi.",
  "thig6.1:2.1": "Maggañca khossa jānāsi,",
  "thig6.1:2.2": "āgatassa gatassa vā;",
  "thig6.1:2.3": "Na naṃ samanusocesi,",
  "thig6.1:2.4": "evaṃdhammā hi pāṇino.",
  "thig6.1:3.1": "Ayācito tatāgacchi,",
  "thig6.1:3.2": "nānuññāto ito gato;",
  "thig6.1:3.3": "Kutoci nūna āgantvā,",
  "thig6.1:3.4": "vasitvā katipāhakaṃ;",
  "thig6.1:3.5": "Itopi aññena gato,",
  "thig6.1:3.6": "tatopaññena gacchati.",
  "thig6.1:4.1": "Peto manussarūpena,",
  "thig6.1:4.2": "saṃsaranto gamissati;",
  "thig6.1:4.3": "Yathāgato tathā gato,",
  "thig6.1:4.4": "kā tattha paridevanā”.",
  "thig6.1:5.1": "“Abbahī vata me sallaṃ,",
  "thig6.1:5.2": "duddasaṃ hadayassitaṃ;",
  "thig6.1:5.3": "Yā me sokaparetāya,",
  "thig6.1:5.4": "puttasokaṃ byapānudi.",
  "thig6.1:6.1": "Sājja abbūḷhasallāhaṃ,",
  "thig6.1:6.2": "Nicchātā parinibbutā;",
  "thig6.1:6.3": "Buddhaṃ dhammañca saṃghañca,",
  "thig6.1:6.4": "Upemi saraṇaṃ muniṃ”.",
  "thig6.1:7.1": "Itthaṃ sudaṃ pañcasatamattā therī bhikkhuniyo …",
  "thig6.1:7.2": "pe…."
}