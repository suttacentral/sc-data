{
  "thig1.12:0.1": "Therīgāthā",
  "thig1.12:0.2": "Ekakanipāta",
  "thig1.12:0.3": "12. Dhammadinnātherīgāthā",
  "thig1.12:1.1": "“Chandajātā avasāyī,",
  "thig1.12:1.2": "Manasā ca phuṭā siyā;",
  "thig1.12:1.3": "Kāmesu appaṭibaddhacittā,",
  "thig1.12:1.4": "Uddhaṁsotāti vuccatī”ti.",
  "thig1.12:2.1": "… Dhammadinnā therī …."
}