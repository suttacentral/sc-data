{
  "thig1.4:0.1": "Therīgāthā",
  "thig1.4:0.2": "Ekakanipāta",
  "thig1.4:0.3": "4. Tissātherīgāthā",
  "thig1.4:1.1": "“Tisse sikkhassu sikkhāya,",
  "thig1.4:1.2": "mā taṁ yogā upaccaguṁ;",
  "thig1.4:1.3": "Sabbayogavisaṁyuttā,",
  "thig1.4:1.4": "cara loke anāsavā”ti.",
  "thig1.4:2.1": "… Tissā therī …."
}