{
  "thig7.2:0.1": "Therīgāthā",
  "thig7.2:0.2": "Sattakanipāta",
  "thig7.2:0.3": "2. Cālātherīgāthā",
  "thig7.2:1.1": "“Satiṁ upaṭṭhapetvāna,",
  "thig7.2:1.2": "bhikkhunī bhāvitindriyā;",
  "thig7.2:1.3": "Paṭivijjhi padaṁ santaṁ,",
  "thig7.2:1.4": "saṅkhārūpasamaṁ sukhaṁ”.",
  "thig7.2:2.1": "“Kaṁ nu uddissa muṇḍāsi,",
  "thig7.2:2.2": "samaṇī viya dissasi;",
  "thig7.2:2.3": "Na ca rocesi pāsaṇḍe,",
  "thig7.2:2.4": "kimidaṁ carasi momuhā”.",
  "thig7.2:3.1": "“Ito bahiddhā pāsaṇḍā,",
  "thig7.2:3.2": "diṭṭhiyo upanissitā;",
  "thig7.2:3.3": "Na te dhammaṁ vijānanti,",
  "thig7.2:3.4": "na te dhammassa kovidā.",
  "thig7.2:4.1": "Atthi sakyakule jāto,",
  "thig7.2:4.2": "buddho appaṭipuggalo;",
  "thig7.2:4.3": "So me dhammamadesesi,",
  "thig7.2:4.4": "diṭṭhīnaṁ samatikkamaṁ.",
  "thig7.2:5.1": "Dukkhaṁ dukkhasamuppādaṁ,",
  "thig7.2:5.2": "Dukkhassa ca atikkamaṁ;",
  "thig7.2:5.3": "Ariyaṁ caṭṭhaṅgikaṁ maggaṁ,",
  "thig7.2:5.4": "Dukkhūpasamagāminaṁ.",
  "thig7.2:6.1": "Tassāhaṁ vacanaṁ sutvā,",
  "thig7.2:6.2": "vihariṁ sāsane ratā;",
  "thig7.2:6.3": "Tisso vijjā anuppattā,",
  "thig7.2:6.4": "kataṁ buddhassa sāsanaṁ.",
  "thig7.2:7.1": "Sabbattha vihatā nandī,",
  "thig7.2:7.2": "tamokhandho padālito;",
  "thig7.2:7.3": "Evaṁ jānāhi pāpima,",
  "thig7.2:7.4": "nihato tvamasi antaka”.",
  "thig7.2:8.1": "… Cālā therī …."
}