{
  "mil3.5.6:0.1": "Milindapañha",
  "mil3.5.6:0.2": "Buddhavagga",
  "mil3.5.6:0.3": "6. Vedagūpañha",
  "mil3.5.6:1.1": "Rājā āha—",
  "mil3.5.6:1.2": "“bhante nāgasena, vedagū upalabbhatī”ti?",
  "mil3.5.6:1.3": "Thero āha—",
  "mil3.5.6:1.4": "“paramatthena kho, mahārāja, vedagū nupalabbhatī”ti.",
  "mil3.5.6:2.1": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.6:3.1": "Vedagūpañho chaṭṭho."
}