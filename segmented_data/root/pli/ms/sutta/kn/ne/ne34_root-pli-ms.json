{
  "ne34:0.1": "Netti",
  "ne34:0.2": "Paṭiniddesavāra",
  "ne34:0.3": "Sampāta 15",
  "ne34:0.4": "Parikkhārahārasampāta",
  "ne34:1.1": "Tattha katamo parikkhāro hārasampāto?",
  "ne34:2.1": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne34:2.2": "Ayaṁ samathavipassanāya parikkhāro.",
  "ne34:2.3": "Niyutto parikkhāro hārasampāto."
}