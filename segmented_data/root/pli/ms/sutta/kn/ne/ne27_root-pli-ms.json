{
  "ne27:0.1": "Netti",
  "ne27:0.2": "Paṭiniddesavāra",
  "ne27:0.3": "Sampāta 8",
  "ne27:0.4": "Vibhattihārasampāta",
  "ne27:1.1": "Tattha katamo vibhattihārasampāto?",
  "ne27:2.1": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne27:2.2": "Kusalapakkho kusalapakkhena niddisitabbo.",
  "ne27:2.3": "Akusalapakkho akusalapakkhena niddisitabbo.",
  "ne27:2.4": "Niyutto vibhattihārasampāto."
}