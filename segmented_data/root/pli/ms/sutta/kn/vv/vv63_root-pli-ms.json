{
  "vv63:0.1": "Vimānavatthu",
  "vv63:0.2": "Purisavimāna",
  "vv63:0.3": "Mahārathavagga",
  "vv63:0.4": "13. Cūḷarathavimānavatthu",
  "vv63:1.1": "“Daḷhadhammā nisārassa,",
  "vv63:1.2": "dhanuṃ olubbha tiṭṭhasi;",
  "vv63:1.3": "Khattiyo nusi rājañño,",
  "vv63:1.4": "adu luddo vanecaro”ti.",
  "vv63:2.1": "“Assakādhipatissāhaṃ,",
  "vv63:2.2": "bhante putto vanecaro;",
  "vv63:2.3": "Nāmaṃ me bhikkhu te brūmi,",
  "vv63:2.4": "sujāto iti maṃ vidū.",
  "vv63:3.1": "Mige gavesamānohaṃ,",
  "vv63:3.2": "Ogāhanto brahāvanaṃ;",
  "vv63:3.3": "Migaṃ tañceva nāddakkhiṃ,",
  "vv63:3.4": "Tañca disvā ṭhito ahan”ti.",
  "vv63:4.1": "“Svāgataṃ te mahāpuñña,",
  "vv63:4.2": "atho te adurāgataṃ;",
  "vv63:4.3": "Etto udakamādāya,",
  "vv63:4.4": "pāde pakkhālayassu te.",
  "vv63:5.1": "Idampi pānīyaṃ sītaṃ,",
  "vv63:5.2": "ābhataṃ girigabbharā;",
  "vv63:5.3": "Rājaputta tato pitvā,",
  "vv63:5.4": "santhatasmiṃ upāvisā”ti.",
  "vv63:6.1": "“Kalyāṇī vata te vācā,",
  "vv63:6.2": "Savanīyā mahāmuni;",
  "vv63:6.3": "Nelā atthavatī vaggu,",
  "vv63:6.4": "Mantvā atthañca bhāsasi.",
  "vv63:7.1": "Kā te rati vane viharato,",
  "vv63:7.2": "Isinisabha vadehi puṭṭho;",
  "vv63:7.3": "Tava vacanapathaṃ nisāmayitvā,",
  "vv63:7.4": "Atthadhammapadaṃ samācaremase”ti.",
  "vv63:8.1": "“Ahiṃsā sabbapāṇīnaṃ,",
  "vv63:8.2": "kumāramhāka ruccati;",
  "vv63:8.3": "Theyyā ca aticārā ca,",
  "vv63:8.4": "majjapānā ca ārati.",
  "vv63:9.1": "Ārati samacariyā ca,",
  "vv63:9.2": "bāhusaccaṃ kataññutā;",
  "vv63:9.3": "Diṭṭheva dhamme pāsaṃsā,",
  "vv63:9.4": "dhammā ete pasaṃsiyāti.",
  "vv63:10.1": "Santike maraṇaṃ tuyhaṃ,",
  "vv63:10.2": "oraṃ māsehi pañcahi;",
  "vv63:10.3": "Rājaputta vijānāhi,",
  "vv63:10.4": "attānaṃ parimocayā”ti.",
  "vv63:11.1": "“Katamaṃ svāhaṃ janapadaṃ gantvā,",
  "vv63:11.2": "Kiṃ kammaṃ kiñca porisaṃ;",
  "vv63:11.3": "Kāya vā pana vijjāya,",
  "vv63:11.4": "Bhaveyyaṃ ajarāmaro”ti.",
  "vv63:12.1": "“Na vijjate so padeso,",
  "vv63:12.2": "kammaṃ vijjā ca porisaṃ;",
  "vv63:12.3": "Yattha gantvā bhave macco,",
  "vv63:12.4": "rājaputtājarāmaro.",
  "vv63:13.1": "Mahaddhanā mahābhogā,",
  "vv63:13.2": "raṭṭhavantopi khattiyā;",
  "vv63:13.3": "Pahūtadhanadhaññāse,",
  "vv63:13.4": "tepi no ajarāmarā.",
  "vv63:14.1": "Yadi te sutā andhakaveṇḍuputtā,",
  "vv63:14.2": "Sūrā vīrā vikkantappahārino;",
  "vv63:14.3": "Tepi āyukkhayaṃ pattā,",
  "vv63:14.4": "Viddhastā sassatīsamā.",
  "vv63:15.1": "Khattiyā brāhmaṇā vessā,",
  "vv63:15.2": "suddā caṇḍālapukkusā;",
  "vv63:15.3": "Ete caññe ca jātiyā,",
  "vv63:15.4": "tepi no ajarāmarā.",
  "vv63:16.1": "Ye mantaṃ parivattenti,",
  "vv63:16.2": "chaḷaṅgaṃ brahmacintitaṃ;",
  "vv63:16.3": "Ete caññe ca vijjāya,",
  "vv63:16.4": "tepi no ajarāmarā.",
  "vv63:17.1": "Isayo cāpi ye santā,",
  "vv63:17.2": "saññatattā tapassino;",
  "vv63:17.3": "Sarīraṃ tepi kālena,",
  "vv63:17.4": "vijahanti tapassino.",
  "vv63:18.1": "Bhāvitattāpi arahanto,",
  "vv63:18.2": "katakiccā anāsavā;",
  "vv63:18.3": "Nikkhipanti imaṃ dehaṃ,",
  "vv63:18.4": "puññapāpaparikkhayā”ti.",
  "vv63:19.1": "“Subhāsitā atthavatī,",
  "vv63:19.2": "gāthāyo te mahāmuni;",
  "vv63:19.3": "Nijjhattomhi subhaṭṭhena,",
  "vv63:19.4": "tvañca me saraṇaṃ bhavā”ti.",
  "vv63:20.1": "“Mā maṃ tvaṃ saraṇaṃ gaccha,",
  "vv63:20.2": "tameva saraṇaṃ vaja;",
  "vv63:20.3": "Sakyaputtaṃ mahāvīraṃ,",
  "vv63:20.4": "yamahaṃ saraṇaṃ gato”ti.",
  "vv63:21.1": "“Katarasmiṃ so janapade,",
  "vv63:21.2": "satthā tumhāka mārisa;",
  "vv63:21.3": "Ahampi daṭṭhuṃ gacchissaṃ,",
  "vv63:21.4": "jinaṃ appaṭipuggalan”ti.",
  "vv63:22.1": "“Puratthimasmiṃ janapade,",
  "vv63:22.2": "okkākakulasambhavo;",
  "vv63:22.3": "Tatthāsi purisājañño,",
  "vv63:22.4": "so ca kho parinibbuto”ti.",
  "vv63:23.1": "“Sace hi buddho tiṭṭheyya,",
  "vv63:23.2": "Satthā tumhāka mārisa;",
  "vv63:23.3": "Yojanāni sahassāni,",
  "vv63:23.4": "Gaccheyyaṃ payirupāsituṃ.",
  "vv63:24.1": "Yato ca kho parinibbuto,",
  "vv63:24.2": "satthā tumhāka mārisa;",
  "vv63:24.3": "Nibbutampi mahāvīraṃ,",
  "vv63:24.4": "gacchāmi saraṇaṃ ahaṃ.",
  "vv63:25.1": "Upemi saraṇaṃ buddhaṃ,",
  "vv63:25.2": "dhammañcāpi anuttaraṃ;",
  "vv63:25.3": "Saṃghañca naradevassa,",
  "vv63:25.4": "gacchāmi saraṇaṃ ahaṃ.",
  "vv63:26.1": "Pāṇātipātā viramāmi khippaṃ,",
  "vv63:26.2": "Loke adinnaṃ parivajjayāmi;",
  "vv63:26.3": "Amajjapo no ca musā bhaṇāmi,",
  "vv63:26.4": "Sakena dārena ca homi tuṭṭho”ti.",
  "vv63:27.1": "“Sahassaraṃsīva yathā mahappabho,",
  "vv63:27.2": "Disaṃ yathā bhāti nabhe anukkamaṃ;",
  "vv63:27.3": "Tathāpakāro tavāyaṃ mahāratho,",
  "vv63:27.4": "Samantato yojanasattamāyato.",
  "vv63:28.1": "Suvaṇṇapaṭṭehi samantamotthaṭo,",
  "vv63:28.2": "Urassa muttāhi maṇīhi cittito;",
  "vv63:28.3": "Lekhā suvaṇṇassa ca rūpiyassa ca,",
  "vv63:28.4": "Sobhenti veḷuriyamayā sunimmitā.",
  "vv63:29.1": "Sīsañcidaṃ veḷuriyassa nimmitaṃ,",
  "vv63:29.2": "Yugañcidaṃ lohitakāya cittitaṃ;",
  "vv63:29.3": "Yuttā suvaṇṇassa ca rūpiyassa ca,",
  "vv63:29.4": "Sobhanti assā ca ime manojavā.",
  "vv63:30.1": "So tiṭṭhasi hemarathe adhiṭṭhito,",
  "vv63:30.2": "Devānamindova sahassavāhano;",
  "vv63:30.3": "Pucchāmi tāhaṃ yasavanta kovidaṃ,",
  "vv63:30.4": "Kathaṃ tayā laddho ayaṃ uḷāro”ti.",
  "vv63:31.1": "“Sujāto nāmahaṃ bhante,",
  "vv63:31.2": "rājaputto pure ahuṃ;",
  "vv63:31.3": "Tvañca maṃ anukampāya,",
  "vv63:31.4": "saññamasmiṃ nivesayi.",
  "vv63:32.1": "Khīṇāyukañca maṃ ñatvā,",
  "vv63:32.2": "sarīraṃ pādāsi satthuno;",
  "vv63:32.3": "Imaṃ sujāta pūjehi,",
  "vv63:32.4": "taṃ te atthāya hehiti.",
  "vv63:33.1": "Tāhaṃ gandhehi mālehi,",
  "vv63:33.2": "pūjayitvā samuyyuto;",
  "vv63:33.3": "Pahāya mānusaṃ dehaṃ,",
  "vv63:33.4": "upapannomhi nandanaṃ.",
  "vv63:34.1": "Nandane ca vane ramme,",
  "vv63:34.2": "nānādijagaṇāyute;",
  "vv63:34.3": "Ramāmi naccagītehi,",
  "vv63:34.4": "accharāhi purakkhato”ti.",
  "vv63:35.1": "Cūḷarathavimānaṃ terasamaṃ."
}