{
  "vv14:0.1": "Vimānavatthu",
  "vv14:0.2": "Itthivimāna",
  "vv14:0.3": "Pīṭhavagga",
  "vv14:0.4": "14. Dutiyasuṇisāvimānavatthu",
  "vv14:1.1": "“Abhikkantena vaṇṇena,",
  "vv14:1.2": "yā tvaṁ tiṭṭhasi devate;",
  "vv14:1.3": "Obhāsentī disā sabbā,",
  "vv14:1.4": "osadhī viya tārakā.",
  "vv14:2.1": "Kena tetādiso vaṇṇo,",
  "vv14:2.2": "kena te idha mijjhati;",
  "vv14:2.3": "Uppajjanti ca te bhogā,",
  "vv14:2.4": "ye keci manaso piyā.",
  "vv14:3.1": "Pucchāmi taṁ devi mahānubhāve,",
  "vv14:3.2": "Manussabhūtā kimakāsi puññaṁ;",
  "vv14:3.3": "Kenāsi evaṁ jalitānubhāvā,",
  "vv14:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv14:4.1": "Sā devatā attamanā,",
  "vv14:4.2": "moggallānena pucchitā;",
  "vv14:4.3": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv14:4.4": "yassa kammassidaṁ phalaṁ.",
  "vv14:5.1": "“Ahaṁ manussesu manussabhūtā,",
  "vv14:5.2": "Suṇisā ahosiṁ sasurassa gehe.",
  "vv14:6.1": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv14:6.2": "vippasannamanāvilaṁ;",
  "vv14:6.3": "Tassa adāsahaṁ bhāgaṁ,",
  "vv14:6.4": "pasannā sehi pāṇibhi;",
  "vv14:6.5": "Kummāsapiṇḍaṁ datvāna,",
  "vv14:6.6": "modāmi nandane vane.",
  "vv14:7.1": "Tena metādiso vaṇṇo,",
  "vv14:7.2": "tena me idha mijjhati;",
  "vv14:7.3": "Uppajjanti ca me bhogā,",
  "vv14:7.4": "ye keci manaso piyā.",
  "vv14:8.1": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv14:8.2": "Manussabhūtā yamakāsi puññaṁ;",
  "vv14:8.3": "Tenamhi evaṁ jalitānubhāvā,",
  "vv14:8.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv14:9.1": "Dutiyasuṇisāvimānaṁ cuddasamaṁ."
}