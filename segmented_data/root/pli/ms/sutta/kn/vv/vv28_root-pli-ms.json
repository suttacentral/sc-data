{
  "vv28:0.1": "Vimānavatthu",
  "vv28:0.2": "Itthivimāna",
  "vv28:0.3": "Cittalatāvagga",
  "vv28:0.4": "11. Dutiyabhikkhādāyikāvimānavatthu",
  "vv28:1.1": "“Abhikkantena vaṇṇena,",
  "vv28:1.2": "yā tvaṁ tiṭṭhasi devate;",
  "vv28:1.3": "Obhāsentī disā sabbā,",
  "vv28:1.4": "osadhī viya tārakā.",
  "vv28:2.1": "Kena tetādiso vaṇṇo,",
  "vv28:2.2": "…pe…",
  "vv28:2.3": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv28:3.1": "Sā devatā attamanā,",
  "vv28:3.2": "…pe…",
  "vv28:3.3": "yassa kammassidaṁ phalaṁ.",
  "vv28:4.1": "“Ahaṁ manussesu manussabhūtā,",
  "vv28:4.2": "Purimāya jātiyā manussaloke.",
  "vv28:5.1": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv28:5.2": "vippasannamanāvilaṁ;",
  "vv28:5.3": "Tassa adāsahaṁ bhikkhaṁ,",
  "vv28:5.4": "pasannā sehi pāṇibhi.",
  "vv28:6.1": "Tena metādiso vaṇṇo,",
  "vv28:6.2": "…pe…",
  "vv28:6.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv28:7.1": "Dutiyabhikkhādāyikāvimānaṁ ekādasamaṁ.",
  "vv28:8.1": "Tassuddānaṁ",
  "vv28:9.1": "Dāsī ceva lakhumā ca,",
  "vv28:9.2": "atha ācāmadāyikā;",
  "vv28:9.3": "Caṇḍālī bhadditthī ceva,",
  "vv28:9.4": "soṇadinnā uposathā;",
  "vv28:9.5": "Niddā ceva suniddā ca,",
  "vv28:9.6": "dve ca bhikkhāya dāyikā;",
  "vv28:9.7": "Vaggo tena pavuccatīti.",
  "vv28:10.1": "Itthivimāne dutiyo vaggo.",
  "vv28:11.1": "Bhāṇavāraṁ paṭhamaṁ."
}