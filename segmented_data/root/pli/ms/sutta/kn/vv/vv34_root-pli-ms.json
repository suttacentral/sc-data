{
  "vv34:0.1": "Vimānavatthu",
  "vv34:0.2": "Itthivimāna",
  "vv34:0.3": "Pāricchattakavagga",
  "vv34:0.4": "6. Daddallavimānavatthu",
  "vv34:1.1": "“Daddallamānā vaṇṇena,",
  "vv34:1.2": "yasasā ca yasassinī;",
  "vv34:1.3": "Sabbe deve tāvatiṁse,",
  "vv34:1.4": "vaṇṇena atirocasi.",
  "vv34:2.1": "Dassanaṁ nābhijānāmi,",
  "vv34:2.2": "idaṁ paṭhamadassanaṁ;",
  "vv34:2.3": "Kasmā kāyā nu āgamma,",
  "vv34:2.4": "nāmena bhāsase maman”ti.",
  "vv34:3.1": "“Ahaṁ bhadde subhaddāsiṁ,",
  "vv34:3.2": "pubbe mānusake bhave;",
  "vv34:3.3": "Sahabhariyā ca te āsiṁ,",
  "vv34:3.4": "bhaginī ca kaniṭṭhikā.",
  "vv34:4.1": "Sā ahaṁ kāyassa bhedā,",
  "vv34:4.2": "vippamuttā tato cutā;",
  "vv34:4.3": "Nimmānaratīnaṁ devānaṁ,",
  "vv34:4.4": "upapannā sahabyatan”ti.",
  "vv34:5.1": "“Pahūtakatakalyāṇā,",
  "vv34:5.2": "te deve yanti pāṇino;",
  "vv34:5.3": "Yesaṁ tvaṁ kittayissasi,",
  "vv34:5.4": "subhadde jātimattano.",
  "vv34:6.1": "Atha tvaṁ kena vaṇṇena,",
  "vv34:6.2": "kena vā anusāsitā;",
  "vv34:6.3": "Kīdiseneva dānena,",
  "vv34:6.4": "subbatena yasassinī.",
  "vv34:7.1": "Yasaṁ etādisaṁ pattā,",
  "vv34:7.2": "visesaṁ vipulamajjhagā;",
  "vv34:7.3": "Devate pucchitācikkha,",
  "vv34:7.4": "kissa kammassidaṁ phalan”ti.",
  "vv34:8.1": "“Aṭṭheva piṇḍapātāni,",
  "vv34:8.2": "yaṁ dānaṁ adadaṁ pure;",
  "vv34:8.3": "Dakkhiṇeyyassa saṅghassa,",
  "vv34:8.4": "pasannā sehi pāṇibhi.",
  "vv34:9.1": "Tena metādiso vaṇṇo,",
  "vv34:9.2": "…pe…",
  "vv34:9.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv34:10.1": "“Ahaṁ tayā bahutare bhikkhū,",
  "vv34:10.2": "Saññate brahmacārayo;",
  "vv34:10.3": "Tappesiṁ annapānena,",
  "vv34:10.4": "Pasannā sehi pāṇibhi.",
  "vv34:11.1": "Tayā bahutaraṁ datvā,",
  "vv34:11.2": "hīnakāyūpagā ahaṁ;",
  "vv34:11.3": "Kathaṁ tvaṁ appataraṁ datvā,",
  "vv34:11.4": "visesaṁ vipulamajjhagā;",
  "vv34:11.5": "Devate pucchitācikkha,",
  "vv34:11.6": "kissa kammassidaṁ phalan”ti.",
  "vv34:12.1": "“Manobhāvanīyo bhikkhu,",
  "vv34:12.2": "sandiṭṭho me pure ahu;",
  "vv34:12.3": "Tāhaṁ bhattena nimantesiṁ,",
  "vv34:12.4": "revataṁ attanaṭṭhamaṁ.",
  "vv34:13.1": "So me atthapurekkhāro,",
  "vv34:13.2": "anukampāya revato;",
  "vv34:13.3": "Saṅghe dehīti maṁvoca,",
  "vv34:13.4": "tassāhaṁ vacanaṁ kariṁ.",
  "vv34:14.1": "Sā dakkhiṇā saṅghagatā,",
  "vv34:14.2": "appameyye patiṭṭhitā;",
  "vv34:14.3": "Puggalesu tayā dinnaṁ,",
  "vv34:14.4": "na taṁ tava mahapphalan”ti.",
  "vv34:15.1": "“Idānevāhaṁ jānāmi,",
  "vv34:15.2": "saṅghe dinnaṁ mahapphalaṁ;",
  "vv34:15.3": "Sāhaṁ gantvā manussattaṁ,",
  "vv34:15.4": "vadaññū vītamaccharā;",
  "vv34:15.5": "Saṅghe dānāni dassāmi,",
  "vv34:15.6": "appamattā punappunan”ti.",
  "vv34:16.1": "“Kā esā devatā bhadde,",
  "vv34:16.2": "tayā mantayate saha;",
  "vv34:16.3": "Sabbe deve tāvatiṁse,",
  "vv34:16.4": "vaṇṇena atirocatī”ti.",
  "vv34:17.1": "“Manussabhūtā devinda,",
  "vv34:17.2": "pubbe mānusake bhave;",
  "vv34:17.3": "Sahabhariyā ca me āsi,",
  "vv34:17.4": "bhaginī ca kaniṭṭhikā;",
  "vv34:17.5": "Saṅghe dānāni datvāna,",
  "vv34:17.6": "katapuññā virocatī”ti.",
  "vv34:18.1": "“Dhammena pubbe bhaginī,",
  "vv34:18.2": "tayā bhadde virocati;",
  "vv34:18.3": "Yaṁ saṅghamhi appameyye,",
  "vv34:18.4": "patiṭṭhāpesi dakkhiṇaṁ”.",
  "vv34:19.1": "“Pucchito hi mayā buddho,",
  "vv34:19.2": "gijjhakūṭamhi pabbate;",
  "vv34:19.3": "Vipākaṁ saṁvibhāgassa,",
  "vv34:19.4": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:20.1": "Yajamānānaṁ manussānaṁ,",
  "vv34:20.2": "puññapekkhāna pāṇinaṁ;",
  "vv34:20.3": "Karotaṁ opadhikaṁ puññaṁ,",
  "vv34:20.4": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:21.1": "Taṁ me buddho viyākāsi,",
  "vv34:21.2": "jānaṁ kammaphalaṁ sakaṁ;",
  "vv34:21.3": "Vipākaṁ saṁvibhāgassa,",
  "vv34:21.4": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:22.1": "Cattāro ca paṭipannā,",
  "vv34:22.2": "cattāro ca phale ṭhitā;",
  "vv34:22.3": "Esa saṅgho ujubhūto,",
  "vv34:22.4": "paññāsīlasamāhito.",
  "vv34:23.1": "Yajamānānaṁ manussānaṁ,",
  "vv34:23.2": "puññapekkhāna pāṇinaṁ;",
  "vv34:23.3": "Karotaṁ opadhikaṁ puññaṁ,",
  "vv34:23.4": "saṅghe dinnaṁ mahapphalaṁ.",
  "vv34:24.1": "Eso hi saṅgho vipulo mahaggato,",
  "vv34:24.2": "Esappameyyo udadhīva sāgaro;",
  "vv34:24.3": "Ete hi seṭṭhā naravīrasāvakā,",
  "vv34:24.4": "Pabhaṅkarā dhammamudīrayanti.",
  "vv34:25.1": "Tesaṁ sudinnaṁ suhutaṁ suyiṭṭhaṁ,",
  "vv34:25.2": "Ye saṅghamuddissa dadanti dānaṁ;",
  "vv34:25.3": "Sā dakkhiṇā saṅghagatā patiṭṭhitā,",
  "vv34:25.4": "Mahapphalā lokavidūna vaṇṇitā.",
  "vv34:26.1": "Etādisaṁ yaññamanussarantā,",
  "vv34:26.2": "Ye vedajātā vicaranti loke;",
  "vv34:26.3": "Vineyya maccheramalaṁ samūlaṁ,",
  "vv34:26.4": "Aninditā saggamupenti ṭhānan”ti.",
  "vv34:27.1": "Daddallavimānaṁ chaṭṭhaṁ."
}