{
  "ja357:0.1": "Jātaka",
  "ja357:0.2": "Pañcakanipāta",
  "ja357:0.3": "Maṇikuṇḍalavagga",
  "ja357:0.4": "7. Laṭukikajātaka",
  "ja357:1.1": "“Vandāmi taṃ kuñjara saṭṭhihāyanaṃ,",
  "ja357:1.2": "Āraññakaṃ yūthapatiṃ yasassiṃ;",
  "ja357:1.3": "Pakkhehi taṃ pañjalikaṃ karomi,",
  "ja357:1.4": "Mā me vadhī puttake dubbalāya”.",
  "ja357:2.1": "“Vandāmi taṃ kuñjara ekacāriṃ,",
  "ja357:2.2": "Āraññakaṃ pabbatasānugocaraṃ;",
  "ja357:2.3": "Pakkhehi taṃ pañjalikaṃ karomi,",
  "ja357:2.4": "Mā me vadhī puttake dubbalāya”.",
  "ja357:3.1": "“Vadhissāmi te laṭukike puttakāni,",
  "ja357:3.2": "Kiṃ me tuvaṃ kāhasi dubbalāsi;",
  "ja357:3.3": "Sataṃ sahassānipi tādisīnaṃ,",
  "ja357:3.4": "Vāmena pādena papothayeyyaṃ”.",
  "ja357:4.1": "“Na heva sabbattha balena kiccaṃ,",
  "ja357:4.2": "Balañhi bālassa vadhāya hoti;",
  "ja357:4.3": "Karissāmi te nāgarājā anatthaṃ,",
  "ja357:4.4": "Yo me vadhī puttake dubbalāya”.",
  "ja357:5.1": "“Kākañca passa laṭukikaṃ,",
  "ja357:5.2": "Maṇḍūkaṃ nīlamakkhikaṃ;",
  "ja357:5.3": "Ete nāgaṃ aghātesuṃ,",
  "ja357:5.4": "Passa verassa verinaṃ;",
  "ja357:5.5": "Tasmā hi veraṃ na kayirātha,",
  "ja357:5.6": "Appiyenapi kenacī”ti.",
  "ja357:6.1": "Laṭukikajātakaṃ sattamaṃ."
}