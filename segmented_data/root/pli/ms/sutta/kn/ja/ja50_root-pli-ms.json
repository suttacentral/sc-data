{
  "ja50:0.1": "Jātaka",
  "ja50:0.2": "Ekakanipāta",
  "ja50:0.3": "Atthakāmavagga",
  "ja50:0.4": "10. Dummedhajātaka",
  "ja50:1.1": "“Dummedhānaṁ sahassena,",
  "ja50:1.2": "yañño me upayācito;",
  "ja50:1.3": "Idāni khohaṁ yajissāmi,",
  "ja50:1.4": "bahu adhammiko jano”ti.",
  "ja50:2.1": "Dummedhajātakaṁ dasamaṁ.",
  "ja50:2.2": "Atthakāmavaggo pañcamo.",
  "ja50:3.1": "Tassuddānaṁ",
  "ja50:4.1": "Atha mittaka mātu kapotavaro,",
  "ja50:4.2": "Tathā veḷuka eḷamūgo rohiṇī;",
  "ja50:4.3": "Kapi vāruṇi cetacarā ca puna,",
  "ja50:4.4": "Tathā tāraka yaññavarena dasāti.",
  "ja50:5.1": "Paṭhamo paṇṇāsako."
}