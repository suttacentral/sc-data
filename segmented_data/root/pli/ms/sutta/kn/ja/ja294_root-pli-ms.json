{
  "ja294:0.1": "Jātaka",
  "ja294:0.2": "Tikanipāta",
  "ja294:0.3": "Kumbhavagga",
  "ja294:0.4": "4. Jambukhādakajātaka",
  "ja294:1.1": "“Koyaṁ bindussaro vaggu,",
  "ja294:1.2": "saravantānamuttamo;",
  "ja294:1.3": "Accuto jambusākhāya,",
  "ja294:1.4": "moracchāpova kūjati”.",
  "ja294:2.1": "“Kulaputtova jānāti,",
  "ja294:2.2": "kulaputtaṁ pasaṁsituṁ;",
  "ja294:2.3": "Byagghacchāpasarīvaṇṇa,",
  "ja294:2.4": "bhuñja samma dadāmi te”.",
  "ja294:3.1": "“Cirassaṁ vata passāmi,",
  "ja294:3.2": "musāvādī samāgate;",
  "ja294:3.3": "Vantādaṁ kuṇapādañca,",
  "ja294:3.4": "aññamaññaṁ pasaṁsake”ti.",
  "ja294:4.1": "Jambukhādakajātakaṁ catutthaṁ."
}