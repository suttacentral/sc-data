{
  "ja37:0.1": "Jātaka",
  "ja37:0.2": "Ekakanipāta",
  "ja37:0.3": "Kulāvakavagga",
  "ja37:0.4": "7. Tittirajātaka",
  "ja37:1.1": "“Ye vuḍḍhamapacāyanti,",
  "ja37:1.2": "narā dhammassa kovidā;",
  "ja37:1.3": "Diṭṭheva dhamme pāsaṁsā,",
  "ja37:1.4": "samparāye ca suggatī”ti.",
  "ja37:2.1": "Tittirajātakaṁ sattamaṁ."
}