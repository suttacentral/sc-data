{
  "ja202:0.1": "Jātaka",
  "ja202:0.2": "Dukanipāta",
  "ja202:0.3": "Nataṁdaḷhavagga",
  "ja202:0.4": "2. Keḷisīlajātaka",
  "ja202:1.1": "“Haṁsā koñcā mayūrā ca,",
  "ja202:1.2": "hatthayo pasadā migā;",
  "ja202:1.3": "Sabbe sīhassa bhāyanti,",
  "ja202:1.4": "natthi kāyasmi tulyatā.",
  "ja202:2.1": "Evameva manussesu,",
  "ja202:2.2": "daharo cepi paññavā;",
  "ja202:2.3": "So hi tattha mahā hoti,",
  "ja202:2.4": "neva bālo sarīravā”ti.",
  "ja202:3.1": "Keḷisīlajātakaṁ dutiyaṁ."
}