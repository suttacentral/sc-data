{
  "ja156:0.1": "Jātaka",
  "ja156:0.2": "Dukanipāta",
  "ja156:0.3": "Daḷhavagga",
  "ja156:0.4": "6. Alīnacittajātaka",
  "ja156:1.1": "“Alīnacittaṁ nissāya,",
  "ja156:1.2": "pahaṭṭhā mahatī camū;",
  "ja156:1.3": "Kosalaṁ senāsantuṭṭhaṁ,",
  "ja156:1.4": "jīvaggāhaṁ agāhayi.",
  "ja156:2.1": "Evaṁ nissayasampanno,",
  "ja156:2.2": "bhikkhu āraddhavīriyo;",
  "ja156:2.3": "Bhāvayaṁ kusalaṁ dhammaṁ,",
  "ja156:2.4": "yogakkhemassa pattiyā;",
  "ja156:2.5": "Pāpuṇe anupubbena,",
  "ja156:2.6": "sabbasaṁyojanakkhayan”ti.",
  "ja156:3.1": "Alīnacittajātakaṁ chaṭṭhaṁ."
}