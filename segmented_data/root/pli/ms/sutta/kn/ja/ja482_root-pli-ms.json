{
  "ja482:0.1": "Jātaka",
  "ja482:0.2": "Terasakanipāta",
  "ja482:0.3": "Ambavagga",
  "ja482:0.4": "9. Rurumigarājajātaka",
  "ja482:1.1": "“Tassa gāmavaraṃ dammi,",
  "ja482:1.2": "Nāriyo ca alaṅkatā;",
  "ja482:1.3": "Yo metaṃ migamakkhāti,",
  "ja482:1.4": "Migānaṃ migamuttamaṃ”.",
  "ja482:2.1": "“Mayhaṃ gāmavaraṃ dehi,",
  "ja482:2.2": "nāriyo ca alaṅkatā;",
  "ja482:2.3": "Ahaṃ te migamakkhissaṃ,",
  "ja482:2.4": "migānaṃ migamuttamaṃ.",
  "ja482:3.1": "Etasmiṃ vanasaṇḍasmiṃ,",
  "ja482:3.2": "ambā sālā ca pupphitā;",
  "ja482:3.3": "Indagopakasañchannā,",
  "ja482:3.4": "ettheso tiṭṭhate migo”.",
  "ja482:4.1": "“Dhanuṃ advejjhaṃ katvāna,",
  "ja482:4.2": "usuṃ sannayhupāgami;",
  "ja482:4.3": "Migo ca disvā rājānaṃ,",
  "ja482:4.4": "dūrato ajjhabhāsatha.",
  "ja482:5.1": "Āgamehi mahārāja,",
  "ja482:5.2": "mā maṃ vijjhi rathesabha;",
  "ja482:5.3": "Ko nu te idamakkhāsi,",
  "ja482:5.4": "ettheso tiṭṭhate migo”.",
  "ja482:6.1": "“Esa pāpacaro poso,",
  "ja482:6.2": "samma tiṭṭhati ārakā;",
  "ja482:6.3": "Soyaṃ me idamakkhāsi,",
  "ja482:6.4": "ettheso tiṭṭhate migo”.",
  "ja482:7.1": "“Saccaṃ kireva māhaṃsu,",
  "ja482:7.2": "narā ekacciyā idha;",
  "ja482:7.3": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "ja482:7.4": "na tvevekacciyo naro”.",
  "ja482:8.1": "“Kiṃ nu ruru garahasi migānaṃ,",
  "ja482:8.2": "Kiṃ pakkhīnaṃ kiṃ pana mānusānaṃ;",
  "ja482:8.3": "Bhayañhi maṃ vindatinapparūpaṃ,",
  "ja482:8.4": "Sutvāna taṃ mānusiṃ bhāsamānaṃ”.",
  "ja482:9.1": "“Yamuddhariṃ vāhane vuyhamānaṃ,",
  "ja482:9.2": "Mahodake salile sīghasote;",
  "ja482:9.3": "Tatonidānaṃ bhayamāgataṃ mama,",
  "ja482:9.4": "Dukkho have rāja asabbhi saṅgamo”.",
  "ja482:10.1": "“Sohaṃ catuppattamimaṃ vihaṅgamaṃ,",
  "ja482:10.2": "Tanucchidaṃ hadaye ossajāmi;",
  "ja482:10.3": "Hanāmi taṃ mittadubbhiṃ akiccakāriṃ,",
  "ja482:10.4": "Yo tādisaṃ kammakataṃ na jāne”.",
  "ja482:11.1": "“Dhīrassa bālassa have janinda,",
  "ja482:11.2": "Santo vadhaṃ nappasaṃsanti jātu;",
  "ja482:11.3": "Kāmaṃ gharaṃ gacchatu pāpadhammo,",
  "ja482:11.4": "Yañcassa bhaṭṭhaṃ tadetassa dehi;",
  "ja482:11.5": "Ahañca te kāmakaro bhavāmi”.",
  "ja482:12.1": "“Addhā rurū aññataro sataṃ so,",
  "ja482:12.2": "Yo dubbhato mānusassa na dubbhi;",
  "ja482:12.3": "Kāmaṃ gharaṃ gacchatu pāpadhammo,",
  "ja482:12.4": "Yañcassa bhaṭṭhaṃ tadetassa dammi;",
  "ja482:12.5": "Ahañca te kāmacāraṃ dadāmi”.",
  "ja482:13.1": "“Suvijānaṃ siṅgālānaṃ,",
  "ja482:13.2": "sakuṇānañca vassitaṃ;",
  "ja482:13.3": "Manussavassitaṃ rāja,",
  "ja482:13.4": "dubbijānataraṃ tato.",
  "ja482:14.1": "Api ce maññatī poso,",
  "ja482:14.2": "ñāti mitto sakhāti vā;",
  "ja482:14.3": "Yo pubbe sumano hutvā,",
  "ja482:14.4": "pacchā sampajjate diso”.",
  "ja482:15.1": "“Samāgatā jānapadā,",
  "ja482:15.2": "negamā ca samāgatā;",
  "ja482:15.3": "Migā sassāni khādanti,",
  "ja482:15.4": "taṃ devo paṭisedhatu”.",
  "ja482:16.1": "“Kāmaṃ janapado māsi,",
  "ja482:16.2": "raṭṭhañcāpi vinassatu;",
  "ja482:16.3": "Na tvevāhaṃ ruruṃ dubbhe,",
  "ja482:16.4": "datvā abhayadakkhiṇaṃ.",
  "ja482:17.1": "Mā me janapado āsi,",
  "ja482:17.2": "raṭṭhañcāpi vinassatu;",
  "ja482:17.3": "Na tvevāhaṃ migarājassa,",
  "ja482:17.4": "varaṃ datvā musā bhaṇe”ti.",
  "ja482:18.1": "Rurumigarājajātakaṃ navamaṃ."
}