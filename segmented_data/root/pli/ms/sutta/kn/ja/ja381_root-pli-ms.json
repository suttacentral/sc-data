{
  "ja381:0.1": "Jātaka",
  "ja381:0.2": "Chakkanipāta",
  "ja381:0.3": "Avāriyavagga",
  "ja381:0.4": "6. Migālopajātaka",
  "ja381:1.1": "“Na me rucci migālopa,",
  "ja381:1.2": "yassa te tādisī gatī;",
  "ja381:1.3": "Atuccaṁ tāta patasi,",
  "ja381:1.4": "abhūmiṁ tāta sevasi.",
  "ja381:2.1": "Catukkaṇṇaṁva kedāraṁ,",
  "ja381:2.2": "yadā te pathavī siyā;",
  "ja381:2.3": "Tato tāta nivattassu,",
  "ja381:2.4": "māssu etto paraṁ gami”.",
  "ja381:3.1": "“Santi aññepi sakuṇā,",
  "ja381:3.2": "pattayānā vihaṅgamā;",
  "ja381:3.3": "Akkhittā vātavegena,",
  "ja381:3.4": "naṭṭhā te sassatīsamā.",
  "ja381:4.1": "Akatvā apanandassa,",
  "ja381:4.2": "pitu vuddhassa sāsanaṁ;",
  "ja381:4.3": "Kālavāte atikkamma,",
  "ja381:4.4": "verambhānaṁ vasaṁ agā.",
  "ja381:5.1": "Tassa puttā ca dārā ca,",
  "ja381:5.2": "ye caññe anujīvino;",
  "ja381:5.3": "Sabbe byasanamāpāduṁ,",
  "ja381:5.4": "anovādakare dije.",
  "ja381:6.1": "Evampi idha vuddhānaṁ,",
  "ja381:6.2": "yo vākyaṁ nāvabujjhati;",
  "ja381:6.3": "Atisīmacaro ditto,",
  "ja381:6.4": "gijjhovātītasāsano;",
  "ja381:6.5": "Sabbe byasanaṁ papponti,",
  "ja381:6.6": "akatvā vuddhasāsanan”ti.",
  "ja381:7.1": "Migālopajātakaṁ chaṭṭhaṁ."
}