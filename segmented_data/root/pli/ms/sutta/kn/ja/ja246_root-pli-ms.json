{
  "ja246:0.1": "Jātaka",
  "ja246:0.2": "Dukanipāta",
  "ja246:0.3": "Siṅgālavagga",
  "ja246:0.4": "6. Bālovādajātaka",
  "ja246:1.1": "“Hantvā chetvā vadhitvā ca,",
  "ja246:1.2": "deti dānaṁ asaññato;",
  "ja246:1.3": "Edisaṁ bhattaṁ bhuñjamāno,",
  "ja246:1.4": "sa pāpena upalippati”.",
  "ja246:2.1": "“Puttadārampi ce hantvā,",
  "ja246:2.2": "Deti dānaṁ asaññato;",
  "ja246:2.3": "Bhuñjamānopi sappañño,",
  "ja246:2.4": "Na pāpena upalippatī”ti.",
  "ja246:3.1": "Bālovādajātakaṁ chaṭṭhaṁ."
}