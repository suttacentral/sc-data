{
  "ja68:0.1": "Jātaka",
  "ja68:0.2": "Ekakanipāta",
  "ja68:0.3": "Itthivagga",
  "ja68:0.4": "8. Sāketajātaka",
  "ja68:1.1": "Yasmiṁ mano nivisati,",
  "ja68:1.2": "cittañcāpi pasīdati;",
  "ja68:1.3": "Adiṭṭhapubbake pose,",
  "ja68:1.4": "kāmaṁ tasmimpi vissase”ti.",
  "ja68:2.1": "Sāketajātakaṁ aṭṭhamaṁ."
}