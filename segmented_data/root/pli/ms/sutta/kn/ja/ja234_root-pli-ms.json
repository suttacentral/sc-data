{
  "ja234:0.1": "Jātaka",
  "ja234:0.2": "Dukanipāta",
  "ja234:0.3": "Upāhanavagga",
  "ja234:0.4": "4. Asitābhūjātaka",
  "ja234:1.1": "“Tvameva dānimakara,",
  "ja234:1.2": "Yaṁ kāmo byagamā tayi;",
  "ja234:1.3": "Soyaṁ appaṭisandhiko,",
  "ja234:1.4": "Kharachinnaṁva renukaṁ”.",
  "ja234:2.1": "“Atricchaṁ atilobhena,",
  "ja234:2.2": "atilobhamadena ca;",
  "ja234:2.3": "Evaṁ hāyati atthamhā,",
  "ja234:2.4": "ahaṁva asitābhuyā”ti.",
  "ja234:3.1": "Asitābhūjātakaṁ catutthaṁ."
}