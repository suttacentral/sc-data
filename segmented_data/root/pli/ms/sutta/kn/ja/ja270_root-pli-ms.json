{
  "ja270:0.1": "Jātaka",
  "ja270:0.2": "Tikanipāta",
  "ja270:0.3": "Padumavagga",
  "ja270:0.4": "10. Ulūkajātaka",
  "ja270:1.1": "“Sabbehi kira ñātīhi,",
  "ja270:1.2": "kosiyo issaro kato;",
  "ja270:1.3": "Sace ñātīhi anuññāto,",
  "ja270:1.4": "bhaṇeyyāhaṁ ekavācikaṁ”.",
  "ja270:2.1": "“Bhaṇa samma anuññāto,",
  "ja270:2.2": "atthaṁ dhammañca kevalaṁ;",
  "ja270:2.3": "Santi hi daharā pakkhī,",
  "ja270:2.4": "paññavanto jutindharā”.",
  "ja270:3.1": "“Na me ruccati bhaddaṁ vo,",
  "ja270:3.2": "ulūkassābhisecanaṁ;",
  "ja270:3.3": "Akkuddhassa mukhaṁ passa,",
  "ja270:3.4": "kathaṁ kuddho karissatī”ti.",
  "ja270:4.1": "Ulūkajātakaṁ dasamaṁ.",
  "ja270:4.2": "Padumavaggo dutiyo.",
  "ja270:5.1": "Tassuddānaṁ",
  "ja270:6.1": "Padumuttama nāgasirivhayano,",
  "ja270:6.2": "Sa-mahaṇṇava yūpa khurappavaro;",
  "ja270:6.3": "Atha bhaddalī kuñjara rukkha puna,",
  "ja270:6.4": "Kharavāca ulūkavarena dasāti."
}