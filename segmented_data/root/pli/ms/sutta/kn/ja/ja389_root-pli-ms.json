{
  "ja389:0.1": "Jātaka",
  "ja389:0.2": "Chakkanipāta",
  "ja389:0.3": "Kharaputtavagga",
  "ja389:0.4": "4. Suvaṇṇakakkaṭajātaka",
  "ja389:1.1": "“Siṅgīmigo āyatacakkhunetto,",
  "ja389:1.2": "Aṭṭhittaco vārisayo alomo;",
  "ja389:1.3": "Tenābhibhūto kapaṇaṁ rudāmi,",
  "ja389:1.4": "Hare sakhā kissa nu maṁ jahāsi”.",
  "ja389:2.1": "So passasanto mahatā phaṇena,",
  "ja389:2.2": "Bhujaṅgamo kakkaṭamajjhapatto;",
  "ja389:2.3": "Sakhā sakhāraṁ paritāyamāno,",
  "ja389:2.4": "Bhujaṅgamaṁ kakkaṭako gahesi.",
  "ja389:3.1": "“Na vāyasaṁ no pana kaṇhasappaṁ,",
  "ja389:3.2": "Ghāsatthiko kakkaṭako adeyya;",
  "ja389:3.3": "Pucchāmi taṁ āyatacakkhunetta,",
  "ja389:3.4": "Atha kissa hetumha ubho gahītā”.",
  "ja389:4.1": "“Ayaṁ puriso mama atthakāmo,",
  "ja389:4.2": "Yo maṁ gahetvāna dakāya neti;",
  "ja389:4.3": "Tasmiṁ mate dukkhamanappakaṁ me,",
  "ja389:4.4": "Ahañca eso ca ubho na homa.",
  "ja389:5.1": "Mamañca disvāna pavaddhakāyaṁ,",
  "ja389:5.2": "Sabbo jano hiṁsitumeva micche;",
  "ja389:5.3": "Sāduñca thūlañca muduñca maṁsaṁ,",
  "ja389:5.4": "Kākāpi maṁ disvāna viheṭhayeyyuṁ”.",
  "ja389:6.1": "“Sacetassa hetumha ubho gahītā,",
  "ja389:6.2": "Uṭṭhātu poso visamāvamāmi;",
  "ja389:6.3": "Mamañca kākañca pamuñca khippaṁ,",
  "ja389:6.4": "Pure visaṁ gāḷhamupeti maccaṁ”.",
  "ja389:7.1": "“Sappaṁ pamokkhāmi na tāva kākaṁ,",
  "ja389:7.2": "Paṭibandhako hohiti tāva kāko;",
  "ja389:7.3": "Purisañca disvāna sukhiṁ arogaṁ,",
  "ja389:7.4": "Kākaṁ pamokkhāmi yatheva sappaṁ”.",
  "ja389:8.1": "“Kāko tadā devadatto ahosi,",
  "ja389:8.2": "Māro pana kaṇhasappo ahosi;",
  "ja389:8.3": "Ānandabhaddo kakkaṭako ahosi,",
  "ja389:8.4": "Ahaṁ tadā brāhmaṇo homi satthā”ti.",
  "ja389:9.1": "Suvaṇṇakakkaṭajātakaṁ catutthaṁ."
}