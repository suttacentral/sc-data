{
  "ja383:0.1": "Jātaka",
  "ja383:0.2": "Chakkanipāta",
  "ja383:0.3": "Avāriyavagga",
  "ja383:0.4": "8. Kukkuṭajātaka",
  "ja383:1.1": "“Sucittapattachadana,",
  "ja383:1.2": "tambacūḷa vihaṅgama;",
  "ja383:1.3": "Oroha dumasākhāya,",
  "ja383:1.4": "mudhā bhariyā bhavāmi te”.",
  "ja383:2.1": "“Catuppadī tvaṁ kalyāṇi,",
  "ja383:2.2": "dvipadāhaṁ manorame;",
  "ja383:2.3": "Migī pakkhī asaññuttā,",
  "ja383:2.4": "aññaṁ pariyesa sāmikaṁ”.",
  "ja383:3.1": "“Komārikā te hessāmi,",
  "ja383:3.2": "Mañjukā piyabhāṇinī;",
  "ja383:3.3": "Vinda maṁ ariyena vedena,",
  "ja383:3.4": "Sāvaya maṁ yadicchasi”.",
  "ja383:4.1": "“Kuṇapādini lohitape,",
  "ja383:4.2": "cori kukkuṭapothini;",
  "ja383:4.3": "Na tvaṁ ariyena vedena,",
  "ja383:4.4": "mamaṁ bhattāramicchasi”.",
  "ja383:5.1": "“Evampi caturā nārī,",
  "ja383:5.2": "disvāna sadhanaṁ naraṁ;",
  "ja383:5.3": "Nenti saṇhāhi vācāhi,",
  "ja383:5.4": "biḷārī viya kukkuṭaṁ.",
  "ja383:6.1": "Yo ca uppatitaṁ atthaṁ,",
  "ja383:6.2": "na khippamanubujjhati;",
  "ja383:6.3": "Amittavasamanveti,",
  "ja383:6.4": "pacchā ca anutappati.",
  "ja383:7.1": "Yo ca uppatitaṁ atthaṁ,",
  "ja383:7.2": "khippameva nibodhati;",
  "ja383:7.3": "Muccate sattusambādhā,",
  "ja383:7.4": "kukkuṭova biḷāriyā”ti.",
  "ja383:8.1": "Kukkuṭajātakaṁ aṭṭhamaṁ."
}