{
  "ja354:0.1": "Jātaka",
  "ja354:0.2": "Pañcakanipāta",
  "ja354:0.3": "Maṇikuṇḍalavagga",
  "ja354:0.4": "4. Uragajātaka",
  "ja354:1.1": "“Uragova tacaṁ jiṇṇaṁ,",
  "ja354:1.2": "hitvā gacchati saṁ tanuṁ;",
  "ja354:1.3": "Evaṁ sarīre nibbhoge,",
  "ja354:1.4": "pete kālaṅkate sati.",
  "ja354:2.1": "Ḍayhamāno na jānāti,",
  "ja354:2.2": "ñātīnaṁ paridevitaṁ;",
  "ja354:2.3": "Tasmā etaṁ na socāmi,",
  "ja354:2.4": "gato so tassa yā gati”.",
  "ja354:3.1": "“Anavhito tato āgā,",
  "ja354:3.2": "ananuññāto ito gato;",
  "ja354:3.3": "Yathāgato tathā gato,",
  "ja354:3.4": "tattha kā paridevanā.",
  "ja354:4.1": "Ḍayhamāno na jānāti,",
  "ja354:4.2": "ñātīnaṁ paridevitaṁ;",
  "ja354:4.3": "Tasmā etaṁ na socāmi,",
  "ja354:4.4": "gato so tassa yā gati”.",
  "ja354:5.1": "“Sace rode kisā assaṁ,",
  "ja354:5.2": "tassā me kiṁ phalaṁ siyā;",
  "ja354:5.3": "Ñātimittasuhajjānaṁ,",
  "ja354:5.4": "bhiyyo no aratī siyā.",
  "ja354:6.1": "Ḍayhamāno na jānāti,",
  "ja354:6.2": "ñātīnaṁ paridevitaṁ;",
  "ja354:6.3": "Tasmā etaṁ na socāmi,",
  "ja354:6.4": "gato so tassa yā gati”.",
  "ja354:7.1": "“Yathāpi dārako candaṁ,",
  "ja354:7.2": "gacchantamanurodati;",
  "ja354:7.3": "Evaṁsampadamevetaṁ,",
  "ja354:7.4": "yo petamanusocati.",
  "ja354:8.1": "Ḍayhamāno na jānāti,",
  "ja354:8.2": "ñātīnaṁ paridevitaṁ;",
  "ja354:8.3": "Tasmā etaṁ na socāmi,",
  "ja354:8.4": "gato so tassa yā gati”.",
  "ja354:9.1": "“Yathāpi udakakumbho,",
  "ja354:9.2": "bhinno appaṭisandhiyo;",
  "ja354:9.3": "Evaṁsampadamevetaṁ,",
  "ja354:9.4": "yo petamanusocati.",
  "ja354:10.1": "Ḍayhamāno na jānāti,",
  "ja354:10.2": "ñātīnaṁ paridevitaṁ;",
  "ja354:10.3": "Tasmā etaṁ na socāmi,",
  "ja354:10.4": "gato so tassa yā gatī”ti.",
  "ja354:11.1": "Uragajātakaṁ catutthaṁ."
}