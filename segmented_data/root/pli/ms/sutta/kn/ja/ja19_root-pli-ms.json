{
  "ja19:0.1": "Jātaka",
  "ja19:0.2": "Ekakanipāta",
  "ja19:0.3": "Sīlavagga",
  "ja19:0.4": "9. Āyācitabhattajātaka",
  "ja19:1.1": "“Sace mucce pecca mucce,",
  "ja19:1.2": "Muccamāno hi bajjhati;",
  "ja19:1.3": "Na hevaṁ dhīrā muccanti,",
  "ja19:1.4": "Mutti bālassa bandhanan”ti.",
  "ja19:2.1": "Āyācitabhattajātakaṁ navamaṁ."
}