{
  "ja333:0.1": "Jātaka",
  "ja333:0.2": "Catukkanipāta",
  "ja333:0.3": "Kokilavagga",
  "ja333:0.4": "3. Pakkagodhajātaka",
  "ja333:1.1": "“Tadeva me tvaṁ vidito,",
  "ja333:1.2": "vanamajjhe rathesabha;",
  "ja333:1.3": "Yassa te khaggabaddhassa,",
  "ja333:1.4": "sannaddhassa tirīṭino;",
  "ja333:1.5": "Assatthadumasākhāya,",
  "ja333:1.6": "pakkā godhā palāyatha”.",
  "ja333:2.1": "“Name namantassa bhaje bhajantaṁ,",
  "ja333:2.2": "Kiccānukubbassa kareyya kiccaṁ;",
  "ja333:2.3": "Nānatthakāmassa kareyya atthaṁ,",
  "ja333:2.4": "Asambhajantampi na sambhajeyya.",
  "ja333:3.1": "Caje cajantaṁ vanathaṁ na kayirā,",
  "ja333:3.2": "Apetacittena na sambhajeyya;",
  "ja333:3.3": "Dijo dumaṁ khīṇaphalanti ñatvā,",
  "ja333:3.4": "Aññaṁ samekkheyya mahā hi loko”.",
  "ja333:4.1": "“So te karissāmi yathānubhāvaṁ,",
  "ja333:4.2": "Kataññutaṁ khattiye pekkhamāno;",
  "ja333:4.3": "Sabbañca te issariyaṁ dadāmi,",
  "ja333:4.4": "Yassicchasī tassa tuvaṁ dadāmī”ti.",
  "ja333:5.1": "Pakkagodhajātakaṁ tatiyaṁ."
}