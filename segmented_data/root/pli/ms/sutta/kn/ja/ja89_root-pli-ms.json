{
  "ja89:0.1": "Jātaka",
  "ja89:0.2": "Ekakanipāta",
  "ja89:0.3": "Apāyimhavagga",
  "ja89:0.4": "9. Kuhakajātaka",
  "ja89:1.1": "“Vācāva kira te āsi,",
  "ja89:1.2": "saṇhā sakhilabhāṇino;",
  "ja89:1.3": "Tiṇamatte asajjittho,",
  "ja89:1.4": "no ca nikkhasataṁ haran”ti.",
  "ja89:2.1": "Kuhakajātakaṁ navamaṁ."
}