{
  "ja486:0.1": "Jātaka",
  "ja486:0.2": "Pakiṇṇakanipāta",
  "ja486:0.3": "Sālikedāravagga",
  "ja486:0.4": "3. Mahāukkusajātaka",
  "ja486:1.1": "“Ukkā cilācā bandhanti dīpe,",
  "ja486:1.2": "Pajā mamaṁ khādituṁ patthayanti;",
  "ja486:1.3": "Mittaṁ sahāyañca vadehi senaka,",
  "ja486:1.4": "Ācikkha ñātibyasanaṁ dijānaṁ”.",
  "ja486:2.1": "“Dijo dijānaṁ pavarosi pakkhima,",
  "ja486:2.2": "Ukkusarāja saraṇaṁ taṁ upema;",
  "ja486:2.3": "Pajā mamaṁ khādituṁ patthayanti,",
  "ja486:2.4": "Luddā cilācā bhava me sukhāya”.",
  "ja486:3.1": "“Mittaṁ sahāyañca karonti paṇḍitā,",
  "ja486:3.2": "Kāle akāle sukhamesamānā;",
  "ja486:3.3": "Karomi te senaka etamatthaṁ,",
  "ja486:3.4": "Ariyo hi ariyassa karoti kiccaṁ”.",
  "ja486:4.1": "“Yaṁ hoti kiccaṁ anukampakena,",
  "ja486:4.2": "Ariyassa ariyena kataṁ tayīdaṁ;",
  "ja486:4.3": "Attānurakkhī bhava mā adayhi,",
  "ja486:4.4": "Lacchāma putte tayi jīvamāne”.",
  "ja486:5.1": "“Taveva rakkhāvaraṇaṁ karonto,",
  "ja486:5.2": "Sarīrabhedāpi na santasāmi;",
  "ja486:5.3": "Karonti heke sakhīnaṁ sakhāro,",
  "ja486:5.4": "Pāṇaṁ cajantā satamesa dhammo”.",
  "ja486:6.1": "“Sudukkaraṁ kammamakāsi,",
  "ja486:6.2": "aṇḍajāyaṁ vihaṅgamo;",
  "ja486:6.3": "Atthāya kuraro putte,",
  "ja486:6.4": "aḍḍharatte anāgate”.",
  "ja486:7.1": "“Cutāpi heke khalitā sakammunā,",
  "ja486:7.2": "Mittānukampāya patiṭṭhahanti;",
  "ja486:7.3": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:7.4": "Atthaṁ caretho mama vāricara”.",
  "ja486:8.1": "“Dhanena dhaññena ca attanā ca,",
  "ja486:8.2": "Mittaṁ sahāyañca karonti paṇḍitā;",
  "ja486:8.3": "Karomi te senaka etamatthaṁ,",
  "ja486:8.4": "Ariyo hi ariyassa karoti kiccaṁ”.",
  "ja486:9.1": "“Appossukko tāta tuvaṁ nisīda,",
  "ja486:9.2": "Putto pitu carati atthacariyaṁ;",
  "ja486:9.3": "Ahaṁ carissāmi tavetamatthaṁ,",
  "ja486:9.4": "Senassa putte paritāyamāno”.",
  "ja486:10.1": "“Addhā hi tāta satamesa dhammo,",
  "ja486:10.2": "Putto pitu yaṁ care atthacariyaṁ;",
  "ja486:10.3": "Appeva maṁ disvāna pavaḍḍhakāyaṁ,",
  "ja486:10.4": "Senassa puttā na viheṭhayeyyuṁ”.",
  "ja486:11.1": "“Pasū manussā migavīraseṭṭha,",
  "ja486:11.2": "Bhayaṭṭitā seṭṭhamupabbajanti;",
  "ja486:11.3": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:11.4": "Tvaṁ nosi rājā bhava me sukhāya”.",
  "ja486:12.1": "“Karomi te senaka etamatthaṁ,",
  "ja486:12.2": "Āyāmi te taṁ disataṁ vadhāya;",
  "ja486:12.3": "Kathañhi viññū pahu sampajāno,",
  "ja486:12.4": "Na vāyame attajanassa guttiyā”.",
  "ja486:13.1": "“Mittañca kayirātha suhadayañca,",
  "ja486:13.2": "Ayirañca kayirātha sukhāgamāya;",
  "ja486:13.3": "Nivatthakocova sarebhihantvā,",
  "ja486:13.4": "Modāma puttehi samaṅgibhūtā.",
  "ja486:14.1": "Sakamittassa kammena,",
  "ja486:14.2": "sahāyassāpalāyino;",
  "ja486:14.3": "Kūjantamupakūjanti,",
  "ja486:14.4": "lomasā hadayaṅgamaṁ.",
  "ja486:15.1": "Mittaṁ sahāyaṁ adhigamma paṇḍito,",
  "ja486:15.2": "So bhuñjatī putta pasuṁ dhanaṁ vā;",
  "ja486:15.3": "Ahañca puttā ca patī ca mayhaṁ,",
  "ja486:15.4": "Mittānukampāya samaṅgibhūtā.",
  "ja486:16.1": "Rājavatā sūravatā ca attho,",
  "ja486:16.2": "Sampannasakhissa bhavanti hete;",
  "ja486:16.3": "So mittavā yasavā uggatatto,",
  "ja486:16.4": "Asmiṁdhaloke modati kāmakāmī.",
  "ja486:17.1": "Karaṇīyāni mittāni,",
  "ja486:17.2": "daliddenāpi senaka;",
  "ja486:17.3": "Passa mittānukampāya,",
  "ja486:17.4": "samaggamhā sañātake.",
  "ja486:18.1": "Sūrena balavantena,",
  "ja486:18.2": "yo mitte kurute dijo;",
  "ja486:18.3": "Evaṁ so sukhito hoti,",
  "ja486:18.4": "yathāhaṁ tvañca senakāti”.",
  "ja486:19.1": "Mahāukkusajātakaṁ tatiyaṁ."
}