{
  "ja91:0.1": "Jātaka",
  "ja91:0.2": "Ekakanipāta",
  "ja91:0.3": "Littavagga",
  "ja91:0.4": "1. Littajātaka",
  "ja91:1.1": "“Littaṁ paramena tejasā,",
  "ja91:1.2": "Gilamakkhaṁ puriso na bujjhati;",
  "ja91:1.3": "Gila re gila pāpadhuttaka,",
  "ja91:1.4": "Pacchā te kaṭukaṁ bhavissatī”ti.",
  "ja91:2.1": "Littajātakaṁ paṭhamaṁ."
}