{
  "ja185:0.1": "Jātaka",
  "ja185:0.2": "Dukanipāta",
  "ja185:0.3": "Asadisavagga",
  "ja185:0.4": "5. Anabhiratijātaka",
  "ja185:1.1": "“Yathodake āvile appasanne,",
  "ja185:1.2": "Na passati sippikasambukañca;",
  "ja185:1.3": "Sakkharaṁ vālukaṁ macchagumbaṁ,",
  "ja185:1.4": "Evaṁ āvilamhi citte;",
  "ja185:1.5": "Na passati attadatthaṁ paratthaṁ.",
  "ja185:2.1": "Yathodake acche vippasanne,",
  "ja185:2.2": "So passati sippikasambukañca;",
  "ja185:2.3": "Sakkharaṁ vālukaṁ macchagumbaṁ,",
  "ja185:2.4": "Evaṁ anāvilamhi citte;",
  "ja185:2.5": "So passati attadatthaṁ paratthan”ti.",
  "ja185:3.1": "Anabhiratijātakaṁ pañcamaṁ."
}