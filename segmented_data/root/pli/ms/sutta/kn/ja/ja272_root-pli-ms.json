{
  "ja272:0.1": "Jātaka",
  "ja272:0.2": "Tikanipāta",
  "ja272:0.3": "Udapānavagga",
  "ja272:0.4": "2. Byagghajātaka",
  "ja272:1.1": "“Yena mittena saṁsaggā,",
  "ja272:1.2": "yogakkhemo vihiyyati;",
  "ja272:1.3": "Pubbevajjhābhavaṁ tassa,",
  "ja272:1.4": "rukkhe akkhīva paṇḍito.",
  "ja272:2.1": "Yena mittena saṁsaggā,",
  "ja272:2.2": "yogakkhemo pavaḍḍhati;",
  "ja272:2.3": "Kareyyattasamaṁ vuttiṁ,",
  "ja272:2.4": "sabbakiccesu paṇḍito”.",
  "ja272:3.1": "“Etha byagghā nivattavho,",
  "ja272:3.2": "Paccupetha mahāvanaṁ;",
  "ja272:3.3": "Mā vanaṁ chindi nibyagghaṁ,",
  "ja272:3.4": "Byagghā māhesu nibbanā”ti.",
  "ja272:4.1": "Byagghajātakaṁ dutiyaṁ."
}