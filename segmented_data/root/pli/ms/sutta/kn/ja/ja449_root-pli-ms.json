{
  "ja449:0.1": "Jātaka",
  "ja449:0.2": "Dasakanipāta",
  "ja449:0.3": "Catudvāravagga",
  "ja449:0.4": "11. Maṭṭhakuṇḍalījātaka",
  "ja449:1.1": "“Alaṅkato maṭṭhakuṇḍalī,",
  "ja449:1.2": "Māladhārī haricandanussado;",
  "ja449:1.3": "Bāhā paggayha kandasi,",
  "ja449:1.4": "Vanamajjhe kiṁ dukkhito tuvaṁ”.",
  "ja449:2.1": "“Sovaṇṇamayo pabhassaro,",
  "ja449:2.2": "Uppanno rathapañjaro mama;",
  "ja449:2.3": "Tassa cakkayugaṁ na vindāmi,",
  "ja449:2.4": "Tena dukkhena jahāmi jīvitaṁ”.",
  "ja449:3.1": "“Sovaṇṇamayaṁ maṇīmayaṁ,",
  "ja449:3.2": "Lohamayaṁ atha rūpiyāmayaṁ;",
  "ja449:3.3": "Pāvada rathaṁ karissāmi te,",
  "ja449:3.4": "Cakkayugaṁ paṭipādayāmi taṁ”.",
  "ja449:4.1": "“So māṇavo tassa pāvadi,",
  "ja449:4.2": "Candasūriyā ubhayettha bhātaro;",
  "ja449:4.3": "Sovaṇṇamayo ratho mama,",
  "ja449:4.4": "Tena cakkayugena sobhati”.",
  "ja449:5.1": "“Bālo kho tvaṁsi māṇava,",
  "ja449:5.2": "Yo tvaṁ patthayase apatthiyaṁ;",
  "ja449:5.3": "Maññāmi tuvaṁ marissasi,",
  "ja449:5.4": "Na hi tvaṁ lacchasi candasūriye”.",
  "ja449:6.1": "“Gamanāgamanampi dissati,",
  "ja449:6.2": "Vaṇṇadhātu ubhayettha vīthiyo;",
  "ja449:6.3": "Peto pana neva dissati,",
  "ja449:6.4": "Ko nu kho kandataṁ bālyataro”.",
  "ja449:7.1": "“Saccaṁ kho vadesi māṇava,",
  "ja449:7.2": "Ahameva kandataṁ bālyataro;",
  "ja449:7.3": "Candaṁ viya dārako rudaṁ,",
  "ja449:7.4": "Petaṁ kālakatābhipatthaye.",
  "ja449:8.1": "Ādittaṁ vata maṁ santaṁ,",
  "ja449:8.2": "ghatasittaṁva pāvakaṁ;",
  "ja449:8.3": "Vārinā viya osiñcaṁ,",
  "ja449:8.4": "sabbaṁ nibbāpaye daraṁ.",
  "ja449:9.1": "Abbahī vata me sallaṁ,",
  "ja449:9.2": "yamāsi hadayassitaṁ;",
  "ja449:9.3": "Yo me sokaparetassa,",
  "ja449:9.4": "puttasokaṁ apānudi.",
  "ja449:10.1": "Sohaṁ abbūḷhasallosmi,",
  "ja449:10.2": "vītasoko anāvilo;",
  "ja449:10.3": "Na socāmi na rodāmi,",
  "ja449:10.4": "tava sutvāna māṇavā”ti.",
  "ja449:11.1": "Maṭṭhakuṇḍalījātakaṁ ekādasamaṁ."
}