{
  "ja253:0.1": "Jātaka",
  "ja253:0.2": "Tikanipāta",
  "ja253:0.3": "Saṅkappavagga",
  "ja253:0.4": "3. Maṇikaṇṭhajātaka",
  "ja253:1.1": "“Mamannapānaṁ vipulaṁ uḷāraṁ,",
  "ja253:1.2": "Uppajjatīmassa maṇissa hetu;",
  "ja253:1.3": "Taṁ te na dassaṁ atiyācakosi,",
  "ja253:1.4": "Na cāpi te assamamāgamissaṁ.",
  "ja253:2.1": "Susū yathā sakkharadhotapāṇī,",
  "ja253:2.2": "Tāsesi maṁ selaṁ yācamāno;",
  "ja253:2.3": "Taṁ te na dassaṁ atiyācakosi,",
  "ja253:2.4": "Na cāpi te assamamāgamissaṁ”.",
  "ja253:3.1": "“Na taṁ yāce yassa piyaṁ jigīse,",
  "ja253:3.2": "Desso hoti atiyācanāya;",
  "ja253:3.3": "Nāgo maṇiṁ yācito brāhmaṇena,",
  "ja253:3.4": "Adassanaṁyeva tadajjhagamā”ti.",
  "ja253:4.1": "Maṇikaṇṭhajātakaṁ tatiyaṁ."
}