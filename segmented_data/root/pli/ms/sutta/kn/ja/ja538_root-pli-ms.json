{
  "ja538:0.1": "Jātaka",
  "ja538:0.2": "Mahānipāta",
  "ja538:0.3": "Mūgapakkhavagga",
  "ja538:0.4": "1 Mūgapakkhajātaka",
  "ja538:1.1": "“Mā paṇḍiccayaṃ vibhāvaya,",
  "ja538:1.2": "Bālamato bhava sabbapāṇinaṃ;",
  "ja538:1.3": "Sabbo taṃ jano ocināyatu,",
  "ja538:1.4": "Evaṃ tava attho bhavissati”.",
  "ja538:2.1": "“Karomi te taṃ vacanaṃ,",
  "ja538:2.2": "yaṃ maṃ bhaṇasi devate;",
  "ja538:2.3": "Atthakāmāsi me amma,",
  "ja538:2.4": "hitakāmāsi devate”.",
  "ja538:3.1": "“Kiṃ nu santaramānova,",
  "ja538:3.2": "kāsuṃ khaṇasi sārathi;",
  "ja538:3.3": "Puṭṭho me samma akkhāhi,",
  "ja538:3.4": "kiṃ kāsuyā karissasi”.",
  "ja538:4.1": "“Rañño mūgo ca pakkho ca,",
  "ja538:4.2": "putto jāto acetaso;",
  "ja538:4.3": "Somhi raññā samajjhiṭṭho,",
  "ja538:4.4": "puttaṃ me nikhaṇaṃ vane”.",
  "ja538:5.1": "“Na badhiro na mūgosmi,",
  "ja538:5.2": "na pakkho na ca vīkalo;",
  "ja538:5.3": "Adhammaṃ sārathi kayirā,",
  "ja538:5.4": "mañce tvaṃ nikhaṇaṃ vane.",
  "ja538:6.1": "Ūrū bāhuñca me passa,",
  "ja538:6.2": "bhāsitañca suṇohi me;",
  "ja538:6.3": "Adhammaṃ sārathi kayirā,",
  "ja538:6.4": "mañce tvaṃ nikhaṇaṃ vane”.",
  "ja538:7.1": "“Devatā nusi gandhabbo,",
  "ja538:7.2": "adu sakko purindado;",
  "ja538:7.3": "Ko vā tvaṃ kassa vā putto,",
  "ja538:7.4": "kathaṃ jānemu taṃ mayaṃ”.",
  "ja538:8.1": "“Namhi devo na gandhabbo,",
  "ja538:8.2": "nāpi sakko purindado;",
  "ja538:8.3": "Kāsirañño ahaṃ putto,",
  "ja538:8.4": "yaṃ kāsuyā nikhaññasi.",
  "ja538:9.1": "Tassa rañño ahaṃ putto,",
  "ja538:9.2": "yaṃ tvaṃ sammūpajīvasi;",
  "ja538:9.3": "Adhammaṃ sārathi kayirā,",
  "ja538:9.4": "mañce tvaṃ nikhaṇaṃ vane.",
  "ja538:10.1": "Yassa rukkhassa chāyāya,",
  "ja538:10.2": "nisīdeyya sayeyya vā;",
  "ja538:10.3": "Na tassa sākhaṃ bhañjeyya,",
  "ja538:10.4": "mittadubbho hi pāpako.",
  "ja538:11.1": "Yathā rukkho tathā rājā,",
  "ja538:11.2": "yathā sākhā tathā ahaṃ;",
  "ja538:11.3": "Yathā chāyūpago poso,",
  "ja538:11.4": "evaṃ tvamasi sārathi;",
  "ja538:11.5": "Adhammaṃ sārathi kayirā,",
  "ja538:11.6": "mañce tvaṃ nikhaṇaṃ vane.",
  "ja538:12.1": "Pahūtabhakkho bhavati,",
  "ja538:12.2": "vippavuttho sakaṃ gharā;",
  "ja538:12.3": "Bahū naṃ upajīvanti,",
  "ja538:12.4": "yo mittānaṃ na dubbhati.",
  "ja538:13.1": "Yaṃ yaṃ janapadaṃ yāti,",
  "ja538:13.2": "nigame rājadhāniyo;",
  "ja538:13.3": "Sabbattha pūjito hoti,",
  "ja538:13.4": "yo mittānaṃ na dubbhati.",
  "ja538:14.1": "Nāssa corā pasāhanti,",
  "ja538:14.2": "nātimaññanti khattiyā;",
  "ja538:14.3": "Sabbe amitte tarati,",
  "ja538:14.4": "yo mittānaṃ na dubbhati.",
  "ja538:15.1": "Akkuddho sagharaṃ eti,",
  "ja538:15.2": "sabhāyaṃ paṭinandito;",
  "ja538:15.3": "Ñātīnaṃ uttamo hoti,",
  "ja538:15.4": "yo mittānaṃ na dubbhati.",
  "ja538:16.1": "Sakkatvā sakkato hoti,",
  "ja538:16.2": "garu hoti sagāravo;",
  "ja538:16.3": "Vaṇṇakittibhato hoti,",
  "ja538:16.4": "yo mittānaṃ na dubbhati.",
  "ja538:17.1": "Pūjako labhate pūjaṃ,",
  "ja538:17.2": "vandako paṭivandanaṃ;",
  "ja538:17.3": "Yaso kittiñca pappoti,",
  "ja538:17.4": "yo mittānaṃ na dubbhati.",
  "ja538:18.1": "Aggi yathā pajjalati,",
  "ja538:18.2": "devatāva virocati;",
  "ja538:18.3": "Siriyā ajahito hoti,",
  "ja538:18.4": "yo mittānaṃ na dubbhati.",
  "ja538:19.1": "Gāvo tassa pajāyanti,",
  "ja538:19.2": "khette vuttaṃ virūhati;",
  "ja538:19.3": "Vuttānaṃ phalamasnāti,",
  "ja538:19.4": "yo mittānaṃ na dubbhati.",
  "ja538:20.1": "Darito pabbatāto vā,",
  "ja538:20.2": "rukkhato patito naro;",
  "ja538:20.3": "Cuto patiṭṭhaṃ labhati,",
  "ja538:20.4": "yo mittānaṃ na dubbhati.",
  "ja538:21.1": "Virūḷhamūlasantānaṃ,",
  "ja538:21.2": "nigrodhamiva māluto;",
  "ja538:21.3": "Amittā nappasāhanti,",
  "ja538:21.4": "yo mittānaṃ na dubbhati”.",
  "ja538:22.1": "“Ehi taṃ paṭinessāmi,",
  "ja538:22.2": "rājaputta sakaṃ gharaṃ;",
  "ja538:22.3": "Rajjaṃ kārehi bhaddante,",
  "ja538:22.4": "kiṃ araññe karissasi”.",
  "ja538:23.1": "“Alaṃ me tena rajjena,",
  "ja538:23.2": "ñātakehi dhanena vā;",
  "ja538:23.3": "Yaṃ me adhammacariyāya,",
  "ja538:23.4": "rajjaṃ labbhetha sārathi”.",
  "ja538:24.1": "“Puṇṇapattaṃ maṃ lābhehi,",
  "ja538:24.2": "rājaputta ito gato;",
  "ja538:24.3": "Pitā mātā ca me dajjuṃ,",
  "ja538:24.4": "rājaputta tayī gate.",
  "ja538:25.1": "Orodhā ca kumārā ca,",
  "ja538:25.2": "vesiyānā ca brāhmaṇā;",
  "ja538:25.3": "Tepi attamanā dajjuṃ,",
  "ja538:25.4": "rājaputta tayī gate.",
  "ja538:26.1": "Hatthārohā anīkaṭṭhā,",
  "ja538:26.2": "rathikā pattikārakā;",
  "ja538:26.3": "Tepi attamanā dajjuṃ,",
  "ja538:26.4": "rājaputta tayī gate.",
  "ja538:27.1": "Bahudhaññā jānapadā,",
  "ja538:27.2": "negamā ca samāgatā;",
  "ja538:27.3": "Upāyanāni me dajjuṃ,",
  "ja538:27.4": "rājaputta tayī gate”.",
  "ja538:28.1": "“Pitu mātu cahaṃ catto,",
  "ja538:28.2": "raṭṭhassa nigamassa ca;",
  "ja538:28.3": "Atho sabbakumārānaṃ,",
  "ja538:28.4": "natthi mayhaṃ sakaṃ gharaṃ.",
  "ja538:29.1": "Anuññāto ahaṃ matyā,",
  "ja538:29.2": "sañcatto pitarā mahaṃ;",
  "ja538:29.3": "Ekoraññe pabbajito,",
  "ja538:29.4": "na kāme abhipatthaye.",
  "ja538:30.1": "Api ataramānānaṃ,",
  "ja538:30.2": "phalāsāva samijjhati;",
  "ja538:30.3": "Vipakkabrahmacariyosmi,",
  "ja538:30.4": "evaṃ jānāhi sārathi.",
  "ja538:31.1": "Api ataramānānaṃ,",
  "ja538:31.2": "sammadattho vipaccati;",
  "ja538:31.3": "Vipakkabrahmacariyosmi,",
  "ja538:31.4": "nikkhanto akutobhayo”.",
  "ja538:32.1": "“Evaṃ vaggukatho santo,",
  "ja538:32.2": "visaṭṭhavacano casi;",
  "ja538:32.3": "Kasmā pitu ca mātucca,",
  "ja538:32.4": "santike na bhaṇī tadā”.",
  "ja538:33.1": "“Nāhaṃ asandhitā pakkho,",
  "ja538:33.2": "na badhiro asotatā;",
  "ja538:33.3": "Nāhaṃ ajivhatā mūgo,",
  "ja538:33.4": "mā maṃ mūgamadhārayi.",
  "ja538:34.1": "Purimaṃ sarāmahaṃ jātiṃ,",
  "ja538:34.2": "yattha rajjamakārayiṃ;",
  "ja538:34.3": "Kārayitvā tahiṃ rajjaṃ,",
  "ja538:34.4": "pāpatthaṃ nirayaṃ bhusaṃ.",
  "ja538:35.1": "Vīsatiñceva vassāni,",
  "ja538:35.2": "tahiṃ rajjamakārayiṃ;",
  "ja538:35.3": "Asītivassasahassāni,",
  "ja538:35.4": "nirayamhi apaccisaṃ.",
  "ja538:36.1": "Tassa rajjassahaṃ bhīto,",
  "ja538:36.2": "mā maṃ rajjābhisecayuṃ;",
  "ja538:36.3": "Tasmā pitu ca mātucca,",
  "ja538:36.4": "santike na bhaṇiṃ tadā.",
  "ja538:37.1": "Ucchaṅge maṃ nisādetvā,",
  "ja538:37.2": "pitā atthānusāsati;",
  "ja538:37.3": "Ekaṃ hanatha bandhatha,",
  "ja538:37.4": "ekaṃ khārāpatacchikaṃ;",
  "ja538:37.5": "Ekaṃ sūlasmiṃ uppetha,",
  "ja538:37.6": "iccassa manusāsati.",
  "ja538:38.1": "Tāyāhaṃ pharusaṃ sutvā,",
  "ja538:38.2": "vācāyo samudīritā;",
  "ja538:38.3": "Amūgo mūgavaṇṇena,",
  "ja538:38.4": "apakkho pakkhasammato;",
  "ja538:38.5": "Sake muttakarīsasmiṃ,",
  "ja538:38.6": "acchāhaṃ samparipluto.",
  "ja538:39.1": "Kasirañca parittañca,",
  "ja538:39.2": "tañca dukkhena saṃyutaṃ;",
  "ja538:39.3": "Komaṃ jīvitamāgamma,",
  "ja538:39.4": "veraṃ kayirātha kenaci.",
  "ja538:40.1": "Paññāya ca alābhena,",
  "ja538:40.2": "dhammassa ca adassanā;",
  "ja538:40.3": "Komaṃ jīvitamāgamma,",
  "ja538:40.4": "veraṃ kayirātha kenaci.",
  "ja538:41.1": "Api ataramānānaṃ,",
  "ja538:41.2": "phalāsāva samijjhati;",
  "ja538:41.3": "Vipakkabrahmacariyosmi,",
  "ja538:41.4": "evaṃ jānāhi sārathi.",
  "ja538:42.1": "Api ataramānānaṃ,",
  "ja538:42.2": "sammadattho vipaccati;",
  "ja538:42.3": "Vipakkabrahmacariyosmi,",
  "ja538:42.4": "nikkhanto akutobhayo”.",
  "ja538:43.1": "“Ahampi pabbajissāmi,",
  "ja538:43.2": "rājaputta tavantike;",
  "ja538:43.3": "Avhāyassu maṃ bhaddante,",
  "ja538:43.4": "pabbajjā mama ruccati”.",
  "ja538:44.1": "“Rathaṃ niyyādayitvāna,",
  "ja538:44.2": "anaṇo ehi sārathi;",
  "ja538:44.3": "Anaṇassa hi pabbajjā,",
  "ja538:44.4": "etaṃ isīhi vaṇṇitaṃ”.",
  "ja538:45.1": "“Yadeva tyāhaṃ vacanaṃ,",
  "ja538:45.2": "akaraṃ bhaddamatthu te;",
  "ja538:45.3": "Tadeva me tvaṃ vacanaṃ,",
  "ja538:45.4": "yācito kattumarahasi.",
  "ja538:46.1": "Idheva tāva acchassu,",
  "ja538:46.2": "yāva rājānamānaye;",
  "ja538:46.3": "Appeva te pitā disvā,",
  "ja538:46.4": "patīto sumano siyā”.",
  "ja538:47.1": "“Karomi tetaṃ vacanaṃ,",
  "ja538:47.2": "yaṃ maṃ bhaṇasi sārathi;",
  "ja538:47.3": "Ahampi daṭṭhukāmosmi,",
  "ja538:47.4": "pitaraṃ me idhāgataṃ.",
  "ja538:48.1": "Ehi samma nivattassu,",
  "ja538:48.2": "kusalaṃ vajjāsi ñātinaṃ;",
  "ja538:48.3": "Mātaraṃ pitaraṃ mayhaṃ,",
  "ja538:48.4": "vutto vajjāsi vandanaṃ”.",
  "ja538:49.1": "Tassa pāde gahetvāna,",
  "ja538:49.2": "katvā ca naṃ padakkhiṇaṃ;",
  "ja538:49.3": "Sārathi rathamāruyha,",
  "ja538:49.4": "rājadvāraṃ upāgami.",
  "ja538:50.1": "Suññaṃ mātā rathaṃ disvā,",
  "ja538:50.2": "ekaṃ sārathimāgataṃ;",
  "ja538:50.3": "Assupuṇṇehi nettehi,",
  "ja538:50.4": "rodantī naṃ udikkhati.",
  "ja538:51.1": "“Ayaṃ so sārathi eti,",
  "ja538:51.2": "nihantvā mama atrajaṃ;",
  "ja538:51.3": "Nihato nūna me putto,",
  "ja538:51.4": "pathabyā bhūmivaḍḍhano.",
  "ja538:52.1": "Amittā nūna nandanti,",
  "ja538:52.2": "patītā nūna verino;",
  "ja538:52.3": "Āgataṃ sārathiṃ disvā,",
  "ja538:52.4": "nihantvā mama atrajaṃ”.",
  "ja538:53.1": "Suññaṃ mātā rathaṃ disvā,",
  "ja538:53.2": "ekaṃ sārathimāgataṃ;",
  "ja538:53.3": "Assupuṇṇehi nettehi,",
  "ja538:53.4": "rodantī paripucchi naṃ.",
  "ja538:54.1": "“Kiṃ nu mūgo kiṃ nu pakkho,",
  "ja538:54.2": "kiṃ nu so vilapī tadā;",
  "ja538:54.3": "Nihaññamāno bhūmiyā,",
  "ja538:54.4": "taṃ me akkhāhi sārathi.",
  "ja538:55.1": "Kathaṃ hatthehi pādehi,",
  "ja538:55.2": "mūgapakkho vivajjayi;",
  "ja538:55.3": "Nihaññamāno bhūmiyā,",
  "ja538:55.4": "taṃ me akkhāhi pucchito”.",
  "ja538:56.1": "“Akkheyyaṃ te ahaṃ ayye,",
  "ja538:56.2": "dajjāsi abhayaṃ mama;",
  "ja538:56.3": "Yaṃ me sutaṃ vā diṭṭhaṃ vā,",
  "ja538:56.4": "rājaputtassa santike”.",
  "ja538:57.1": "“Abhayaṃ samma te dammi,",
  "ja538:57.2": "abhīto bhaṇa sārathi;",
  "ja538:57.3": "Yaṃ te sutaṃ vā diṭṭhaṃ vā,",
  "ja538:57.4": "rājaputtassa santike”.",
  "ja538:58.1": "“Na so mūgo na so pakkho,",
  "ja538:58.2": "visaṭṭhavacano ca so;",
  "ja538:58.3": "Rajjassa kira so bhīto,",
  "ja538:58.4": "akarā ālaye bahū.",
  "ja538:59.1": "Purimaṃ sarati so jātiṃ,",
  "ja538:59.2": "yattha rajjamakārayi;",
  "ja538:59.3": "Kārayitvā tahiṃ rajjaṃ,",
  "ja538:59.4": "pāpattha nirayaṃ bhusaṃ.",
  "ja538:60.1": "Vīsatiñceva vassāni,",
  "ja538:60.2": "tahiṃ rajjamakārayi;",
  "ja538:60.3": "Asītivassasahassāni,",
  "ja538:60.4": "nirayamhi apacci so.",
  "ja538:61.1": "Tassa rajjassa so bhīto,",
  "ja538:61.2": "mā maṃ rajjābhisecayuṃ;",
  "ja538:61.3": "Tasmā pitu ca mātucca,",
  "ja538:61.4": "santike na bhaṇī tadā.",
  "ja538:62.1": "Aṅgapaccaṅgasampanno,",
  "ja538:62.2": "ārohapariṇāhavā;",
  "ja538:62.3": "Visaṭṭhavacano pañño,",
  "ja538:62.4": "magge saggassa tiṭṭhati.",
  "ja538:63.1": "Sace tvaṃ daṭṭhukāmāsi,",
  "ja538:63.2": "rājaputtaṃ tavatrajaṃ;",
  "ja538:63.3": "Ehi taṃ pāpayissāmi,",
  "ja538:63.4": "yattha sammati temiyo”.",
  "ja538:64.1": "“Yojayantu rathe asse,",
  "ja538:64.2": "Kacchaṃ nāgāna bandhatha;",
  "ja538:64.3": "Udīrayantu saṅkhapaṇavā,",
  "ja538:64.4": "Vādantu ekapokkharā.",
  "ja538:65.1": "Vādantu bherī sannaddhā,",
  "ja538:65.2": "vaggū vādantu dundubhī;",
  "ja538:65.3": "Negamā ca maṃ anventu,",
  "ja538:65.4": "gacchaṃ puttanivedako.",
  "ja538:66.1": "Orodhā ca kumārā ca,",
  "ja538:66.2": "vesiyānā ca brāhmaṇā;",
  "ja538:66.3": "Khippaṃ yānāni yojentu,",
  "ja538:66.4": "gacchaṃ puttanivedako.",
  "ja538:67.1": "Hatthārohā anīkaṭṭhā,",
  "ja538:67.2": "rathikā pattikārakā;",
  "ja538:67.3": "Khippaṃ yānāni yojentu,",
  "ja538:67.4": "gacchaṃ puttanivedako.",
  "ja538:68.1": "Samāgatā jānapadā,",
  "ja538:68.2": "negamā ca samāgatā;",
  "ja538:68.3": "Khippaṃ yānāni yojentu,",
  "ja538:68.4": "gacchaṃ puttanivedako”.",
  "ja538:69.1": "Asse ca sārathī yutte,",
  "ja538:69.2": "sindhave sīghavāhane;",
  "ja538:69.3": "Rājadvāraṃ upāgacchuṃ,",
  "ja538:69.4": "“yuttā deva ime hayā”.",
  "ja538:70.1": "“Thūlā javena hāyanti,",
  "ja538:70.2": "kisā hāyanti thāmunā;",
  "ja538:70.3": "Kise thūle vivajjetvā,",
  "ja538:70.4": "saṃsaṭṭhā yojitā hayā”.",
  "ja538:71.1": "Tato rājā taramāno,",
  "ja538:71.2": "yuttamāruyha sandanaṃ;",
  "ja538:71.3": "Itthāgāraṃ ajjhabhāsi,",
  "ja538:71.4": "“sabbāva anuyātha maṃ.",
  "ja538:72.1": "Vālabījanimuṇhīsaṃ,",
  "ja538:72.2": "khaggaṃ chattañca paṇḍaraṃ;",
  "ja538:72.3": "Upādhī rathamāruyha,",
  "ja538:72.4": "suvaṇṇehi alaṅkatā”.",
  "ja538:73.1": "Tato sa rājā pāyāsi,",
  "ja538:73.2": "purakkhatvāna sārathiṃ;",
  "ja538:73.3": "Khippameva upāgacchi,",
  "ja538:73.4": "yattha sammati temiyo.",
  "ja538:74.1": "Tañca disvāna āyantaṃ,",
  "ja538:74.2": "jalantamiva tejasā;",
  "ja538:74.3": "Khattasaṅghaparibyūḷhaṃ,",
  "ja538:74.4": "temiyo etadabravi.",
  "ja538:75.1": "“Kacci nu tāta kusalaṃ,",
  "ja538:75.2": "kacci tāta anāmayaṃ;",
  "ja538:75.3": "Sabbā ca rājakaññāyo,",
  "ja538:75.4": "arogā mayha mātaro”.",
  "ja538:76.1": "“Kusalañceva me putta,",
  "ja538:76.2": "atho putta anāmayaṃ;",
  "ja538:76.3": "Sabbā ca rājakaññāyo,",
  "ja538:76.4": "arogā tuyha mātaro”.",
  "ja538:77.1": "“Kacci amajjapo tāta,",
  "ja538:77.2": "kacci te suramappiyaṃ;",
  "ja538:77.3": "Kacci sacce ca dhamme ca,",
  "ja538:77.4": "dāne te ramate mano”.",
  "ja538:78.1": "“Amajjapo ahaṃ putta,",
  "ja538:78.2": "atho me suramappiyaṃ;",
  "ja538:78.3": "Atho sacce ca dhamme ca,",
  "ja538:78.4": "dāne me ramate mano”.",
  "ja538:79.1": "“Kacci arogaṃ yoggaṃ te,",
  "ja538:79.2": "kacci vahati vāhanaṃ;",
  "ja538:79.3": "Kacci te byādhayo natthi,",
  "ja538:79.4": "sarīrassupatāpanā”.",
  "ja538:80.1": "“Atho arogaṃ yoggaṃ me,",
  "ja538:80.2": "atho vahati vāhanaṃ;",
  "ja538:80.3": "Atho me byādhayo natthi,",
  "ja538:80.4": "sarīrassupatāpanā”.",
  "ja538:81.1": "“Kacci antā ca te phītā,",
  "ja538:81.2": "majjhe ca bahalā tava;",
  "ja538:81.3": "Koṭṭhāgārañca kosañca,",
  "ja538:81.4": "kacci te paṭisanthataṃ”.",
  "ja538:82.1": "“Atho antā ca me phītā,",
  "ja538:82.2": "majjhe ca bahalā mama;",
  "ja538:82.3": "Koṭṭhāgārañca kosañca,",
  "ja538:82.4": "sabbaṃ me paṭisanthataṃ”.",
  "ja538:83.1": "“Svāgataṃ te mahārāja,",
  "ja538:83.2": "atho te adurāgataṃ;",
  "ja538:83.3": "Patiṭṭhapentu pallaṅkaṃ,",
  "ja538:83.4": "yattha rājā nisakkati.",
  "ja538:84.1": "Idheva te nisīdassu,",
  "ja538:84.2": "niyate paṇṇasanthare;",
  "ja538:84.3": "Etto udakamādāya,",
  "ja538:84.4": "pāde pakkhālayassu te.",
  "ja538:85.1": "Idampi paṇṇakaṃ mayhaṃ,",
  "ja538:85.2": "randhaṃ rāja aloṇakaṃ;",
  "ja538:85.3": "Paribhuñja mahārāja,",
  "ja538:85.4": "pāhuno mesidhāgato”.",
  "ja538:86.1": "“Na cāhaṃ paṇṇaṃ bhuñjāmi,",
  "ja538:86.2": "Na hetaṃ mayha bhojanaṃ;",
  "ja538:86.3": "Sālīnaṃ odanaṃ bhuñje,",
  "ja538:86.4": "Suciṃ maṃsūpasecanaṃ”.",
  "ja538:87.1": "“Accherakaṃ maṃ paṭibhāti,",
  "ja538:87.2": "ekakampi rahogataṃ;",
  "ja538:87.3": "Edisaṃ bhuñjamānānaṃ,",
  "ja538:87.4": "kena vaṇṇo pasīdati”.",
  "ja538:88.1": "“Eko rāja nipajjāmi,",
  "ja538:88.2": "niyate paṇṇasanthare;",
  "ja538:88.3": "Tāya me ekaseyyāya,",
  "ja538:88.4": "rāja vaṇṇo pasīdati.",
  "ja538:89.1": "Na ca nettiṃsabandhā me,",
  "ja538:89.2": "rājarakkhā upaṭṭhitā;",
  "ja538:89.3": "Tāya me sukhaseyyāya,",
  "ja538:89.4": "rāja vaṇṇo pasīdati.",
  "ja538:90.1": "Atītaṃ nānusocāmi,",
  "ja538:90.2": "nappajappāmināgataṃ;",
  "ja538:90.3": "Paccuppannena yāpemi,",
  "ja538:90.4": "tena vaṇṇo pasīdati.",
  "ja538:91.1": "Anāgatappajappāya,",
  "ja538:91.2": "atītassānusocanā;",
  "ja538:91.3": "Etena bālā sussanti,",
  "ja538:91.4": "naḷova harito luto”.",
  "ja538:92.1": "“Hatthānīkaṃ rathānīkaṃ,",
  "ja538:92.2": "asse pattī ca vammino;",
  "ja538:92.3": "Nivesanāni rammāni,",
  "ja538:92.4": "ahaṃ putta dadāmi te.",
  "ja538:93.1": "Itthāgārampi te dammi,",
  "ja538:93.2": "sabbālaṅkārabhūsitaṃ;",
  "ja538:93.3": "Tā putta paṭipajjassu,",
  "ja538:93.4": "tvaṃ no rājā bhavissasi.",
  "ja538:94.1": "Kusalā naccagītassa,",
  "ja538:94.2": "sikkhitā cāturitthiyo;",
  "ja538:94.3": "Kāme taṃ ramayissanti,",
  "ja538:94.4": "kiṃ araññe karissasi.",
  "ja538:95.1": "Paṭirājūhi te kaññā,",
  "ja538:95.2": "ānayissaṃ alaṅkatā;",
  "ja538:95.3": "Tāsu putte janetvāna,",
  "ja538:95.4": "atha pacchā pabbajissasi.",
  "ja538:96.1": "Yuvā ca daharo cāsi,",
  "ja538:96.2": "paṭhamuppattiko susu;",
  "ja538:96.3": "Rajjaṃ kārehi bhaddante,",
  "ja538:96.4": "kiṃ araññe karissasi”.",
  "ja538:97.1": "“Yuvā care brahmacariyaṃ,",
  "ja538:97.2": "brahmacārī yuvā siyā;",
  "ja538:97.3": "Daharassa hi pabbajjā,",
  "ja538:97.4": "etaṃ isīhi vaṇṇitaṃ.",
  "ja538:98.1": "Yuvā care brahmacariyaṃ,",
  "ja538:98.2": "brahmacārī yuvā siyā;",
  "ja538:98.3": "Brahmacariyaṃ carissāmi,",
  "ja538:98.4": "nāhaṃ rajjena matthiko.",
  "ja538:99.1": "Passāmi vohaṃ daharaṃ,",
  "ja538:99.2": "amma tāta vadantaraṃ;",
  "ja538:99.3": "Kicchāladdhaṃ piyaṃ puttaṃ,",
  "ja538:99.4": "appatvāva jaraṃ mataṃ.",
  "ja538:100.1": "Passāmi vohaṃ dahariṃ,",
  "ja538:100.2": "kumāriṃ cārudassaniṃ;",
  "ja538:100.3": "Navavaṃsakaḷīraṃva,",
  "ja538:100.4": "paluggaṃ jīvitakkhayaṃ.",
  "ja538:101.1": "Daharāpi hi miyyanti,",
  "ja538:101.2": "narā ca atha nāriyo;",
  "ja538:101.3": "Tattha ko vissase poso,",
  "ja538:101.4": "daharomhīti jīvite.",
  "ja538:102.1": "Yassa ratyā vivasāne,",
  "ja538:102.2": "āyu appataraṃ siyā;",
  "ja538:102.3": "Appodakeva macchānaṃ,",
  "ja538:102.4": "kiṃ nu komārakaṃ tahiṃ.",
  "ja538:103.1": "Niccamabbhāhato loko,",
  "ja538:103.2": "niccañca parivārito;",
  "ja538:103.3": "Amoghāsu vajantīsu,",
  "ja538:103.4": "kiṃ maṃ rajjebhisiñcasi”.",
  "ja538:104.1": "“Kena mabbhāhato loko,",
  "ja538:104.2": "kena ca parivārito;",
  "ja538:104.3": "Kāyo amoghā gacchanti,",
  "ja538:104.4": "taṃ me akkhāhi pucchito”.",
  "ja538:105.1": "“Maccunābbhāhato loko,",
  "ja538:105.2": "jarāya parivārito;",
  "ja538:105.3": "Ratyo amoghā gacchanti,",
  "ja538:105.4": "evaṃ jānāhi khattiya.",
  "ja538:106.1": "Yathāpi tante vitate,",
  "ja538:106.2": "yaṃ yadevūpaviyyati;",
  "ja538:106.3": "Appakaṃ hoti vetabbaṃ,",
  "ja538:106.4": "evaṃ maccāna jīvitaṃ.",
  "ja538:107.1": "Yathā vārivaho pūro,",
  "ja538:107.2": "gacchannupanivattati;",
  "ja538:107.3": "Evamāyu manussānaṃ,",
  "ja538:107.4": "gacchannupanivattati.",
  "ja538:108.1": "Yathā vārivaho pūro,",
  "ja538:108.2": "vahe rukkhepakūlaje;",
  "ja538:108.3": "Evaṃ jarāmaraṇena,",
  "ja538:108.4": "vuyhante sabbapāṇino”.",
  "ja538:109.1": "“Hatthānīkaṃ rathānīkaṃ,",
  "ja538:109.2": "asse pattī ca vammino;",
  "ja538:109.3": "Nivesanāni rammāni,",
  "ja538:109.4": "ahaṃ putta dadāmi te.",
  "ja538:110.1": "Itthāgārampi te dammi,",
  "ja538:110.2": "sabbālaṅkārabhūsitaṃ;",
  "ja538:110.3": "Tā putta paṭipajjassu,",
  "ja538:110.4": "tvaṃ no rājā bhavissasi.",
  "ja538:111.1": "Kusalā naccagītassa,",
  "ja538:111.2": "sikkhitā cāturitthiyo;",
  "ja538:111.3": "Kāme taṃ ramayissanti,",
  "ja538:111.4": "kiṃ araññe karissasi.",
  "ja538:112.1": "Paṭirājūhi te kaññā,",
  "ja538:112.2": "ānayissaṃ alaṅkatā;",
  "ja538:112.3": "Tāsu putte janetvāna,",
  "ja538:112.4": "atha pacchā pabbajissasi.",
  "ja538:113.1": "Yuvā ca daharo cāsi,",
  "ja538:113.2": "paṭhamuppattiko susu;",
  "ja538:113.3": "Rajjaṃ kārehi bhaddante,",
  "ja538:113.4": "kiṃ araññe karissasi.",
  "ja538:114.1": "Koṭṭhāgārañca kosañca,",
  "ja538:114.2": "vāhanāni balāni ca;",
  "ja538:114.3": "Nivesanāni rammāni,",
  "ja538:114.4": "ahaṃ putta dadāmi te.",
  "ja538:115.1": "Gomaṇḍalaparibyūḷho,",
  "ja538:115.2": "dāsisaṅghapurakkhato;",
  "ja538:115.3": "Rajjaṃ kārehi bhaddante,",
  "ja538:115.4": "kiṃ araññe karissasi”.",
  "ja538:116.1": "“Kiṃ dhanena yaṃ khīyetha,",
  "ja538:116.2": "Kiṃ bhariyāya marissati;",
  "ja538:116.3": "Kiṃ yobbanena jiṇṇena,",
  "ja538:116.4": "Yaṃ jarāyābhibhuyyati.",
  "ja538:117.1": "Tattha kā nandi kā khiḍḍā,",
  "ja538:117.2": "kā rati kā dhanesanā;",
  "ja538:117.3": "Kiṃ me puttehi dārehi,",
  "ja538:117.4": "rāja muttosmi bandhanā.",
  "ja538:118.1": "Yohaṃ evaṃ pajānāmi,",
  "ja538:118.2": "maccu me nappamajjati;",
  "ja538:118.3": "Antakenādhipannassa,",
  "ja538:118.4": "kā ratī kā dhanesanā.",
  "ja538:119.1": "Phalānamiva pakkānaṃ,",
  "ja538:119.2": "niccaṃ patanato bhayaṃ;",
  "ja538:119.3": "Evaṃ jātāna maccānaṃ,",
  "ja538:119.4": "niccaṃ maraṇato bhayaṃ.",
  "ja538:120.1": "Sāyameke na dissanti,",
  "ja538:120.2": "pāto diṭṭhā bahū janā;",
  "ja538:120.3": "Pāto eke na dissanti,",
  "ja538:120.4": "sāyaṃ diṭṭhā bahū janā.",
  "ja538:121.1": "Ajjeva kiccaṃ ātapaṃ,",
  "ja538:121.2": "ko jaññā maraṇaṃ suve;",
  "ja538:121.3": "Na hi no saṅgaraṃ tena,",
  "ja538:121.4": "mahāsenena maccunā.",
  "ja538:122.1": "Corā dhanassa patthenti,",
  "ja538:122.2": "rāja muttosmi bandhanā;",
  "ja538:122.3": "Ehi rāja nivattassu,",
  "ja538:122.4": "nāhaṃ rajjena matthiko”ti.",
  "ja538:123.1": "Mūgapakkhajātakaṃ paṭhamaṃ."
}