{
  "ja160:0.1": "Jātaka",
  "ja160:0.2": "Dukanipāta",
  "ja160:0.3": "Daḷhavagga",
  "ja160:0.4": "10. Vinīlajātaka",
  "ja160:1.1": "“Evameva nūna rājānaṁ,",
  "ja160:1.2": "vedehaṁ mithilaggahaṁ;",
  "ja160:1.3": "Assā vahanti ājaññā,",
  "ja160:1.4": "yathā haṁsā vinīlakaṁ”.",
  "ja160:2.1": "“Vinīla duggaṁ bhajasi,",
  "ja160:2.2": "abhūmiṁ tāta sevasi;",
  "ja160:2.3": "Gāmantakāni sevassu,",
  "ja160:2.4": "etaṁ mātālayaṁ tavāti.",
  "ja160:3.1": "Vinīlajātakaṁ dasamaṁ.",
  "ja160:3.2": "Daḷhavaggo paṭhamo.",
  "ja160:4.1": "Tassuddānaṁ",
  "ja160:5.1": "Varaballika daddara sūkarako,",
  "ja160:5.2": "Uraguttama pañcamabhaggavaro;",
  "ja160:5.3": "Mahatīcamu yāva siṅgālavaro,",
  "ja160:5.4": "Suhanuttama mora vinīlaṁ dasā”ti."
}