{
  "ja269:0.1": "Jātaka",
  "ja269:0.2": "Tikanipāta",
  "ja269:0.3": "Padumavagga",
  "ja269:0.4": "9. Sujātajātaka",
  "ja269:1.1": "“Na hi vaṇṇena sampannā,",
  "ja269:1.2": "mañjukā piyadassanā;",
  "ja269:1.3": "Kharavācā piyā hoti,",
  "ja269:1.4": "asmiṁ loke paramhi ca.",
  "ja269:2.1": "Nanu passasimaṁ kāḷiṁ,",
  "ja269:2.2": "dubbaṇṇaṁ tilakāhataṁ;",
  "ja269:2.3": "Kokilaṁ saṇhavācena,",
  "ja269:2.4": "bahūnaṁ pāṇinaṁ piyaṁ.",
  "ja269:3.1": "Tasmā sakhilavācassa,",
  "ja269:3.2": "mantabhāṇī anuddhato;",
  "ja269:3.3": "Atthaṁ dhammañca dīpeti,",
  "ja269:3.4": "madhuraṁ tassa bhāsitan”ti.",
  "ja269:4.1": "Sujātajātakaṁ navamaṁ."
}