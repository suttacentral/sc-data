{
  "ja203:0.1": "Jātaka",
  "ja203:0.2": "Dukanipāta",
  "ja203:0.3": "Nataṁdaḷhavagga",
  "ja203:0.4": "3. Khaṇḍajātaka",
  "ja203:1.1": "“Virūpakkhehi me mettaṁ,",
  "ja203:1.2": "Mettaṁ erāpathehi me;",
  "ja203:1.3": "Chabyāputtehi me mettaṁ,",
  "ja203:1.4": "Mettaṁ kaṇhāgotamakehi ca.",
  "ja203:2.1": "Apādakehi me mettaṁ,",
  "ja203:2.2": "mettaṁ dvipādakehi me;",
  "ja203:2.3": "Catuppadehi me mettaṁ,",
  "ja203:2.4": "mettaṁ bahuppadehi me.",
  "ja203:3.1": "Mā maṁ apādako hiṁsi,",
  "ja203:3.2": "mā maṁ hiṁsi dvipādako;",
  "ja203:3.3": "Mā maṁ catuppado hiṁsi,",
  "ja203:3.4": "mā maṁ hiṁsi bahuppado.",
  "ja203:4.1": "Sabbe sattā sabbe pāṇā,",
  "ja203:4.2": "sabbe bhūtā ca kevalā;",
  "ja203:4.3": "Sabbe bhadrāni passantu,",
  "ja203:4.4": "mā kañci pāpamāgamā.",
  "ja203:5.1": "Appamāṇo buddho,",
  "ja203:5.2": "Appamāṇo dhammo;",
  "ja203:5.3": "Appamāṇo saṅgho,",
  "ja203:5.4": "Pamāṇavantāni sarīsapāni;",
  "ja203:5.5": "Ahivicchikasatapadī,",
  "ja203:5.6": "Uṇṇanābhi sarabū mūsikā.",
  "ja203:6.1": "Katā me rakkhā katā me parittā,",
  "ja203:6.2": "Paṭikkamantu bhūtāni;",
  "ja203:6.3": "Sohaṁ namo bhagavato,",
  "ja203:6.4": "Namo sattannaṁ sammāsambuddhānan”ti.",
  "ja203:7.1": "Khaṇḍajātakaṁ tatiyaṁ."
}