{
  "ja511:0.1": "Jātaka",
  "ja511:0.2": "Tiṁsanipāta",
  "ja511:0.3": "Kiṁchandavagga",
  "ja511:0.4": "1. Kiṁchandajātaka",
  "ja511:1.1": "“Kiṁchando kimadhippāyo,",
  "ja511:1.2": "eko sammasi ghammani;",
  "ja511:1.3": "Kiṁ patthayāno kiṁ esaṁ,",
  "ja511:1.4": "kena atthena brāhmaṇa”.",
  "ja511:2.1": "“Yathā mahā vāridharo,",
  "ja511:2.2": "kumbho supariṇāhavā;",
  "ja511:2.3": "Tathūpamaṁ ambapakkaṁ,",
  "ja511:2.4": "vaṇṇagandharasuttamaṁ.",
  "ja511:3.1": "Taṁ vuyhamānaṁ sotena,",
  "ja511:3.2": "disvānāmalamajjhime;",
  "ja511:3.3": "Pāṇīhi naṁ gahetvāna,",
  "ja511:3.4": "agyāyatanamāhariṁ.",
  "ja511:4.1": "Tato kadalipattesu,",
  "ja511:4.2": "nikkhipitvā sayaṁ ahaṁ;",
  "ja511:4.3": "Satthena naṁ vikappetvā,",
  "ja511:4.4": "khuppipāsaṁ ahāsi me.",
  "ja511:5.1": "Sohaṁ apetadaratho,",
  "ja511:5.2": "Byantībhūto dukhakkhamo;",
  "ja511:5.3": "Assādaṁ nādhigacchāmi,",
  "ja511:5.4": "Phalesvaññesu kesuci.",
  "ja511:6.1": "Sosetvā nūna maraṇaṁ,",
  "ja511:6.2": "taṁ mamaṁ āvahissati;",
  "ja511:6.3": "Ambaṁ yassa phalaṁ sādu,",
  "ja511:6.4": "madhuraggaṁ manoramaṁ;",
  "ja511:6.5": "Yamuddhariṁ vuyhamānaṁ,",
  "ja511:6.6": "udadhismā mahaṇṇave.",
  "ja511:7.1": "Akkhātaṁ te mayā sabbaṁ,",
  "ja511:7.2": "yasmā upavasāmahaṁ;",
  "ja511:7.3": "Rammaṁ pati nisinnosmi,",
  "ja511:7.4": "puthulomāyutā puthu.",
  "ja511:8.1": "Tvañca kho meva akkhāhi,",
  "ja511:8.2": "attānamapalāyini;",
  "ja511:8.3": "Kā vā tvamasi kalyāṇi,",
  "ja511:8.4": "kissa vā tvaṁ sumajjhime.",
  "ja511:9.1": "Ruppapaṭṭapalimaṭṭhīva,",
  "ja511:9.2": "byagghīva girisānujā;",
  "ja511:9.3": "Yā santi nāriyo devesu,",
  "ja511:9.4": "devānaṁ paricārikā.",
  "ja511:10.1": "Yā ca manussalokasmiṁ,",
  "ja511:10.2": "Rūpenānvāgatitthiyo;",
  "ja511:10.3": "Rūpena te sadisī natthi,",
  "ja511:10.4": "Devesu gandhabbamanussaloke;",
  "ja511:10.5": "Puṭṭhāsi me cārupubbaṅgi,",
  "ja511:10.6": "Brūhi nāmañca bandhave”.",
  "ja511:11.1": "“Yaṁ tvaṁ pati nisinnosi,",
  "ja511:11.2": "rammaṁ brāhmaṇa kosikiṁ;",
  "ja511:11.3": "Sāhaṁ bhusālayāvutthā,",
  "ja511:11.4": "varavārivahoghasā.",
  "ja511:12.1": "Nānādumagaṇākiṇṇā,",
  "ja511:12.2": "bahukā girikandarā;",
  "ja511:12.3": "Mameva pamukhā honti,",
  "ja511:12.4": "abhisandanti pāvuse.",
  "ja511:13.1": "Atho bahū vanatodā,",
  "ja511:13.2": "nīlavārivahindharā;",
  "ja511:13.3": "Bahukā nāgavittodā,",
  "ja511:13.4": "abhisandanti vārinā.",
  "ja511:14.1": "Tā ambajambulabujā,",
  "ja511:14.2": "nīpā tālā cudumbarā;",
  "ja511:14.3": "Bahūni phalajātāni,",
  "ja511:14.4": "āvahanti abhiṇhaso.",
  "ja511:15.1": "Yaṁ kiñci ubhato tīre,",
  "ja511:15.2": "phalaṁ patati ambuni;",
  "ja511:15.3": "Asaṁsayaṁ taṁ sotassa,",
  "ja511:15.4": "phalaṁ hoti vasānugaṁ.",
  "ja511:16.1": "Etadaññāya medhāvi,",
  "ja511:16.2": "puthupañña suṇohi me;",
  "ja511:16.3": "Mā rocaya mabhisaṅgaṁ,",
  "ja511:16.4": "paṭisedha janādhipa.",
  "ja511:17.1": "Na vāhaṁ vaḍḍhavaṁ maññe,",
  "ja511:17.2": "Yaṁ tvaṁ raṭṭhābhivaḍḍhana;",
  "ja511:17.3": "Āceyyamāno rājisi,",
  "ja511:17.4": "Maraṇaṁ abhikaṅkhasi.",
  "ja511:18.1": "Tassa jānanti pitaro,",
  "ja511:18.2": "gandhabbā ca sadevakā;",
  "ja511:18.3": "Ye cāpi isayo loke,",
  "ja511:18.4": "saññatattā tapassino;",
  "ja511:18.5": "Asaṁsayaṁ tepi jānanti,",
  "ja511:18.6": "paṭṭhabhūtā yasassino”.",
  "ja511:19.1": "“Evaṁ viditvā vidū sabbadhammaṁ,",
  "ja511:19.2": "Viddhaṁsanaṁ cavanaṁ jīvitassa;",
  "ja511:19.3": "Na cīyatī tassa narassa pāpaṁ,",
  "ja511:19.4": "Sace na ceteti vadhāya tassa.",
  "ja511:20.1": "Isipūgasamaññāte,",
  "ja511:20.2": "evaṁ lokyā viditā sati;",
  "ja511:20.3": "Anariyaparisambhāse,",
  "ja511:20.4": "pāpakammaṁ jigīsasi.",
  "ja511:21.1": "Sace ahaṁ marissāmi,",
  "ja511:21.2": "tīre te puthusussoṇi;",
  "ja511:21.3": "Asaṁsayaṁ taṁ asiloko,",
  "ja511:21.4": "mayi pete āgamissati.",
  "ja511:22.1": "Tasmā hi pāpakaṁ kammaṁ,",
  "ja511:22.2": "rakkhasseva sumajjhime;",
  "ja511:22.3": "Mā taṁ sabbo jano pacchā,",
  "ja511:22.4": "pakuṭṭhāyi mayi mate”.",
  "ja511:23.1": "“Aññātametaṁ avisayhasāhi,",
  "ja511:23.2": "Attānamambañca dadāmi te taṁ;",
  "ja511:23.3": "So duccaje kāmaguṇe pahāya,",
  "ja511:23.4": "Santiñca dhammañca adhiṭṭhitosi.",
  "ja511:24.1": "Yo hitvā pubbasaññogaṁ,",
  "ja511:24.2": "pacchā saṁyojane ṭhito;",
  "ja511:24.3": "Adhammañceva carati,",
  "ja511:24.4": "pāpañcassa pavaḍḍhati.",
  "ja511:25.1": "Ehi taṁ pāpayissāmi,",
  "ja511:25.2": "kāmaṁ appossuko bhava;",
  "ja511:25.3": "Upānayāmi sītasmiṁ,",
  "ja511:25.4": "viharāhi anussuko.",
  "ja511:26.1": "Taṁ puppharasamattebhi,",
  "ja511:26.2": "vakkaṅgehi arindama;",
  "ja511:26.3": "Koñcā mayūrā diviyā,",
  "ja511:26.4": "kolaṭṭhimadhusāḷikā;",
  "ja511:26.5": "Kūjitā haṁsapūgehi,",
  "ja511:26.6": "kokilettha pabodhare.",
  "ja511:27.1": "Ambettha vippasākhaggā,",
  "ja511:27.2": "palālakhalasannibhā;",
  "ja511:27.3": "Kosambasaḷalā nīpā,",
  "ja511:27.4": "pakkatālavilambino”.",
  "ja511:28.1": "“Mālī tiriṭī kāyūrī,",
  "ja511:28.2": "aṅgadī candanussado;",
  "ja511:28.3": "Rattiṁ tvaṁ paricāresi,",
  "ja511:28.4": "divā vedesi vedanaṁ.",
  "ja511:29.1": "Soḷasitthisahassāni,",
  "ja511:29.2": "yā temā paricārikā;",
  "ja511:29.3": "Evaṁ mahānubhāvosi,",
  "ja511:29.4": "abbhuto lomahaṁsano.",
  "ja511:30.1": "Kiṁ kammamakarī pubbe,",
  "ja511:30.2": "pāpaṁ attadukhāvahaṁ;",
  "ja511:30.3": "Yaṁ karitvā manussesu,",
  "ja511:30.4": "piṭṭhimaṁsāni khādasi”.",
  "ja511:31.1": "“Ajjhenāni paṭiggayha,",
  "ja511:31.2": "kāmesu gadhito ahaṁ;",
  "ja511:31.3": "Acariṁ dīghamaddhānaṁ,",
  "ja511:31.4": "paresaṁ ahitāyahaṁ.",
  "ja511:32.1": "Yo piṭṭhimaṁsiko hoti,",
  "ja511:32.2": "evaṁ ukkacca khādati;",
  "ja511:32.3": "Yathāhaṁ ajja khādāmi,",
  "ja511:32.4": "piṭṭhimaṁsāni attano”ti.",
  "ja511:33.1": "Kiṁchandajātakaṁ paṭhamaṁ."
}