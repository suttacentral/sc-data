{
  "ja326:0.1": "Jātaka",
  "ja326:0.2": "Catukkanipāta",
  "ja326:0.3": "Kuṭidūsakavagga",
  "ja326:0.4": "6. Kakkārujātaka",
  "ja326:1.1": "“Kāyena yo nāvahare,",
  "ja326:1.2": "vācāya na musā bhaṇe;",
  "ja326:1.3": "Yaso laddhā na majjeyya,",
  "ja326:1.4": "sa ve kakkārumarahati”.",
  "ja326:2.1": "“Dhammena vittameseyya,",
  "ja326:2.2": "na nikatyā dhanaṁ hare;",
  "ja326:2.3": "Bhoge laddhā na majjeyya,",
  "ja326:2.4": "sa ve kakkārumarahati”.",
  "ja326:3.1": "“Yassa cittaṁ ahāliddaṁ,",
  "ja326:3.2": "saddhā ca avirāginī;",
  "ja326:3.3": "Eko sāduṁ na bhuñjeyya,",
  "ja326:3.4": "sa ve kakkārumarahati”.",
  "ja326:4.1": "“Sammukhā vā tirokkhā vā,",
  "ja326:4.2": "Yo sante na paribhāsati;",
  "ja326:4.3": "Yathāvādī tathākārī,",
  "ja326:4.4": "Sa ve kakkārumarahatī”ti.",
  "ja326:5.1": "Kakkārujātakaṁ chaṭṭhaṁ."
}