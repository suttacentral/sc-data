{
  "ja318:0.1": "Jātaka",
  "ja318:0.2": "Catukkanipāta",
  "ja318:0.3": "Pucimandavagga",
  "ja318:0.4": "8. Kaṇaverajātaka",
  "ja318:1.1": "“Yaṁ taṁ vasantasamaye,",
  "ja318:1.2": "kaṇaveresu bhāṇusu;",
  "ja318:1.3": "Sāmaṁ bāhāya pīḷesi,",
  "ja318:1.4": "sā taṁ ārogyamabravi”.",
  "ja318:2.1": "“Ambho na kira saddheyyaṁ,",
  "ja318:2.2": "yaṁ vāto pabbataṁ vahe;",
  "ja318:2.3": "Pabbatañce vahe vāto,",
  "ja318:2.4": "sabbampi pathaviṁ vahe;",
  "ja318:2.5": "Yattha sāmā kālakatā,",
  "ja318:2.6": "sā maṁ ārogyamabravi”.",
  "ja318:3.1": "“Na ceva sā kālakatā,",
  "ja318:3.2": "na ca sā aññamicchati;",
  "ja318:3.3": "Ekabhattikinī sāmā,",
  "ja318:3.4": "tameva abhikaṅkhati”.",
  "ja318:4.1": "“Asanthutaṁ maṁ cirasanthutena,",
  "ja318:4.2": "Nimīni sāmā adhuvaṁ dhuvena;",
  "ja318:4.3": "Mayāpi sāmā nimineyya aññaṁ,",
  "ja318:4.4": "Ito ahaṁ dūrataraṁ gamissan”ti.",
  "ja318:5.1": "Kaṇaverajātakaṁ aṭṭhamaṁ."
}