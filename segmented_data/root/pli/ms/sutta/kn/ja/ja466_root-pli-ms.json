{
  "ja466:0.1": "Jātaka",
  "ja466:0.2": "Dvādasakanipāta",
  "ja466:0.3": "Cūḷakuṇālavagga",
  "ja466:0.4": "3. Samuddavāṇijajātaka",
  "ja466:1.1": "“Kasanti vapanti te janā,",
  "ja466:1.2": "Manujā kammaphalūpajīvino;",
  "ja466:1.3": "Nayimassa dīpakassa bhāgino,",
  "ja466:1.4": "Jambudīpā idameva no varaṃ”.",
  "ja466:2.1": "“Tipañcarattūpagatamhi cande,",
  "ja466:2.2": "Vego mahā hehiti sāgarassa;",
  "ja466:2.3": "Uplavissaṃ dīpamimaṃ uḷāraṃ,",
  "ja466:2.4": "Mā vo vadhī gacchatha leṇamaññaṃ”.",
  "ja466:3.1": "“Na jātuyaṃ sāgaravārivego,",
  "ja466:3.2": "Uplavissaṃ dīpamimaṃ uḷāraṃ;",
  "ja466:3.3": "Taṃ me nimittehi bahūhi diṭṭhaṃ,",
  "ja466:3.4": "Mā bhetha kiṃ socatha modathavho.",
  "ja466:4.1": "Pahūtabhakkhaṃ bahuannapānaṃ,",
  "ja466:4.2": "Pattattha āvāsamimaṃ uḷāraṃ;",
  "ja466:4.3": "Na vo bhayaṃ paṭipassāmi kiñci,",
  "ja466:4.4": "Āputtaputtehi pamodathavho”.",
  "ja466:5.1": "“Yo devoyaṃ dakkhiṇāyaṃ disāyaṃ,",
  "ja466:5.2": "Khemanti pakkosati tassa saccaṃ;",
  "ja466:5.3": "Na uttaro vedi bhayābhayassa,",
  "ja466:5.4": "Mā bhetha kiṃ socatha modathavho”.",
  "ja466:6.1": "“Yathā ime vippavadanti yakkhā,",
  "ja466:6.2": "Eko bhayaṃ saṃsati khemameko;",
  "ja466:6.3": "Tadiṅgha mayhaṃ vacanaṃ suṇātha,",
  "ja466:6.4": "Khippaṃ lahuṃ mā vinassimha sabbe.",
  "ja466:7.1": "Sabbe samāgamma karoma nāvaṃ,",
  "ja466:7.2": "Doṇiṃ daḷhaṃ sabbayantūpapannaṃ;",
  "ja466:7.3": "Sace ayaṃ dakkhiṇo saccamāha,",
  "ja466:7.4": "Moghaṃ paṭikkosati uttaroyaṃ;",
  "ja466:7.5": "Sā ceva no hehiti āpadatthā,",
  "ja466:7.6": "Imañca dīpaṃ na pariccajema.",
  "ja466:8.1": "Sace ca kho uttaro saccamāha,",
  "ja466:8.2": "Moghaṃ paṭikkosati dakkhiṇoyaṃ;",
  "ja466:8.3": "Tameva nāvaṃ abhiruyha sabbe,",
  "ja466:8.4": "Evaṃ mayaṃ sotthi taremu pāraṃ.",
  "ja466:9.1": "Na ve sugaṇhaṃ paṭhamena seṭṭhaṃ,",
  "ja466:9.2": "Kaniṭṭhamāpāthagataṃ gahetvā;",
  "ja466:9.3": "Yo cīdha tacchaṃ paviceyya gaṇhati,",
  "ja466:9.4": "Sa ve naro seṭṭhamupeti ṭhānaṃ.",
  "ja466:10.1": "Yathāpi te sāgaravārimajjhe,",
  "ja466:10.2": "Sakammunā sotthi vahiṃsu vāṇijā;",
  "ja466:10.3": "Anāgatatthaṃ paṭivijjhiyāna,",
  "ja466:10.4": "Appampi nācceti sa bhūripañño.",
  "ja466:11.1": "Bālā ca mohena rasānugiddhā,",
  "ja466:11.2": "Anāgataṃ appaṭivijjhiyatthaṃ;",
  "ja466:11.3": "Paccuppanne sīdanti atthajāte,",
  "ja466:11.4": "Samuddamajjhe yathā te manussā.",
  "ja466:12.1": "Anāgataṃ paṭikayirātha kiccaṃ,",
  "ja466:12.2": "‘Mā maṃ kiccaṃ kiccakāle byadhesi’;",
  "ja466:12.3": "Taṃ tādisaṃ paṭikatakiccakāriṃ,",
  "ja466:12.4": "Na taṃ kiccaṃ kiccakāle byadhetī”ti.",
  "ja466:13.1": "Samuddavāṇijajātakaṃ tatiyaṃ."
}