{
  "ja201:0.1": "Jātaka",
  "ja201:0.2": "Dukanipāta",
  "ja201:0.3": "Nataṁdaḷhavagga",
  "ja201:0.4": "1. Bandhanāgārajātaka",
  "ja201:1.1": "“Na taṁ daḷhaṁ bandhanamāhu dhīrā,",
  "ja201:1.2": "Yadāyasaṁ dārujapabbajañca;",
  "ja201:1.3": "Sārattarattā maṇikuṇḍalesu,",
  "ja201:1.4": "Puttesu dāresu ca yā apekkhā.",
  "ja201:2.1": "Etaṁ daḷhaṁ bandhanamāhu dhīrā,",
  "ja201:2.2": "Ohārinaṁ sīthilaṁ duppamuñcaṁ;",
  "ja201:2.3": "Etampi chetvāna vajanti dhīrā,",
  "ja201:2.4": "Anapekkhino kāmasukhaṁ pahāyā”ti.",
  "ja201:3.1": "Bandhanāgārajātakaṁ paṭhamaṁ."
}