{
  "ja387:0.1": "Jātaka",
  "ja387:0.2": "Chakkanipāta",
  "ja387:0.3": "Kharaputtavagga",
  "ja387:0.4": "2. Sūcijātaka",
  "ja387:1.1": "“Akakkasaṁ apharusaṁ,",
  "ja387:1.2": "kharadhotaṁ supāsiyaṁ;",
  "ja387:1.3": "Sukhumaṁ tikhiṇaggañca,",
  "ja387:1.4": "ko sūciṁ ketumicchati.",
  "ja387:2.1": "Sumajjañca supāsañca,",
  "ja387:2.2": "anupubbaṁ suvaṭṭitaṁ;",
  "ja387:2.3": "Ghanaghātimaṁ paṭithaddhaṁ,",
  "ja387:2.4": "ko sūciṁ ketumicchati”.",
  "ja387:3.1": "“Ito dāni patāyanti,",
  "ja387:3.2": "sūciyo baḷisāni ca;",
  "ja387:3.3": "Koyaṁ kammāragāmasmiṁ,",
  "ja387:3.4": "sūciṁ vikketumicchati.",
  "ja387:4.1": "Ito satthāni gacchanti,",
  "ja387:4.2": "Kammantā vividhā puthū;",
  "ja387:4.3": "Koyaṁ kammāragāmasmiṁ,",
  "ja387:4.4": "Sūciṁ vikketumicchati”.",
  "ja387:5.1": "“Sūciṁ kammāragāmasmiṁ,",
  "ja387:5.2": "vikketabbā pajānatā;",
  "ja387:5.3": "Ācariyāva jānanti,",
  "ja387:5.4": "kammaṁ sukatadukkaṭaṁ.",
  "ja387:6.1": "Imañce te pitā bhadde,",
  "ja387:6.2": "Sūciṁ jaññā mayā kataṁ;",
  "ja387:6.3": "Tayā ca maṁ nimanteyya,",
  "ja387:6.4": "Yañcatthaññaṁ ghare dhanan”ti.",
  "ja387:7.1": "Sūcijātakaṁ dutiyaṁ."
}