{
  "ja24:0.1": "Jātaka",
  "ja24:0.2": "Ekakanipāta",
  "ja24:0.3": "Kuruṅgavagga",
  "ja24:0.4": "4. Ājaññajātaka",
  "ja24:1.1": "“Yadā yadā yattha yadā,",
  "ja24:1.2": "yattha yattha yadā yadā;",
  "ja24:1.3": "Ājañño kurute vegaṁ,",
  "ja24:1.4": "hāyanti tattha vāḷavā”ti.",
  "ja24:2.1": "Ājaññajātakaṁ catutthaṁ."
}