{
  "ja446:0.1": "Jātaka",
  "ja446:0.2": "Dasakanipāta",
  "ja446:0.3": "Catudvāravagga",
  "ja446:0.4": "8. Takkalajātaka",
  "ja446:1.1": "“Na takkalā santi na āluvāni,",
  "ja446:1.2": "Na biḷāliyo na kaḷambāni tāta;",
  "ja446:1.3": "Eko araññamhi susānamajjhe,",
  "ja446:1.4": "Kimatthiko tāta khaṇāsi kāsuṁ”.",
  "ja446:2.1": "“Pitāmaho tāta sudubbalo te,",
  "ja446:2.2": "Anekabyādhīhi dukhena phuṭṭho;",
  "ja446:2.3": "Tamajjahaṁ nikhaṇissāmi sobbhe,",
  "ja446:2.4": "Na hissa taṁ jīvitaṁ rocayāmi”.",
  "ja446:3.1": "“Saṅkappametaṁ paṭiladdha pāpakaṁ,",
  "ja446:3.2": "Accāhitaṁ kamma karosi luddaṁ;",
  "ja446:3.3": "Mayāpi tāta paṭilacchase tuvaṁ,",
  "ja446:3.4": "Etādisaṁ kamma jarūpanīto;",
  "ja446:3.5": "Taṁ kullavattaṁ anuvattamāno,",
  "ja446:3.6": "Ahampi taṁ nikhaṇissāmi sobbhe”.",
  "ja446:4.1": "“Pharusāhi vācāhi pakubbamāno,",
  "ja446:4.2": "Āsajja maṁ tvaṁ vadase kumāra;",
  "ja446:4.3": "Putto mamaṁ orasako samāno,",
  "ja446:4.4": "Ahitānukampī mama tvaṁsi putta”.",
  "ja446:5.1": "“Na tāhaṁ tāta ahitānukampī,",
  "ja446:5.2": "Hitānukampī te ahampi tāta;",
  "ja446:5.3": "Pāpañca taṁ kamma pakubbamānaṁ,",
  "ja446:5.4": "Arahāmi no vārayituṁ tato”.",
  "ja446:6.1": "“Yo mātaraṁ vā pitaraṁ saviṭṭha,",
  "ja446:6.2": "Adūsake hiṁsati pāpadhammo;",
  "ja446:6.3": "Kāyassa bhedā abhisamparāyaṁ,",
  "ja446:6.4": "Asaṁsayaṁ so nirayaṁ upeti.",
  "ja446:7.1": "Yo mātaraṁ vā pitaraṁ saviṭṭha,",
  "ja446:7.2": "Annena pānena upaṭṭhahāti;",
  "ja446:7.3": "Kāyassa bhedā abhisamparāyaṁ,",
  "ja446:7.4": "Asaṁsayaṁ so sugatiṁ upeti.",
  "ja446:8.1": "Na me tvaṁ putta ahitānukampī,",
  "ja446:8.2": "Hitānukampī me tvaṁsi putta;",
  "ja446:8.3": "Ahañca taṁ mātarā vuccamāno,",
  "ja446:8.4": "Etādisaṁ kamma karomi luddaṁ”.",
  "ja446:9.1": "“Yā te sā bhariyā anariyarūpā,",
  "ja446:9.2": "Mātā mamesā sakiyā janetti;",
  "ja446:9.3": "Niddhāpaye tañca sakā agārā,",
  "ja446:9.4": "Aññampi te sā dukhamāvaheyya.",
  "ja446:10.1": "Yā te sā bhariyā anariyarūpā,",
  "ja446:10.2": "Mātā mamesā sakiyā janetti;",
  "ja446:10.3": "Dantā kareṇūva vasūpanītā,",
  "ja446:10.4": "Sā pāpadhammā punarāvajātū”ti.",
  "ja446:11.1": "Takkalajātakaṁ aṭṭhamaṁ."
}