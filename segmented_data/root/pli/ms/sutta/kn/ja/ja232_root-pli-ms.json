{
  "ja232:0.1": "Jātaka",
  "ja232:0.2": "Dukanipāta",
  "ja232:0.3": "Upāhanavagga",
  "ja232:0.4": "2. Vīṇāguṇajātaka",
  "ja232:1.1": "“Ekacintito yamattho,",
  "ja232:1.2": "bālo apariṇāyako;",
  "ja232:1.3": "Na hi khujjena vāmena,",
  "ja232:1.4": "bhoti saṅgantumarahasi”.",
  "ja232:2.1": "“Purisūsabhaṁ maññamānā,",
  "ja232:2.2": "ahaṁ khujjamakāmayiṁ;",
  "ja232:2.3": "Soyaṁ saṅkuṭito seti,",
  "ja232:2.4": "chinnatanti yathā viṇā”ti.",
  "ja232:3.1": "Vīṇāguṇajātakaṁ dutiyaṁ."
}