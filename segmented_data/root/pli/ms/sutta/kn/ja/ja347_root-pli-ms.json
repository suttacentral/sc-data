{
  "ja347:0.1": "Jātaka",
  "ja347:0.2": "Catukkanipāta",
  "ja347:0.3": "Cūḷakuṇālavagga",
  "ja347:0.4": "7. Ayakūṭajātaka",
  "ja347:1.1": "“Sabbāyasaṁ kūṭamatippamāṇaṁ,",
  "ja347:1.2": "Paggayha yo tiṭṭhasi antalikkhe;",
  "ja347:1.3": "Rakkhāya me tvaṁ vihito nusajja,",
  "ja347:1.4": "Udāhu me cetayase vadhāya”.",
  "ja347:2.1": "“Dūto ahaṁ rājidha rakkhasānaṁ,",
  "ja347:2.2": "Vadhāya tuyhaṁ pahitohamasmi;",
  "ja347:2.3": "Indo ca taṁ rakkhati devarājā,",
  "ja347:2.4": "Tenuttamaṅgaṁ na te phālayāmi”.",
  "ja347:3.1": "“Sace ca maṁ rakkhati devarājā,",
  "ja347:3.2": "Devānamindo maghavā sujampati;",
  "ja347:3.3": "Kāmaṁ pisācā vinadantu sabbe,",
  "ja347:3.4": "Na santase rakkhasiyā pajāya.",
  "ja347:4.1": "Kāmaṁ kandantu kumbhaṇḍā,",
  "ja347:4.2": "Sabbe paṁsupisācakā;",
  "ja347:4.3": "Nālaṁ pisācā yuddhāya,",
  "ja347:4.4": "Mahatī sā vibhiṁsikā”ti.",
  "ja347:5.1": "Ayakūṭajātakaṁ sattamaṁ."
}