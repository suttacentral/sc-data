{
  "ja477:0.1": "Jātaka",
  "ja477:0.2": "Terasakanipāta",
  "ja477:0.3": "Ambavagga",
  "ja477:0.4": "4. Cūḷanāradajātaka",
  "ja477:1.1": "“Na te kaṭṭhāni bhinnāni,",
  "ja477:1.2": "na te udakamābhataṁ;",
  "ja477:1.3": "Aggīpi te na hāpito,",
  "ja477:1.4": "kiṁ nu mandova jhāyasi”.",
  "ja477:2.1": "“Na ussahe vane vatthuṁ,",
  "ja477:2.2": "kassapāmantayāmi taṁ;",
  "ja477:2.3": "Dukkho vāso araññasmiṁ,",
  "ja477:2.4": "raṭṭhaṁ icchāmi gantave.",
  "ja477:3.1": "Yathā ahaṁ ito gantvā,",
  "ja477:3.2": "Yasmiṁ janapade vasaṁ;",
  "ja477:3.3": "Ācāraṁ brahme sikkheyyaṁ,",
  "ja477:3.4": "Taṁ dhammaṁ anusāsa maṁ”.",
  "ja477:4.1": "“Sace araññaṁ hitvāna,",
  "ja477:4.2": "vanamūlaphalāni ca;",
  "ja477:4.3": "Raṭṭhe rocayase vāsaṁ,",
  "ja477:4.4": "taṁ dhammaṁ nisāmehi me.",
  "ja477:5.1": "Visaṁ mā paṭisevittho,",
  "ja477:5.2": "papātaṁ parivajjaya;",
  "ja477:5.3": "Paṅke ca mā visīdittho,",
  "ja477:5.4": "yatto cāsīvise care”.",
  "ja477:6.1": "“Kiṁ nu visaṁ papāto vā,",
  "ja477:6.2": "paṅko vā brahmacārinaṁ;",
  "ja477:6.3": "Kaṁ tvaṁ āsīvisaṁ brūsi,",
  "ja477:6.4": "taṁ me akkhāhi pucchito”.",
  "ja477:7.1": "“Āsavo tāta lokasmiṁ,",
  "ja477:7.2": "surā nāma pavuccati;",
  "ja477:7.3": "Manuñño surabhī vaggu,",
  "ja477:7.4": "sādu khuddarasūpamo;",
  "ja477:7.5": "Visaṁ tadāhu ariyā se,",
  "ja477:7.6": "brahmacariyassa nārada.",
  "ja477:8.1": "Itthiyo tāta lokasmiṁ,",
  "ja477:8.2": "pamattaṁ pamathenti tā;",
  "ja477:8.3": "Haranti yuvino cittaṁ,",
  "ja477:8.4": "tūlaṁ bhaṭṭhaṁva māluto;",
  "ja477:8.5": "Papāto eso akkhāto,",
  "ja477:8.6": "brahmacariyassa nārada.",
  "ja477:9.1": "Lābho siloko sakkāro,",
  "ja477:9.2": "pūjā parakulesu ca;",
  "ja477:9.3": "Paṅko eso ca akkhāto,",
  "ja477:9.4": "brahmacariyassa nārada.",
  "ja477:10.1": "Sasatthā tāta rājāno,",
  "ja477:10.2": "āvasanti mahiṁ imaṁ;",
  "ja477:10.3": "Te tādise manussinde,",
  "ja477:10.4": "mahante tāta nārada.",
  "ja477:11.1": "Issarānaṁ adhipatīnaṁ,",
  "ja477:11.2": "na tesaṁ pādato care;",
  "ja477:11.3": "Āsīvisoti akkhāto,",
  "ja477:11.4": "brahmacariyassa nārada.",
  "ja477:12.1": "Bhattattho bhattakāle ca,",
  "ja477:12.2": "yaṁ gehamupasaṅkame;",
  "ja477:12.3": "Yadettha kusalaṁ jaññā,",
  "ja477:12.4": "tattha ghāsesanaṁ care.",
  "ja477:13.1": "Pavisitvā parakulaṁ,",
  "ja477:13.2": "pānatthaṁ bhojanāya vā;",
  "ja477:13.3": "Mitaṁ khāde mitaṁ bhuñje,",
  "ja477:13.4": "na ca rūpe manaṁ kare.",
  "ja477:14.1": "Goṭṭhaṁ majjaṁ kirāṭañca,",
  "ja477:14.2": "sabhā nikiraṇāni ca;",
  "ja477:14.3": "Ārakā parivajjehi,",
  "ja477:14.4": "yānīva visamaṁ pathan”ti.",
  "ja477:15.1": "Cūḷanāradajātakaṁ catutthaṁ."
}