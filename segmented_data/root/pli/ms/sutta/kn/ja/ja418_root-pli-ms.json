{
  "ja418:0.1": "Jātaka",
  "ja418:0.2": "Aṭṭhakanipāta",
  "ja418:0.3": "Kaccānivagga",
  "ja418:0.4": "2. Aṭṭhasaddajātaka",
  "ja418:1.1": "“Idaṁ pure ninnamāhu,",
  "ja418:1.2": "bahumacchaṁ mahodakaṁ;",
  "ja418:1.3": "Āvāso bakarājassa,",
  "ja418:1.4": "pettikaṁ bhavanaṁ mama;",
  "ja418:1.5": "Tyajja bhekena yāpema,",
  "ja418:1.6": "okaṁ na vijahāmase”.",
  "ja418:2.1": "“Ko dutiyaṁ asīlissa,",
  "ja418:2.2": "bandharassakkhi bhecchati;",
  "ja418:2.3": "Ko me putte kulāvakaṁ,",
  "ja418:2.4": "mañca sotthiṁ karissati”.",
  "ja418:3.1": "“Sabbā parikkhayā pheggu,",
  "ja418:3.2": "yāva tassā gatī ahu;",
  "ja418:3.3": "Khīṇabhakkho mahārāja,",
  "ja418:3.4": "sāre na ramatī ghuṇo”.",
  "ja418:4.1": "“Sā nūnāhaṁ ito gantvā,",
  "ja418:4.2": "rañño muttā nivesanā;",
  "ja418:4.3": "Attānaṁ ramayissāmi,",
  "ja418:4.4": "dumasākhaniketinī”.",
  "ja418:5.1": "“So nūnāhaṁ ito gantvā,",
  "ja418:5.2": "rañño mutto nivesanā;",
  "ja418:5.3": "Aggodakāni pissāmi,",
  "ja418:5.4": "yūthassa purato vajaṁ”.",
  "ja418:6.1": "“Taṁ maṁ kāmehi sammattaṁ,",
  "ja418:6.2": "rattaṁ kāmesu mucchitaṁ;",
  "ja418:6.3": "Ānayī bharato luddo,",
  "ja418:6.4": "bāhiko bhaddamatthu te”.",
  "ja418:7.1": "“Andhakāratimisāyaṁ,",
  "ja418:7.2": "tuṅge uparipabbate;",
  "ja418:7.3": "Sā maṁ saṇhena mudunā,",
  "ja418:7.4": "mā pādaṁ khali yasmani”.",
  "ja418:8.1": "“Asaṁsayaṁ jātikhayantadassī,",
  "ja418:8.2": "Na gabbhaseyyaṁ punarāvajissaṁ;",
  "ja418:8.3": "Ayamantimā pacchimā gabbhaseyyā,",
  "ja418:8.4": "Khīṇo me saṁsāro punabbhavāyā”ti.",
  "ja418:9.1": "Aṭṭhasaddajātakaṁ dutiyaṁ."
}