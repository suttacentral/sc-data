{
  "ja52:0.1": "Jātaka",
  "ja52:0.2": "Ekakanipāta",
  "ja52:0.3": "Āsīsavagga",
  "ja52:0.4": "2. Cūḷajanakajātaka",
  "ja52:1.1": "“Vāyametheva puriso,",
  "ja52:1.2": "na nibbindeyya paṇḍito;",
  "ja52:1.3": "Passāmi vohaṁ attānaṁ,",
  "ja52:1.4": "udakā thalamubbhatan”ti.",
  "ja52:2.1": "Cūḷajanakajātakaṁ dutiyaṁ."
}