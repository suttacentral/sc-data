{
  "ja46:0.1": "Jātaka",
  "ja46:0.2": "Ekakanipāta",
  "ja46:0.3": "Atthakāmavagga",
  "ja46:0.4": "6. Ārāmadūsakajātaka",
  "ja46:1.1": "“Na ve anatthakusalena,",
  "ja46:1.2": "atthacariyā sukhāvahā;",
  "ja46:1.3": "Hāpeti atthaṁ dummedho,",
  "ja46:1.4": "kapi ārāmiko yathā”ti.",
  "ja46:2.1": "Ārāmadūsakajātakaṁ chaṭṭhaṁ."
}