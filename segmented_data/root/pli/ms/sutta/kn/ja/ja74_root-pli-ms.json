{
  "ja74:0.1": "Jātaka",
  "ja74:0.2": "Ekakanipāta",
  "ja74:0.3": "Varuṇavagga",
  "ja74:0.4": "4. Rukkhadhammajātaka",
  "ja74:1.1": "“Sādhū sambahulā ñātī,",
  "ja74:1.2": "api rukkhā araññajā;",
  "ja74:1.3": "Vāto vahati ekaṭṭhaṁ,",
  "ja74:1.4": "brahantampi vanappatin”ti.",
  "ja74:2.1": "Rukkhadhammajātakaṁ catutthaṁ."
}